    <page filename="BL615d7.001v.tif" pagination="[1v]" reader="John Dee" signature="[A[v]]"/> 
    <annotation>
        
        <underline language="IT" method="pen" text="discono alcune cose, o le diminuiscono" type="straight"/>
        <underline language="IT" method="pen" text="taciono quello, che giustamente" type="straight"/>
        <underline language="IT" method="pen" text="scriuere con molta particolarità" type="straight"/>
        <underline language="IT" method="pen" text="mia scrittura si" type="straight"/>
        <underline language="IT" method="pen" text="disetto, nó ci sarà quello" type="straight"/>
        <underline language="IT" method="pen" text="ch'è la poca, &amp; incerta verità di quello, che scriuono" type="straight"/> 
        <underline language="IT" method="pen" text="gli scritti, &amp; lettere" type="straight"/>
        
    </annotation>
