    <annotation>
        
        <marginalia hand="Italian" method="pen">
            <language ident="EN">
                <position book_orientation="0" place="head">
                    <marginalia_text>before he entered at his charge</marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <underline language="IT" method="pen" text="ad vna, ò a tre persone, ch'egli" type="straight"/>
        <underline language="IT" method="pen" text="contrattasse con l'Indie, egli mettesse guidici, che guidicassero sopra quello" type="straight"/>
        <underline language="IT" method="pen" text="à tal materia" type="straight"/>
        <underline language="IT" method="pen" text="Ammiraglio" type="straight"/>
        <underline language="IT" method="pen" text="Vicere" type="straight"/>
        <underline language="IT" method="pen" text="Gouernatore" type="straight"/>
        <underline language="IT" method="pen" text="dimandò il decimo di tutto quello, che si comprasse, barattasse, si trouasse, si guadagnasse" type="straight"/>
        <underline language="IT" method="pen" text="dentro de' confini del suo Ammiragliato" type="straight"/>
        <underline language="IT" method="pen" text="le spese fatte in acquistarlo" type="straight"/>
        <underline language="IT" method="pen" text="Capitano di vna" type="straight"/>
        <underline language="IT" method="pen" text="fusse data l'ottaua parte di" type="straight"/>
        <underline language="IT" method="pen" text="Altezze loro non volendo concederle: l'Ammiraglio" type="straight"/>
        <underline language="IT" method="pen" text="data in Francia" type="straight"/>
        <underline language="IT" method="pen" text="che il Re glie" type="straight"/>
        
