        <underline language="IT" method="pen" text="con lagrime mostrò" type="straight"/>
        <underline language="IT" method="pen" text="dolore del nostro" type="straight"/>
        <underline language="IT" method="pen" text="a scaricare" type="double"/>
        <underline language="IT" method="pen" text="scaricammo tutta" type="straight"/>
        <underline language="IT" method="pen" text="breue spatio di tempo" type="straight"/>
        <underline language="IT" method="pen" text="aiuto, che questo Re" type="straight"/>
        <underline language="IT" method="pen" text="persona co' suoi" type="straight"/>
        <underline language="IT" method="pen" text="diligenza, cosi" type="straight"/>
        <underline language="IT" method="pen" text="piagendo" type="straight"/>
        <underline language="IT" method="pen" text="che io non mi pre[n]dessi fastidio" type="straight"/>
        <underline language="IT" method="pen" text="donerebbe tutto qnello, che haueua" type="straight"/>
        <underline language="IT" method="pen" text="di Castiglia non si haurebbe" type="straight"/>
        <underline language="IT" method="pen" text="gouerno per" type="straight"/>
        <underline language="IT" method="pen" text="cò pure vna stringa" type="straight"/>
        <underline language="IT" method="pen" text="robbe egli fece" type="straight"/>
        <underline language="IT" method="pen" text="palazzo" type="straight"/>
        <underline language="IT" method="pen" text="ch'ei voleua dar per saluarle" type="straight"/>
        <underline language="IT" method="pen" text="huomini armati" type="straight"/>
        <underline language="IT" method="pen" text="tutta le notte" type="straight"/>
        <underline language="IT" method="pen" text="terra piangeuano" type="straight"/>
        <underline language="IT" method="pen" text="è la gente amoreuole, &amp; senza auidità, e trattabile" type="straight"/>
