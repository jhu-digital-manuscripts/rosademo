        <underline language="IT" method="pen" text="Castiglia con soccorso" type="double"/>
        <underline language="IT" method="pen" text="chi il guidasse" type="triple"/>
        <underline language="IT" method="pen" text="popolatione, &amp; dominio" type="double"/>
        <underline language="IT" method="pen" text="gli si offeriuano" type="straight"/>
        <underline language="IT" method="pen" text="vi sariano restati" type="straight"/>
        <underline language="IT" method="pen" text="fabricarui vna torre col legname della naue perduta, di cui niuna cosa lasciò che no[n] cauasse fuori" type="straight"/>
        <underline language="IT" method="pen" text="non ne tragesse qualche vtile" type="straight"/>
        <underline language="IT" method="pen" text="Carauella Pinta" type="straight"/>
        <underline language="IT" method="pen" text="Guacanagari" type="straight"/>
        <underline language="IT" method="pen" text="XX leghe" type="straight"/>
        <underline language="IT" method="pen" text="che non fu data" type="double"/>
        <underline language="IT" method="pen" text="auanti venduta" type="straight"/>
        <underline language="IT" method="pen" text="al rimaner de' Christiani in quel luogo" type="straight"/>
        <underline language="IT" method="pen" text="quali ogni di più conosceuano la bontà, &amp; ricchezza di quella terra, portando" type="straight"/>
        <underline language="IT" method="pen" text="di molte Prouincie" type="straight"/>
        <underline language="IT" method="pen" text="cotal'oro nasceua" type="straight"/>
        
