        <underline language="IT" method="pen" text="chiamarono S. Martino" type="straight"/>
        <underline language="IT" method="pen" text="cauauano pezzi di corallo" type="straight"/>
        <underline language="IT" method="pen" text="per andare à soccorrer quelli, ch'egli haueua" type="straight"/>
        <underline language="IT" method="pen" text="che si prendesse" type="straight"/>
        <underline language="IT" method="pen" text="per saper" type="straight"/>
        <underline language="IT" method="pen" text="quattro donne" type="straight"/>
        <underline language="IT" method="pen" text="tre fanciulli" type="straight"/>
        <underline language="IT" method="pen" text="che la donna paßò" type="straight"/>
        <underline language="IT" method="pen" text="targa dall'vn lato all'altro" type="straight"/>
        <underline language="IT" method="pen" text="tiraua di molte freccie" type="straight"/>
        <underline language="IT" method="pen" text="fosse stato in terra" type="straight"/>
        <underline language="IT" method="pen" text="e poi castrati" type="double"/>
        <underline language="IT" method="pen" text="ingrassino" type="straight"/>
        <underline language="IT" method="pen" text="siano piu saporiti" type="double"/>
        
        
        
