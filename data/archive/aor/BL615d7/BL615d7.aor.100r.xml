        <underline language="IT" method="pen" text="Ammiraglio vn fratello del" type="straight"/>
        <underline language="IT" method="pen" text="sapeuano esprimere alcune parole in" type="straight"/>
        <underline language="IT" method="pen" text="per nome tutti i" type="straight"/>
        <underline language="IT" method="pen" text="Christiani" type="double"/>
        <underline language="IT" method="pen" text="luogo rimasi" type="double"/>
        <underline language="IT" method="pen" text="i Christiani subito incominciarono a venire in discordia tra loro, &amp; a pigliar ciascuno le donne" type="straight"/>
        <underline language="IT" method="pen" text="l'oro" type="double"/>
        <underline language="IT" method="pen" text="che egli poteua: &amp;" type="straight"/>
        <underline language="IT" method="pen" text="vccisero vn Giacopo" type="straight"/>
        <underline language="IT" method="pen" text="poscia insieme con altri noue" type="straight"/>
        <underline language="IT" method="pen" text="andati con le loro donne ad vn Cacique, chiamato Caunabo" type="straight"/>
        <underline language="IT" method="pen" text="delle minere" type="straight"/>
        <underline language="IT" method="pen" text="questi gli amazzò, &amp;" type="straight"/>
        <underline language="IT" method="pen" text="gente al Natale" type="straight"/>
        <underline language="IT" method="pen" text="oue non era altri, che Diego di Arana con dieci persone" type="straight"/>
        <underline language="IT" method="pen" text="s'erano sparsi per diuersi luoghi" type="straight"/>
        <underline language="IT" method="pen" text="il Caunabo" type="straight"/>
        <underline language="IT" method="pen" text="quali i Christiani habitauano con le loro donne" type="straight"/>
        <underline language="IT" method="pen" text="si annegarono otto" type="straight"/>
        <underline language="IT" method="pen" text="tre perirono" type="straight"/>
        <underline language="IT" method="pen" text="che non mostrauano" type="straight"/>
        <underline language="IT" method="pen" text="per difendere i Christiani, col Caunabo, fu ferito, &amp; se ne fuggì" type="straight"/>
        
