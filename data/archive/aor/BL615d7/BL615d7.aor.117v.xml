        <underline language="IT" method="pen" text="Capo di Croce" type="straight"/>
        <underline language="IT" method="pen" text="doue da gl'Indiani fu raccolto amicheuolme[n]te" type="straight"/>
        <underline language="IT" method="pen" text="Questi gli portarono molto" type="straight"/>
        <underline language="IT" method="pen" text="fertilità, la quale hauea eccellenti porti di lega in lega, &amp; tutta la Costa piena di popolationi" type="straight"/>
        <underline language="IT" method="pen" text="portando delle vettouaglie" type="straight"/>
        <underline language="IT" method="pen" text="Christiani assai migliori stimate di quelle" type="straight"/>
        <underline language="IT" method="pen" text="tutte l'altre Isole haueano" type="straight"/>
        <underline language="IT" method="pen" text="Giamaica ogni dì sull'hora del vespro si apparecchiaua vn nembo con" type="straight"/>
        <underline language="IT" method="pen" text="vn'hora, &amp; alcuna volta più" type="straight"/>
        <underline language="IT" method="pen" text="attribuiua alle gran selue" type="straight"/>
        
