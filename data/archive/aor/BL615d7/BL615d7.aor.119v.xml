        <marginalia hand="Italian" method="pen">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>Jo[ha]n[n]es de Regio monte ponit hanc eclips[em] die 14. hor[ae] 19 m[inutae] 45 ad meridia[m] vt mense[m]. Iste verò ponit die 15 nocte.</marginalia_text> 
                    <person name="Regiomontanus"/>
                </position>
            </language>
            <translation>Regiomontanus sets this eclipse on the fourteenth day, nineteenth hour, 45 minutes, toward midday, as the month. Actually, he places [it] in the night of the fifteenth day.</translation>  <!--JG: Unsure abt translation-->
        </marginalia>
        
