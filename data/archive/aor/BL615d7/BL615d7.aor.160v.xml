    <annotation>
        
        <marginalia anchor_text="I quali, intesa la buona relatione, che questi gli fecero, tutti con le lor Canoe uennero a' nauigli a barattar di quelle cose" hand="Italian">
            <language ident="EN">
                <position book_orientation="0" place="left_margin">
