                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Oh Traitor</marginalia_text> 
                </position>
            </language>
        </marginalia>
        
        <marginalia anchor_text="Manicauter" hand="Italian">
            <language ident="IT">
                <position book_orientation="0" place="right_margin">
