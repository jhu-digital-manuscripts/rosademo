                    <person name="Manicauter"/> <!-- M.B. a 'Cacique'/chieftian. -->
                </position>
            </language>
        </marginalia>
        
        <marginalia anchor_text="percioche peso non hebbero mai" hand="Italian">
            <language ident="EN">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>No waights</marginalia_text> 
                </position>
            </language>
        </marginalia>
        
        <underline language="IT" method="pen" text="gridando, ch'erano protettori de gl'Indiani: doue le opere" type="straight"/>
        <underline language="IT" method="pen" text="loro era di Ladroni" type="straight"/>
        <underline language="IT" method="pen" text="percioche ogniun rubbaua quel, che poteua, &amp; l'Orla[n]do loro Capo rubbaua piu di tutti" type="straight"/>
        <underline language="IT" method="pen" text="percioch'egli volea difender gl'Indiani, &amp; i ribellati" type="straight"/>
        <underline language="IT" method="pen" text="dimandaua a loro" type="straight"/>
        <underline language="IT" method="pen" text="Manicauter" type="straight"/>
        <underline language="IT" method="pen" text="tenea vn suo figliuolo, &amp; vn nepote appresso di se" type="straight"/>
        <underline language="IT" method="pen" text="percioche peso non hebbero mai" type="straight"/>
        <underline language="IT" method="pen" text="cosi diuisi i Christiani" type="straight"/>
        
        <mark method="pen" name="tripunctus" place="right_margin"/>       
        
