        <underline language="IT" method="pen" text="volte sparar qualche pezzo di artiglieria: al qual" type="straight"/>
        <underline language="IT" method="pen" text="haueano paura del suo romore, perche in effetto pensauano, che sol fossero tuoni per cagionare spauento" type="straight"/>
        <underline language="IT" method="pen" text="hauessero tanta superbia, ne sprezzassero i Christiani" type="straight"/>
        <underline language="IT" method="pen" text="fece loro conoscere, che quella burla era cosi fulmine, come tuono" type="straight"/>
        <underline language="IT" method="pen" text="paese la piu disposta, che fino" type="straight"/>
        <underline language="IT" method="pen" text="no alti, &amp; asciutti" type="straight"/>
        <underline language="IT" method="pen" text="alcuna go[n]fiezza" type="straight"/>
        <underline language="IT" method="pen" text="musco del mondo" type="straight"/>
        <underline language="IT" method="pen" text="tanto carnefici, &amp; crudeli, che prendono vn'huomo" type="straight"/>
        
