    <page filename="BL615d7.237r.tif" pagination="237" reader="John Dee"/>
    <annotation>
        
        <underline language="IT" method="pen" text="la nostra paga, &amp; riscatto" type="straight"/>
        <underline language="IT" method="pen" text="quella notte attenti" type="straight"/>
        <underline language="IT" method="pen" text="venir fuori adirata, e infiammata" type="straight"/>
        <underline language="IT" method="pen" text="apparir della Luna l'Eclissi" type="straight"/>
        <underline language="IT" method="pen" text="ascendeua, aumentando" type="straight"/>
        <underline language="IT" method="pen" text="e strida d'ogni parte veniuano" type="straight"/>
        <underline language="IT" method="pen" text="che in ogni modo intercedesse per loro appresso Dio" type="straight"/>
        <underline language="IT" method="pen" text="disse di volere un poco parlar col suo Dio; &amp;" type="straight"/>
        <underline language="IT" method="pen" text="che douesse aiutarli" type="straight"/>
        <underline language="IT" method="pen" text="tornerebbe a scemare, venne fuor della" type="straight"/>
        
        <errata amendedtext="Indiani" copytext="India ni" language="IT"/>
        
    </annotation>
