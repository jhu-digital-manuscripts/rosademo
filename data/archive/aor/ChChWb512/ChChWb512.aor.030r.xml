        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="270" place="tail">
                    <marginalia_text>
                        A Brenno ergo ad [Cassibelatu[m]?] interstitiu[m] annor[um] 223:
                    </marginalia_text> 
                    <person name="Brennus"/>
                    <person name="Cassibelanus"/>
                </position>
            </language>
            <translation>Therefore from Brenne to Cassibelan a span of 223 years.</translation>
        </marginalia>
        
