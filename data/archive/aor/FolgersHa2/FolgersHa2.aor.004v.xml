            <language ident="LA">
                <position book_orientation="0" place="intext">
                    <marginalia_text>
                        = Semper idem.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Always the same.
            </translation>
        </marginalia>
        <marginalia hand="Italian">
            <language ident="EN">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        Still extent, &amp; multiplication: but refined, &amp; cumlie.
                    </marginalia_text>
                    <emphasis method="chalk" type="straight"/>
                </position>
            </language>
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        nihil vanum.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                nothing in vain.
            </translation>
        </marginalia>
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="intext">
                    <marginalia_text>
                        dextrè.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                skillfully.
            </translation>
        </marginalia>
        <marginalia hand="Italian">
            <language ident="EN">
                <position book_orientation="0" place="intext">
                    <marginalia_text>
                        ) An extent, &amp; emproouement in all deeds
                    </marginalia_text>
                </position>
            </language>
        </marginalia>
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="intext">
                    <marginalia_text>
                        su[pra] /
                    </marginalia_text>
                </position>
            </language>
            <translation>
                above
            </translation>
        </marginalia>
        <marginalia hand="Italian">
            <language ident="EN">
                <position book_orientation="0" place="intext">
                    <marginalia_text>
                       Discourse, &amp; dispatch attonce.
                    </marginalia_text>
                </position>
            </language>
        </marginalia>
        <marginalia hand="Italian">
            <language ident="EN">
                <position book_orientation="270" place="right_margin">
                    <marginalia_text>
