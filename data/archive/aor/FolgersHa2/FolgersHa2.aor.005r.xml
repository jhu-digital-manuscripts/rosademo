    	<underline language="IT" method="pen" text="furono affogati" type="straight"/>
    	<underline language="IT" method="pen" text="le truffe, che" type="straight"/>
    	<underline language="IT" method="pen" text="il Borgomastro se n'andò" type="straight"/>
     
