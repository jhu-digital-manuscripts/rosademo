            	<!-- Marginalia -->
    		
        		<marginalia hand="Italian">
        			<language ident="EN">
            			<position book_orientation="0" place="head">
                    				<marginalia_text>
                    					Witt dailie prouides for the maine chauner
                    					of his
                      			            </marginalia_text>
            				<emphasis method="chalk" type="straight"/>
            			</position>
        				<position book_orientation="0" place="left_margin">
        					<marginalia_text>
        						boddie, and state.
        					</marginalia_text>
        				</position>
        			</language>
        		</marginalia>
    		<marginalia hand="Italian">
    			<language ident="LA">
    				<position book_orientation="0" place="intext">
    					<marginalia_text>
    						scitè, et profundè.
    					</marginalia_text>
    				</position>
    			</language>
    			<translation>
    				Knowingly, and profoundly.
    			</translation>
    		</marginalia>
    		<marginalia hand="Italian">
    			<language ident="LA">
    				<position book_orientation="0" place="intext">
    					<marginalia_text>
    						Qui à seipso non dependet, nullus est.
    					</marginalia_text>
    					<emphasis method="chalk" type="straight"/>
    				</position>
    			</language>
    			<translation>
    				He who does not depend on himself is nothing.
    			</translation>
    		</marginalia>
    		<marginalia hand="Italian">
    			<language ident="LA">
    				<position book_orientation="0" place="intext">
    					<marginalia_text>
    						Qui ab alio dependet, accidens est, non substantia.
    					</marginalia_text>
    					<emphasis method="chalk" type="straight"/>
    				</position>
    			</language>
    			<translation>
    				He who depends on another is secondary, not fundamental. 
    			</translation>
    		</marginalia>
    		<marginalia hand="Italian">
    			<language ident="LA">
    				<position book_orientation="0" place="left_margin">
    					<marginalia_text>
    						Alterius adiunctum, non subiectum sui. 
    					</marginalia_text>
    					<emphasis method="chalk" type="straight"/>
    				</position>
    				<position book_orientation="0" place="tail">
    					<marginalia_text>
    						Homo nullius pra[e]dicamenti:
    						quia accidens non subsistit sine substantia.    						
    					</marginalia_text>
    				</position>
    			</language>
    			<translation>
    				Another's addition, not his own subject. A man of no predication: 
    				because secondary qualities cannot exist without a foundation. 
    			</translation>
    		</marginalia>
    		
    		<marginalia hand="Italian">
    			<language ident="EN">
    				<position book_orientation="270" place="left_margin">
    					<marginalia_text>
    						All great volumes, in one little
    						Epitome. All dead autours, in one liuing Center.
    					</marginalia_text>
    					<emphasis method="chalk" type="straight"/>
    				</position>
    			</language>
    		</marginalia>
    		
