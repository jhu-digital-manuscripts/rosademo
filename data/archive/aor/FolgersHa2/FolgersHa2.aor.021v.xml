    	<annotation>
    		
        		<marginalia hand="Italian">
        			<language ident="LA">
            			<position book_orientation="0" place="head">
                    				<marginalia_text>
                    					Optimus maximus Rationis et Diligentia[e] respectus.
                      			</marginalia_text>
            			</position>
        				<position book_orientation="0" place="left_margin">
        					<marginalia_text>
        						Cum Decori discretiss[im]a et Cautela[e] circu[m]spectissima arte.
        					</marginalia_text>
        					<emphasis method="chalk" type="straight"/>
        					<emphasis method="chalk" type="straight"/>
        				</position>
        			</language>
        			<translation>
        				The best and greatest regard for Reason and Diligence. With the most discreet art of Decorum and the most circumspect art of Caution.
        			</translation>
        		</marginalia>
    		<marginalia hand="Italian">
    			<language ident="IT">
