    		<marginalia hand="Italian">
    			<language ident="LA">
    				<position book_orientation="0" place="intext">
    					<marginalia_text>
    						Sublata causa, tollitur Effectus.
    					</marginalia_text>
    				</position>
    			</language>
    			<translation>
    				With the cause removed, the effect is removed.
    			</translation>
    		</marginalia>
    		<marginalia hand="Italian">
