        		<marginalia hand="Italian">
        			<language ident="LA">
            			<position book_orientation="0" place="head">
                    				<marginalia_text>
                    					Cum agitur maximè, tum viuitur maximè.
                      			</marginalia_text>
            			</position>
        			</language>
        			<translation>
        				When one acts the most, then one lives the most.
        			</translation>
        		</marginalia>
