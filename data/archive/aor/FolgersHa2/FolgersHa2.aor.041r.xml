	<page filename="FolgersHa2.041r.tif" pagination="401" reader="Harvey"/>	<!-- ABO 45right -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						eleganter.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Elegantly.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Idem Semper omnium alacerrimus: agillimus:
					</marginalia_text>
					<person name="Eutrapelus"/> <!-- "Idem" likely refers back to Eutrapelus. -->
				</position>
				<position book_orientation="0" place="right_margin">
					<marginalia_text>
						in dictis, factisq[ue] eminentissimus. 
						Vir in omni Seria industria incomparabilis.
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				The same man is always the quickest of all, the most agile, and the most eminent in words and deeds.
				A man imcomparable in every serious enterprise.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="IT">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						ridicolo.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Ridiculous.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						In maxima qua[e]q[ue] operando, aut negociando, nihil
					</marginalia_text>
				</position>
				<position book_orientation="0" place="right_margin">
					<marginalia_text>
						tam potens, aut velox.
					</marginalia_text>
				</position>
			</language>
			<translation>
				And in working or negotiating the greatest things, nothing is as powerful or as swift.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="IT">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						ridicolo.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Ridiculous.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Ætherea[e] dia[e]ta[e], entelechia[e], multiplicationis semper
					</marginalia_text>
				</position>
				<position book_orientation="0" place="right_margin">
					<marginalia_text>
						memor: omniu[m]q[ue] pra[e]stantissima[e]
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
				<position book_orientation="0" place="tail">
					<marginalia_text>
						Euschemosynes. Singula facilè consumatissima.
						Eutrapelus in flore. Nunq[uam] deficiens ullo modo.
					</marginalia_text>
					<person name="Eutrapelus"/>
					<emphasis method="pen" text="Eutrapelus in flore." type="straight"/>
				</position>
			</language>
			<translation>
				Always mindful of a lofty way of living, of actualization, of multiplying, and of refinement, which most surpasses everything else.
				Singular matters which are easily the most consummate. Eutrapelus in bloom. Never faltering in any way.
			</translation>
		</marginalia>
		
	
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="90" place="left_margin">
					<marginalia_text>
						Sola curat selecta: et ad suum ipsius Scopum pra[e]valida.
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				He cares only for select matters, and those most effective in reaching his goal.
			</translation>
		</marginalia>
		
