	<page filename="FolgersHa2.042v.tif" pagination="404" reader="Harvey"/>	<!-- ABO 47left -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="head">
					<marginalia_text>
						Nihil in animo, aut corpore permittatur impurum, aut inualidum: sed curandum, vt sint solida,
					</marginalia_text>
				</position>
				<position book_orientation="0" place="left_margin">
					<marginalia_text>
						nitidaq[ue] singula.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Nothing impure or weak is permitted in the soul or body, but one must see that they are solid and shining singular things. 
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						) Nihil in tota vita, aut ali[a] ) actionum parte infirmu[m], aut corruptum: sed
					</marginalia_text>
				</position>
				<position book_orientation="0" place="left_margin">
					<marginalia_text>
						strenua, consta[n]tiaq[ue] omnia.
					</marginalia_text>
				</position>
			</language>
			<translation>
				No weak or corrupt activity in the whole of one's life or in any of its parts, but all things vigorous and constant. 
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Corruptela[e] ista[e] inficiu[n]t animu[m]: infirmant corpus:
					</marginalia_text>
				</position>
				<position book_orientation="0" place="left_margin">
					<marginalia_text>
						extenuant statu[m]: omnia deniq[ue] bona Labefactant. (
						Effoeminata omnia viros emasculant:
					</marginalia_text>
				</position>
				<position book_orientation="0" place="intext">
					<marginalia_text>
						robustos eneruant mollia.
					</marginalia_text>
				</position>
				<position book_orientation="0" place="left_margin">
					<marginalia_text>
						Qua[m]uis Luxuries effoetum corpus
					</marginalia_text>
				</position>
				<position book_orientation="0" place="tail">
					<marginalia_text>
						tradit senectuti. Vndè vitanda[e] mali
						Cuiusq[ue] occasiuncula[e]. Principijs obsta.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Such corruption infects the soul, weakens the body, and diminishes one's condition. In short, it undermines
				all good things. All effiminate matters emasculate men, and soft things enervate the robust. Luxury exceedingly 
				betrays the tired body in old age. Whence every opportunity for evil must be avoided. Oppose the origins of evil. 
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						argutè.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Cleverly.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						( descant. )
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Medicus profitetur Hippocratem, non Petrarcham.
					</marginalia_text>
					<person name="Hippocrates"/>
					<person name="Petrarch"/>
				</position>
			</language>
			<translation>
				The doctor professes to be Hippocrates, not Petrarch.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="IT">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						( Roma. Papa. s[upr]a, 199. )
					</marginalia_text>
					<location name="Rome"/>
				</position>
			</language>
			<translation>
				Rome. The pope. Above, p. 199.
			</translation>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="270" place="right_margin">
					<marginalia_text>
						Sapit, qui absolutè corruptelas contemnit, ab omniq[ue] abhorret
						Luxurie.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Whoever condemns absolutely corruption and abhors every luxury truly knows.
			</translation>
		</marginalia>
		
