				<position book_orientation="0" place="head">
					<marginalia_text>
						Libro 7. delli giuochi.
					</marginalia_text>
					<book title="De giuocchi discorso"/>
				</position>
			</language>
			<translation>
				Book 7 of the games.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="head">
					<marginalia_text>
						Romana[e] autoritatis
					</marginalia_text>
				</position>
				<position book_orientation="0" place="right_margin">
					<marginalia_text>
						valor.
					</marginalia_text>
				</position>
			</language>
			<translation>
				The value of Roman authority.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						s[upr]a / argutè.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Above. Cleverly.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						) Domat omnia virtus. Domat virtutem
					</marginalia_text>
				</position>
				<position book_orientation="0" place="right_margin">
					<marginalia_text>
						Venus.
					</marginalia_text>
					<person name="Venus"/>
				</position>
			</language>
			<translation>
				Virtue conquers all. Venus conquers virtue.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="right_margin">
					<marginalia_text>
						Veneratio antiqua[e] dignitatis. )
					</marginalia_text>
				</position>
			</language>
			<translation>
				The veneration of ancient dignity.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="IT">
				<position book_orientation="0" place="right_margin">
					<marginalia_text>
						(è priapo Virg.)
					</marginalia_text>
					<person name="Virgil"/>
				</position>
			</language>
			<translation>
				It's a Virgilian priapus.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						dextrè.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Skillfully.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Moderatus Bacchus corroborat: immoderatus eneruat.
					</marginalia_text>
				</position>
			</language>
			<translation>
				A moderate amount of Bacchus fortifies; an immoderate amount weakens.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						scitè.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Knowingly.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="IT">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						risoluto, come Epitteto.
					</marginalia_text>
					<person name="Epictetus"/>
				</position>
			</language>
			<translation>
				Resolute, like Epictetus.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Vita prima linea hominum: Mors vltima linea rerum.
					</marginalia_text>
					<X-ref book_title="Epistles" language="LA" person="Horace" text="Mors vltima linea rerum est"/>
				</position>
			</language>
			<translation>
				Life is the first boundary of man: "Death is the ultimate boundary of things." 
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="right_margin">
					<marginalia_text>
						Nuda Marij maiestas terruit homicidas. 
					</marginalia_text>
					<person name="Gaius Marius"/>
				</position>
			</language>
			<translation>
				The naked majesty of Marius terrified the murderers.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						s[upr]a /
					</marginalia_text>
					<internal_ref>
						<target ref="FolgersHa2:Ha2.036v" text="s[upr]a"/> <!--Francesce-->
					</internal_ref>
				</position>
			</language>
			<translation>
				[See] above.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						mordace.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Biting.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Vino corrigitur: vino corrumpitur a[e]tas.
					</marginalia_text>
				</position>
			</language>
			<translation>
				One improves with wine; age is corrupted by wine.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="tail">
					<marginalia_text>
						Ca[e]sar Cibi, Vini, somni parcissimus: Veneris
						etiam, nisi politica[e]. Quanto magis, Enteleches.
					</marginalia_text>
					<person name="Julius Caesar"/>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				Caesar is most sparing in food, wine, and sleep: and even in love,
				unless it was political. He is much greater in accomplishment. 
			</translation>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="270" place="left_margin">
					<marginalia_text>
						Venerandi Pontificis augusta maiestas obstupefecit Carnifices.
					</marginalia_text>
				</position>
			</language>
			<translation>
				The august majesty of the venerable pope paralyzed the murderers.
			</translation>
		</marginalia>
		
	
