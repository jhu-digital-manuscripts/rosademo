		<!-- Marginalia -->
		<marginalia book_id="FolgersHa2" hand="Italian" id="Ha2.050v_0001" marginalia_continues_to="Ha2.051r_0001" marginalia_to_transcription="Ha2.051r.xml">
			<language ident="LA">
				<position book_orientation="0" place="head">
					<marginalia_text>
						Ille vir, ille mihi, cui vocula nulla tepescit, = [p. 421: = vlteriùs volitat nescia stare ratis. Axiophilus.]
							<!-- Continues onto facing page, head, Ha2.051r -->
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				That man of mine, whom no feeble voice can warm, [. 421: = flies above those matters incapable of standing firm. Axiophilus.]
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="IT">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						ridicolo.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Ridiculous.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Doctor Bond Etymologicus. <!-- CG: Can't figure this out. Specialist help required. -->
					</marginalia_text>
				</position>
			</language>
			<translation>
				Doctor Bond, the etymologist.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="EN">
				<position book_orientation="0" place="left_margin">
					<marginalia_text>
						I
					</marginalia_text>
				</position>
				<position book_orientation="0" place="intext">
					<marginalia_text>
						haue heard more Witt from sum apt Mercurial boyes,
						then from manie learned &amp; graue men.
					</marginalia_text>
				</position>
				<position book_orientation="0" place="left_margin">
					<marginalia_text>
						So much worth is a quick spirit, &amp; dexteritie.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						dextrè. )
					</marginalia_text>
				</position>
			</language>
			<translation>
				Skillfully.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						) distinctissima oratio, maximèq[ue] emphatica.
					</marginalia_text>
				</position>
			</language>
			<translation>
				A most distinct oration, and especially emphatic.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="tail">
					<marginalia_text>
						Intrà Ironia; extrà euschemosyne:
						circà Entelechia.
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				Irony within, gracefulness without, and actualization all around.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position book_orientation="90" place="left_margin">
					<marginalia_text>
						Aparrel alwais clenlie, &amp; hansome, &amp; sumtime
						fine: but neuer uaine, or glorious.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="270" place="right_margin">
					<marginalia_text>
						Singula tam efficacissimè, quàm Specioicissimè.
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				Singular things are both most effective and most beautiful.
			</translation>
		</marginalia>
		
