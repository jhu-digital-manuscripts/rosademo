	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="IT">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						ridicolo.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Ridiculous.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						) Vnicus mundi
						incessans actor, et semper Multiplicans.
					</marginalia_text>
				</position>
			</language>
			<translation>
				The only unceasing actor of the world, always multiplaying.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="IT">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						ridicolo, e arguto.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Ridiculous and clever.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						) Ridet graues istos
						Eutrapelus: et leuissimus peragit longè grauiss[im]a.
					</marginalia_text>
					<person name="Eutrapelus"/>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				Eutrapelus laughs at these serious things, and 
				most nimbly brings to conclusion matters by far most serious.
			</translation>
		</marginalia>
				
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						distinctio Ciuilis.
					</marginalia_text>
				</position>
			</language>
			<translation>
				A civil distinction.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="EN">
				<position book_orientation="0" place="left_margin">
					<marginalia_text>
						The
					</marginalia_text>
				</position>
				<position book_orientation="0" place="intext">
					<marginalia_text>
						most industrious, &amp; impetuous man of all other.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="left_margin">
					<marginalia_text>
						s[upr]a /
					</marginalia_text>
				</position>
			</language>
			<translation>
				[See] above.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="tail">
					<marginalia_text>
						Vnum pro multis Caput.
					</marginalia_text>
				</position>
			</language>
			<translation>
				One head alone for many.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position book_orientation="270" place="left_margin">
					<marginalia_text>
						Giue mee the finest platforms of Wittie Conceits; and the
						gallantest Paragons of Pleasant deuises.
					</marginalia_text>
				</position>
			</language>
			<language ident="LA">
				<position book_orientation="270" place="left_margin">
					<marginalia_text>
						Specula aurea.
					</marginalia_text>
				</position>
			</language>
			<translation>
				A golden mirror.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position book_orientation="270" place="right_margin">
					<marginalia_text>
						The goulden fleece: &amp; the diamant prise: at home &amp; abrode.  
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
