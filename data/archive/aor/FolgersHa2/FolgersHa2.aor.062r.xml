	<page filename="FolgersHa2.062r.tif" pagination="443" reader="Harvey"/>	<!-- ABO 66right -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="head">
					<marginalia_text>
						Nec inutilibus argutijs quicq[uam] vanius: nec utilibus subtilitatibus quicq[uam]
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
				<position book_orientation="0" place="right_margin">
					<marginalia_text>
						preciosius.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Nothing is more vain than useless sophistry, nor is anything more precious than useful subtleties.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						dextrè.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Skillfully.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="IT">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						ridicolo.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Ridiculous.
			</translation>
		</marginalia>
		<marginalia anchor_text="diuersi humori di persone" hand="Italian">
			<language ident="IT">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						( humori di persone.
					</marginalia_text>
				</position>
			</language>
			<translation>
				The humors of different people.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						( Ingenium ingeniorum ad vivum. )
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				The genius of geniuses, in the flesh.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="tail">
					<marginalia_text>
						Totius subtilitatis subtilitas subtiliss[im]a
						sed in vtilissimis. Vt s[upr]a, 26.
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				The most subtle subtley of all subtlety,
				but in useful things. As above, p. 26.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="90" place="left_margin">
					<marginalia_text>
						Infinitos supplet defectus, ingenij et lingua[e] celeritas.
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				Haste produces infinite failures in character and language. 
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="270" place="right_margin">
					<marginalia_text>
						Nihil in mundo, plus prodest, aut delectat, quàm ingenij acumen: quo
						omnes vel inuiti capiuntur.
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				Nothing in the world is more useful or delightful than a sharp wit, with which
				all reluctant people can be overcome.
			</translation>
		</marginalia>
		
