	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="head">
					<marginalia_text>
						Historia egregia, et singularis.
					</marginalia_text>
				</position>
			</language>
			<translation>
				A distinguished and singular history.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="IT">
				<position book_orientation="0" place="tail">
					<marginalia_text>
						discretamente.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Discreetly.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="tail">
					<marginalia_text>
						) Non est
						doctus, qui non est sua bibliotheca.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Whoever is not in his library is not learned.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="90" place="left_margin">
					<marginalia_text>
						Noster etiam Proteus in quaslibet formas conuertibilis eodem die:
						ter, decies, vicies, centies; quoties, quotiesque opus.
					</marginalia_text>
					<person name="Proteus"/>
				</position>
			</language>
			<translation>
				Likwise our Proteus is changeable into whatever forms on the same day:
				three times, ten times, twenty times, one hundred times; as often as is useful.
			</translation>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="90" place="right_margin">
					<marginalia_text>
						Mutata ratione, aut occasione; mutanda opinio, affectio, actio.
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				In the case of a changed reason or occasion, an opinion, purpose, and action must be changed.
			</translation>
		</marginalia>
		
