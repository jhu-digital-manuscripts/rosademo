	<page filename="FolgersHa2.064v.tif" pagination="448" reader="Harvey"/>	<!-- ABO 69left -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="head">
					<marginalia_text>
						Quotidianum gallicinium: Salue festa dies.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Daily at dawn: hail the festive day.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position book_orientation="0" place="left_margin">
					<marginalia_text>
						He is noboddie, that hath not his witt, &amp; toung at will.
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Sera nimis vita est crastina: viue hodiè.
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				Tomorrow's life is too late: live in the present.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						( Qui poterit sanum fingere, sanus erit. )
					</marginalia_text>
				</position>
			</language>
			<translation>
				Whoever can feign health will be healty.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Panacea.
					</marginalia_text>
					<emphasis method="pen" text="Panacea" type="straight"/>
				</position>
			</language>
			<translation>Panacea.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Viuidus exultet nocte, dieq[ue] vigor.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Let the lively vigor jump about by night and day. 
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="tail">
					<marginalia_text>
						Plurima quotidiè circunfluunt: qua[e] uix (
						ego stans pede in vno.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Many things flow around daily, due to which
				I can barely stay upright on my feet.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position book_orientation="270" place="left_margin">
					<marginalia_text>
						Virgil refined Ennius: a second Pindar refined
						Virgil. That Pindar may still be refined.
					</marginalia_text>
					<person name="Virgil"/>
					<person name="Ennius"/>
					<person name="Pindar"/>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="90" place="right_margin">
					<marginalia_text>
						In cespite paucorum Principiorum latet lepus plurimorum Experimentorum.
					</marginalia_text>
					<X-ref book_title="Three proper, and wittie, familiar letters" language="LA" person="Edmund Spenser" text="In exiguo quandoque cespite latet lepus"/>
				</position>
			</language>
			<translation>
				In the short grass of small beginnings hides the hare of many experiences.
			</translation>
		</marginalia>
		
