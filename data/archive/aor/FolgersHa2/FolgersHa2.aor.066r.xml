	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="head">
					<marginalia_text>
						Mera alacritas, celeritas, violentia, constantia.
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				Pure eagerness, quickness, aggressiveness, perseverance.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="IT">
				<position book_orientation="0" place="tail">
					<marginalia_text>
						( ridicolo.)
					</marginalia_text>
				</position>
			</language>
			<translation>
				Ridiculous.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="tail">
					<marginalia_text>
						Nescia virtus stare Loco: solusq[ue]
						pudor non vincere cunctos. )
					</marginalia_text>
					<X-ref book_title="Pharsalia" language="LA" person="Lucan" text="Nescia virtus stare loco"/>
				</position>
			</language>
			<translation>
				"The virtue of not knowing how to stay in one place,"
				and the decency of not conquering all together.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="90" place="left_margin">
					<marginalia_text>
						Utrunq[ue] conuenit, Et sibi sapere, et alijs: nec se, nec mundu[m] despicere. 
					</marginalia_text>
				</position>
			</language>
			<translation>
				It is appropriate to know both one's self and others and not to disdain one's self nor the world.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="270" place="right_margin">
					<marginalia_text>
						Nemo cuiusq[uam] mundana[e] facultatis satis practicus: vt nec sua[e]
						ipsius persona[e]. Quam in quotidiana Praxi maximè desidero, praxis
					</marginalia_text>
				</position>
				<position book_orientation="270" place="tail">
					<marginalia_text>
						est.
					</marginalia_text>
				</position>
			</language>
			<translation>
				No one is practical enough with any wordly skill; likewise, neither are they practiced enough
				with their very own persona. That which I most desire in daily praxis is praxis.	
			</translation>
		</marginalia>
		
