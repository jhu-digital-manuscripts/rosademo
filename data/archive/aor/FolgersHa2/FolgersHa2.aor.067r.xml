		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="head">
					<marginalia_text>
						Cui est ingenium, non deest infinita copia Iocoru[m]. Nec ulli
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
				<position book_orientation="0" place="right_margin">
					<marginalia_text>
						apud Porcacum, aut Domenicu[m], qui non facilè
					</marginalia_text>
					<person name="Tommaso Porcacchi"/>
					<person name="Lodovico Domenichi"/>
				</position>
				<position book_orientation="0" place="intext">
					<marginalia_text>
						) possunt
					</marginalia_text>
				</position>
				<position book_orientation="0" place="right_margin">
					<marginalia_text>
						arte
					</marginalia_text>
				</position>
				<position book_orientation="0" place="intext">
					<marginalia_text>
						superari.
					</marginalia_text>
				</position>
			</language>
			<translation>
				For whoever has genius, an infinite abundance of jokes should not be wanting. Nor are there
				any in Porcacchi or Domenichi who can be easily overcome by art.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="IT">
