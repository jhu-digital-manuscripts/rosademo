		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="tail">
					<marginalia_text>
						Stratagema.
					</marginalia_text>
				</position>
			</language>
			<translation>
				A stratagem.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="EN">
				<position book_orientation="0" place="tail">
					<marginalia_text>
						Sum brauados haue good luck.
					</marginalia_text>
				</position>
			</language>
		</marginalia>		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="tail">
					<marginalia_text>
						Diuina, regia, propria diligentia.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Diligence: divine, royal, and one's own.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="90" place="left_margin">
					<marginalia_text>
						Maxima scire, et optima perficere; hoc est proficere. Sparta Euscopi.
					</marginalia_text>
					<person name="Euscopus"/>
					<location name="Sparta"/>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				To know the greatest things and to accomplish the best; this is accomplishment. Euscopus' Sparta. 
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="90" place="right_margin">
					<marginalia_text>
						Regia, et diuina professio; scire, dicere; 
						agere optima: semper pergere.
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				A royal and divine profession: to know and to speak; to perform the best things; to always go forward.
			</translation>
		</marginalia>
		
