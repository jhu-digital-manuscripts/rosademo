	<page filename="FolgersHa2.081r.tif" reader="Harvey" signature="**[r]"/>	<!-- ABO 85right -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="head">
					<marginalia_text>
						Dies breuis, et opus multu[m]. Apophthegma Hebra[e]u[m].
					</marginalia_text>
				</position>
			</language>
			<translation>
				The day is short and the work is long. A Jewish aphorism.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="head">
					<marginalia_text>
						Dia[e]ta cita, non tarda. Neoterismus.
					</marginalia_text>
				</position>
			</language>
			<translation>
				An early regimen, not a late one. A new expression.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						A snatch, &amp; away.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Nihil Epictetus extra suum Horizontem.
					</marginalia_text>
					<person name="Epictetus"/>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				For Epictetus, nothing beyond his horizon.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="EN">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						A napp, &amp; away.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="EN">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						( Pride, &amp; a Fall, ar Twinnes. )
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Alexander in altero Hemispha[e]rio magnus: in vtroq[ue]
						Columbus. Magni Principis egregium Exemplar. Cui vterq[ue]
						polus patefactus.
					</marginalia_text>
					<person name="Columbus"/>
				</position>
			</language>
			<translation>
				Alexander is great in the other hemisphere. In both hemispheres, Columbus.
				He is a notable example of a great prince, for whom both poles lay open.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="tail">
					<marginalia_text>
						Experto crede: in polemicis
						Ca[e]sari: in pragmaticis Durando: in
						aulicis Sidneio. Serij magistri, et fideles.
					</marginalia_text>		
					<person name="Julius Caesar"/>
					<person name="Guillaume Durand"/>
					<person name="Philip Sidney"/>					
				</position>
			</language>
			<translation>
				Trust the expert: Caesar in battles, Durandus in practical matters,
				Sidney in courtly ones. Earnest and faithful masters.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="90" place="left_margin">
					<marginalia_text>
						Valdè etiam conducunt Flores Legum, Doctorum, Poetarum. In quibus
						multa pra[e]gnantia, et arguta.
					</marginalia_text>
				</position>
			</language>
			<translation>
				The flowers of law, teachers, and poets are also exceedingly useful. In them are many pregnant
				and clever things.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="90" place="right_margin">
					<marginalia_text>
						Argutia[e] argutiarum ad viuum, in Seneca[e] tragoedijs: ab Axiophilo selecta[e].
					</marginalia_text>
					<person name="Seneca"/>
					<person name="Axiophilus"/>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				The cleverest things that cut to the quick are in Seneca's tragedies, singled out by Axiophilus.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position book_orientation="270" place="right_margin">
					<marginalia_text>
						Nashes swan is a goose. Harueys cuckooe is a nightingale.
					</marginalia_text>
					<person name="Thomas Nash"/>
					<person name="Gabriel Harvey"/>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
		</marginalia>
		
