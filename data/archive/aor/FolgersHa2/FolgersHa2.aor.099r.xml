	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="IT">
				<position book_orientation="0" place="head">
					<marginalia_text>
						Unicismo dell'Vnico.
					</marginalia_text>
					<person name="Bernardo Accolti"/>
				</position>
			</language>
			<translation>
				Something unique from Bernardo Accolti.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="head">
					<marginalia_text>
						Acrior inflammet nocte, dieq[ue] furor.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Night and day, let the fury be kindled more vigorusly.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Pra[e]clarissima Mundi Securitas, Nihil omnium reru[m]
						curare, pra[e]ter vnu[m] ( ) in manu Solem.
					</marginalia_text>
				</position>
			</language>
			<language ident="IT">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Questo vno, vnissimo.
					</marginalia_text>
				</position>
			</language>
			<translation>
				The most illustrious safety of the world; of all things, nothing to worry about
				beyond the one sun at hand. This one alone, this very one.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="tail">
					<marginalia_text>
						Pra[e]ter Hoc vnum,
					</marginalia_text>
				</position>
			</language>
			<language ident="ES">
				<position book_orientation="0" place="tail">
					<marginalia_text>
						Todos es nada.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Beyond this one thing here, everything is nothing.
			</translation>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position book_orientation="90" place="left_margin">
					<marginalia_text>
						He that doth not allwais affect to be best; shall often waxe base; &amp; sumtime
						sceme one of the worst. Eutrapelus hateth nothing more, then
					</marginalia_text>
					<person name="Eutrapelus"/>
				</position>
				<position book_orientation="90" place="intext">
					<marginalia_text>
						to sceme a Vulgar: or to be cou[n]teed an ordinarie ma[n].
						Therefore euer intermixeth su[m]thing extraordinarie.
						Allwais a fine, &amp; rare
					</marginalia_text>
				</position>
				<position book_orientation="90" place="right_margin">
					<marginalia_text>
						man: &amp; the most excelling personage in [the] companie.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="270" place="right_margin">
					<marginalia_text>
						Ca[e]tera abijcienda pro tempore: vt nullius iam nunc momenti.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Other things must be cast away for the time being; just as this very moment is null and void.
			</translation>
		</marginalia>
		
