	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="IT">
				<position book_orientation="0" place="head">
					<marginalia_text>
						Vno, mà Vnico.  
					</marginalia_text>
				</position>
			</language>
			<language ident="ES">
				<position book_orientation="0" place="head">
					<marginalia_text>
						Examen de ingenios.
					</marginalia_text>
				</position>
			</language>
			<translation>
				One, but unique. A test of genius.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="IT">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						{ Unicismo, d'Eudromo.
					</marginalia_text>
					<person name="Eudromus"/>
				</position>
			</language>
			<translation>
				A singular deed from Eudromus.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Polytechni
						scopus semper practicus, et
						polychrestus.
					</marginalia_text>
					<emphasis method="pen" text="Polytechni" type="straight"/>
					<emphasis method="pen" text="scopus" type="straight"/>
				</position>
			</language>
			<translation>
				The goal of the man of multiple skills is always practical and multi-purpose. <!-- Neologism -->
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						( Non hoc, et illud:
						sed hoc vnicè, aut illud vnicè.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Not simply this or that, but this or that to a singular degree.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Polytechni monophilia absoluta.
					</marginalia_text>
				</position>
			</language>
			<translation>
				The absolute sole friendship of a man of multiple skills.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="tail">
					<marginalia_text>
						stratagema pragmaticum.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Pragmatic stratagem.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="tail">
					<marginalia_text>
						) Mea Cynosura, vnica Isagoge.
					</marginalia_text>
				</position>
			</language>
			<translation>
				My guiding star, my singular isagoge.
			</translation>
		</marginalia>

		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="90" place="left_margin">
					<marginalia_text>
						Valet animus in seipsum collectus: cum virtus vnita solidescit; et omnes
						circumferentia[e] tendunt ad Centrum.
					</marginalia_text>
				</position>
			</language>
			<translation>
				The mind held together in itself is powerful; united with virtue it solidifies,
				and all circumferences aim toward a center.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="270" place="right_margin">
					<marginalia_text>
						( Cor mundi maximè cordatum. Sed tam ironicè, quà emphaticè. )
					</marginalia_text>
				</position>
			</language>
			<translation>
				The heart of the world is most wise. But as ironically as emphatically.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="270" place="right_margin">
					<marginalia_text>
						Cuiusq[ue] artis vnus liber fundamentalis. Ca[e]teri ad placitum: gloriosi
					</marginalia_text>
				</position>
				<position book_orientation="270" place="intext">
					<marginalia_text>
						magis, quàm necessarij.
					</marginalia_text>
				</position>
			</language>
			<translation>
				The one fundamental book of any art. The others, at pleasure; they are more famous than necessary.
			</translation>
		</marginalia>
		
