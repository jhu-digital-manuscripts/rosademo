		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Industria est incessans, qua[e] operatur Miracula.
					</marginalia_text>
				</position>
			</language>
			<translation>
				The industriousness which works miracles is unceasing.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="90" place="left_margin">
					<marginalia_text>
