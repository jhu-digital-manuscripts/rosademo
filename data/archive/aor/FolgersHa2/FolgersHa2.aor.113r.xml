		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="head">
					<marginalia_text>
						Vidi ingentem bibliothecam scholasticoru[m] scriptoru[m].
						Risi, et merito dixi,
					</marginalia_text>
				</position>
			</language>
			<language ident="ES">
				<position book_orientation="0" place="head">
					<marginalia_text>
						Todos es nada.
					</marginalia_text>
				</position>
			</language>
			<translation>
				I saw a great library of scholastic writings.
				I laughed and said justly, "All is for naught."
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
