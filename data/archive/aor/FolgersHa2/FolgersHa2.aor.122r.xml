<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
	
	<page filename="FolgersHa2.122r.tif" pagination="67" reader="Harvey" signature="E2[r]"/>	<!-- ABO 126right -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="head">
					<marginalia_text>
						Epicteto armatior Eutrapelus.
					</marginalia_text>
					<person name="Epictetus"/>
					<person name="Eutrapelus"/>
				</position>
			</language>
			<translation>
				Eutrapelus is more equiped than Epictetus.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Quiesce Doctor subtilis, in problematis supernaturalib[us].
					</marginalia_text>
					<person name="Duns Scotus"/>
				</position>
			</language>
			<translation>
				Seek the subtle Doctor in supernatural problems.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Non curo horas Planetarum: Sed
						fixarum stellarum, Artis, et Virtutis.
					</marginalia_text>
				</position>
			</language>
			<translation>
				I care not for the hours of the planets, but of the fixed stars, art, and virtue.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="IT">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Esopo.
					</marginalia_text>
					<person name="Aesop"/>
				</position>
			</language>
			<translation>
				Aesop.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						) Alij Elenctici:
						Ego apodicticus.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Others refute; I prove.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						plures factiones minùs periculosa[e] quàm vna.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Many factions are less dangerous than one.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						( Quisq[ue] sophus, dominus hora[e] cuiusq[ue].
					</marginalia_text>
				</position>
			</language>
			<translation>
				Whoever is wise is the master of every hour.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="tail">
					<marginalia_text>
						Quicquid, aut quantumcunq[ue] accidit: Idem
						Eutrapelus. Non alienus, sed suus.
					</marginalia_text>
					<person name="Eutrapelus"/>
				</position>
			</language>
			<translation>
				Whatever happens, and however great it is, Eutrapelus will be the same.
				Not another, but himself.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="90" place="left_margin">
					<marginalia_text>
						Foelix quem fortuna[e] ascendens hora. Cui applaudit qua[e]uis hora.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Happy is he for whom the hour of fortune is ascendant, for whom whatetever hour applauds.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="90" place="left_margin">
					<marginalia_text>
						Hodiè Entelechia: cras Apologia. Luna non curat
						Latrantem
					</marginalia_text>
				</position>
				<position book_orientation="90" place="intext">
					<marginalia_text>
						Canem.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Today the actualization; tomorrow the apology.
				The Moon cares not for the barking Dog. 
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="270" place="right_margin">
					<marginalia_text>
						Eutrapelus sese cataphractus, corde pyropum; Magnetem Lingua fert; adamanta
						manu Axiophili panoplia.
					</marginalia_text>
					<person name="Eutrapelus"/>
					<person name="Axiophilus"/>
				</position>
			</language>
			<translation>
				Eutrapelus has armed himself with bronze in his heart and a magnet on his tongue;
				the armor of Axiophilus is a steel in his hand.
			</translation>
		</marginalia>
		
