	<page filename="FolgersHa2.122v.tif" pagination="68" reader="Harvey" signature="[E2v]"/>	<!-- ABO 127left -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="head">
					<marginalia_text>
						Rhetorica, non est ars Multa dicendi, sed benè. Nec plurima
						scripserunt excellentissimi viri, sed rarissima.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Rhetoric is not the art of saying many things, but of speaking well.
				Nor did most excellent men write many different works, but most singular ones.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						egregia Antitechna; et pra[e]gnans stratagema.
					</marginalia_text>
				</position>
			</language>
			<translation>
				An excellent counter-maneuver and a pregnant stratagem. <!-- CG: Antitechna? Unclear.--> <!--JG: Settled for counter-attack, counter-movement-->
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Machiauelli.
					</marginalia_text>
					<person name="Machiavelli"/>
				</position>
			</language>
			<language ident="EL">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						ἀπιστία
					</marginalia_text>
				</position>
			</language>
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						politica.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Machiavelli: political distrust.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Autotechni monopraxia: monotechni autopraxia.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Engagement with the one activity of self-instruction. The freedom of action of one skill.<!-- CG: Neologisms. Unclear. --> 
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="tail">
					<marginalia_text>
						Pulcherrimus est, qui est dignissimus.
					</marginalia_text>
				</position>
			</language>
			<translation>
				He who is most dignified is most beautiful.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="90" place="left_margin">
					<marginalia_text>
						Nec longitudo; nec latitudo; nec profunditas aliena; sed tua.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Neither the longitude, latitude, nor profundity of someone else, but of you yourself.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="90" place="right_margin">
					<marginalia_text>
						Tua statura, tibi vnica. Aliena, parerga. Autocosmia.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Your size is unique to you alone. Another's size is supplementary. A universe unto itself. <!-- CG: These neologisms are mind-boggling. --> 
			</translation>
		</marginalia>
		
