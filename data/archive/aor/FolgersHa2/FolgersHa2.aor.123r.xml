		<marginalia hand="Italian">
			<language ident="IT">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Il diuino Aretino,	
					</marginalia_text>
					<person name="Pietro Aretino"/>
				</position>
			</language>
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Flagellum Principum:
					</marginalia_text>
				</position>
			</language>
			<language ident="IT">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						et esso Apollonio Tianeo,
					</marginalia_text>
					<person name="Apollonius of Tyana"/>
				</position>
			</language>
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Malleus Tyrannorum. Etiam hodiè Junius Brutus, Tyrannomastix.
					</marginalia_text>
					<person name="Lucius Junius Brutus"/>
				</position>
			</language>
			<translation>
				The divine Aretino was the scourge of princes and Apollonius of Tyana was the hammer of tyrants.
				Even today, Junius Brutus is the Tyrant-Whip.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="tail">
					<marginalia_text>
						Hermes, Microcosmus.
					</marginalia_text>
					<person name="Hermes"/>
				</position>
			</language>
			<translation>
				Hermes, a Microcosm.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="tail">
					<marginalia_text>
						Eudromus, Neocosmus.
					</marginalia_text>
					<person name="Eudromus"/>
				</position>
			</language>
			<translation>
				Eudromus, a new world.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="90" place="left_margin">
					<marginalia_text>
						Meus mihi Efficiens: mea Materia: mea Forma: meus
						Finis. Mea vnius Effecta, et Adiuncta.
					</marginalia_text>
				</position>
			</language>
			<translation>
				My own efficient cause, material cause, formal cause, and final cause.
				My only effect and supplement.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="90" place="right_margin">
					<marginalia_text>
						Quod non potest Deucalion, potest Vulcanus: et contra.
					</marginalia_text>
					<person name="Deucalion"/>
					<person name="Vulcan"/>
				</position>
			</language>
			<translation>
				That which Deucaleon cannot do, Vulcan can; and vice-versa.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position book_orientation="270" place="right_margin">
					<marginalia_text>
						The sacking of Gadis. Shall I tell in a word, What conquer'd Cales? A strong Imagination,
						&amp; braue tales.
					</marginalia_text>
					<location name="Cadiz"/>
					<location name="Cales"/>
				</position>
			</language>
		</marginalia>
		
