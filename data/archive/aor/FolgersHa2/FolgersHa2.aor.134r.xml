			<language ident="EN">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Eutrapelus can do more bie the White art, then anie
						Coniurer in Europe, can do bie the black art.
					</marginalia_text>
					<person name="Eutrapelus"/>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="IT">
				<position book_orientation="0" place="tail">
					<marginalia_text>
						Maestro di tutti tre mezzi; l'uno, ma l'unico,
						Homero.
					</marginalia_text>
					<person name="Homer"/>
				</position>
			</language>
			<translation>
				A master of all three ways: the one and only Homer.
			</translation>
		</marginalia>
		
