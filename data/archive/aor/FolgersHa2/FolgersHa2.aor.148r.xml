	<page filename="FolgersHa2.148r.tif" pagination="119" reader="Harvey" signature="H4[r]"/>	<!-- ABO 152right -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="head">
					<marginalia_text>
						{ Plus adhuc, plusq[ue] spiritûs: et semper meliùs adhuc,
						meliusq[ue] dicendum, atq[ue] faciendum. Aut frustra
					</marginalia_text>
				</position>
				<position book_orientation="0" place="intext">
					<marginalia_text>
						tot Eutrapeli egregia Nota[e].
					</marginalia_text>
					<person name="Eutrapelus"/>
				</position>
			</language>
			<translation>
				Still more and more spirit; and always greater and greater still must something be said and done. 
				Else all the illustrious notes of Eutrapelus are in vain.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="right_margin">
					<marginalia_text>
						Satage, q[uo]d animus dictat: et perge, du[m] feruet manus.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Busy yourself with what your mind tells you, and carry on while your hand is active.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Erranti Sit medicina Confessio.)
					</marginalia_text>
				</position>
			</language>
			<translation>
				Let a confession be medicine for the errant. 
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="EN">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						A Liuing hurlewind. a flowing streame.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="EN">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						A liuelie
						hurlepoole, gulfe, &amp; abisse of profounde
						skill, &amp; practis.
					</marginalia_text>
				</position>
			</language>
			<language ident="FR">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						trespuissant.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Most powerful.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="tail">
					<marginalia_text>
						Quis putet? sed quibus hodiè inuidetur, Comiti Essexio, Equiti
						Raleio, alijs quibusdam in Aula gratiosis; suorum aliquandò
						miserabuntur etiam inimici.
					</marginalia_text>
					<person name="Robert Devereux"/> <!-- JG: Could also be Walter, Robert's father?--> <!-- CG: We might want to ask a specialist then! -->
					<person name="Walter Raleigh"/>
				</position>
			</language>
			<translation>
				Who stinks? But one is daily envied by some men, by the Count of Essex, by Sir Raleigh,
				by certain other gracious men in court; sometimes they will pity even their enemies. 
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="90" place="left_margin">
					<marginalia_text>
						{ fortunatis inuident amici: infoelices miserantur inimici.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Friends envy the fortune; enemies pity the unfortunate. 
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="90" place="right_margin">
					<marginalia_text>
						Nunq[uam] excedit Euschemosyne in pulchris: semper deficit Entelechia
						in bonis. Benè multi: nemo satis benè.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Refinement never exceeds in beautiful men, but Actualization is always lacking in good men.
				Many are good, but no one is ever good enough.
			</translation>
		</marginalia>
		
