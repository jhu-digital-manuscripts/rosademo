		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Venus, venenum Saturni, et Mercurij.
					</marginalia_text>
					<person name="Venus"/>
					<person name="Saturn"/>
					<person name="Mercury"/>
				</position>
			</language>
			<translation>
				Venus, the poison of Saturn and Mercury.
			</translation>
		</marginalia>
