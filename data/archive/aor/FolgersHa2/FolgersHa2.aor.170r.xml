		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="head">
					<marginalia_text>
						Lege saepè et tibi, et tuis Carissimis.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Read often, both to yourself and your dearest friends.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="head">
					<marginalia_text>
						} Nulla Venus permittenda declinanti a[e]tati.
					</marginalia_text>
				</position>
			</language>
			<translation>
				No love must be allowed in old age.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
