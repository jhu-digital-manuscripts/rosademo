	<page filename="FolgersHa2.175r.tif" pagination="173" reader="Harvey" signature="[L7r]"/>	<!-- ABO 179right -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="head">
					<marginalia_text>
						Nondùm Ars venit in mundum: Extat
						modò quiddam artificiale.
					</marginalia_text>
				</position>
			</language>
			<translation>Art has not yet come into the world: a certain thing stands out in an artificial manner.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Nondu[m] Ille Prouidus.
					</marginalia_text>
				</position>
			</language>
			<translation>He is not yet prophectic.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						{ Idem dico de Virtute, quod de Arte
						nondum venit in mundu[m].
					</marginalia_text>
				</position>
			</language>
			<translation>Likewise I say about virtue, what I say about art. It has not yet come into the world.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Noua Creatura, Virtus.
					</marginalia_text>
				</position>
			</language>
			<translation>Virtue, a new creature.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="IT">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						risoluto.	
					</marginalia_text>
				</position>
			</language>
			<translation>Resolute.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						) Neotechnia, et Neopraxia.
					</marginalia_text>
				</position>
			</language>
			<translation>New skills and new praxis.</translation> <!--CG: Neologisms-->
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Excipe Euangelium: et quid est dignum admiratione?
					</marginalia_text>
				</position>
			</language>
			<translation>Follow the Gospel: and what is worthy of admiration?</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="IT">
				<position book_orientation="0" place="tail">
					<marginalia_text>
						Piu Oltre: di Carlo Quinto.
					</marginalia_text>
					<person name="Charles V"/>
				</position>
			</language>
			<translation>Much further than Charles V.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="tail">
					<marginalia_text>
						Sapere,
						humanu[m] est: tempestiue, heroicu[m]: maturè, diuinum.
					</marginalia_text>
				</position>
			</language>
			<translation>To understand is human: to understand timely is heroic: to understand maturely is divine.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="90" place="left_margin">
					<marginalia_text>
						Tardè procedit frigideq[ue] proficit qua[e]uis Genesis, aut Analysis, seu co[m]munis,						
					</marginalia_text>
				</position>
				<position book_orientation="90" place="intext">
					<marginalia_text>
						seu propria; qua[e] non est aphoristica; et "
					</marginalia_text>
				</position>
				<position book_orientation="90" place="right_margin">
					<marginalia_text>
						" et quasi virgo nuda; sine omnibus vestium ornamentis, id est, impedimentis.
					</marginalia_text>
				</position>
			</language>
			<translation>Whatever creation or analysis of things (whether public or private) advances slowly and proceeds indifferently, when it is is not aphoristic; and, it is like a bare virgin, without
			all ornaments of clothes, that is, without impediments.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="270" place="right_margin">
					<marginalia_text>
						Singula ordine disposita: omnia confestim expedita. tam maturimè, quàm celerrimè.
					</marginalia_text>
				</position>
			</language>
			<translation>Singular things managed by order: everything obtained immediately. Both most maturely and quickest.</translation>
		</marginalia>
		
