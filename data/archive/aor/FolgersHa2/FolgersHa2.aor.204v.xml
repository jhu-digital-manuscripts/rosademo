	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="IT">
				<position book_orientation="0" place="head">
					<marginalia_text>
						Vn raro assai piu, che Cento Mediocri.	
					</marginalia_text>
				</position>
			</language>
			<translation>One rare man is much more than one hundred mediocre men.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="head">
					<marginalia_text>
						Viua tempestas, diluuiuum, furia.
					</marginalia_text>
				</position>
			</language>
			<translation>A living storm, a flood, rage.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="IT">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						oh brauo scampo.	
					</marginalia_text>
				</position>
			</language>
			<translation>Oh fine escape.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Du[m] Canis mingit, euadit Vulpes.
						Anguilla inter digitos dabitur.
					</marginalia_text>
				</position>
			</language>
			<translation>While the dog urinates, the fox escapes. The eel will escape between the fingers.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Inter malleum, et /
						incudem.
						
						Inter Calicem, et labra.
						Inter lectu[m], et Sepulcrum.
					</marginalia_text>
				</position>
			</language>
			<translation>Between the hammer and the anvil. Between the cup and the lip. Between the bed and the grave.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="IT">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						oh fina risolutione.	
					</marginalia_text>
				</position>
			</language>
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Entelechismus.
					</marginalia_text>
				</position>
			</language>
			<translation>Oh fine resolution. Actualisation.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="intext">
					<marginalia_text>
						Vel pulcrum: vel Sepulcrum.
					</marginalia_text>
				</position>
			</language>
			<translation>Either illustrious, or the grave.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="0" place="tail">
					<marginalia_text>
						Vincitur pondere magis, quàm numero.
					</marginalia_text>
				</position>
			</language>
			<translation>He is defeated more by weight than by number.</translation>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="90" place="left_margin">
					<marginalia_text>
						Eâ curet Sapiens, qua[e] hominem faciunt prudentissimum, virumq[ue] dignissimum.
					</marginalia_text>
				</position>
			</language>
			<translation>The wise man cares for those things which make a man most prudent and most worth of men.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="EN">
				<position book_orientation="90" place="left_margin">
					<marginalia_text>
						It becummes euere notable professour, to be a Mirrour of excellencie, &amp;
						perfection in his facultie.
					</marginalia_text>
				</position>
			</language>
			<language ident="LA">
				<position book_orientation="90" place="left_margin">
					<marginalia_text>
						Alioru[m] magistri, debent esse aurea Mundi
						Specula.
					</marginalia_text>
				</position>
			</language>
			<translation>The masters of others should be the golden mirrors of the world.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position book_orientation="270" place="right_margin">
					<marginalia_text>
						Heu vana, et imprudens Diligentia: Numerus comparatur, vbi qua[e]ritur
						Pondus. Plinius L.7.c.40. Scientia non est quantitas,
					</marginalia_text>
					<person name="Pliny the Elder"/>
					<book title="Natural History"/>
					<emphasis method="pen" text="imprudens Diligentia" type="straight"/>
					<emphasis method="pen" text="Numerus comparatur" type="straight"/>
					<emphasis method="pen" text="Pondus" type="straight"/>
				</position>
				<position book_orientation="270" place="intext">
					<marginalia_text>
						sed qualitas.
					</marginalia_text>
				</position>
			</language>
			<translation>Alas, vain and imprudent diligence; number is established, where weight is sought. Pliny, book 7, chapter 40. Understanding is not quantity, but quality.</translation>
		</marginalia>
		
