            <language ident="EN">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>A false Rumor.</marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <underline language="EN" method="pen" text="the other syde" type="straight"/>
        <underline language="EN" method="pen" text="armye by the vpper part of the ryuer" type="straight"/>
        <underline language="EN" method="pen" text="besette the narowe see" type="straight"/>
        <underline language="EN" method="pen" text="spred abrode a rumoure" type="straight"/>
        <underline language="EN" method="pen" text="warre" type="straight"/>
        <underline language="EN" method="pen" text="bycause he hadde taken" type="straight"/>
        <underline language="EN" method="pen" text="hande" type="straight"/>
        <underline language="EN" method="pen" text="without consente of the people" type="straight"/>
        <underline language="EN" method="pen" text="made a face, as" type="straight"/>
        <underline language="EN" method="pen" text="beleuynge, that he wolde sayle thyther" type="straight"/>
        <underline language="EN" method="pen" text="about his shippes, and arriued" type="straight"/>
        <underline language="EN" method="pen" text="dekt and redy vnto warre" type="straight"/>
        <underline language="EN" method="pen" text="they had taken in battayle" type="straight"/>
        <underline language="EN" method="pen" text="the Carthagin. to go out before" type="straight"/>
        <underline language="EN" method="pen" text="thoughe they had come home agayne" type="straight"/>
        <underline language="EN" method="pen" text="with vyctorye" type="straight"/>
        <underline language="EN" method="pen" text="cowplynge theyr" type="straight"/>
        <underline language="EN" method="pen" text="shyppes vnto them on both sydes" type="straight"/>
        
