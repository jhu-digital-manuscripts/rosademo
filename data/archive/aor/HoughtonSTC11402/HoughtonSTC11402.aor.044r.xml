        <marginalia anchor_text="C. Cesar; Alexander" hand="Italian">
            <language ident="EN">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Ca[e]sar, &amp; Alexander, euer prouided,</marginalia_text>
                    <person name="Julius Caesar"/>
                    <person name="Alexander the Great"/>
                </position>
            </language>
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>ad omnia quare.</marginalia_text>
                </position>
            </language>
            <language ident="EN">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>neuer taken tardy, or napping. A present remedy for euery mischif.</marginalia_text>
                </position>
            </language>
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>aut reuerâ, aut sophisticè</marginalia_text>
                </position>
            </language>
            <translation>Ca[e]sar, &amp; Alexander, ever prouided, hence towards everything. Never taken tardy, or napping. A present remedy for every mischief. Either in reality or sophistically.</translation>
        </marginalia>
        
