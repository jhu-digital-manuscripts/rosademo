        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>Impedimentu[m] pro occasione.</marginalia_text>
                </position>
            </language>
            <translation>An impediment for the opportunity.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Bis interimit[u]r, qui suis Armis perit.</marginalia_text>
                </position>
            </language>
            <translation>He, who dies by his own weapons, is destroyed twice.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="EN">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>So Annibals Elephants seruid him.</marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <marginalia anchor_text="great shyps" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Nauale pra[e]liu[m].</marginalia_text>
                </position>
            </language>
            <translation>A naval battle.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Polymechania nunq[uam] deficit.</marginalia_text>
                </position>
            </language>
            <translation>Polymechanics never fails.</translation>
        </marginalia>
        
        <marginalia anchor_text="How to trouble and disorder the araye." hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="intext">
                    <marginalia_text>Pars victoria[e] est,</marginalia_text>
