        <marginalia hand="Italian">
            <language ident="EN">
                <position book_orientation="0" place="head">
                    <marginalia_text>Great marches, &amp; suddain surprizes, sumtime of woonderfull valour.</marginalia_text>
                </position>
            </language>
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>Vt i[nfr]a, c.I.l.3.</marginalia_text>
