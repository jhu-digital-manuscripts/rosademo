            <language ident="EN">
                <position book_orientation="0" place="intext">
                    <marginalia_text>How easely Ca[e]sar Borgia, the Dukedom of Vrbia</marginalia_text>
                    <person name="Cesare Borgia"/>
                </position>
            </language>
        </marginalia>
        
