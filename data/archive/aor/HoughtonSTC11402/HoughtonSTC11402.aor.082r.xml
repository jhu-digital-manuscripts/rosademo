        <marginalia hand="Italian">
            <language ident="EN">
                <position book_orientation="0" place="intext">
                    <marginalia_text>The Spanish</marginalia_text>
                </position>
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Armada, thie sommer, 1588. could not espy any such advantage against Ingland or other wise pryvaile, ether
                    by force, or pollicy.</marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>( Non solent periti Duces proclamare aduentum, aut minari conqua[e]stum, sed securos ex inopinato inuadere.</marginalia_text>
                </position>
            </language>
            <translation>Experienced generals are not accustomed to proclaim invasion, or to threaten conquest, 
                but to invade by surprise those secure.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>hypocritica affabilitas. ad Legatos.</marginalia_text>
                </position>
            </language>
            <translation>Hypocritic friendliness towards ambassadors.</translation>
        </marginalia>  
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>Fistula dulce canit, volucre[m] du[m] decipit Auceps. Lysandri humanissima oratio, l.I. Polua[e]ni.</marginalia_text>
                    <person name="Lysander"/>
                    <book title="Stratagems"/>
                    <X-ref book_title="Distichs of Cato " person="Dionysius Cato"/>
                </position>
            </language>
            <translation>'The shepherds-pipe sings sweetly, while the fowler traps the bird.' The most civilized speech of Lysander, book 1 of Polyaenus' Stratagems. </translation>
        </marginalia> 
        
