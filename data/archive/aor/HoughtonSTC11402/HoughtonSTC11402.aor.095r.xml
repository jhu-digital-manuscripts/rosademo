        <underline language="EN" method="pen" text="ageynst his commaundement" type="straight"/>
        <underline language="EN" method="pen" text="not withstandyng he hadde the vpper hande" type="straight"/>
        <underline language="EN" method="pen" text="he forgyue the punyshement" type="straight"/>
        <underline language="EN" method="pen" text="contention or intercession and" type="straight"/>
        <underline language="EN" method="pen" text="requeste of the souldiours" type="straight"/>
        <underline language="EN" method="pen" text="Rome pursued" type="straight"/>
        <underline language="EN" method="pen" text="remytte the dredefull punyshment" type="straight"/>
        <underline language="EN" method="pen" text="Fabius with his father" type="straight"/>
        <underline language="EN" method="pen" text="downe at his knees" type="straight"/>
        <underline language="EN" method="pen" text="senate &amp; people made intercession" type="straight"/>
        <underline language="EN" method="pen" text="named Manlius the proude or imperious" type="straight"/>
        <underline language="EN" method="pen" text="his sonne" type="straight"/>
        <underline language="EN" method="pen" text="agaynste" type="straight"/>
