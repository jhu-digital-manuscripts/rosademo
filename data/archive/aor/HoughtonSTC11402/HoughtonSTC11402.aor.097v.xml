        <marginalia hand="Italian">
            <language ident="EN">
                <position book_orientation="0" place="head">
                    <marginalia_text>A spare, and hard Diett. A snatch, &amp; away.</marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <marginalia anchor_text="Annibal, which vsed to ryse very yerly" hand="Italian">
            <language ident="EN">
