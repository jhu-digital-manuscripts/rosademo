                    <person name="Francis Drake"/>
                </position>
            </language>
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>cuius Contrarium, Verum est.</marginalia_text>
                </position>
            </language>
            <language ident="EN">
                <position book_orientation="0" place="tail">
                    <marginalia_text>A compound Stratagem</marginalia_text>
                </position>
            </language>
