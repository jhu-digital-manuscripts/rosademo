                <position book_orientation="0" place="right_margin">
                    <marginalia_text>( sed sine effectu.</marginalia_text>
                </position>
            </language>
            <translation>On of Mendozas fresh practises: but without effect.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="EN">
                <position book_orientation="0" place="tail">
                    <marginalia_text>Owr Admiral slain, &amp; Drake fledd:</marginalia_text>
