        <underline language="EN" method="pen" text="lyft wynge" type="straight"/>
        <underline language="EN" method="pen" text="in bothe wynges" type="straight"/>
        <underline language="EN" method="pen" text="souldiours, and well" type="straight"/>
        <underline language="EN" method="pen" text="on bothe wynges at ones" type="straight"/>
        <underline language="EN" method="pen" text="beste rule" type="straight"/>
        <underline language="EN" method="pen" text="his lyght harneised" type="straight"/>
        <underline language="EN" method="pen" text="the fyfte maner" type="straight"/>
        <underline language="EN" method="pen" text="neyther in the" type="straight"/>
        <underline language="EN" method="pen" text="strength" type="straight"/>
        <underline language="EN" method="pen" text="must nedes fyght, let" type="straight"/>
        <underline language="EN" method="pen" text="ennemies left wynge" type="straight"/>
        
