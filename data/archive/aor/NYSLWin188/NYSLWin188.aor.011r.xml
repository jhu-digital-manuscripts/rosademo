        <underline language="DE" method="pen" text="sein Spiritus" type="double"/>
        <underline language="DE" method="pen" text="Seel" type="double"/>
        <underline language="DE" method="pen" text="Geist ist der" type="straight"/>
        <underline language="DE" method="pen" text="Farbe der geist" type="triple"/>
        <underline language="DE" method="pen" text="Also seindt alle Früchte seinem Element gleich mit dem" type="straight"/>
        <underline language="LA" method="pen" text="Corpus vnd Spiritum" type="double"/>
        <underline language="DE" method="pen" text="d[as] der Spiritus ist" type="double"/>
        <underline language="DE" method="pen" text="natürlichen Geist" type="straight"/>
        <underline language="DE" method="pen" text="vnd unnatürlichen" type="straight"/>
        <underline language="DE" method="pen" text="Natürlich ist Corporalisch" type="double"/>
        <underline language="DE" method="pen" text="der vnnattürliche Geist ist" type="straight"/>
        <underline language="DE" method="pen" text="gar ohn ein Corpus" type="double"/>
        <underline language="DE" method="pen" text="Rötte der Corallen" type="double"/>
        <underline language="DE" method="pen" text="ists nicht" type="straight"/>
        <underline language="DE" method="pen" text="Rötte ein Corpus" type="double"/>
        <underline language="DE" method="pen" text="ist aber so gar allein des" type="straight"/>
        <underline language="DE" method="pen" text="nichts fauls oder grobs mehr da ist" type="triple"/>
        <underline language="DE" method="pen" text="Geist der früchten" type="triple"/>
