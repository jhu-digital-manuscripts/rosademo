<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="NYSLWin188.011r.tif" pagination="11" reader="John Dee" signature="Ciij"/> 
    <annotation>
        
        <marginalia anchor_text="Also seindt alle Früchte seinem Element gleich mit dem Corpus vnd Spiritum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Regula.</marginalia_text>
                </position>
            </language>
            <translation>Rule.</translation>
        </marginalia>
        
        <marginalia anchor_text="Es ist aber ein vnderscheidt zwischen dem natürlichen Geist vnd vnnatürlichen der Natürlich ist Corporalisch vn[d] natürlich eingeleibt in ein besonder Corpus der vnnattürliche Geist ist gar ohn ein Corpus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>NOTA.</marginalia_text>
                </position>
            </language>
            <translation>Note.</translation>
        </marginalia>
        
        <marginalia anchor_text="Der Buxus hat ein geschmack derselbige geschmack ist sein Spiritus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Spiritus Buxi ex gustu sentitur.</marginalia_text>
                </position>
            </language>
            <translation>The spirit of the boxweed is perceived by taste.</translation>
        </marginalia>
        
        <marginalia anchor_text="Also im Corallen ist die Farbe der geist das ander ein Corpus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Color rubeus Coralloru[m], est eoru[m] spiritus.</marginalia_text>
                </position>
            </language>
            <translation>The red colour of corals is their spirit.</translation>
        </marginalia>
        
        <marginalia anchor_text="die Frucht" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Fructus quid.</marginalia_text>
                </position>
            </language>
            <translation>What is the fruit.</translation>
        </marginalia>
        
        <marginalia anchor_text="Es ist aber ein vnderscheidt zwischen dem natürlichen Geist vnd vnnatürlichen" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Spiritus</marginalia_text>
                </position>
            </language>
            <language ident="EN">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>- natural - Innatur.</marginalia_text>
                </position>
            </language>
            <translation>Spirit. Natural. Innatural.</translation>
        </marginalia>
        
        <marginalia anchor_text="die Rötte der Corallen" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Corallorum Rubedo</marginalia_text>
                </position>
            </language>
            <translation>The redness of the corals.</translation>
        </marginalia>
        
        <marginalia anchor_text="Corpus ist aber so gar allein des Geists also das nichts fauls oder grobs mehr da ist" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Corpus spirituale</marginalia_text>
                </position>
            </language>
            <translation>The spiritual body.</translation>
        </marginalia>
        
