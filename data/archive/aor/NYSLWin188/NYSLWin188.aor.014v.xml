    <annotation>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>Stella[e] ex Co[e]lo orta[e] sunt, et in eo manent.</marginalia_text>
                </position>
            </language>
            <translation>The stars arise from the heaven and remain in it.</translation>
        </marginalia>
        
        <underline language="DE" method="pen" text="nach dem vnd" type="straight"/>
        <underline language="DE" method="pen" text="Sternen vnd jrem vrsprung" type="straight"/>
        <underline language="DE" method="pen" text="in jr nichts" type="double"/>
        <underline language="DE" method="pen" text="das treibt sie auff die Erden vnd behalt nichts in jr gibt" type="double"/>
        <underline language="DE" method="pen" text="von der Erden" type="straight"/>
        <underline language="DE" method="pen" text="vnd auff" type="straight"/>
        <underline language="DE" method="pen" text="in der Erden ist nichts" type="straight"/>
        <underline language="DE" method="pen" text="auß dem Himmel" type="straight"/>
        <underline language="DE" method="pen" text="alle Gestirn" type="double"/>
        <underline language="DE" method="pen" text="vnd auß jm genommen" type="double"/>
        <underline language="DE" method="pen" text="aber müssen nicht still stehen sondern sie müssen fürgehen jren Circkel" type="straight"/>
        <underline language="DE" method="pen" text="sie kugeln" type="double"/>
        <underline language="DE" method="pen" text="für waltzen vnd wallendt" type="straight"/>
        <underline language="DE" method="pen" text="Vnd seindt also von Himmel gescheiden vn[d] doch im Himmel" type="straight"/>
        <underline language="DE" method="pen" text="eingeleibt" type="double"/>
        
        <drawing anchor_text="Vnd seindt also von Himmel gescheiden vn[d] doch im Himmel" book_orientation="0" method="pen" place="tail" type="manicule"/>
        
