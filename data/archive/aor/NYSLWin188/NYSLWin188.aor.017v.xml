    <page filename="NYSLWin188.017v.tif" pagination="[17[v]]" reader="John Dee" signature="[E[v]]"/> 
    <annotation>
        
        <underline language="DE" method="pen" text="ist der Mond vnd die finsternuß" type="straight"/>
        <underline language="DE" method="pen" text="das die hinweg gehung der Sonnen die Nacht nicht macht" type="straight"/>
        <underline language="DE" method="pen" text="mitternacht vnder vns ist noch würd der schein auf Erden seinen Reflexum geben" type="straight"/>
        <underline language="DE" method="pen" text="das ein sondere arth ist der Sternen die dunckel machen" type="straight"/>
        <underline language="DE" method="pen" text="vnd befinstert so viel wo der" type="straight"/> 
        <underline language="DE" method="pen" text="nicht ist" type="double"/>
        <underline language="DE" method="pen" text="solche gantze Finsternuß in welcher Finsternuß allein der Mond sein schein hat durch die" type="straight"/>
        
    </annotation>
</transcription>
