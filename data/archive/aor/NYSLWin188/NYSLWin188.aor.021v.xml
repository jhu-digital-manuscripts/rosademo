    <page filename="NYSLWin188.021v.tif" pagination="[21[v]]" reader="John Dee" signature="[F[v]]"/> 
    <annotation>
        
        <marginalia anchor_text="Die Superi seindt die Pennates" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Superi Pennates</marginalia_text>
                </position>
            </language>
            <translation>Penates are gods.</translation>
        </marginalia>
        
        <marginalia anchor_text="es ist warhafftig vo[n] Gnomis von Nymphen" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Verissimè, su[n]t Nympha[e] e[t] Gnomi.</marginalia_text>
                </position>
            </language>
            <translation>Most truthfully, there are nymphs and gnomes.</translation>
        </marginalia>
        
        <underline language="DE" method="pen" text="durch verstandt" type="triple"/>
        <underline language="DE" method="pen" text="obern Superorum" type="double"/>
        <underline language="DE" method="pen" text="wissenheit" type="double"/>
        <underline language="DE" method="pen" text="ein Geist" type="double"/>
        <underline language="DE" method="pen" text="Was in der Welt ist das auch im Firmament wissenhafft bein jn sein. Dann" type="straight"/>
        <underline language="DE" method="pen" text="vber des Mensche[n] verstandt wie bißher so ist" type="straight"/>
        <underline language="DE" method="pen" text="den obern" type="double"/>
        <underline language="DE" method="pen" text="als schützen zu einem zil" type="double"/>
        <underline language="DE" method="pen" text="geschickligkeit" type="straight"/>
        <underline language="DE" method="pen" text="was gewaldt sie haben" type="triple"/>
        <underline language="DE" method="pen" text="der Philosophey" type="straight"/>
        
    </annotation>
