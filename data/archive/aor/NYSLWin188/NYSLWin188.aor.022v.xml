    <annotation>
        
        <marginalia anchor_text="Theologia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Theologia</marginalia_text>
                </position>
            </language>
            <translation>Theology.</translation>
        </marginalia>
        
        <marginalia anchor_text="das Gott den Menschen beschaffen hat vnd dem alles vnderworffen was er geschaffen" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>O[mn]ia subiecisti sub pedibus eius.</marginalia_text>
                </position>
            </language>
            <translation>You place everthing under his feet.</translation>
        </marginalia>
        
        <marginalia anchor_text="Darumb seindt sie auch de[m] Menschen gar nichts vnderworffen" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Ista[e] [quatuor] species no[n] sunt ho[min]ibus subiecta[e]: i[m]mo no[n] possu[n]t ab ho[min]ibus cogi.</marginalia_text>
                </position>
            </language>
            <translation>Those four species are not subjected to mankind: indeed, they cannot be confined by men.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Nota.</marginalia_text>
                </position>
            </language>
            <translation>Note.</translation>
        </marginalia>
        
        <underline language="DE" method="pen" text="beschaffen hat vnd dem alles vnderworffen was" type="straight"/>
