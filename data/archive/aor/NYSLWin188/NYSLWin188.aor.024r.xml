            </language>
            <translation>Note the example.</translation>
        </marginalia>
        
        <marginalia anchor_text="Vulcanus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>in o[mn]ibus Elementis</marginalia_text> 
                </position>
            </language>
            <translation>[Vulcan is] in all elements.</translation>
        </marginalia>
        
