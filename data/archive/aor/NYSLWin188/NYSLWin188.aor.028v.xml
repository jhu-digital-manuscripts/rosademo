            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>frigidus humidus</marginalia_text>
                </position>
            </language>
            <translation>Cold [and] humid.</translation>
        </marginalia>
        
        <marginalia anchor_text="die Natur des Windes materien" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>Venti Materia in tribus primis.</marginalia_text>
                </position>
            </language>
            <translation>The matter of the wind in three primes.</translation>
        </marginalia>
        
        <underline language="DE" method="pen" text="gehet der Windt durch den lufft vber das Meer vnd" type="straight"/>
        <underline language="DE" method="pen" text="langer vbung vnd weither reise sich selbst verzert" type="straight"/>
        <underline language="DE" method="pen" text="kein Materia" type="double"/>
        <underline language="DE" method="pen" text="ist warm vn[d] trucken" type="straight"/>
        <underline language="DE" method="pen" text="den dreien Ersten ist" type="double"/>
        <underline language="DE" method="pen" text="zu sein vnd nicht wider sein natur" type="straight"/>
        <underline language="DE" method="pen" text="mitgeselle[n] kein andere natur vn[d] operation erfunden alle warm vnd trucken" type="straight"/>
        <underline language="DE" method="pen" text="kaldt vnd feucht" type="straight"/>
        <underline language="DE" method="pen" text="der Nidergang ist der Complexion wie der auffgang aber" type="straight"/>
        <underline language="DE" method="pen" text="Natur des Windes materien im Nidergang" type="straight"/>
        <underline language="DE" method="pen" text="geschaffen kalt vnd feuchte" type="straight"/>
        
