    <annotation>
        
        <marginalia anchor_text="Dan[n] auch Christus in Parabolis sein Wort verkündet hat" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Parabolaris Doctrina</marginalia_text>
                </position>
            </language>
            <translation>An allegorical doctrine.</translation>
        </marginalia>
        
        <marginalia anchor_text="die zeit vnd stunde solcher Winder aufferstehung" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Tempus et hora quando ventus suborietur</marginalia_text>
                </position>
            </language>
            <translation>The time and hour when the wind will come into being.</translation>
        </marginalia>
        
        <marginalia anchor_text="Astronomisch" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>Astronomia tempora ventoru[m] docere debet</marginalia_text>
                </position>
            </language>
            <translation>Astronomy ought to teach the times of the winds.</translation>
        </marginalia>
        
