    <annotation>
        
        <marginalia anchor_text="ist kaldt vnd feuchte in der höhe" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>Aer est frigidus et humidus in summo gradu.</marginalia_text>
                </position>
            </language>
            <translation>The air is cold and humid in its highest degree.</translation>
        </marginalia>
        
        <marginalia anchor_text="warme Regen" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Pluuia[e] Calida[e]</marginalia_text>
                </position>
            </language>
            <translation>Warm rains.</translation>
        </marginalia>
        
        <marginalia anchor_text="Zephyrische Windt" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Zephirus pluuiosus</marginalia_text>
                </position>
            </language>
            <translation>Rainy west wind.</translation>
        </marginalia>
        
        <marginalia anchor_text="er würdt vom dürren vertrieben vnd verzert" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Pluuia co[n]sumitur</marginalia_text>
                </position>
            </language>
            <translation>The rain is reduced.</translation>
        </marginalia>
        
