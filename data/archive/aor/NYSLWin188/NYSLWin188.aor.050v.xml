        <marginalia anchor_text="Superis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Nota. praxim supernam</marginalia_text>
                </position>
            </language>
            <translation>Note. Celestial practice.</translation>
        </marginalia>
        
        <underline language="DE" method="pen" text="jrdischen Sulphur" type="straight"/>
        <underline language="DE" method="pen" text="jrdisch Salnitrum" type="straight"/>
        <underline language="DE" method="pen" text="jrdischen Mercurium" type="straight"/>
        <underline language="DE" method="pen" text="kunst in ein Corpus" type="double"/>
        <underline language="DE" method="pen" text="nach jrdischer arth ein wund" type="straight"/>
        <underline language="DE" method="pen" text="erbarlichen impetum finden" type="straight"/>
        <underline language="DE" method="pen" text="genommen wird von den" type="straight"/>
        <underline language="DE" method="pen" text="Superis" type="double"/>
        <underline language="DE" method="pen" text="Magistris" type="double"/>
        <underline language="DE" method="pen" text="vn[d] zusam[m]en in Aludel das ist in seinen Sternen" type="straight"/>
        <underline language="DE" method="pen" text="zerlassen den Sulphur" type="double"/>
        <underline language="DE" method="pen" text="der Salniter" type="double"/>
        <underline language="DE" method="pen" text="vereinigt er sich zusammen" type="straight"/>
        <underline language="DE" method="pen" text="doch ohne bewegung vnd impetu" type="straight"/>
        <underline language="DE" method="pen" text="vbrige gradus der Hitze wie vor gemelt" type="straight"/>
