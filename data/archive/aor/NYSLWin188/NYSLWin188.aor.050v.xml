    <annotation>
        
        <marginalia anchor_text="Nement ein jrdischen Sulphur, ein jrdisch Salnitrum, ein jrdischen Mercurium, vnd die drey bringent durch kunst in ein Corpus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>Sal nitru[m] vulgare ☿ &#128781; } in vnu[m] componere</marginalia_text>
                    <symbol_in_text name="Mercury"/>
                    <symbol_in_text name="Salt"/>
                </position>
            </language>
            <translation>Vulgar salnitrum, mercury, and salt composed into one.</translation>
        </marginalia>
        
        <marginalia anchor_text="Superis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Superi</marginalia_text>
                </position>
