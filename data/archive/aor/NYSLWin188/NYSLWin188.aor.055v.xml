        <marginalia anchor_text="Tonitru fulguris Christi" hand="Italian" method="pen">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Tonitru fulguris Christi
                    </marginalia_text>
                    <person name="Jesus Christ"/>
                </position>
            </language>
            <translation>Thunder and lightning of Jesus Christ.</translation>
        </marginalia>
        
        <marginalia anchor_text="fro[e]sche Würme oder andere lebendige Thier von oben herab geboren" hand="Italian" method="pen">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
