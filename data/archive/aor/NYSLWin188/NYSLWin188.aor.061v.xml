            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        fulminus lapis
                    </marginalia_text>
                </position>
            </language>
            <translation>Crushed stone.</translation>
        </marginalia>
        
        <underline language="DE" method="pen" text="ist sondern ein seltzames" type="straight"/>
        <underline language="DE" method="pen" text="Stral der kalt" type="double"/>
        <underline language="DE" method="pen" text="ist nichts als" type="straight"/>
        <underline language="DE" method="pen" text="härter sandt" type="double"/>
        <underline language="DE" method="pen" text="So nun die natur" type="straight"/>
        <underline language="DE" method="pen" text="kalten Stral" type="double"/>
        <underline language="DE" method="pen" text="ein stein gebürt" type="straight"/>
        <underline language="DE" method="pen" text="wol möglich" type="double"/>
        <underline language="DE" method="pen" text="steine falle[n] ohne Strals in die Erden" type="straight"/>
        <underline language="DE" method="pen" text="Pra[e]sagia seindt" type="straight"/>
        <underline language="DE" method="pen" text="Denn Sam haben sie wol von kalter influxion generationis" type="straight"/>
        <underline language="DE" method="pen" text="dieselbige Materien nit zum resoluiere[n] kom[m]en lassen" type="straight"/>
        <underline language="DE" method="pen" text="als dann so die zeit kompt der stunde fallen lassen in die Erden" type="straight"/>
        <underline language="DE" method="pen" text="sie seindt fix bleiben auff Erden liegen" type="straight"/>
        
