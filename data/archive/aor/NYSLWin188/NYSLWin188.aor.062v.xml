        <marginalia anchor_text="dann mit fewr erhalten werden" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Igne aluntur sydera
                    </marginalia_text>
                </position>
            </language>
            <translation>Stars are maintained by fire.</translation>
        </marginalia>

