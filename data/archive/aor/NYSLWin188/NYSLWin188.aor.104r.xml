    <page filename="NYSLWin188.104r.tif" pagination="104" reader="John Dee"/> 
    <annotation>
        
        <marginalia anchor_text="Als so der Centrum matricis kranck wirdt so wirdt er offt von der Matrix curiert" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Matrix integru[m] corpus. Centru[m]. Matrix vulgariter.
                    </marginalia_text>
                </position>
            </language>
            <translation>The matrix, an uninjured body. The centre. The matrix, commonly.</translation>
        </marginalia>
        
        <underline language="DE" method="pen" text="ein geschmack" type="straight"/>
        <underline language="DE" method="pen" text="Also seind alle kra[n]ckheite[n] so auß" type="straight"/>
        <underline language="DE" method="pen" text="Die aber auß dem leib in die Mutter gehe[n]t die" type="straight"/>
        <underline language="DE" method="pen" text="seind leiblich mit jre[n] Corporib[us]" type="straight"/>
        <underline language="DE" method="pen" text="d[ass] die Geist so auß der Muter in den leib gehent geferbt seindt d[ass] ist sie haben" type="straight"/>
        <underline language="DE" method="pen" text="vo[n] vitriol" type="straight"/>
        <underline language="DE" method="pen" text="ferbet" type="double"/>
        <underline language="DE" method="pen" text="Als so der Centrum matricis kranck wirdt so" type="straight"/>
        <underline language="DE" method="pen" text="von der Matrix" type="straight"/>
        <underline language="DE" method="pen" text="Matrix vo[n] Centro" type="straight"/>
        <underline language="DE" method="pen" text="Dan[n] gleich auß der wurtzen da die kra[n]ckheit gehet auß der selbe[n] muß auch die gesundheit gohn" type="straight"/>
        <underline language="DE" method="pen" text="Vnd da die gesundtheit außgehet das muß auch die kra[n]ckheit außgohn" type="straight"/>
        
        <mark method="pen" name="bracket" place="right_margin"/>
        
    </annotation>
