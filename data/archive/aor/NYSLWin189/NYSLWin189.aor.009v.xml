    <annotation>
        
        <marginalia anchor_text="Denn was in oder ob der erden ist můß alles dem himmel zů" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>A Centro o[mn]ia contendu[n]t [a]d Circumfere[n]tia[m]</marginalia_text>
                </position>
            </language>
            <translation>Everything strives from the centre for the circumference.</translation>
        </marginalia>
        
        <marginalia anchor_text="das solcher wasser gengen ein abschneiden geschicht" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Cataracta[e] Thermaru[m] no[n] numq[am] interclusa[e]</marginalia_text>
                </position>
            </language>
            <translation>The cataracts of the baths are always blockaded.</translation>
        </marginalia>
         
        <underline language="DE" method="pen" text="nicht warm wasser cataracten vnderligen" type="straight"/>
