    <annotation>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>Istam primam materia[m] triu[m], Ego, Q, Ess[enti]am eorundem haud male iudicari censeo</marginalia_text> <!--JG: Could Essam be 'Essentiam', since 'am' appears in superscript?-->
                </position>
            </language>
            <translation>I, Q, recommend that this prime matter of the three, their essence, is by no means to be judged badly.</translation> <!-- JG: Not sure, no clue about 'Q'-->
        </marginalia>
        
        <marginalia anchor_text="die sieben mineralia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>♄♃♂☉♀☿</marginalia_text>
                    <symbol_in_text name="Saturn"/>
                    <symbol_in_text name="Jupiter"/>
                    <symbol_in_text name="Mars"/>
                    <symbol_in_text name="Sun"/>
                    <symbol_in_text name="Venus"/>
                    <symbol_in_text name="Mercury"/>
                    <symbol_in_text name="Moon"/>
                </position>
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>7 Mineralia</marginalia_text>
                </position>
            </language>
            <translation>Saturn, Jupiter, Mars, Sun, Venus, Mercury, Moon. 7 Minerals.</translation>
        </marginalia>
               
        <marginalia anchor_text="die tugend krafft vnnd eygenschafft" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Thermarum virtus vnde</marginalia_text>
                </position>
            </language>
            <translation>Hence the properties of the baths.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Confortatio</marginalia_text>
                </position>
            </language>
            <translation>Invigoration.</translation>
        </marginalia>
        
        <underline language="DE" method="pen" text="So die sieben mineralia ligen in primis tribus" type="straight"/>
        <underline language="DE" method="pen" text="verböseret" type="straight"/>
        <underline language="DE" method="pen" text="so mischt sich die krafft" type="straight"/>
        <underline language="DE" method="pen" text="in ein vereinigung" type="straight"/>
        <underline language="DE" method="pen" text="auß disem" type="straight"/>
        <underline language="DE" method="pen" text="primarum trium" type="straight"/>
        <underline language="DE" method="pen" text="inn eim wasser" type="straight"/>
        <underline language="DE" method="pen" text="dem selbigen" type="straight"/>
        <underline language="DE" method="pen" text="durch es laufft" type="straight"/>
        <underline language="DE" method="pen" text="das in jnen ist die prima materia trium" type="straight"/>
        <underline language="DE" method="pen" text="der contractur" type="straight"/>
        <underline language="LA" method="pen" text="hyposarcha" type="straight"/>
        <underline language="LA" method="pen" text="materice" type="straight"/>
        <underline language="DE" method="pen" text="so auß der sterckung kommen" type="straight"/>
        <underline language="DE" method="pen" text="schedlich aber den kranckheyten so die glider" type="straight"/>
        <underline language="LA" method="pen" text="alopiciam" type="straight"/>
        <underline language="LA" method="pen" text="morpheam" type="straight"/>
        <underline language="LA" method="pen" text="pruritum" type="straight"/>
        <underline language="LA" method="pen" text="vlcera communia" type="straight"/>
        <underline language="LA" method="pen" text="in sale metallorum" type="straight"/>
        <underline language="DE" method="pen" text="febriu[m]" type="straight"/>

