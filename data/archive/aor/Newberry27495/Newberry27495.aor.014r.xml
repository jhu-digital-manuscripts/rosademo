    <annotation>
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>Egregia Praxis perferendi malam, fortunam: ut s[upr]a, in Duce marito, Guidubaldo.</marginalia_text>
                    <person name="Guidobaldo da Montefeltro"/>
                    <internal_ref>
                        <target ref="Newberry27495:case_712_c27495_035" text="s[upr]a"/>
                    </internal_ref>
                </position>
            </language>
            <translation>The wonderful practice of enduring bad fortune: as above, in the married Duke, Guidobaldo.</translation>
        </marginalia>
        
        <underline language="EN" method="pen" text="contentacyon" type="straight"/>
        <underline language="EN" method="pen" text="into the dutchesse" type="straight"/>
        <underline language="EN" method="pen" text="a chaine that kept all lincked together in loue" type="straight"/>
        <underline language="EN" method="pen" text="agrement of wyll or hearty loue greater betweene brethren" type="straight"/>
        <underline language="EN" method="pen" text="was there beetweene vs all" type="straight"/>
        <underline language="EN" method="pen" text="the women" type="straight"/>
        <underline language="EN" method="pen" text="free and honest conuersation" type="straight"/>
        <underline language="EN" method="pen" text="myght commune, syt, daly, and laugh with whome he had lusted" type="straight"/>
        <underline language="EN" method="pen" text="Dutchesse wyll" type="straight"/>
        <underline language="EN" method="pen" text="libertye was" type="straight"/>
        <underline language="EN" method="pen" text="great bridle" type="straight"/>
        <underline language="EN" method="pen" text="there anye that" type="straight"/>
        <underline language="EN" method="pen" text="greatest pleasure he coulde haue in the worlde" type="straight"/>
        <underline language="EN" method="pen" text="please her" type="straight"/>
        <underline language="EN" method="pen" text="the greatest griefe to offende her" type="straight"/>
        <underline language="EN" method="pen" text="deuises of pastimes" type="straight"/>
        <underline language="EN" method="pen" text="laughine matters tempred in her" type="straight"/>
        <underline language="EN" method="pen" text="wyttye iestes" type="straight"/>
        <underline language="EN" method="pen" text="comelye and graue a" type="straight"/>
        <underline language="EN" method="pen" text="moode and greatnesse that" type="straight"/>
        <underline language="EN" method="pen" text="iesting and laughynge" type="straight"/>
        <underline language="EN" method="pen" text="seene her in their lief before" type="straight"/>
        <underline language="EN" method="pen" text="count her a verye greate Ladye" type="straight"/>
        <underline language="EN" method="pen" text="had made them to her becke" type="straight"/>
        <underline language="EN" method="pen" text="and ensample" type="straight"/>
        <underline language="EN" method="pen" text="faire condicions" type="straight"/>
        <underline language="EN" method="pen" text="presence of so greate and so vertuous a Lady" type="straight"/>
        <underline language="EN" method="pen" text="fortune" type="straight"/>
        <underline language="EN" method="pen" text="wondreth at so rare vertues" type="straight"/>
        <underline language="EN" method="pen" text="with many aduersities and temptatyons of miseries to disclose them" type="straight"/>
        <underline language="EN" method="pen" text="trial thereby" type="straight"/>
        
        <underline language="EN" method="pen" text="behauy[.]oure of" type="straight"/> <!--JG: Unsure. Other copies have 'behauyoure'-->
        <underline language="EN" method="pen" text="Dutchesse" type="straight"/>
        
