        </marginalia>
        
        <underline language="EN" method="pen" text="few men excellent in" type="straight"/>
        <underline language="EN" method="pen" text="they praise the[m] selues" type="straight"/>
        <underline language="EN" method="pen" text="of skill" type="straight"/>
        <underline language="EN" method="pen" text="the ignoraunte" type="straight"/>
        <underline language="EN" method="pen" text="disdeigne" type="straight"/>
        <underline language="EN" method="pen" text="should lye buried" type="straight"/>
        <underline language="EN" method="pen" text="he open it" type="straight"/>
        <underline language="EN" method="pen" text="defrauded of the estimation" type="straight"/>
        <underline language="EN" method="pen" text="auncient" type="straight"/>
        <underline language="EN" method="pen" text="muche excelleth" type="straight"/>
        <underline language="EN" method="pen" text="forbeare praisyng hymself" type="straight"/>
        <underline language="EN" method="pen" text="hauyng no skill" type="straight"/>
        <underline language="EN" method="pen" text="praysynge of a mans selfe impudently and withoute respecte" type="straight"/>
        <underline language="EN" method="pen" text="skilfull man that praiseth hymselfe dyscretely, but" type="straight"/>
        <underline language="EN" method="pen" text="for a more certaine witnes" type="straight"/>
        <underline language="EN" method="pen" text="out of an other mans mouth" type="straight"/>
        <underline language="EN" method="pen" text="praising himselfe falleth not into errour" type="straight"/>
        <underline language="EN" method="pen" text="purchaseth himself lothsomenes or hatred of the hearers" type="straight"/>
        <underline language="EN" method="pen" text="moste discrete" type="straight"/>
        <underline language="EN" method="pen" text="himselfe" type="straight"/>
        <underline language="EN" method="pen" text="very harde" type="straight"/>
        <underline language="EN" method="pen" text="auntient writers" type="straight"/>
        <underline language="EN" method="pen" text="taught" type="straight"/>
        <underline language="EN" method="pen" text="after a sort" type="straight"/>
        <underline language="EN" method="pen" text="not rehearsed to that ende" type="straight"/>
        <underline language="EN" method="pen" text="come so to purpose" type="straight"/>
        <underline language="EN" method="pen" text="not refrayne tellyng" type="straight"/>
        <underline language="EN" method="pen" text="tell the trueth" type="straight"/>
        <underline language="EN" method="pen" text="open their mouthe" type="straight"/>
        <underline language="EN" method="pen" text="beyng pusshed" type="straight"/>
        
        <underline language="EN" method="pen" text="In what sort a man maye priase himself" type="straight"/>
        
