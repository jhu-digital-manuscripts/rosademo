    <annotation>
        
        <underline language="EN" method="pen" text="your countenaunce is most acceptable" type="straight"/>
        <underline language="EN" method="pen" text="pleasant to beholde" type="straight"/>
        <underline language="EN" method="pen" text="manly and" type="straight"/>
