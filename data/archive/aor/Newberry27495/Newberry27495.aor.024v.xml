        <underline language="EN" method="pen" text="in goyng" type="straight"/>
        <underline language="EN" method="pen" text="tender and feint, that" type="straight"/>
        <underline language="EN" method="pen" text="drawningly, that" type="straight"/>
        <underline language="EN" method="pen" text="the higher in degree" type="straight"/>
        <underline language="EN" method="pen" text="vse suche" type="straight"/>
