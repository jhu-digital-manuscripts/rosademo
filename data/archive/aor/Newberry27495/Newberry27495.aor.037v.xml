    <annotation>
        
        <underline language="EN" method="pen" text="enriching this tunge," type="straight"/>
        <underline language="EN" method="pen" text="it maiestye and light" type="straight"/>
        <underline language="EN" method="pen" text="it poore" type="straight"/>
        <underline language="EN" method="pen" text="to shut it vp into so narrowe a rowme" type="straight"/>
        <underline language="EN" method="pen" text="folow onely" type="straight"/>
        <underline language="EN" method="pen" text="Laurence" type="straight"/>
        <underline language="EN" method="pen" text="great miserye" type="straight"/>
        <underline language="EN" method="pen" text="to stoppe" type="straight"/>
        <underline language="EN" method="pen" text="without wading any farther" type="straight"/>
        <underline language="EN" method="pen" text="many and so noble wittes shall neuer find out" type="straight"/> 
        <underline language="EN" method="pen" text="then one good maner of speach in [the] tung that" type="straight"/>
        <underline language="EN" method="pen" text="proper &amp; naturall" type="straight"/>
        <underline language="EN" method="pen" text="scrupulous" type="straight"/>
        <underline language="EN" method="pen" text="religion &amp; high" type="straight"/>
        <underline language="EN" method="pen" text="such an awe" type="straight"/>
        <underline language="EN" method="pen" text="open their mouth" type="straight"/>
        <underline language="EN" method="pen" text="of their nurses" type="straight"/>
        <underline language="EN" method="pen" text="their cradel" type="straight"/>
        <underline language="EN" method="pen" text="the opinions and wittes of men are" type="straight"/>
        <underline language="EN" method="pen" text="vehement and quicke of nature" type="straight"/>
        <underline language="EN" method="pen" text="soft and quiet" type="straight"/>
        <underline language="EN" method="pen" text="seuere and graue" type="straight"/>
        <underline language="EN" method="pen" text="should apply him" type="straight"/>
        <underline language="EN" method="pen" text="own proper inclinatio[n]" type="straight"/>
        <underline language="EN" method="pen" text="the nature of their scholers" type="straight"/>
        <underline language="EN" method="pen" text="the yll husbandmanne" type="straight"/>
        <underline language="EN" method="pen" text="sometime in a solyle" type="straight"/>
        <underline language="EN" method="pen" text="for vynes" type="straight"/>
        <underline language="EN" method="pen" text="will sowe graine" type="straight"/>
        
        <mark language="EN" method="pen" name="bracket" place="left_margin" text="And truly it should be a great miserye to stoppe, without wading any farther then almost the first that euer wrote: and to dispaire, that             so many and so noble wittes shall neuer find out any mo then one good maner of speach in [the] tung that vnto them is proper &amp; naturall"/>
