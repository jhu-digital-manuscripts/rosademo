    <annotation>
        
        <underline language="EN" method="pen" text="true glorye" type="straight"/>
        <underline language="EN" method="pen" text="is stored vp" type="straight"/>
        <underline language="EN" method="pen" text="holy treasure of letters" type="straight"/>
        <underline language="EN" method="pen" text="so base a courage" type="straight"/>
        <underline language="EN" method="pen" text="actes and greatnesse" type="straight"/>
        <underline language="EN" method="pen" text="not incensed with a most feruent" type="straight"/>
        <underline language="EN" method="pen" text="be like them" type="straight"/>
        <underline language="EN" method="pen" text="perpetuall fame" type="straight"/>
        <underline language="EN" method="pen" text="lasteth twoo dayes" type="straight"/>
        <underline language="EN" method="pen" text="sauoureth not the sweetnesse of" type="straight"/>
        <underline language="EN" method="pen" text="greatnesse of glorye" type="straight"/>
        <underline language="EN" method="pen" text="perserued by them" type="straight"/>
        <underline language="EN" method="pen" text="this shorte glorye" type="straight"/>
        <underline language="EN" method="pen" text="is euerlastinge" type="straight"/>
        <underline language="EN" method="pen" text="not barred from" type="straight"/>
        <underline language="EN" method="pen" text="neuer hasard" type="straight"/>
        <underline language="EN" method="pen" text="hymself so much to come by it" type="straight"/>
        <underline language="EN" method="pen" text="the contrarye effectes" type="straight"/>
        <underline language="EN" method="pen" text="the Italians with" type="straight"/>
        <underline language="EN" method="pen" text="their knowleage of letters" type="straight"/>
        <underline language="EN" method="pen" text="small prowesse in armes" type="straight"/>
        <underline language="EN" method="pen" text="offence of a few" type="straight"/>
        <underline language="EN" method="pen" text="euerlasting reproche vnto all" type="straight"/>
        <underline language="EN" method="pen" text="our confusion" type="straight"/>
        <underline language="EN" method="pen" text="neglecting of vertue" type="straight"/>
        <underline language="EN" method="pen" text="dead" type="straight"/>
        <underline language="EN" method="pen" text="to publishe" type="straight"/>
        <underline language="EN" method="pen" text="Frenchmen the ignoraunce in letters" type="straight"/>
        <underline language="EN" method="pen" text="whom in letter I will haue" type="straight"/>
        <underline language="EN" method="pen" text="more then" type="straight"/>
        
        <underline language="EN" method="pen" text="true glorye" type="straight"/>
        <underline language="EN" method="pen" text="Noble courages enflamed in readyng the actes of famous captaines" type="straight"/>
