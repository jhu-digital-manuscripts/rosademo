    <page filename="Newberry27495.062v.tif" reader="Harvey" signature="[N.iiii.[v]]"/>
    <annotation>
        
        <underline language="EN" method="pen" text="dissemble [the] malice" type="straight"/>
        <underline language="EN" method="pen" text="men laugh them to scorne" type="straight"/>
        <underline language="EN" method="pen" text="fauour some one" type="straight"/>
        <underline language="EN" method="pen" text="to spite them" type="straight"/>
        <underline language="EN" method="pen" text="enter in fauour" type="straight"/>
        <underline language="EN" method="pen" text="so dronken" type="straight"/>
        <underline language="EN" method="pen" text="that thei wist not what wer become of their feete and handes" type="straight"/>
        <underline language="EN" method="pen" text="to cal company to behoulde them" type="straight"/>
        <underline language="EN" method="pen" text="him esteame fauour and" type="straight"/>
        <underline language="EN" method="pen" text="could not liue without" type="straight"/>
        <underline language="EN" method="pen" text="whan he hath" type="straight"/>
        <underline language="EN" method="pen" text="let him not shew himself new or straunge" type="straight"/>
        <underline language="EN" method="pen" text="wonder at it whan it is offred him" type="straight"/>
        <underline language="EN" method="pen" text="refuse it in such sort as some" type="straight"/>
        <underline language="EN" method="pen" text="for very ignorance receiue it not, and" type="straight"/>
        <underline language="EN" method="pen" text="make men beleue that thei acknowledge themselues vnworthy of" type="straight"/>
        <underline language="EN" method="pen" text="humble himself somewhat vnder his" type="straight"/>
        <underline language="EN" method="pen" text="fauour &amp; promocions so easilye" type="straight"/>
        <underline language="EN" method="pen" text="be offred" type="straight"/>
        <underline language="EN" method="pen" text="refuse them modestlye" type="straight"/>
        <underline language="EN" method="pen" text="he much estemeth them" type="straight"/>
        <underline language="EN" method="pen" text="to offer them" type="straight"/>
        <underline language="EN" method="pen" text="great deale more instance" type="straight"/>
        <underline language="EN" method="pen" text="more resistance" type="straight"/>
        <underline language="EN" method="pen" text="to be estemed" type="straight"/>
        <underline language="EN" method="pen" text="benefite whiche he bestoweth is" type="straight"/>
        <underline language="EN" method="pen" text="muche" type="straight"/>
        <underline language="EN" method="pen" text="seemeth to make of it" type="straight"/>
        <underline language="EN" method="pen" text="much honoured" type="straight"/>
        <underline language="EN" method="pen" text="perfect promotions that" type="straight"/>
        <underline language="EN" method="pen" text="esteamed of such" type="straight"/>
        <underline language="EN" method="pen" text="not craued" type="straight"/>
        <underline language="EN" method="pen" text="true vertue" type="straight"/>
        <underline language="EN" method="pen" text="accompanied with modestie" type="straight"/>
        <underline language="EN" method="pen" text="lowest" type="straight"/>
        <underline language="EN" method="pen" text="friende come higher" type="straight"/>
        
