    <annotation>
        
        <marginalia anchor_text="Neyther doe I beleaue [that] there are any more in the world, those Pylades             &amp; Orestes, Theseus and Perithous, nor Scipio and Laelius," hand="Italian">
            <language ident="EN">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Musidorus &amp; Pyrocles.
                    </marginalia_text>
                    <person name="Musidorus"/>
                    <person name="Pyrocles"/>
                    <X-ref book_title="The Countess of Pembroke's Arcadia" person="Philip Sidney"/>
                </position>
            </language>
        </marginalia>
        
        <underline language="EN" method="pen" text="a foolishe and light person" type="straight"/>
        <underline language="EN" method="pen" text="he wried his head" type="straight"/>
        <underline language="EN" method="pen" text="bowed his" type="straight"/>
        <underline language="EN" method="pen" text="inuited with a cheerfull countenaunce" type="straight"/>
        <underline language="EN" method="pen" text="of their cappes to" type="straight"/>
        <underline language="EN" method="pen" text="gase earnestly with his eyes abashed" type="straight"/>
        <underline language="EN" method="pen" text="had lytle" type="straight"/>
        <underline language="EN" method="pen" text="laugheth so fondly" type="straight"/>
        <underline language="EN" method="pen" text="wennes in theyr throte, that" type="straight"/>
        <underline language="EN" method="pen" text="a verye foole" type="straight"/>
        <underline language="EN" method="pen" text="beehauiours, maners and gestures" type="straight"/>
        <underline language="EN" method="pen" text="menne knowne" type="straight"/>
        <underline language="EN" method="pen" text="dimynisheth muche reputation" type="straight"/>
        <underline language="EN" method="pen" text="choyse of friendes" type="straight"/>
        <underline language="EN" method="pen" text="must haue inwarde conuersation" type="straight"/>
        <underline language="EN" method="pen" text="in streicte amitie and vnseparble companye," type="straight"/>
        <underline language="EN" method="pen" text="alike in wyll" type="straight"/>
        <underline language="EN" method="pen" text="in mynde" type="straight"/>
        <underline language="EN" method="pen" text="iudgement and inclunation" type="straight"/>
        <underline language="EN" method="pen" text="ignorant or wycked" type="straight"/>
        <underline language="EN" method="pen" text="the good, wyse, and dyscreete" type="straight"/>
        <underline language="EN" method="pen" text="doeth willingly felowshippe with his lyke" type="straight"/>
        <underline language="EN" method="pen" text="haue a respect in the" type="straight"/>
        <underline language="EN" method="pen" text="beeginning of these frendshippes" type="straight"/>
        <underline language="EN" method="pen" text="friendes," type="straight"/>
        <underline language="EN" method="pen" text="the other to bee of thesame" type="straight"/>
        <underline language="EN" method="pen" text="getting or leesing reputation" type="straight"/>
        <underline language="EN" method="pen" text="finde very fewe true friendes" type="straight"/>
        <underline language="EN" method="pen" text="those Pylades &amp; Orestes" type="straight"/>
        <underline language="EN" method="pen" text="Theseus and Perithous" type="straight"/>
        <underline language="EN" method="pen" text="nor Scipio and Laelius" type="straight"/>
        
        <underline language="EN" method="pen" text="Men in the mountaines, with great bottles" type="straight"/>
        <underline language="EN" method="pen" text="vnder their" type="straight"/>
        <underline language="EN" method="pen" text="the drinki[n]g of snow water" type="straight"/>
        <underline language="EN" method="pen" text="Choise of friendes." type="straight"/>
        
        <symbol language="EN" name="SS" place="right_margin" text="Choise of friendes."/>
        <symbol language="EN" name="SS" place="right_margin" text="the good, wyse, and dyscreete"/>
        
        <mark language="EN" method="pen" name="quotation_mark" place="head" text="a foolishe and light person"/>
        
        <mark method="pen" name="quotation_mark" place="left_margin"/>
