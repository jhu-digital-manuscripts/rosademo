    <annotation>
        
        <marginalia hand="Italian">
            <language ident="EN">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        A fine &amp; iudicious Courtier. 
                    </marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <underline language="EN" method="pen" text="nor riotous and vnordinate in any il condicion: not filthy and vnclenly in his liuing, with" type="straight"/>
        <underline language="EN" method="pen" text="boysterous beehauiours that smell of the plough and cart a thousand mile of, for he" type="straight"/>
        <underline language="EN" method="pen" text="make a good Courtier" type="straight"/>
        <underline language="EN" method="pen" text="no better vse then to kepe sheepe" type="straight"/>
        <underline language="EN" method="pen" text="perfect vnderstandinge in that" type="straight"/>
        <underline language="EN" method="pen" text="euery possible thinge may" type="straight"/>
        <underline language="EN" method="pen" text="be easye" type="straight"/>
        <underline language="EN" method="pen" text="and all men wonder at him, and" type="straight"/>
        <underline language="EN" method="pen" text="he at no manne" type="straight"/>
        <underline language="EN" method="pen" text="certaine loftye and vnmanerlye stubburnnesse" type="straight"/>
        <underline language="EN" method="pen" text="showe themselues" type="straight"/>
        <underline language="EN" method="pen" text="not to wonder at the thinges" type="straight"/>
        <underline language="EN" method="pen" text="can do them much better" type="straight"/>
        <underline language="EN" method="pen" text="their silence" type="straight"/>
        <underline language="EN" method="pen" text="vnworthy to be spoken of" type="straight"/>
        <underline language="EN" method="pen" text="make a gesture" type="straight"/>
        <underline language="EN" method="pen" text="vndersanding of [the] profoundnes of their couning" type="straight"/>
        <underline language="EN" method="pen" text="these fateful maners" type="straight"/>
        <underline language="EN" method="pen" text="&amp; with gentlenesse &amp; courtesie praise other mens good dedes" type="straight"/>
        <underline language="EN" method="pen" text="thoughe he perceyue himselfe execllent and farr aboue others" type="straight"/>
        <underline language="EN" method="pen" text="showe that he esteameth not hymselfe" type="straight"/>
        <underline language="EN" method="pen" text="a one" type="straight"/>
        <underline language="EN" method="pen" text="full perfections are" type="straight"/>
        <underline language="EN" method="pen" text="to lay aside his hope to come to a good passe" type="straight"/>
        <underline language="EN" method="pen" text="perfect &amp; high excellency which" type="straight"/>
        <underline language="EN" method="pen" text="aspireth vnto" type="straight"/>
        <underline language="EN" method="pen" text="all praisewoorthye" type="straight"/>
        <underline language="EN" method="pen" text="by [the] highest" type="straight"/>
        <underline language="EN" method="pen" text="sildome sene that he" type="straight"/>
        <underline language="EN" method="pen" text="[the] meane" type="straight"/>
        <underline language="EN" method="pen" text="himself excellent in anie thinge" type="straight"/>
        <underline language="EN" method="pen" text="beeside armes" type="straight"/>
        <underline language="EN" method="pen" text="gete him estymatyon by it after an honest sorte" type="straight"/>
        <underline language="EN" method="pen" text="so dyscreete and of so good" type="straight"/>
        <underline language="EN" method="pen" text="a iudgemente" type="straight"/>
        <underline language="EN" method="pen" text="the vnderstandinge after a comelye maner" type="straight"/>
        
        <underline language="EN" method="pen" text="commende not thynges well" type="straight"/>
        <underline language="EN" method="pen" text="Many places" type="straight"/>
        <underline language="EN" method="pen" text="beeside the best" type="straight"/>
        <underline language="EN" method="pen" text="his counin[g]." type="straight"/>
        
