    <annotation>
        
        <marginalia anchor_text="lette him come alwaies prouided and thinke on it beefore hande, showyng notwithstanding, the whole to bee done ex tempore, and at the first sight" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        semper pra[e]meditatus Augustus; sed quasi ex tempore.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Augustus always considered in advance, but as if suddenly.
            </translation>
        </marginalia>
        
        <underline language="EN" method="pen" text="allure men to heare or to looke on that he supposeth himselfe" type="straight"/>
        <underline language="EN" method="pen" text="making ssemblant" type="straight"/>
        <underline language="EN" method="pen" text="to doe it" type="straight"/>
        <underline language="EN" method="pen" text="not for a bragge and to shewe it for vainglory" type="straight"/>
        <underline language="EN" method="pen" text="at a chaunce, &amp; rahter praied by others" type="straight"/>
        <underline language="EN" method="pen" text="commyng of his owne free will" type="straight"/>
        <underline language="EN" method="pen" text="lette him come alwaies prouided " type="straight"/>
        <underline language="EN" method="pen" text="and thinke on it beefore hande" type="straight"/>
        <underline language="EN" method="pen" text="showyng notwithstanding" type="straight"/>
        <underline language="EN" method="pen" text="the whole to bee done" type="straight"/>
        <underline language="LA" method="pen" text="ex tempore" type="straight"/>
        <underline language="EN" method="pen" text="and at the first sight" type="straight"/>
        <underline language="EN" method="pen" text="meane" type="straight"/>
        <underline language="EN" method="pen" text="touche them" type="straight"/>
        <underline language="EN" method="pen" text="without grounding muche" type="straight"/>
        <underline language="EN" method="pen" text="great deale more cunning therein, then he vttereth" type="straight"/>
        <underline language="EN" method="pen" text="as certein Poetes sometime that harped vpon verye subtill pointes of Philsosophie" type="straight"/>
        <underline language="EN" method="pen" text="other sciences, and parauenture" type="straight"/>
        <underline language="EN" method="pen" text="had small vnderstanding" type="straight"/>
        <underline language="EN" method="pen" text="altogether ignoraunt in" type="straight"/>
        <underline language="EN" method="pen" text="haue im make any profession at all" type="straight"/>
        <underline language="EN" method="pen" text="him anye fame by it" type="straight"/>
        <underline language="EN" method="pen" text="whan occasion serueth" type="straight"/>
        <underline language="EN" method="pen" text="confesse to haue no" type="straight"/>
        <underline language="EN" method="pen" text="Nicholetto neuer haue" type="straight"/>
        <underline language="EN" method="pen" text="verye excellent Philosopher" type="straight"/>
        <underline language="EN" method="pen" text="more skilfull in the lawe then in feeling" type="straight"/>
        <underline language="EN" method="pen" text="of those Lectures in the lawe" type="straight"/>
        <underline language="EN" method="pen" text="neuer yelde at the" type="straight"/>
        <underline language="EN" method="pen" text="to deceyue the" type="straight"/>
        <underline language="EN" method="pen" text="that he had no vnderstanding in it" type="straight"/>
        <underline language="EN" method="pen" text="Socrates opinion" type="straight"/>
        <underline language="EN" method="pen" text="is not a Physosophers" type="straight"/>
        <underline language="EN" method="pen" text="that he hath no vnderstanding" type="straight"/>
        <underline language="EN" method="pen" text="say he hath no vnderstandyng, without it bee required of him: for" type="straight"/>
        <underline language="EN" method="pen" text="allowe not this fondnesse to accuse &amp; debase himselfe. Againe" type="straight"/>
        <underline language="EN" method="pen" text="willingly disclose some matters" type="straight"/>
        <underline language="EN" method="pen" text="without any faulte" type="straight"/>
        <underline language="EN" method="pen" text="shadowe of sclaunder" type="straight"/>
        
        <underline language="EN" method="pen" text="Somtyme a mannes ignoraunce is to be confessed." type="straight"/>
        <underline language="EN" method="pen" text="their shame" type="straight"/>
        
