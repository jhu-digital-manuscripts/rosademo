                    </marginalia_text>
                </position>
            </language>
        </marginalia> 
        
        <underline language="EN" method="pen" text="ware for the nones a garlande of Laurell" type="straight"/>
        <underline language="EN" method="pen" text="hyde hys baldenesse withall" type="straight"/>
        <underline language="EN" method="pen" text="verye circumspecte" type="straight"/>
        <underline language="EN" method="pen" text="least he passe hys" type="straight"/>
        <underline language="EN" method="pen" text="one errour" type="straight"/>
        <underline language="EN" method="pen" text="an other" type="straight"/>
        <underline language="EN" method="pen" text="him praise" type="straight"/>
        <underline language="EN" method="pen" text="purchaseth blame" type="straight"/>
        <underline language="EN" method="pen" text="surest way in the worlde" type="straight"/>
        <underline language="EN" method="pen" text="gouerne himself alwaies with a certeine honest meane" type="straight"/>
        <underline language="EN" method="pen" text="is a great and most sure shield againste enuie" type="straight"/>
        <underline language="EN" method="pen" text="whiche a manne ought to auoide in what he is able" type="straight"/>
        <underline language="EN" method="pen" text="lyar" type="straight"/>
        <underline language="EN" method="pen" text="vaine person" type="straight"/>
        <underline language="EN" method="pen" text="deserue it" type="straight"/>
        <underline language="EN" method="pen" text="alwayes heedfull not" type="straight"/>
        <underline language="EN" method="pen" text="goe out of the lykelyhoode of truth" type="straight"/>
        <underline language="EN" method="pen" text="often those truthes" type="straight"/>
        <underline language="EN" method="pen" text="face of a lye" type="straight"/>
        <underline language="EN" method="pen" text="of wonders" type="straight"/>
        <underline language="EN" method="pen" text="firste entringe into a friendshipp wyth a newe friende, to" type="straight"/>
        <underline language="EN" method="pen" text="thynge they speake" type="straight"/>
        <underline language="EN" method="pen" text="sweare that there is not a person in the world whom" type="straight"/>
        <underline language="EN" method="pen" text="wyllynge to ieoparde their lyfe for" type="straight"/>
        <underline language="EN" method="pen" text="matters out of reason" type="straight"/>
        <underline language="EN" method="pen" text="makewise to weepe" type="straight"/>
        <underline language="EN" method="pen" text="speake a woorde for sorowe" type="straight"/>
        <underline language="EN" method="pen" text="counted to louynge woormes" type="straight"/>
        <underline language="EN" method="pen" text="them lyars" type="straight"/>
        <underline language="EN" method="pen" text="and fonde flatterers" type="straight"/>
        <underline language="EN" method="pen" text="suche a one that shall" type="straight"/>
        <underline language="EN" method="pen" text="wante good communycaton and fytte for them he talketh wythall" type="straight"/>
        <underline language="EN" method="pen" text="and haue a good vnderstandynge" type="straight"/>
        
