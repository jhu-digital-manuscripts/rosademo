    <annotation>
        
        <underline language="EN" method="pen" text="judgement and art doeth polishe and correct them" type="straight"/> 
        <underline language="EN" method="pen" text="belongeth to witt" type="straight"/>
        <underline language="EN" method="pen" text="in art:" type="straight"/>
        <underline language="EN" method="pen" text="jestes and meery conceites" type="straight"/>
        <underline language="EN" method="pen" text="moue laughter" type="straight"/>
        <underline language="EN" method="pen" text="are meete for the Courtier" type="straight"/>
        <underline language="EN" method="pen" text="whyche are" type="straight"/>
        <underline language="EN" method="pen" text="maner they ought" type="straight"/>
        <underline language="EN" method="pen" text="smilynge" type="straight"/>
        <underline language="EN" method="pen" text="will not geue place vnto" type="straight"/>
        <underline language="EN" method="pen" text="in Iestinge" type="straight"/>
        <underline language="EN" method="pen" text="folies:" type="straight"/>
        <underline language="EN" method="pen" text="menne laugh manie" type="straight"/>
        <underline language="EN" method="pen" text="wittie saiynges" type="straight"/>
        <underline language="EN" method="pen" text="Bernarde Bibiena" type="straight"/>
        <underline language="EN" method="pen" text="be the maisters of this facultie" type="straight"/>
        <underline language="EN" method="pen" text="whom in case" type="straight"/>
        <underline language="EN" method="pen" text="must first learne" type="straight"/>
        <underline language="EN" method="pen" text="practise that" type="straight"/>
        <underline language="EN" method="pen" text="are not skilfull" type="straight"/>
        <underline language="EN" method="pen" text="Lordes laughe in mocking M. Bernarde" type="straight"/>
        <underline language="EN" method="pen" text="much more perfectly in you" type="straight"/>
        <underline language="EN" method="pen" text="to deferr the" type="straight"/>
        <underline language="EN" method="pen" text="talke till tomorrowe" type="straight"/>
        <underline language="EN" method="pen" text="consumed vpon your praise" type="straight"/>
        <underline language="EN" method="pen" text="verie well knowen all" type="straight"/>
        <underline language="EN" method="pen" text="diuided not the paines takinge equallye" type="straight"/>
        <underline language="EN" method="pen" text="charge of speakynge" type="straight"/>
        <underline language="EN" method="pen" text="M. Bernarde Bibiena" type="straight"/>
        <underline language="EN" method="pen" text="quicke wytted in talkynge" type="straight"/>
