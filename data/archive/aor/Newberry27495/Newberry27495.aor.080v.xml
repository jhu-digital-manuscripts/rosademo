    <annotation>
        
        <underline language="EN" method="chalk" text="in Catallus time gates beegan to speake without tunge and to heare without eares,              and in that sort discouered aduouteries." type="straight"/>
        <underline language="EN" method="pen" text="gates beegan" type="straight"/>
        <underline language="EN" method="pen" text="speak without tunge" type="straight"/>
