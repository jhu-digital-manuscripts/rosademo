<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    
    <page filename="Newberry27495.097v.tif" reader="Harvey" signature="[Y.iii.[v]]"/>
    <annotation>  
        
        <underline language="EN" method="pen" text="in Meerie Pranckes" type="straight"/>
        <underline language="EN" method="pen" text="to expectacion" type="straight"/>
        <underline language="EN" method="pen" text="more delite and" type="straight"/>
        <underline language="EN" method="pen" text="wittie and modest" type="straight"/>
        <underline language="EN" method="pen" text="without respect" type="straight"/>
        <underline language="EN" method="pen" text="the places that" type="straight"/>
        <underline language="EN" method="pen" text="verie same that" type="straight"/>
        <underline language="EN" method="pen" text="there be two kyndes of Meerie Pranckes" type="straight"/>
        <underline language="EN" method="pen" text="any man whoeuer he be" type="straight"/>
        <underline language="EN" method="pen" text="deceyued wittilie, and" type="straight"/>
        <underline language="EN" method="pen" text="after a feat maner" type="straight"/>
        <underline language="EN" method="pen" text="manne layeth" type="straight"/>
        <underline language="EN" method="pen" text="a nett" type="straight"/>
        <underline language="EN" method="pen" text="and showeth a piece of a bayte so" type="straight"/>
        <underline language="EN" method="pen" text="deceyued of himself" type="straight"/>
        <underline language="EN" method="pen" text="The first is" type="straight"/>
        <underline language="EN" method="pen" text="of a Spaniarde called Castilio" type="straight"/>
        <underline language="EN" method="pen" text="you not name them" type="straight"/>
        <underline language="EN" method="pen" text="them to take it in yll" type="straight"/>
        <underline language="EN" method="pen" text="againste good maner" type="straight"/>
        <underline language="EN" method="pen" text="to vse" type="straight"/>
        <underline language="EN" method="pen" text="with great men" type="straight"/>
        <underline language="EN" method="pen" text="bine played to Duke Fridericke" type="straight"/>
        <underline language="EN" method="pen" text="kinge Alphonsus" type="straight"/>
        <underline language="EN" method="pen" text="Queene Isabel of Spaine" type="straight"/>
        <underline language="EN" method="pen" text="manie other great Princis, and not" type="straight"/>
        <underline language="EN" method="pen" text="rewarded very largely them" type="straight"/>
        <underline language="EN" method="pen" text="neyther vpon this hope do I entend" type="straight"/>
        <underline language="EN" method="pen" text="name" type="straight"/>
        <underline language="EN" method="pen" text="Court that I meane" type="straight"/>
        <underline language="EN" method="pen" text="Bergamo" type="straight"/>
        <underline language="EN" method="pen" text="with garmentes and so finely clad" type="straight"/>
        <underline language="EN" method="pen" text="in keapinge" type="straight"/>
        <underline language="EN" method="pen" text="had not hearde" type="straight"/>
        
        <symbol language="EN" name="SS" place="head" text="to expectacion"/>
        
        <symbol name="SS" place="left_margin"/>
        
        <mark language="EN" method="pen" name="plus_sign" place="head" text="doe"/>
        
        <mark language="EN" method="pen" name="bracket" place="left_margin" text="The one is, whan any man whoeuer he be, is deceyued wittilie, and after a feat maner and with              pleasantnesse. The other, whan a manne layeth (as it were) a nett, an showeth a piece of a bayte so, that a man renneth to be deceyued of himself."/>
        <mark method="pen" name="plus_sign" place="left_margin"/>
        <mark language="EN" method="pen" name="vertical_bar" place="left_margin" text="And I haue heard of manie that haue bine played to Duke Fridericke, to kinge Alphonsus of Aragon,              to Queene Isabel of Saine, and to manie other great Princis, and not onlie they tooke it not in ill part, but rewarded very largely them that plaied them these partes. M Bernarde              answered: neyther vpon this hope to I entend to name them."/>
        <mark language="EN" method="pen" name="equal_sign" place="left_margin" text="with great men"/>
        <mark method="pen" name="straight_quotation_mark" place="left_margin"/>
        
        <mark method="pen" name="comma" place="right_margin"/> <!-- from -->
        <mark language="EN" method="pen" name="quotation_mark" place="right_margin" text="neyther vpon this hope to I entend to name them."/>
        <mark language="EN" method="pen" name="quotation_mark" place="right_margin" text="with garmentes and so finely clad"/>
        
        <mark method="chalk" name="vertical_bar" place="intext"/>
        <mark method="pen" name="dot" place="intext"/> <!-- laughter, so -->
        <mark language="EN" method="pen" name="equal_sign" place="intext" text="wittie and modest"/>
        <mark language="EN" method="pen" name="equal_sign" place="intext" text="without respect"/>
        <mark method="pen" name="comma" place="intext"/> <!-- offende, and -->
        <mark method="pen" name="dot" place="intext"/> <!-- Pranckes, euerye -->
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="deceyued wittilie, and"/>
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="deceyued of himself"/>
        <mark method="pen" name="dot" place="intext"/> <!-- Princis, and -->
        <mark method="chalk" name="vertical_bar" place="intext"/>
        <mark language="EN" method="pen" name="dot" place="intext" text="it"/>
        <mark language="EN" method="pen" name="dot" place="intext" text="in"/> 
        <mark language="EN" method="pen" name="dot" place="intext" text="ill"/> 
        <mark language="EN" method="pen" name="dot" place="intext" text="this"/>
        
        <numeral language="EN" place="left_margin" text="The one is, whan any man whoeuer he be, is deceyued wittilie, and after a feat maner and with pleasantnesse.">1.</numeral>        
        <numeral language="EN" place="left_margin" text="The other, whan a manne layeth (as it were) a nett, an showeth a piece of a bayte so, that a man renneth to be deceyued              of himself.">2.</numeral>  
        
    </annotation>
