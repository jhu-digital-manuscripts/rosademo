    <annotation>  
        
        <underline language="EN" method="pen" text="farthynge left him to blesse himselfe" type="straight"/>
        <underline language="EN" method="pen" text="chafe, to curse, and to" type="straight"/>
        <underline language="EN" method="pen" text="tearinge of God" type="straight"/>
        <underline language="EN" method="pen" text="blew out the candels" type="straight"/>
        <underline language="EN" method="pen" text="raked vp the fire and" type="straight"/>
        <underline language="EN" method="pen" text="speake aloude" type="straight"/>
        <underline language="EN" method="pen" text="greatest hurly buryle in the worlde, makinge wise to" type="straight"/>
        <underline language="EN" method="pen" text="carde vnderneath" type="straight"/>
        <underline language="EN" method="pen" text="viede vpon flush" type="straight"/>
        <underline language="EN" method="pen" text="vs mount" type="straight"/>
        <underline language="EN" method="pen" text="open his eyes" type="straight"/>
        <underline language="EN" method="pen" text="maner light in the" type="straight"/>
        <underline language="EN" method="pen" text="he awoke" type="straight"/>
        <underline language="EN" method="pen" text="for certeintie" type="straight"/>
        <underline language="EN" method="pen" text="neyther fire nor anye kinde of lighte and" type="straight"/>
        <underline language="EN" method="pen" text="cardes without light" type="straight"/>
        <underline language="EN" method="pen" text="lost thy sight as wel as thy money" type="straight"/>
        <underline language="EN" method="pen" text="Seest thou" type="straight"/>
        <underline language="EN" method="pen" text="laughinge and" type="straight"/>
        <underline language="EN" method="pen" text="about to mock them" type="straight"/>
        <underline language="EN" method="pen" text="I see you" type="straight"/>
        <underline language="EN" method="pen" text="seeme to wonder much" type="straight"/>
        <underline language="EN" method="pen" text="By good Lord, I" type="straight"/>
        <underline language="EN" method="pen" text="reach me the candell, and lett vs see" type="straight"/>
        
