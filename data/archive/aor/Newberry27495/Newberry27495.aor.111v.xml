    <annotation>  
        
        <underline language="EN" method="pen" text="woman play vpon" type="straight"/>
        <underline language="EN" method="pen" text="tabour or drumm, or blowe" type="straight"/>
        <underline language="EN" method="pen" text="flute or tromper, or" type="straight"/>
        <underline language="EN" method="pen" text="boisterousnesse" type="straight"/>
        <underline language="EN" method="pen" text="that sweete mildenes" type="straight"/>
        <underline language="EN" method="pen" text="commeth to daunse" type="straight"/>
        <underline language="EN" method="pen" text="brought to" type="straight"/>
        <underline language="EN" method="pen" text="fringe her self somewhat to be prayed, and" type="straight"/>
        <underline language="EN" method="pen" text="bashfulnes" type="straight"/>
        <underline language="EN" method="pen" text="noble shamefastnes" type="straight"/>
        <underline language="EN" method="pen" text="garmentes" type="straight"/>
        <underline language="EN" method="pen" text="appeere not fonde and light" type="straight"/>
        <underline language="EN" method="pen" text="to sett more by their beawty" type="straight"/>
        <underline language="EN" method="pen" text="sundrie kindes of beawtie" type="straight"/>
        <underline language="EN" method="pen" text="maner garmentes set her best out, and" type="straight"/>
        <underline language="EN" method="pen" text="fitt for the exercises" type="straight"/>
        <underline language="EN" method="pen" text="sightlye and cheerfull beawtye, she" type="straight"/>
        <underline language="EN" method="pen" text="to farther" type="straight"/>
        <underline language="EN" method="pen" text="gestures, wordes and apparaile" type="straight"/>
        <underline language="EN" method="pen" text="betoken mirth" type="straight"/>
        <underline language="EN" method="pen" text="milde and graue disposition" type="straight"/>
        <underline language="EN" method="pen" text="with facions fo the like sort" type="straight"/>
        <underline language="EN" method="pen" text="is [the] gift of nature" type="straight"/>
        <underline language="EN" method="pen" text="fatter or leaner then reasonable" type="straight"/>
        <underline language="EN" method="pen" text="helpe it with garmentes" type="straight"/>
        <underline language="EN" method="pen" text="feiningly asmuch" type="straight"/>
        <underline language="EN" method="pen" text="keapinge herselfe clenlye and handsome" type="straight"/>
        <underline language="EN" method="pen" text="bestoweth no pein nor diliigence" type="straight"/>
        <underline language="EN" method="pen" text="manye thinges" type="straight"/>
        <underline language="EN" method="pen" text="to haue a sight" type="straight"/>
        <underline language="EN" method="pen" text="to entertein, and" type="straight"/>
        <underline language="EN" method="pen" text="vnderstande that" type="straight"/>
        <underline language="EN" method="pen" text="those exercises that we haue" type="straight"/>
        <underline language="EN" method="pen" text="that iudgement" type="straight"/>
        
        <underline language="EN" method="pen" text="should come" type="straight"/>
