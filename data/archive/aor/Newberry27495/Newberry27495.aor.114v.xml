                    <marginalia_text>
                        [the] 2. Argument of Imperfection.
                    </marginalia_text>
                </position>
            </language>
        </marginalia>

        <marginalia hand="Italian">
            <language ident="EN">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        [the] 3. Argument.
                    </marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <underline language="EN" method="pen" text="[the] female that hath not a male" type="straight"/>
        <underline language="EN" method="pen" text="one kinde alone" type="straight"/>
        <underline language="EN" method="pen" text="betokeneth an imperfection" type="straight"/>
        <underline language="EN" method="pen" text="of olde" type="straight"/>
        <underline language="EN" method="pen" text="referr both the one and" type="straight"/>
        <underline language="EN" method="pen" text="to God" type="straight"/>
        <underline language="EN" method="pen" text="Iupiter was both male and femae" type="straight"/>
        <underline language="EN" method="pen" text="facioned male and female to his" type="straight"/>
        <underline language="EN" method="pen" text="meddle the kindes" type="straight"/>
        <underline language="EN" method="pen" text="these subtill pointes" type="straight"/>
        <underline language="EN" method="pen" text="that I am in the wrong" type="straight"/>
        <underline language="EN" method="pen" text="is likened to the Fourme" type="straight"/>
        <underline language="EN" method="pen" text="woman to the Mattier" type="straight"/>
        <underline language="EN" method="pen" text="Fourme" type="straight"/>
        <underline language="EN" method="pen" text="perfecter then the" type="straight"/>
        <underline language="EN" method="pen" text="him his beeing" type="straight"/>
        <underline language="EN" method="pen" text="in certein Problemes" type="straight"/>
        <underline language="EN" method="pen" text="naturally" type="straight"/>
        <underline language="EN" method="pen" text="the man" type="straight"/>
        <underline language="EN" method="pen" text="bine the first to receiue of her" type="straight"/>
        <underline language="EN" method="pen" text="amorous pleasures" type="straight"/>
        <underline language="EN" method="pen" text="hateth the woman" type="straight"/>
        <underline language="EN" method="pen" text="to coople" type="straight"/>
        <underline language="EN" method="pen" text="this act" type="straight"/>
        <underline language="EN" method="pen" text="woman receyueth of the man" type="straight"/>
        <underline language="EN" method="pen" text="perfection" type="straight"/>
        <underline language="EN" method="pen" text="the woman imperfection" type="straight"/>
        <underline language="EN" method="pen" text="him perfect" type="straight"/>
        <underline language="EN" method="pen" text="maketh him vnperfect" type="straight"/>
        <underline language="EN" method="pen" text="euerye woman wisheth she were a man" type="straight"/>
        <underline language="EN" method="pen" text="wishe for" type="straight"/>
        <underline language="EN" method="pen" text="wish not" type="straight"/>
        <underline language="EN" method="pen" text="a man to" type="straight"/>
        <underline language="EN" method="pen" text="haue libertye," type="straight"/>
        
        <underline language="EN" method="pen" text="male can not" type="straight"/>
        <underline language="EN" method="pen" text="female." type="straight"/>
        
        <mark method="pen" name="quotation_mark" place="head"/>           
        
        <mark language="EN" method="pen" name="plus_sign" place="left_margin" text="male can not be without female"/>          
        <mark method="pen" name="plus_sign" place="left_margin"/>      
        <mark method="pen" name="X_sign" place="left_margin"/>
        <mark language="EN" method="pen" name="bracket" place="left_margin" text="For that in this act, the woman receyueth of the man perfection, and the man of the woman              imperfection: and therefore euerie man naturallye loueth that maketh him perfect, and hateth that maketh him vnperfect."/>
        <mark language="EN" method="pen" name="plus_sign" place="left_margin" text="euerye woman wisheth she were a man"/>      
        
        <mark method="pen" name="plus_sign" place="right_margin"/>
        <mark method="pen" name="plus_sign" place="right_margin"/>
        <mark method="pen" name="plus_sign" place="right_margin"/>
        
