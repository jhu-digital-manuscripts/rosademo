    <annotation>  
        
        <marginalia hand="Italian">
            <language ident="EN">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        Commendable women.
                    </marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <marginalia anchor_text="accursed hypochrites" hand="Italian">
            <language ident="EN">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        against Hypocrisy.
                    </marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="EN">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        against Munkes, and friars.
                    </marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <underline language="EN" method="pen" text="amended by a woman" type="straight"/>
        <underline language="EN" method="pen" text="all creatures of mankinde be inferiour to" type="straight"/>
        <underline language="EN" method="pen" text="virgin our Lady" type="straight"/>
        <underline language="EN" method="pen" text="meddlinge holye matters" type="straight"/>
        <underline language="EN" method="pen" text="fonde reasoninges" type="straight"/>
        <underline language="EN" method="pen" text="seelie poore soules are kept close w[ith]out the pompous pride to seeke a name of holinesse emong" type="straight"/>
        <underline language="EN" method="pen" text="whan he fasteth" type="straight"/>
        <underline language="EN" method="pen" text="to annoint his face" type="straight"/>
        <underline language="EN" method="pen" text="that he maye appeere not to faste, and" type="straight"/>
        <underline language="EN" method="pen" text="in the markett place" type="straight"/>
        <underline language="EN" method="pen" text="Sinagoges" type="straight"/>
        <underline language="EN" method="pen" text="that the left hande knowe" type="straight"/>
        <underline language="EN" method="pen" text="no treasure in the world to be greater then to giue" type="straight"/>
        <underline language="EN" method="pen" text="example" type="straight"/>
        <underline language="EN" method="pen" text="thus hanging their head aside and" type="straight"/>
        <underline language="EN" method="pen" text="fastning their eyes vpon the grounde" type="straight"/>
        <underline language="EN" method="pen" text="spreadinge a report about" type="straight"/>
        <underline language="EN" method="pen" text="that they will not once speake to a woman" type="straight"/>
        <underline language="EN" method="pen" text="not eate anye thinge but raw herbes" type="straight"/>
        <underline language="EN" method="pen" text="smokye" type="straight"/>
        <underline language="EN" method="pen" text="with their side garmentes all to ragged and torne" type="straight"/>
        <underline language="EN" method="pen" text="they beeguile the simple" type="straight"/>
        <underline language="EN" method="pen" text="from falsifiynge willes sowinge" type="straight"/>
        
