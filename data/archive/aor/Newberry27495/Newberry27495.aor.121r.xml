    <annotation>  
        
        <underline language="EN" method="pen" text="attemptinge certein other wayes for" type="straight"/>
        <underline language="EN" method="pen" text="tender pitie so to quicken" type="straight"/>
        <underline language="EN" method="pen" text="called Pail, [that] he had respect to no kind of daunger" type="straight"/>
        <underline language="EN" method="pen" text="eyther to die or to deliuer his father" type="straight"/>
        <underline language="EN" method="pen" text="brought to passe" type="straight"/>
        <underline language="EN" method="pen" text="such priuie conueiaunce" type="straight"/>
        <underline language="EN" method="pen" text="first in Ligurno beefore" type="straight"/>
        <underline language="EN" method="pen" text="writ to his wief" type="straight"/>
        <underline language="EN" method="pen" text="his settinge at libertie" type="straight"/>
        <underline language="EN" method="pen" text="and sodeine ioye" type="straight"/>
        <underline language="EN" method="pen" text="throughe the zeale as prowesse" type="straight"/>
        <underline language="EN" method="pen" text="lifted her eyes" type="straight"/>
        <underline language="EN" method="pen" text="starke dead to the grounde" type="straight"/>
        <underline language="EN" method="pen" text="to temper the willies of men" type="straight"/>
        <underline language="EN" method="pen" text="died for sorowe or" type="straight"/>
        <underline language="EN" method="pen" text="her lief was nothinge" type="straight"/>
        <underline language="EN" method="pen" text="tary the lingering" type="straight"/>
