    <annotation>  
        
        <marginalia anchor_text="Queene Isabel of Spaine" hand="Italian">
            <language ident="EN">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        Queene Isabell:
                    </marginalia_text>
                    <person name="Isabella I of Castile"/>
                </position>
            </language>
        </marginalia>
        
        <underline language="EN" method="pen" text="liuelode" type="straight"/>
        <underline language="EN" method="pen" text="their lief" type="straight"/>
        <underline language="EN" method="pen" text="for their law, and to" type="straight"/>
        <underline language="EN" method="pen" text="with counsell and" type="straight"/>
        <underline language="EN" method="pen" text="somuch vertue &amp; prowesse," type="straight"/>
        <underline language="EN" method="pen" text="fewe Princis haue had" type="straight"/>
        <underline language="EN" method="pen" text="steppes, but to enuie her" type="straight"/>
        <underline language="EN" method="pen" text="suche a diuine maner of gouernment" type="straight"/>
        <underline language="EN" method="pen" text="that her will onlye" type="straight"/>
        <underline language="EN" method="pen" text="to do [that] he" type="straight"/>
        <underline language="EN" method="pen" text="scase durst a man in his owne" type="straight"/>
        <underline language="EN" method="pen" text="suspected he woulde displease" type="straight"/>
        <underline language="EN" method="pen" text="had in knowinge and cousing" type="straight"/>
        <underline language="EN" method="pen" text="ministers meete for the offices" type="straight"/>
        <underline language="EN" method="pen" text="ioigne the rigour of iustice with the mildenesse of mercye" type="straight"/>
        <underline language="EN" method="pen" text="smallye rewarded" type="straight"/>
        <underline language="EN" method="pen" text="anye yll, to sore punished" type="straight"/>
        <underline language="EN" method="pen" text="sprange a verie great reuerence diriued" type="straight"/>
        <underline language="EN" method="pen" text="of loue and feare" type="straight"/>
        <underline language="EN" method="pen" text="so settled" type="straight"/>
        <underline language="EN" method="pen" text="looked she [that] should beehoulde" type="straight"/>
        <underline language="EN" method="pen" text="from heauen" type="straight"/>
        <underline language="EN" method="pen" text="Realmes are still ruled, in" type="straight"/>
        <underline language="EN" method="pen" text="lke a whiele that longe symnged about with violence, keepeth [the] same course a good while after of it" type="straight"/>
        <underline language="EN" method="pen" text="no man moue it anye more" type="straight"/>
        <underline language="EN" method="pen" text="all the great men of Spaine" type="straight"/>
        <underline language="EN" method="pen" text="bine made by" type="straight"/>
        <underline language="EN" method="pen" text="Queene Isabel" type="straight"/>
        <underline language="EN" method="pen" text="great Capitain Gonsalue" type="straight"/>
        <underline language="EN" method="pen" text="more setbye for" type="straight"/>
        <underline language="EN" method="pen" text="that in peace &amp; warr" type="straight"/>
        <underline language="EN" method="pen" text="so notable and famous, that" type="straight"/>
        <underline language="EN" method="pen" text="case fame be" type="straight"/>
        
        <underline language="EN" method="pen" text="Gonsaluo" type="straight"/>
        
