    <annotation>  
        
        <underline language="EN" method="pen" text="manne to serue" type="straight"/>
        <underline language="EN" method="pen" text="shoulde beehaue herselfe vprightlye" type="straight"/>
        <underline language="EN" method="pen" text="that loueth her indeed" type="straight"/>
        <underline language="EN" method="pen" text="maketh false semblant" type="straight"/>
        <underline language="EN" method="pen" text="to dissemble" type="straight"/>
        <underline language="EN" method="pen" text="knowe them" type="straight"/>
        <underline language="EN" method="pen" text="semblant to loue" type="straight"/>
        <underline language="EN" method="pen" text="that loue in" type="straight"/>
        <underline language="EN" method="pen" text="beeinge answerable in" type="straight"/>
        <underline language="EN" method="pen" text="not to be guided by any" type="straight"/>
        <underline language="EN" method="pen" text="owne self" type="straight"/>
        <underline language="EN" method="pen" text="surest tokens" type="straight"/>
        <underline language="EN" method="pen" text="false loue from true" type="straight"/>
        <underline language="EN" method="pen" text="so craftye" type="straight"/>
        <underline language="EN" method="pen" text="infinite false semblantes" type="straight"/>
        <underline language="EN" method="pen" text="weepe" type="straight"/>
        <underline language="EN" method="pen" text="sent to the constant" type="straight"/>
        <underline language="EN" method="pen" text="Ile" type="straight"/>
        <underline language="EN" method="pen" text="Arch of faithfull" type="straight"/>
        <underline language="EN" method="pen" text="my charge nad" type="straight"/>
        <underline language="EN" method="pen" text="is my creature" type="straight"/>
        <underline language="EN" method="pen" text="not be light of credence that she is beloued" type="straight"/>
        <underline language="EN" method="pen" text="the first woorde accept all the prayses that be giuen them" type="straight"/>
        <underline language="EN" method="pen" text="elles denie them" type="straight"/>
        <underline language="EN" method="pen" text="rather an alluringe for them" type="straight"/>
        <underline language="EN" method="pen" text="of enterteinment" type="straight"/>
        <underline language="EN" method="pen" text="to shonn beeleauinge that whoso talketh of loue, loueth her anye whitt the more" type="straight"/>
        
