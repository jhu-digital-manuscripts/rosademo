    <annotation>  
        
        <marginalia anchor_text="threw herselfe into the riuer" hand="Italian">
            <language ident="EN">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        How [the] Gentlewoman is to Behaue herself towards Her Loouers.
                    </marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <underline language="EN" method="pen" text="malapert" type="straight"/>
        <underline language="EN" method="pen" text="smalle respect to her in his" type="straight"/>
        <underline language="EN" method="pen" text="shall shape him such" type="straight"/>
        <underline language="EN" method="pen" text="she is not pleased withall" type="straight"/>
        <underline language="EN" method="pen" text="demure and vseth" type="straight"/>
        <underline language="EN" method="pen" text="facions" type="straight"/>
        <underline language="EN" method="pen" text="woordes of loue couertlie" type="straight"/>
        <underline language="EN" method="pen" text="suche honest" type="straight"/>
        <underline language="EN" method="pen" text="beeleaue the Courtier" type="straight"/>
        <underline language="EN" method="pen" text="not to vnderstand him" type="straight"/>
        <underline language="EN" method="pen" text="woordes to another sense" type="straight"/>
        <underline language="EN" method="pen" text="discretion snd wisdome" type="straight"/>
        <underline language="EN" method="pen" text="stray from" type="straight"/>
        <underline language="EN" method="pen" text="feigne not to vnderstande" type="straight"/>
        <underline language="EN" method="pen" text="a meerie diuise" type="straight"/>
        <underline language="EN" method="pen" text="to honour her withall" type="straight"/>
        <underline language="EN" method="pen" text="debasinge her desertes" type="straight"/>
        <underline language="EN" method="pen" text="courtesie the prayses" type="straight"/>
        <underline language="EN" method="pen" text="counted discreete" type="straight"/>
        <underline language="EN" method="pen" text="surer hande for beeinge deceiued" type="straight"/>
        <underline language="EN" method="pen" text="Thus me seemeth the Gentilwoman of the Palaice ought to behaue herself" type="straight"/>
        <underline language="EN" method="pen" text="communication of loue" type="straight"/>
        <underline language="EN" method="pen" text="speake with women" type="straight"/>
        <underline language="EN" method="pen" text="shoulde tell lyes" type="straight"/>
        <underline language="EN" method="pen" text="say your lessons" type="straight"/>
        <underline language="EN" method="pen" text="enterteineth" type="straight"/>
        <underline language="EN" method="pen" text="in very deere" type="straight"/>
        <underline language="EN" method="pen" text="feeleth the passion that" type="straight"/>
        <underline language="EN" method="pen" text="at no time you" type="straight"/>
        <underline language="EN" method="pen" text="shall beeleaue him in any thinge" type="straight"/>
        <underline language="EN" method="pen" text="Shall othes, teares, and so" type="straight"/>
        <underline language="EN" method="pen" text="beeside the naturall crueltye whiche" type="straight"/>
        <underline language="EN" method="pen" text="them yet more. The" type="straight"/>
        <underline language="EN" method="pen" text="not of him that loueth" type="straight"/>
        <underline language="EN" method="pen" text="him that enterteineth" type="straight"/>
        
        <mark language="EN" method="pen" name="equal_sign" place="left_margin" text="malapert"/>
        <mark language="EN" method="pen" name="bracket" place="left_margin" text="[She] shall shape him such an answere, that he shall plainly vnderstande she is not pleased              withall."/>
