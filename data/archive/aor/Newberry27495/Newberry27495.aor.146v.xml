    <annotation>  
        
        <underline language="EN" method="chalk" text="they lye nightlie with most vile men" type="straight"/>
        <underline language="EN" method="pen" text="with most vile men" type="straight"/>
        <underline language="EN" method="pen" text="complaintes of some woorthie gentilman" type="straight"/>
        <underline language="EN" method="pen" text="perhappes with some excuse they might" type="straight"/>
        <underline language="EN" method="pen" text="nourish emonge them ieolosies" type="straight"/>
        <underline language="EN" method="pen" text="of the one" type="straight"/>
        <underline language="EN" method="pen" text="the others" type="straight"/>
        <underline language="EN" method="pen" text="now assured and oute of doubt that he is" type="straight"/>
        <underline language="EN" method="pen" text="the signes and tokens that be made him" type="straight"/>
        <underline language="EN" method="chalk" text="doubtfull woordes and feigned disdeignes they put him in an vncerteintie and nippe him              at the verie hart" type="straight"/>
        <underline language="EN" method="pen" text="doubtfull woordes and feigned disdeignes" type="straight"/>
        <underline language="EN" method="pen" text="an vncerteintie and nippe" type="straight"/>
        <underline language="EN" method="pen" text="extreme passion which he fealeth" type="straight"/>
        <underline language="EN" method="pen" text="tokens of loue &amp;" type="straight"/>
        <underline language="EN" method="pen" text="beegine to beethinke themselues" type="straight"/>
        <underline language="EN" method="pen" text="beegine a freshe" type="straight"/>
        <underline language="EN" method="pen" text="beegane his seruice but" type="straight"/>

