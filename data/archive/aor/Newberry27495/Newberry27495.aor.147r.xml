        <underline language="EN" method="chalk" text="whan he can talke with her, to swearing, to blaspheminge" type="straight"/>
        <underline language="EN" method="chalk" text="that haue greater thirst of blood the the verie Tygres." type="straight"/>
        <underline language="EN" method="chalk" text="noted with a thousande eyes" type="straight"/>
        <underline language="EN" method="pen" text="be any pleasures" type="straight"/>
        <underline language="EN" method="pen" text="are ghessed and iudged of all" type="straight"/>
        <underline language="EN" method="pen" text="determineth aduisedlye" type="straight"/>
        <underline language="EN" method="pen" text="beegine they to make" type="straight"/>
        <underline language="EN" method="pen" text="loue him hartely" type="straight"/>
        <underline language="EN" method="pen" text="and do him al pleasures" type="straight"/>
        <underline language="EN" method="pen" text="that feruent desire" type="straight"/>
        <underline language="EN" method="pen" text="frute of loue shoulde withall" type="straight"/>
        <underline language="EN" method="chalk" text="he ken them the lesse thanke" type="straight"/>
        <underline language="EN" method="pen" text="thignes contrarily" type="straight"/>
        <underline language="EN" method="pen" text="already knowe[n] abrode" type="straight"/>
        <underline language="EN" method="chalk" text="so mortified with those bitter passions" type="straight"/>
        <underline language="EN" method="pen" text="speakinge yll of women" type="straight"/>
