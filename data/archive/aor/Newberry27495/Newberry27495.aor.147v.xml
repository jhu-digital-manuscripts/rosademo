    <annotation>

        <underline language="EN" method="pen" text="plucke vppe your strength" type="straight"/>
        <underline language="EN" method="pen" text="backewarde to giue a greater pushe at the encounter" type="straight"/>
        <underline language="EN" method="chalk" text="plucke vppe your strength, like them that reture backewarde to giue a greater pushe at              the encounter. And to say the truth, it is ill done of you" type="straight"/>
        <underline language="EN" method="pen" text="ennemies begine to breake and to square one" type="straight"/>
        <underline language="EN" method="chalk" text="and to square one wyth" type="straight"/>
        <underline language="EN" method="pen" text="your aduersarie" type="straight"/>
        <underline language="EN" method="chalk" text="reuile them" type="straight"/>
        <underline language="EN" method="pen" text="reuile them more then he" type="straight"/>
        <underline language="EN" method="pen" text="to praise them perhappes somwhat" type="straight"/>
        <underline language="EN" method="pen" text="length of the talke" type="straight"/>
        <underline language="EN" method="pen" text="manye other pretye matters" type="straight"/>
        <underline language="EN" method="pen" text="yet beehinde to be said of the Courtier" type="straight"/>
        <underline language="EN" method="pen" text="excellent a Gentilwoman" type="straight"/>
        <underline language="EN" method="pen" text="what they" type="straight"/>
        <underline language="EN" method="pen" text="you, ne any man elles can ad ought therto" type="straight"/>
        <underline language="EN" method="pen" text="malice you beare to the honour of women" type="straight"/>
        <underline language="EN" method="pen" text="alreadie spoken, of" type="straight"/>
        <underline language="EN" method="pen" text="muche more in him" type="straight"/>
        <underline language="EN" method="chalk" text="somwhat more frindlye to women, then" type="straight"/>
        <underline language="EN" method="pen" text="youre witt" type="straight"/>
        <underline language="EN" method="chalk" text="a greater perfection" type="straight"/>
        <underline language="EN" method="pen" text="not ad vnto him more then hath alreadie bine" type="straight"/>
        <underline language="EN" method="pen" text="to diminish the praises and worthinesse" type="straight"/>
        
