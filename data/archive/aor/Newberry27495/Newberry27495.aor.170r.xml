    <annotation>
        
        <underline language="EN" method="pen" text="controuersye is ended, and I" type="straight"/>
        <underline language="EN" method="pen" text="now leaue him hardely" type="straight"/>
        <underline language="EN" method="pen" text="haue him suche a one" type="straight"/>
        <underline language="EN" method="pen" text="woman of the qualities" type="straight"/>
        <underline language="EN" method="pen" text="prince fo the qualities" type="straight"/>
        <underline language="EN" method="chalk" text="in him. Therfore (I feare me) he is like the Commune weale of Plato, and we shall neuer see suche a one, onlesse it bee              perhappes in heauen." type="straight"/>
        <underline language="EN" method="pen" text="the Commune" type="straight"/>
        <underline language="EN" method="pen" text="suhe a one" type="straight"/>
        <underline language="EN" method="pen" text="perhappes in heauen" type="straight"/>
        <underline language="EN" method="pen" text="possible" type="straight"/>
        <underline language="EN" method="chalk" text="that they maye be: therefore maye we yet parhappes see him vpon the earth in oure time. For althoughe the heauens" type="straight"/>
        <underline language="EN" method="pen" text="parhappes see" type="straight"/>
        <underline language="EN" method="pen" text="the earth in oure" type="straight"/>
        <underline language="EN" method="pen" text="the heauens" type="straight"/>
        <underline language="EN" method="pen" text="excellent Princis" type="straight"/>
        <underline language="EN" method="pen" text="see one" type="straight"/>
        <underline language="EN" method="pen" text="luke happen to" type="straight"/>
        <underline language="EN" method="chalk" text="I haue a good hope of it. For beeside the three great ones that we haue named, of whom may be hoped it, that beelongeth to the              high degree of a perfect Prince" type="straight"/>
        <underline language="EN" method="pen" text="a good hope" type="straight"/>
        <underline language="EN" method="pen" text="the three" type="straight"/>
        <underline language="EN" method="pen" text="ones that we" type="straight"/>
        <underline language="EN" method="pen" text="high degree of a perfect Prince" type="straight"/>
        <underline language="EN" method="pen" text="Italy certein Princes children" type="straight"/>
        <underline language="EN" method="chalk" text="although they be not like" type="straight"/>
        <underline language="EN" method="pen" text="such powre" type="straight"/>
        <underline language="EN" method="pen" text="with vertue" type="straight"/>
        <underline language="EN" method="chalk" text="L. Friderick Gonzaga, sonn and heyr to the marquesse of Mantua, and nephewe to oure Dutchesse here" type="straight"/>
        <underline language="EN" method="pen" text="Friderick Gonzaga, sonn and" type="straight"/>
        <underline language="EN" method="pen" text="of Mantua" type="straight"/>
        <underline language="EN" method="pen" text="the honest inclunation" type="straight"/>
        <underline language="EN" method="pen" text="discreation that" type="straight"/>
        <underline language="EN" method="pen" text="wonderous thinges as touchinge" type="straight"/>
        <underline language="EN" method="pen" text="beeing wittye" type="straight"/>
        <underline language="EN" method="pen" text="glory" type="straight"/>
        <underline language="EN" method="pen" text="stouthearted" type="straight"/>
        <underline language="EN" method="pen" text="courteious" type="straight"/>
        <underline language="EN" method="pen" text="good a beeginning" type="straight"/>
        <underline language="EN" method="chalk" text="but a verye good ende" type="straight"/>
        <underline language="EN" method="pen" text="verye good ende" type="straight"/>
        <underline language="EN" method="pen" text="will pray vnto" type="straight"/>
        <underline language="EN" method="pen" text="this your hope fulfilled" type="straight"/>
        
        <underline language="EN" method="pen" text="Princes verye scant." type="straight"/>
        <underline language="EN" method="pen" text="Gonzaga Duke of Mantua" type="straight"/>

