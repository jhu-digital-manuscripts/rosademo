    <annotation>
        
        <underline language="EN" method="pen" text="without him all the other" type="straight"/>
        <underline language="EN" method="chalk" text="myndes were well tylled, and of theym speake I" type="straight"/>
        <underline language="EN" method="chalk" text="albe to frame their minde to a right trade" type="straight"/>
        <underline language="EN" method="pen" text="vertue" type="straight"/>
        <underline language="EN" method="pen" text="consisteth in doing &amp; practise" type="straight"/>
        <underline language="EN" method="pen" text="iustice" type="straight"/>
        <underline language="EN" method="pen" text="noble courage" type="straight"/>
        <underline language="EN" method="pen" text="his greatnesse" type="straight"/>
        <underline language="EN" method="pen" text="put in vse" type="straight"/>
        <underline language="EN" method="pen" text="make it custome" type="straight"/>
        <underline language="EN" method="pen" text="whiche the Courtier" type="straight"/>
        <underline language="EN" method="pen" text="no meanes to practise" type="straight"/>
        <underline language="EN" method="pen" text="the Prince inclined to vertue" type="straight"/>
        <underline language="EN" method="pen" text="more vertuous then the" type="straight"/>
        <underline language="EN" method="chalk" text="that, you muste conceyue that the whettstone which cutteth not a whitt, doeth yet make a toole sharpe: therefore althoughe the              Courtier instructeth his Prince" type="straight"/>
        <underline language="EN" method="pen" text="the whettstone" type="straight"/>
        <underline language="EN" method="pen" text="a toole sharpe" type="straight"/>
        <underline language="EN" method="pen" text="ende of this Courtier is harde" type="straight"/>
        <underline language="EN" method="pen" text="vnpossible" type="straight"/>
        <underline language="EN" method="pen" text="hardenesse, bicause" type="straight"/>
        <underline language="EN" method="pen" text="out so excellent a Courtier" type="straight"/>
        <underline language="EN" method="pen" text="by such an ende" type="straight"/>
        <underline language="EN" method="pen" text="many thinges to be" type="straight"/>
        <underline language="EN" method="pen" text="yonge" type="straight"/>
        <underline language="EN" method="pen" text="Prince to be so wise" type="straight"/>
        <underline language="EN" method="pen" text="needeth no exhortations" type="straight"/>
        
        <underline language="EN" method="pen" text="Virtue" type="straight"/>
        <underline language="EN" method="pen" text="actione" type="straight"/>
        <underline language="EN" method="pen" text="The ende" type="straight"/>
        <underline language="EN" method="pen" text="harde" type="straight"/>
        
