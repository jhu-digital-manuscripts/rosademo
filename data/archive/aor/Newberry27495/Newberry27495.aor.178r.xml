    <page filename="Newberry27495.178r.tif" reader="Harvey" signature="Tt.iiii.[r]"/>
    <annotation>
        
        
        <underline language="EN" method="chalk" text="M. Morello: But I reme[m]bre rather that I haue" type="straight"/>
        <underline language="EN" method="chalk" text="manie beautifull women of a most" type="straight"/>
        <underline language="EN" method="pen" text="beautifull women of a most yll" type="straight"/>
        <underline language="EN" method="chalk" text="spitefull" type="straight"/>
        <underline language="EN" method="chalk" text="(in a maner( it happeneth alwaies so, for beawtie maketh them proude: and pride, cruell" type="straight"/>
        <underline language="EN" method="pen" text="old men ought to couet beawtye" type="straight"/>
        <underline language="EN" method="pen" text="Ladies handes, and what" type="straight"/>
        <underline language="EN" method="pen" text="somwhat out of pacience" type="straight"/>
        <underline language="EN" method="pen" text="not knowe the thinge" type="straight"/>
        <underline language="EN" method="pen" text="that toucheth me not" type="straight"/>
        <underline language="EN" method="pen" text="the yonge men" type="straight"/>
        <underline language="EN" method="pen" text="are not so fresh and lusty as" type="straight"/>
        <underline language="EN" method="pen" text="is not altogether" type="straight"/>
        <underline language="EN" method="pen" text="out of the way" type="straight"/>
        <underline language="EN" method="pen" text="beautye of women" type="straight"/>
        <underline language="EN" method="chalk" text="in the worlde, hatred, warr, mortality, &amp; destruction, wherof the rasinge of Troye can be a good witnesse:" type="straight"/>
        <underline language="EN" method="pen" text="proude" type="straight"/>
        <underline language="EN" method="pen" text="cruell" type="straight"/>
        <underline language="EN" method="pen" text="vnchast" type="straight"/>
        <underline language="EN" method="pen" text="Morello woulde finde no faulte" type="straight"/>
        <underline language="EN" method="chalk" text="may be the redier to deceiue, and that this amiable looke were like a baite that couereth the hooke" type="straight"/>
        <underline language="EN" method="pen" text="that couereth the hooke" type="straight"/>
        <underline language="EN" method="pen" text="beautie is alwayes good" type="straight"/>
        
