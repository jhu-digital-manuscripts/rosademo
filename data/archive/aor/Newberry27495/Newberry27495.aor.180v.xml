    <page filename="Newberry27495.180v.tif" reader="Harvey" signature="[Vv.ii.[v]]"/>
    <annotation>
        
        <underline language="EN" method="pen" text="loue more happelye then" type="straight"/>
        <underline language="EN" method="pen" text="was my drift" type="straight"/>
        <underline language="EN" method="pen" text="what waye they must" type="straight"/>
        <underline language="EN" method="pen" text="guided by reason" type="straight"/>
        <underline language="EN" method="pen" text="vnpossible for loue to stand with reason" type="straight"/>
        <underline language="EN" method="pen" text="Dutchesse desired" type="straight"/>
        <underline language="EN" method="pen" text="nourish it with that onelye, which is commune" type="straight"/>
        <underline language="EN" method="pen" text="to the other" type="straight"/>
        <underline language="EN" method="pen" text="parte, whiche is propre to her" type="straight"/>
        <underline language="EN" method="pen" text="vpon this noble matter" type="straight"/>
        <underline language="EN" method="pen" text="vnworthy to talke of" type="straight"/>
        <underline language="EN" method="pen" text="holye misteries of loue" type="straight"/>
        <underline language="EN" method="pen" text="him to" type="straight"/>
        <underline language="EN" method="pen" text="this excelent" type="straight"/>
        <underline language="EN" method="pen" text="how to loue contrarye" type="straight"/>
        <underline language="EN" method="pen" text="commune ignorant sort" type="straight"/>
        <underline language="EN" method="pen" text="haue dedicated all my wholl lief vnto" type="straight"/>
        <underline language="EN" method="pen" text="my wordes may be answerable" type="straight"/>
        <underline language="EN" method="pen" text="prayse of him" type="straight"/>
        <underline language="EN" method="pen" text="sense" type="straight"/>
        <underline language="EN" method="pen" text="he is yong" type="straight"/>
        <underline language="EN" method="pen" text="to lour sensuallye" type="straight"/>
        <underline language="EN" method="pen" text="be good &amp; circumspect" type="straight"/>
        <underline language="EN" method="pen" text="beeguyle not him self" type="straight"/>
        <underline language="EN" method="pen" text="men deserueth more to be pitied" type="straight"/>
        <underline language="EN" method="pen" text="then blamed" type="straight"/>
        <underline language="EN" method="pen" text="be blamed then pitied" type="straight"/>
        
