        <underline language="EN" method="pen" text="to smell those spirituall" type="straight"/>
        <underline language="EN" method="pen" text="relieue the vertues of the" type="straight"/>
        <underline language="EN" method="pen" text="the heauenlye harmonie so tunable" type="straight"/>
        <underline language="EN" method="pen" text="of passion" type="straight"/>
        <underline language="EN" method="pen" text="dronken with the bottomlesse fountain of contentation" type="straight"/>
        <underline language="EN" method="pen" text="giueth a smacke of the right blisse vnto" type="straight"/>
        <underline language="EN" method="pen" text="the renning and cleere water" type="straight"/>
        <underline language="EN" method="pen" text="shininge beames of thy light our eyes" type="straight"/>
        <underline language="EN" method="pen" text="from mysty ignoraunce" type="straight"/>
        <underline language="EN" method="pen" text="be not" type="straight"/>
        <underline language="EN" method="pen" text="be in effect" type="straight"/>
        <underline language="EN" method="pen" text="oure soules" type="straight"/>
        <underline language="EN" method="pen" text="for a sacrifice" type="straight"/>
        <underline language="EN" method="pen" text="Barn them in the liuelye" type="straight"/>
        <underline language="EN" method="pen" text="wasteth al grosse filthines" type="straight"/>
        <underline language="EN" method="pen" text="euerlasting &amp; most sweet bonde to the heauenly beawty" type="straight"/>
        <underline language="EN" method="pen" text="chaunged" type="straight"/>
        <underline language="EN" method="pen" text="right louers into the beloued" type="straight"/>
        <underline language="EN" method="pen" text="admitted to the feast of the aungelles" type="straight"/>
        <underline language="EN" method="pen" text="where fed with immortall ambrosia and nectar" type="straight"/>
        <underline language="EN" method="pen" text="liuelye death" type="straight"/>
        <underline language="EN" method="pen" text="past died the fathers of olde" type="straight"/>
        <underline language="EN" method="pen" text="diddest hale from the" type="straight"/>
        <underline language="EN" method="pen" text="bodye and" type="straight"/>
        <underline language="EN" method="pen" text="coopleddest them with God" type="straight"/>
        <underline language="EN" method="pen" text="vehemencye, that" type="straight"/>
        <underline language="EN" method="pen" text="man woulde haue thought him" type="straight"/>
        <underline language="EN" method="pen" text="rauished and beeside himselfe" type="straight"/>
        <underline language="EN" method="pen" text="without once moouing" type="straight"/>
        <underline language="EN" method="pen" text="houldynge his eyes towarde heauen as astonied" type="straight"/>
        <underline language="EN" method="pen" text="most diligent eare to" type="straight"/>
        <underline language="EN" method="pen" text="the plaite of hys garment and pluckinge" type="straight"/>
        <underline language="EN" method="pen" text="your soule also to forsake" type="straight"/>
        <underline language="EN" method="pen" text="the bodye" type="straight"/>

