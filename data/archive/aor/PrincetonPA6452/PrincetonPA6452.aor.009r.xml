            </marginalia>
            
            <marginalia hand="Italian">
                <language ident="EL">
                    <position book_orientation="0" place="tail">
                        <marginalia_text>διότι</marginalia_text>
                    </position>
                </language>
                <language ident="LA">
                    <position book_orientation="0" place="tail">
