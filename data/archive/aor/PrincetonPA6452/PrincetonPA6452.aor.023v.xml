        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>" Primus Liber, Monarchicus: reliqui, partim Aristocratici, aut Oligarchici; partim Democratici.
                    </marginalia_text>
                    <emphasis method="pen" text="Monarchicus" type="straight"/>
                    <emphasis method="pen" text="Democratici" type="straight"/>
                </position>
            </language>
            <translation>The first book is monarchical; the others are partly aristocratic or oligarchical, partly democratic.
            </translation>
        </marginalia>
       
