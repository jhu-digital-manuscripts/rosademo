        <marginalia hand="Italian">
            <language ident="EN">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Ca[e]so, a lustie and dowtie fellow. a terrible killcowe in his kinde.
                    </marginalia_text>
                    <person name="Caeso Quinctius"/>
                </position>
            </language>
        </marginalia>
        
        <marginalia hand="Italian">
