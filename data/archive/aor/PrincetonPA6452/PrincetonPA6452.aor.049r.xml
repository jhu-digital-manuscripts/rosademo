        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>Nocet empta dolore voluptas. Sapere decet magistratus.</marginalia_text>
                </position>
            </language>
            <translation>Pleasure bought with pain harms. It is appropriate for Magistrates to be wise.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
