    <annotation>
        
        <underline language="LA" method="pen" text="Nebula" type="straight"/>
        <underline language="LA" method="pen" text="uelut latebra insidiarum freti" type="straight"/>
        <underline language="LA" method="pen" text="Improuiso oppressis, nec animi satis ad resistendum, nec uirium fuit" type="straight"/>
        <underline language="LA" method="pen" text="captum qu[a]estoriu[m], quaestorq[ue]" type="straight"/>
        <underline language="LA" method="pen" text="co[n]clamatu[m] inde ad arma" type="straight"/>
        <underline language="LA" method="pen" text="dum satis aptatis" type="straight"/>
        <underline language="LA" method="pen" text="in ordines" type="straight"/>
        <underline language="LA" method="pen" text="clamore magis" type="straight"/>
        <underline language="LA" method="pen" text="expulsi ne extra uallu[m]" type="straight"/>
        <underline language="LA" method="pen" text="castra deinde sua oppugnaturi" type="straight"/>
        <underline language="LA" method="pen" text="impulsos semel terrore eodem agunt, quo coeperunt" type="straight"/>
        <underline language="LA" method="pen" text="expellu[n]t extra" type="straight"/>
        <underline language="LA" method="pen" text="quia turbida lux metum circà insidiarum faciebat" type="straight"/>
        <underline language="LA" method="pen" text="non ausi, libertatis" type="straight"/>
        <underline language="LA" method="pen" text="Animos inde Samnitibus non infelix audacia auxit" type="straight"/>
        <underline language="LA" method="pen" text="modò proferre inde castra" type="straight"/>
        <underline language="LA" method="pen" text="ne pabulari quidem" type="straight"/>
        <underline language="LA" method="pen" text="fama tumultuosior" type="straight"/>
        <underline language="LA" method="pen" text="perlata Romam" type="straight"/>
        <underline language="LA" method="pen" text="aedem Victoriae, quam" type="straight"/>
        <underline language="LA" method="pen" text="dedicauit" type="straight"/>
        <underline language="LA" method="pen" text="diffisi duobus exercitib[us] resisti" type="straight"/>
        <underline language="LA" method="pen" text="Coss. ad uastandos agros, urbesq[ue] oppugnandas discedunt" type="straight"/>
        <underline language="LA" method="pen" text="Posthumius" type="straight"/>
        <underline language="LA" method="pen" text="ui primò, atq[ue] impetu" type="straight"/>
        <underline language="LA" method="pen" text="opere ac uineis demum" type="straight"/>
        <underline language="LA" method="pen" text="ut sile[n]tiu[m] uastu[m]" type="straight"/>
        <underline language="LA" method="pen" text="auidu[m] inuade[n]di deserta moenia milite[m]" type="straight"/>
        <underline language="LA" method="pen" text="ne qua[m] occulta[m] in fraude[m] incautus rueret" type="straight"/>
        <underline language="LA" method="pen" text="circu[m]equitare moenia, atq[ue] explorare omnia iubet" type="straight"/>
        <underline language="LA" method="pen" text="ijs uestigia nocturn[a]e hostiu[m] fugae, adequita[n]t" type="straight"/>
        <underline language="LA" method="pen" text="solitudine" type="straight"/>
        <underline language="LA" method="pen" text="uestigijs fug[a]e, ac strage reru[m] in" type="straight"/>
        <underline language="LA" method="pen" text="quinq[ue] equites iubet intrare urbe[m] &amp; modicu[m] spaciu[m]" type="straight"/>
        <underline language="LA" method="pen" text="tres manere" type="straight"/>
        <underline language="LA" method="pen" text="duos explorata ad se referre" type="straight"/>
        <underline language="LA" method="pen" text="progressos, unde in omnes partes circu[m]spectus" type="straight"/>
        <underline language="LA" method="pen" text="cohortes expeditas in urbe[m]" type="straight"/>
        <underline language="LA" method="pen" text="paucos graues aetate, aut inualidos" type="straight"/>
        <underline language="LA" method="pen" text="co[m]muni" type="straight"/>
        <underline language="LA" method="pen" text="urbes co[n]cisse fuga[m], suos" type="straight"/>
        <underline language="LA" method="pen" text="prima uigilia p[er]fectos, credere ea[n]de[m] " type="straight"/>
        <underline language="LA" method="pen" text="urbib[us] solitudine[m] inue[n]turos" type="straight"/>
        <underline language="LA" method="pen" text="captiuoru[m] fides extitit" type="straight"/>
        <underline language="LA" method="pen" text="desertis oppidis" type="straight"/>
        
