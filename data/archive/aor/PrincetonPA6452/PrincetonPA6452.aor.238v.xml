    <annotation>
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>dulcis, et diuinus Scipio, non modò inuictus.</marginalia_text>
                    <person name="Scipio Africanus"/>
                </position>
            </language>
            <language ident="EN">
                <position book_orientation="0" place="head">
                    <marginalia_text>As peerles fine, as matchles braue: a mirrour of sweetest courtesie, &amp; terriblest valour.</marginalia_text>
                </position>
            </language>
            <translation>Scipio was not only invincible but also agreeable and godlike.</translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="terram Asdrubal" type="straight"/>
        <underline language="LA" method="pen" text="Scipio &amp; Laelius egressi" type="straight"/>
        <underline language="LA" method="pen" text="Magnificumq[ue] id Syphaci" type="straight"/>
        <underline language="LA" method="pen" text="duorum opulentissimorum ea tempestate duces populorum uno die" type="straight"/>
        <underline language="LA" method="pen" text="fors eos sub uno tecto esse, atq[ue] ad eosdem penates" type="straight"/>
        <underline language="LA" method="pen" text="Scipione abnuente" type="straight"/>
        <underline language="LA" method="pen" text="alter hospitum" type="straight"/>
        <underline language="LA" method="pen" text="ad easdem uenire epulas" type="straight"/>
        <underline language="LA" method="pen" text="coenatumq[ue] simul apud regem est" type="straight"/>
        <underline language="LA" method="pen" text="etia[m] lecto Scipio atq[ue] Asdrubal" type="straight"/>
        <underline language="LA" method="pen" text="Tanta autem inerat comitas Scipioni, atque" type="straight"/>
        <underline language="LA" method="pen" text="naturalis ingenij dexteritas" type="straight"/>
        <underline language="LA" method="pen" text="non Syphacem modò barbarum, insuetumq[ue] moribus" type="straight"/>
        <underline language="LA" method="pen" text="sed hostem etiam infestissimum facundê alloquendo sibi conciliarit" type="straight"/> 
        <underline language="LA" method="pen" text="mirabilioremq[ue] sibi eum congresso coram uisum prae se ferebat" type="straight"/>
        <underline language="LA" method="pen" text="quàm bello rebus gestis" type="straight"/>
        <underline language="LA" method="pen" text="quin Syphax, regnumque eius in Romanorum esset potestate" type="straight"/>
        <underline language="LA" method="pen" text="eam artem" type="straight"/>
        <underline language="LA" method="pen" text="uiro ad conciliandos animos esse. Itaq[ue]" type="straight"/>
        <underline language="LA" method="pen" text="quemadmodum Annibal in Italia" type="straight"/>
        <underline language="LA" method="pen" text="Scipio in Africa bellum gereret" type="straight"/>
        <underline language="LA" method="pen" text="Scipio foedere icto cum" type="straight"/>
        <underline language="LA" method="pen" text="metu magis quàm fide quietas esse apparebat" type="straight"/>
        <underline language="LA" method="pen" text="Castulonenses cum properis rebus socij fuissent" type="straight"/>
        <underline language="LA" method="pen" text="caesos cum" type="straight"/>
        <underline language="LA" method="pen" text="defecerant ad Poenos" type="straight"/>
        <underline language="LA" method="pen" text="prodendis, qui ex" type="straight"/>
        <underline language="LA" method="pen" text="scelus etia[m] defectioni addidera[n]t" type="straight"/>
        <underline language="LA" method="pen" text="meritò magis quàm utiliter saeuitum foret" type="straight"/>
        <underline language="LA" method="pen" text="tempus expetendae poenae uidebatur" type="straight"/>
        <underline language="LA" method="pen" text="uenisse" type="straight"/>
        <underline language="LA" method="pen" text="Martium" type="straight"/>
        <underline language="LA" method="pen" text="ad Castulonem" type="straight"/>
        <underline language="LA" method="pen" text="ad Illiturgim" type="straight"/>
        <underline language="LA" method="pen" text="conscientia, quid se meritos scirent" type="straight"/>
        <underline language="LA" method="pen" text="pro indicto eis bello fuerat" type="straight"/>
        <underline language="LA" method="pen" text="hortari" type="straight"/>
        <underline language="LA" method="pen" text="Ipsos claudendo portas" type="straight"/>
        <underline language="LA" method="pen" text="meriti essent" type="straight"/>
        <underline language="LA" method="pen" text="multo infestioribus animis cu[m] eis" type="straight"/>
        <underline language="LA" method="pen" text="quàm cu[m] Carthaginiensibus" type="straight"/>
        <underline language="LA" method="pen" text="de imperio &amp; gloria certari" type="straight"/>
        <underline language="LA" method="pen" text="perfidiae &amp; crudelitatis &amp; sceleris poenas expetendas" type="straight"/>
        <underline language="LA" method="pen" text="uenisse tempus" type="straight"/>
        <underline language="LA" method="pen" text="in omne" type="straight"/>
        <underline language="LA" method="pen" text="graui documento sancirent" type="straight"/>
        <underline language="LA" method="pen" text="ne quis" type="straight"/>
        <underline language="LA" method="pen" text="Romanum ciuem, militemue in" type="straight"/>
        <underline language="LA" method="pen" text="ulla fortuna opportunum iniuriae duceret" type="straight"/>
        <underline language="LA" method="pen" text="exhortatione ducis incitati" type="straight"/>
        <underline language="LA" method="pen" text="duobus simul locis" type="straight"/>
        <underline language="LA" method="pen" text="terrore urbem aggrediuntur" type="straight"/>
        <underline language="LA" method="pen" text="sed suus ipsoru[m]" type="straight"/>
        <underline language="LA" method="pen" text="metus ad" type="straight"/>
        <underline language="LA" method="pen" text="defendendam impigrè urbem hortatur" type="straight"/>
        <underline language="LA" method="pen" text="supplicium ex se, non uictoriam peti" type="straight"/>
        <underline language="LA" method="pen" text="utrum in pugna &amp; in acie" type="straight"/>
        <underline language="LA" method="pen" text="communis &amp; uictum saepe erigeret" type="straight"/>
        <underline language="LA" method="pen" text="inter uerbera &amp; uincula" type="straight"/>
        <underline language="LA" method="pen" text="omnia foeda atq[ue] indigna passi expirarent" type="straight"/>
        <underline language="LA" method="pen" text="supra animi" type="straight"/>
        
