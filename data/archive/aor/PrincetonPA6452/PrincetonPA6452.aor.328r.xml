        <underline language="LA" method="pen" text="Polyxenidas satis omnib[us]" type="straight"/>
        <underline language="LA" method="pen" text="quàm quod co[n]spici proficisce[n]tem classem nolebat" type="straight"/>
        <underline language="LA" method="pen" text="post solis occasum profectus" type="straight"/>
        <underline language="LA" method="pen" text="uento aduerso" type="straight"/>
        <underline language="LA" method="pen" text="ante luce[m]" type="straight"/>
        <underline language="LA" method="pen" text="Pygela portu[m] tenuit" type="straight"/>
        <underline language="LA" method="pen" text="interdiu ob eandem causam quieuisset" type="straight"/>
        <underline language="LA" method="pen" text="nocte" type="straight"/>
        <underline language="LA" method="pen" text="Nicandro quodam archipirata" type="straight"/>
        <underline language="LA" method="pen" text="Pausistratus primò, ut in re nec opinata" type="straight"/>
        <underline language="LA" method="pen" text="turbatus parumper" type="straight"/>
        <underline language="LA" method="pen" text="uetus miles celeriter collecto animo" type="straight"/>
        <underline language="LA" method="pen" text="terra melius arceri" type="straight"/>
        <underline language="LA" method="pen" text="mari hostes" type="straight"/>
        <underline language="LA" method="pen" text="facile telis ancipitibus" type="straight"/>
        <underline language="LA" method="pen" text="inceptum eius Nicander à terra uisus cum turbasset" type="straight"/>
        <underline language="LA" method="pen" text="repentè mutato co[n]silio" type="straight"/>
        <underline language="LA" method="pen" text="naues conscendere omnes" type="straight"/>
        <underline language="LA" method="pen" text="uerò ingens pariter militum nautarumq[ue]" type="straight"/>
        <underline language="LA" method="pen" text="trepidatio" type="straight"/>
        <underline language="LA" method="pen" text="ueluti fuga in naues" type="straight"/>
        <underline language="LA" method="pen" text="cum se mari terraq[ue] simul cernerent circu[m]uentos" type="straight"/>
        <underline language="LA" method="pen" text="Pausistratus una[m] uiam salutis esse ratus" type="straight"/>
        <underline language="LA" method="pen" text="uim facere per fauces portus" type="straight"/>
        <underline language="LA" method="pen" text="erumpere in mare apertum posset" type="straight"/>
        <underline language="LA" method="pen" text="Superantem iam" type="straight"/>
        <underline language="LA" method="pen" text="naue[m] eius" type="straight"/>
        <underline language="LA" method="pen" text="Polyxenidas tribus quinqueremibus circu[m]sistit" type="straight"/>
        <underline language="LA" method="pen" text="Nauis rostris icta" type="straight"/>
        <underline language="LA" method="pen" text="supprimitur" type="straight"/>
        <underline language="LA" method="pen" text="telis obruu[n]tur" type="straight"/>
        <underline language="LA" method="pen" text="inter quos &amp; Pausistratus impigrè pugna[n]s interficitur" type="straight"/>
        <underline language="LA" method="pen" text="Seleucus" type="straight"/>
        <underline language="LA" method="pen" text="prodita[m] Phocaeam porta" type="straight"/>
