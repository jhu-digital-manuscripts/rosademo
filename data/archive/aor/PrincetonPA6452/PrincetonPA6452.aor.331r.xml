    <page filename="PrincetonPA6452.331r.tif" pagination="645" reader="Harvey"/>
    <annotation>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text> 
                        Romanorum practica.
                    </marginalia_text>
                </position>
            </language>
            <translation>
               The actions of the Romans. 
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="Exurgite" type="straight"/>
        <underline language="LA" method="pen" text="spectaculu[m] capessite oculis" type="straight"/>
        <underline language="LA" method="pen" text="trepidationem fuga[m]que hostium" type="straight"/>
        <underline language="LA" method="pen" text="exclamauere omnes, ut sequere[n]tur" type="straight"/>
        <underline language="LA" method="pen" text="Eudami multis ictibus uulnerata nauis erat" type="straight"/>
        <underline language="LA" method="pen" text="terr[a]e appropinquabat Annibal" type="straight"/>
        <underline language="LA" method="pen" text="includerentur uento in hostium" type="straight"/>
        <underline language="LA" method="pen" text="non ta[m] uictoria laeti, quàm alius aliu[m] accusantes" type="straight"/>
        <underline language="LA" method="pen" text="potuisset, no[n] omnis summersa aut capta classis hostium foret, redierunt" type="straight"/>
        <underline language="LA" method="pen" text="Annibal ictus uno praelio aduerso" type="straight"/>
        <underline language="LA" method="pen" text="ne tu[m] quide[m]" type="straight"/>
        <underline language="LA" method="pen" text="Lyciam audebat" type="straight"/>
        <underline language="LA" method="pen" text="coniungi ueteri regiae" type="straight"/>
        <underline language="LA" method="pen" text="Antiochus no[n] ciuitatu[m] modò" type="straight"/>
        <underline language="LA" method="pen" text="co[n]trahebat praesidia" type="straight"/>
        <underline language="LA" method="pen" text="ad Prusia[m] Bithyniae regem" type="straight"/>
        <underline language="LA" method="pen" text="quibus transitu[m] in Asia[m] Romanoru[m] increpabat" type="straight"/>
        <underline language="LA" method="pen" text="Venire eos" type="straight"/>
        <underline language="LA" method="pen" text="omia regna tollenda" type="straight"/> 
        <underline language="LA" method="pen" text="nullum usqua[m] orbis terrarum, nisi Romanum imperium" type="straight"/>
        <underline language="LA" method="pen" text="Philippu[m] &amp; Nabin expugnatos" type="straight"/>
        <underline language="LA" method="pen" text="se tertium peti" type="straight"/>
        <underline language="LA" method="pen" text="quisq[ue] proximus ab oppresso" type="straight"/>
        <underline language="LA" method="pen" text="omneis uelut continens incendium peruasurum" type="straight"/>
        <underline language="LA" method="pen" text="gradum in Bithyniam" type="straight"/>
        <underline language="LA" method="pen" text="Eumenes in uoluntariam seruitutem co[n]cessisset" type="straight"/>
        <underline language="LA" method="pen" text="motum Prusiam" type="straight"/>
        <underline language="LA" method="pen" text="literae Scipionis" type="straight"/>
        <underline language="LA" method="pen" text="eius fratris Africani, à suspicione tali auerterunt." type="straight"/>
        <underline language="LA" method="pen" text="qui praeter consuetudinem" type="straight"/>
        <underline language="LA" method="pen" text="augendi omni honore regum" type="straight"/>
        <underline language="LA" method="pen" text="sociorum maiestatem" type="straight"/>
        <underline language="LA" method="pen" text="domesticis ipse exemplis Prusia[m] ad promere[n]dam amicitia[m] sua[m] co[m]pulit" type="straight"/>
        <underline language="LA" method="pen" text="Regulos se acceptos in" type="straight"/>
        <underline language="LA" method="pen" text="reges reliquisse" type="straight"/>
        <underline language="LA" method="pen" text="Masanissam no[n]" type="straight"/>
        <underline language="LA" method="pen" text="in Syphacis, à quo ante" type="straight"/>
        <underline language="LA" method="pen" text="eu[m] no[n] Africae modò regu[m] longè" type="straight"/>
        <underline language="LA" method="pen" text="sed toto in orbe terrarum" type="straight"/>
        <underline language="LA" method="pen" text="regu[m] uel maiestate" type="straight"/>
        <underline language="LA" method="pen" text="uiribus parem" type="straight"/>
        <underline language="LA" method="pen" text="Philippum" type="straight"/>
        <underline language="LA" method="pen" text="Nabin hostes" type="straight"/>
        <underline language="LA" method="pen" text="in regno relictos" type="straight"/>
        <underline language="LA" method="pen" text="Philippo quide[m] anno" type="straight"/>
        <underline language="LA" method="pen" text="filium obsidem redditum" type="straight"/>
        <underline language="LA" method="pen" text="dignitate &amp; Nabin futurum" type="straight"/>
        <underline language="LA" method="pen" text="suus primò furor" type="straight"/>
        <underline language="LA" method="pen" text="fraus Aetoloru[m] absumpsisset" type="straight"/>
        <underline language="LA" method="pen" text="confirmatus est animus regis" type="straight"/>
        <underline language="LA" method="pen" text="Liuius, qui" type="straight"/>
        <underline language="LA" method="pen" text="legatos ab Roma uenit" type="straight"/>
        <underline language="LA" method="pen" text="quanto &amp; spes uictoriae certior Romanis" type="straight"/>
        <underline language="LA" method="pen" text="amicitia sanctior firmiorq[ue]" type="straight"/>

        <symbol name="SS" place="right_margin"/>
        <symbol name="Opposite_planets" place="right_margin"/>

        <mark language="LA" method="pen" name="plus_sign" place="left_margin" text="potuisset, no[n] omnis summersa aut capta classis hostium foret, redierunt"/>
        <mark method="pen" name="bracket" place="left_margin"/>
        <mark method="pen" name="equal_sign" place="left_margin"/>
        
        <mark method="pen" name="dash" place="right_margin"/> <!-- ap -->
        <mark language="LA" method="pen" name="equal_sign" place="right_margin" text="Annibal ictus uno praelio aduerso"/>
        <mark method="pen" name="dash" place="right_margin"/> <!-- regio -->
        <mark method="pen" name="comma" place="right_margin"/> <!-- Prusiam -->
        
        <mark method="pen" name="dot" place="intext"/> <!-- praesidia, sed-->
        <mark method="pen" name="horizontal_bar" place="intext"/> <!--omia-->
        <mark method="pen" name="dot" place="intext"/> <!-- expugnatos, se-->
        <mark method="pen" name="equal_sign" place="intext"/>
        <mark language="LA" method="pen" name="equal_sign" place="intext" text="praeter consuetudinem"/>
        <mark method="pen" name="comma" place="intext"/> <!-- Romani augendi-->
        <mark language="LA" method="pen" name="equal_sign" place="intext" text="augendi omni honore regum"/>
        <mark language="LA" method="pen" name="straight_quotation_mark" place="intext" text="domesticis"/>
        <mark method="pen" name="dot" place="intext"/> <!-- Antiocho, & -->
        <mark method="pen" name="vertical_bar" place="intext"/> <!-- esset. Antiochus-->
        
        <errata amendedtext="om[n]ia" copytext="omia" language="LA"/>
        
