                    <marginalia_text> 
                        Scipio Africanus reus.
                    </marginalia_text>
                <person name="Scipio Africanus"/>
                </position>
            </language>
            <translation>
                Scipio Africanus as defendant
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="orationem adeò magnificam de rebus ab se gestis est exorsus" type="straight"/>
        <underline language="LA" method="pen" text="neminem unquam" type="straight"/>
        <underline language="LA" method="pen" text="neq[ue] uerius laudatum esse." type="straight"/>
        <underline language="LA" method="pen" text="ab eodem animo ingenioq[ue], à quo gesta erant;" type="straight"/>
        <underline language="LA" method="pen" text="quia pro periculo" type="straight"/>
        <underline language="LA" method="pen" text="non in gloriam referebantur" type="straight"/>
        <underline language="LA" method="pen" text="uetera luxuriae crimina" type="straight"/>
        <underline language="LA" method="pen" text="Syracusanorum hybernorum" type="straight"/>
        <underline language="LA" method="pen" text="Locris Pleminianum tumultum" type="straight"/>
        <underline language="LA" method="pen" text="suspicionibus magis" type="straight"/>
        <underline language="LA" method="pen" text="quàm argumentis" type="straight"/>
        <underline language="LA" method="pen" text="pecuniae captae reum accusarunt" type="straight"/>
        <underline language="LA" method="pen" text="Filium captum" type="straight"/>
        <underline language="LA" method="pen" text="sine precio Romana bellumq[ue] esset" type="straight"/>
        <underline language="LA" method="pen" text="Antiocho cultum" type="straight"/>
        <underline language="LA" method="pen" text="dictatorem eum" type="straight"/>
        <underline language="LA" method="pen" text="non legatum" type="straight"/>
        <underline language="LA" method="pen" text="ad aliam rem eò profectum" type="straight"/>
        <underline language="LA" method="pen" text="id quod Hispaniae, Galliae, Siciliae, Africae iampridem persuasum esset" type="straight"/>
        <underline language="LA" method="pen" text="Graeciae Asiaeq[ue] &amp; omnibus ad orientem uersis regibus gentibusq[ue] appareret" type="straight"/>
        <underline language="LA" method="pen" text="unum hominem" type="straight"/>
        <underline language="LA" method="pen" text="caput columenq[ue] imperij Romani esse" type="straight"/>
        <underline language="LA" method="pen" text="umbra Scipionis ciuitatem" type="straight"/>
        <underline language="LA" method="pen" text="dominam orbis terrarum latêre" type="straight"/>
        <underline language="LA" method="pen" text="nutus eius pro decretis patru[m]" type="straight"/>
        <underline language="LA" method="pen" text="pro populi iussis esse." type="straight"/>
        <underline language="LA" method="pen" text="Infamia intactum" type="straight"/>
        <underline language="LA" method="pen" text="inuidia qua possunt" type="straight"/>
        <underline language="LA" method="pen" text="citatus reus magno" type="straight"/>
        <underline language="LA" method="pen" text="agmine amicorum clientiumq[ue] per mediam" type="straight"/>
        <underline language="LA" method="pen" text="ad Rostra subijt" type="straight"/>
        <underline language="LA" method="pen" text="silentioq[ue]" type="straight"/>
        <underline language="LA" method="pen" text="Hoc" type="straight"/>
        <underline language="LA" method="pen" text="die tribuni" type="straight"/>
        <underline language="LA" method="pen" text="cum Annibale &amp; Carthaginiensibus" type="straight"/>
        <underline language="LA" method="pen" text="bene ac foeliciter pugnaui" type="straight"/>
        <underline language="LA" method="pen" text="litibus &amp; iurgijs superseder[i] aequum" type="straight"/>
        <underline language="LA" method="pen" text="ego hinc extemplò in Capitolium ad Iouem OPT" type="straight"/>
        <underline language="LA" method="pen" text="salutandos ibo" type="straight"/>
        <underline language="LA" method="pen" text="gratias agam, quòd mihi &amp; hoc ipso die, &amp;" type="straight"/>
        <underline language="LA" method="pen" text="egregiè reip. gerendae mentem facultatemq[ue] dederu[n]t" type="straight"/>
        <underline language="LA" method="pen" text="uestrûm quoq[ue]" type="straight"/>
        <underline language="LA" method="pen" text="ite mecum Quirires" type="straight"/>
        <underline language="LA" method="pen" text="mei similes principes habeatis" type="straight"/>
        <underline language="LA" method="pen" text="ab annis septemdecim ad senectutem" type="straight"/>
        <underline language="LA" method="pen" text="semper uos aetate[m] mea[m] honoribus uestris anteistis" type="straight"/>
        <underline language="LA" method="pen" text="uestros honores rebus" type="straight"/>
        <underline language="LA" method="pen" text="gerendis pr[a]ecessi" type="straight"/>
        <underline language="LA" method="pen" text="Capitolium ascendit" type="straight"/>
        <underline language="LA" method="pen" text="se uniuersa co[n]cio auertit" type="straight"/>
        <underline language="LA" method="pen" text="secuta Scipionem" type="straight"/>
        <underline language="LA" method="pen" text="postremò scribae uiatoresq[ue]" type="straight"/>
        <underline language="LA" method="pen" text="tribunos relinquerent" type="straight"/>
        <underline language="LA" method="pen" text="cum ijs praeter seruilem comitatu[m], &amp; praeconem, qui" type="straight"/>
        <underline language="LA" method="pen" text="Scipio non in Capitolio" type="straight"/>
        <underline language="LA" method="pen" text="totam urbem" type="straight"/>
        <underline language="LA" method="pen" text="omnia templa deûm" type="straight"/>
        <underline language="LA" method="pen" text="Celebratior is propè dies fauore hominu[m], &amp; aestimatione uerae" type="straight"/>
        <underline language="LA" method="pen" text="magnitudinis eius fuit" type="straight"/>
        <underline language="LA" method="pen" text="triumphans de Syphace rege &amp; Carthaginiensibus" type="straight"/>
        <underline language="LA" method="pen" text="Hic speciosus ultimus dies P. Scipioni illuxit" type="straight"/>
        <underline language="LA" method="pen" text="cum inuidiam &amp; certamina cum tribunis" type="straight"/>
        <underline language="LA" method="pen" text="die longiore producta" type="straight"/>
        <underline language="LA" method="pen" text="Literniu[m] concessit" type="straight"/>
        <underline language="LA" method="pen" text="ne ad causam dicendam" type="straight"/>
        <underline language="LA" method="pen" text="maior animus &amp; natura erat, ac maiori fortunae assuetus" type="straight"/>
        <underline language="LA" method="pen" text="ut reus esse sciret, &amp; summitttere se in humilitatem causam dicentium" type="straight"/>
        <underline language="LA" method="pen" text="citariq[ue] absens est" type="straight"/>
        <underline language="LA" method="pen" text="L. Scipio morbu[m] causae esse" type="straight"/>
        <underline language="LA" method="pen" text="quam excusationem" type="straight"/>
        <underline language="LA" method="pen" text="qui diem dixera[n]t, non acciperent" type="straight"/>
        <underline language="LA" method="pen" text="ab eadem superbia non uenire ad causam dicendam" type="straight"/>
        <underline language="LA" method="pen" text="iudicium" type="straight"/>
        <underline language="LA" method="pen" text="tribunos pl. &amp; concionem" type="straight"/>
        <underline language="LA" method="pen" text="quibus ius de se dicendae sente[n]tiae &amp; libertatem" type="straight"/>
        <underline language="LA" method="pen" text="his comitatus" type="straight"/>
        <underline language="LA" method="pen" text="uelut captos trahens, triumphum de PR" type="straight"/>
        <underline language="LA" method="pen" text="secessionemq[ue] eo die in Capitolium" type="straight"/>
        <underline language="LA" method="pen" text="temeritatis illius mercedem" type="straight"/>
        <underline language="LA" method="pen" text="quo duce &amp; autore nos reliquistis" type="straight"/>
        <underline language="LA" method="pen" text="eo ipsi relicti estis." type="straight"/>
        <underline language="LA" method="pen" text="ad quem antè" type="straight"/>
        <underline language="LA" method="pen" text="exercitum &amp; classem habente[m] tribunos plebis aedilemq[ue] in Siciliam" type="straight"/>
        <underline language="LA" method="pen" text="qui prehe[n]derent eum, &amp;" type="straight"/>
        <underline language="LA" method="pen" text="eum priuatum ex uilla sua extrahendum ad causam" type="straight"/>
        <underline language="LA" method="pen" text="non audeamus" type="straight"/>
        <underline language="LA" method="pen" text="Tribuni pl. appellati ab L. Scipione" type="straight"/>
        <underline language="LA" method="pen" text="decreueru[n]t" type="straight"/>
        <underline language="LA" method="pen" text="morbi causa excusaretur" type="straight"/>
        <underline language="LA" method="pen" text="sibi placere accipi eam" type="straight"/>
        <underline language="LA" method="pen" text="diemq[ue] à collegis produci" type="straight"/>
        <underline language="LA" method="pen" text="Tribunus ple." type="straight"/>
        <underline language="LA" method="pen" text="Tib. Sempronius Gracchus" type="straight"/>
        <underline language="LA" method="pen" text="cui inimicitiae cum P. Scipione intercedebant" type="straight"/>
        <underline language="LA" method="pen" text="uetuisset nomen suum decreto collegaru[m] ascribi" type="straight"/>
        <underline language="LA" method="pen" text="L. Scipio excuset morbum esse" type="straight"/>
        <underline language="LA" method="pen" text="id sibi uideri, se P. Scipionem" type="straight"/>
        <underline language="LA" method="pen" text="accusari non passurum" type="straight"/>
        <underline language="LA" method="pen" text="se appellet, auxilio ei futurum ne causam dicat" type="straight"/>
        <underline language="LA" method="pen" text="id fastigium rebus gestis" type="straight"/>
        
