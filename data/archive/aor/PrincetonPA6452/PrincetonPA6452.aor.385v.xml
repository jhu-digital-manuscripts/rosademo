        <underline language="LA" method="pen" text="Attalum" type="straight"/>
        <underline language="LA" method="pen" text="regis Eumenis fratrem legatu[m] uenisse" type="straight"/>
        <underline language="LA" method="pen" text="ipsum Eumenem uenisse" type="straight"/>
        <underline language="LA" method="pen" text="Eumenes igitur ut Romam uenit, exceptus cum tanto honore," type="straight"/>
        <underline language="LA" method="pen" text="meritis tantum eius" type="straight"/>
        <underline language="LA" method="pen" text="sed beneficijs etiam suis, ingentia quae in" type="straight"/>
        <underline language="LA" method="pen" text="Causam ueniendi sibi" type="straight"/>
        <underline language="LA" method="pen" text="praeter cupiditatem uisendi deos, hominesq[ue] quorum" type="straight"/>
        <underline language="LA" method="pen" text="supra quam ne optare quidem" type="straight"/>
        <underline language="LA" method="pen" text="Persei conatis obuiam iret" type="straight"/>
        <underline language="LA" method="pen" text="Persea haereditarium" type="straight"/>
        <underline language="LA" method="pen" text="Quae cum corporis robore ac uiribus uigeat" type="straight"/>
        <underline language="LA" method="pen" text="animum esse inueteratum diutina arte atque usu belli." type="straight"/>
        <underline language="LA" method="pen" text="Romanis quoque bellis, non finitimis" type="straight"/>
        <underline language="LA" method="pen" text="expeditiones multas, uarias que." type="straight"/>
        <underline language="LA" method="pen" text="uereri maiestatem eius" type="straight"/>
        <underline language="LA" method="pen" text="His eum fultum societatibus atque amicitijs" type="straight"/>
        <underline language="LA" method="pen" text="eos domesticos apparatus belli habere, ut externis non egeat" type="straight"/>
        <underline language="LA" method="pen" text="XXX milia peditum" type="straight"/>
        <underline language="LA" method="pen" text="V millia equitum" type="straight"/>
        <underline language="LA" method="pen" text="in decem" type="straight"/>
        <underline language="LA" method="pen" text="annos frumentum praeparare, ut" type="straight"/>
        <underline language="LA" method="pen" text="pecuniam tantam" type="straight"/>
        <underline language="LA" method="pen" text="decem millia mercenariorum" type="straight"/>
        <underline language="LA" method="pen" text="stipendium in totidem annos praeparatum" type="straight"/>
        <underline language="LA" method="pen" text="praeter annuum" type="straight"/>
        <underline language="LA" method="pen" text="metallis regijs capiat uectigal." type="straight"/>
        <underline language="LA" method="pen" text="Arma uel tribus tantum" type="straight"/>
