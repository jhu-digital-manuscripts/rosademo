        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text> 
                        LL. Consulum, consensu populi.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The laws of the consuls, by general assent of the people.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="intext">
                    <marginalia_text> 
                        polemicè, et politicè.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Militarily and politically. 
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="intext">
                    <marginalia_text> 
                        Vnus princeps, maior toto populo.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                One ruler is mightier than the entire people.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="intext">
                    <marginalia_text> 
                        Lex popularis. 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The popular law.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Affectatorem regni" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text> 
                        Democratica Lex contra Regni affectatorem.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The democratic law against the one who strives for a kingdom.
            </translation>
        </marginalia>
        
