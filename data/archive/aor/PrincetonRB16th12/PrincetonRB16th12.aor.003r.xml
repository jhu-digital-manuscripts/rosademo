    <annotation>
        
        <underline language="EN" method="pen" text="him that the king" type="straight"/>
        <underline language="EN" method="pen" text="deadly hatrit" type="straight"/>
        <underline language="EN" method="pen" text="tooke it so displeasantly" type="straight"/>
        <underline language="EN" method="pen" text="in her grace" type="straight"/>
        <underline language="EN" method="pen" text="determinit, so" type="straight"/>
        <underline language="EN" method="pen" text="opportunitie serued" type="straight"/>
        <underline language="EN" method="pen" text="to murder" type="straight"/>
        <underline language="EN" method="pen" text="him" type="straight"/>
        <underline language="EN" method="pen" text="he should choose" type="straight"/>
        <underline language="EN" method="pen" text="some yong gentlewoman, quhairof" type="straight"/>
        <underline language="EN" method="pen" text="thare was great store quhose" type="straight"/>
        <underline language="EN" method="pen" text="he might use in the meane time" type="straight"/>
        <underline language="EN" method="pen" text="baude to" type="straight"/>
        
        <mark language="EN" method="pen" name="bracket" place="right_margin" text="informing him that the             king boyled in such deadly hatrit against him, and tooke it so displeasantly that he stood so hyely in             her grace, that he was fully determinit, so soome as any possible opportunitie serued, to murder him."/>
        <mark language="EN" method="pen" name="bracket" place="right_margin" text="he should             choose him some yon gentlewoman, quhairf thare was great store, quhose companie he              might use in the meane time."/>
        
        
    </annotation>
