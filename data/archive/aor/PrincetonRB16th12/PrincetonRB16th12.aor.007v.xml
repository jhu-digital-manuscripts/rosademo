    <annotation>
        <underline language="EN" method="pen" text="was a point of maist barbarous" type="straight"/>
        <underline language="EN" method="pen" text="inhumanitie vsed against him" type="straight"/>
        <underline language="EN" method="pen" text="Nobilitie and all the officers" type="straight"/>
        <underline language="EN" method="pen" text="Court that were present" type="straight"/>
        <underline language="EN" method="pen" text="forbidden, not anes to" type="straight"/>
        <underline language="EN" method="pen" text="comming" type="straight"/>
        <underline language="EN" method="pen" text="nor to harber" type="straight"/>
        <underline language="EN" method="pen" text="for ane night" type="straight"/>
        <underline language="EN" method="pen" text="Erle of Murray" type="straight"/>
        <underline language="EN" method="pen" text="wife" type="straight"/>
        <underline language="EN" method="pen" text="goe home in hast and faine hir selfe sicke" type="straight"/>
        <underline language="EN" method="pen" text="and kepe hir bed" type="straight"/>
        <underline language="EN" method="pen" text="by thys colour," type="straight"/>
        <underline language="EN" method="pen" text="hir sicknesse, the king" type="straight"/>
        <underline language="EN" method="pen" text="freindes" type="straight"/>
        <underline language="EN" method="pen" text="scant with begging" type="straight"/>
        <underline language="EN" method="pen" text="findeth rowme in a cotage" type="straight"/>
        <underline language="EN" method="pen" text="were in triumph ouer" type="straight"/>
        <underline language="EN" method="pen" text="gloriously remoued in sight" type="straight"/>
        
        <mark language="EN" method="pen" name="bracket" place="left_margin" text="But this was a point of maist             barbarous inhumanitie bled against him, that the Nobilitie and all the officers of the Court that were present             were specially forbidden, not anes to do him reuerence at hys comming, nor to yeld him thair lodging, nor to             harber him so mekle as for ane night. And quhairas the Quene suspected that the Erle of Murray"/>
        <mark language="EN" method="pen" name="plus_sign" place="left_margin" text="goe home in hast and faine hir selfe sicke"/>
        <mark method="pen" name="plus_sign" place="left_margin"/>
        
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="scant with begging"/>
