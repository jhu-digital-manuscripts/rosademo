    <page filename="PrincetonRB16th12.068r.tif" reader="Harvey" signature="S.i.[r]"/>
    <annotation>
        
        <underline language="EN" method="pen" text="My hart" type="straight"/>
        <underline language="EN" method="pen" text="my bloud" type="straight"/>
        <underline language="EN" method="pen" text="my soule" type="straight"/>
        <underline language="EN" method="pen" text="my care" type="straight"/>
        <underline language="EN" method="pen" text="Helas you had promisit that" type="straight"/>
        <underline language="EN" method="pen" text="I should haue that pleasure" type="straight"/>
        <underline language="EN" method="pen" text="deuise" type="straight"/>
        <underline language="EN" method="pen" text="wyth you at leysure" type="straight"/>
        <underline language="EN" method="pen" text="the night" type="straight"/>
        <underline language="EN" method="pen" text="I lye and languishe" type="straight"/>
        <underline language="EN" method="pen" text="hart beyng ouerset wyth extreme" type="straight"/>
        <underline language="EN" method="pen" text="tymes I feare sum" type="straight"/>
        <underline language="EN" method="pen" text="by the way should turne abacke" type="straight"/>
        <underline language="EN" method="pen" text="sum troublous and new accident" type="straight"/>
        <underline language="EN" method="pen" text="God turne abacke all vnhapy" type="straight"/>
        <underline language="EN" method="pen" text="knaw well quho can best loue" type="straight"/>
        <underline language="EN" method="pen" text="may tell" type="straight"/>
        <underline language="EN" method="pen" text="who shall winne maist" type="straight"/>
        <underline language="EN" method="pen" text="suspicions of practising hys" type="straight"/>
        
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="wyth you at leysure"/>
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="knaw well quho can best loue"/>
        
    </annotation>
