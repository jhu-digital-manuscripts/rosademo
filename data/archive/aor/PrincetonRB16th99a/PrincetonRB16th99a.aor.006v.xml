    <annotation>
        
        <underline language="EN" method="pen" text="Graue sentences" type="straight"/>
        <underline language="EN" method="pen" text="Such, Poets haue twentie and ten" type="straight"/>
        <underline language="EN" method="pen" text="Good lessons for thee and they wife" type="straight"/>
        <underline language="EN" method="pen" text="Then keepe them in memorie fast" type="straight"/>
        <underline language="EN" method="pen" text="Points needfull and meete to be knowne" type="straight"/>
        <underline language="EN" method="pen" text="To saue to be suer thine owne" type="straight"/>
        
        <mark language="EN" method="pen" name="bracket" place="left_margin" text="Good lessons for thee and they wife? Then keepe them in memorie fast. To helpe as a comfort to life."/>
         
