<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    
    <page filename="PrincetonRB16th99a.007v.tif" pagination="[7v]" reader="Harvey"/>
    <annotation>
        
        <underline language="EN" method="pen" text="Where huswife and huswiferie, ioineth with thease" type="straight"/>
        <underline language="EN" method="pen" text="there wealth in abundance, is gotten with" type="straight"/>
        <underline language="EN" method="pen" text="husband is he, that to labour doth fall" type="straight"/>
        <underline language="EN" method="pen" text="thrift by that labour, be any way caught" type="straight"/>
        <underline language="EN" method="pen" text="houshold and housholdrie" type="straight"/>
        <underline language="EN" method="pen" text="and huswiferie, if it be good" type="straight"/>
        <underline language="EN" method="pen" text="togither, as cosins in blood" type="straight"/>
        
        <underline language="EN" method="pen" text="Ladder to thrift" type="straight"/>
        <underline language="EN" method="pen" text="grudge in youth no drudgery" type="straight"/>
        <underline language="EN" method="pen" text="come by knowledge perfectly" type="straight"/>
        <underline language="EN" method="pen" text="count no trauell slauerie" type="straight"/>
        <underline language="EN" method="pen" text="brings in penie fauerlie" type="straight"/>
        <underline language="EN" method="pen" text="kéepe thy gettings couertlie" type="straight"/>
        <underline language="EN" method="pen" text="To lash not out, too lashinglie" type="straight"/>
        <underline language="EN" method="pen" text="for feare of pinching" type="straight"/>
        <underline language="EN" method="pen" text="To get good plot to occupie" type="straight"/>
        <underline language="EN" method="pen" text="kéepe they couenants" type="straight"/>
        
        <symbol language="EN" name="SS" place="right_margin" text="Ladder to thrift"/>
        
        <mark language="EN" method="pen" name="plus_sign" place="left_margin" text="that brings in penie sauerlie"/>
        
        <mark language="EN" method="pen" name="plus_sign" place="right_margin" text="thrift by that labour, be any way caught"/>
        
        <mark method="pen" name="dot" place="intext"/> <!--drudgery, to-->
        <mark method="pen" name="plus_sign" place="intext"/>
        <mark method="chalk" name="vertical_bar" place="intext"/>
        <mark language="EN" method="pen" name="bracket" place="intext" text="To lash not out, too lashinglie, for feare of pinching penurie."/>
        
    </annotation>
</transcription>
