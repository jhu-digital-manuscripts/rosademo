<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    
    <page filename="PrincetonRB16th99a.008r.tif" pagination="8[r]" reader="Harvey" signature="ij."/>
    <annotation>
        
        <underline language="EN" method="pen" text="that thine is lawfullie" type="straight"/>
        <underline language="EN" method="pen" text="furnish house with housholdry" type="straight"/>
        <underline language="EN" method="pen" text="none to kéepe for brauerie" type="straight"/>
        <underline language="EN" method="pen" text="suffer none liue idlelie" type="straight"/>
        <underline language="EN" method="pen" text="kéepe no more but néedfullie" type="straight"/>
        <underline language="EN" method="pen" text="To raise betimes the lubberlie" type="straight"/>
        <underline language="EN" method="pen" text="walke thy pastures vsuallie" type="straight"/>
        <underline language="EN" method="pen" text="ill neighbours subtiltie" type="straight"/>
        <underline language="EN" method="pen" text="hate reuengement hastilie" type="straight"/>
        <underline language="EN" method="pen" text="neighbor neighborly" type="straight"/>
        <underline language="EN" method="pen" text="answere stranger ciuilie" type="straight"/>
        <underline language="EN" method="pen" text="shew him not thy secresie" type="straight"/>
        <underline language="EN" method="pen" text="offer no man villeny" type="straight"/>
        <underline language="EN" method="pen" text="learne how foe to pacifie" type="straight"/>
        <underline language="EN" method="pen" text="him not too trustilie" type="straight"/>
        <underline language="EN" method="pen" text="keepe thy touch substanciallie" type="straight"/>
        
        <underline language="EN" method="pen" text="make thy bandes aduisedly" type="straight"/>
        <underline language="EN" method="pen" text="com not bound through suerty" type="straight"/>
        <underline language="EN" method="pen" text="To meddle not with vsurie" type="straight"/>
        <underline language="EN" method="pen" text="liuing shiftingly" type="straight"/>
        <underline language="EN" method="pen" text="treason, endeth horribly" type="straight"/>
        <underline language="EN" method="pen" text="to eschew ill co[m]pany" type="straight"/>
        <underline language="EN" method="pen" text="To stop mischa[n]ce, through policy" type="straight"/>
        <underline language="EN" method="pen" text="beare thy crosses paciently" type="straight"/>
        <underline language="EN" method="pen" text="to kéepe from miserie" type="straight"/>
        <underline language="EN" method="pen" text="age, comming" type="straight"/>
        <underline language="EN" method="pen" text="liue in conscience quietly" type="straight"/>
        <underline language="EN" method="pen" text="To ease thy sicknes spéedilie" type="straight"/>
        <underline language="EN" method="pen" text="er helpe be past recouerie" type="straight"/>
        <underline language="EN" method="pen" text="for witches prooue vnluckilie" type="straight"/>
        <underline language="EN" method="pen" text="to climbe to thrift, by husbandrie" type="straight"/>
        <underline language="EN" method="pen" text="both reach" type="straight"/>
        <underline language="EN" method="pen" text="teach" type="straight"/>
        <underline language="EN" method="pen" text="by thrift" type="straight"/>
        <underline language="EN" method="pen" text="to shift" type="straight"/>
        
        <mark language="EN" method="pen" name="plus_sign" place="head" text="that thine is lawfullie"/>
        <mark language="EN" method="pen" name="plus_sign" place="head" text="make thy bandes aduisedly"/>
        
        <mark language="EN" method="pen" name="bracket" place="left_margin" text="To answere stranger cuilie, but shew him not thy secresie."/>
        
        <mark language="EN" method="pen" name="plus_sign" place="right_margin" text="To stop mischa[n]ce, through policy"/>
        <mark language="EN" method="pen" name="plus_sign" place="right_margin" text="beare thy crosses paciently"/>
        <mark language="EN" method="pen" name="plus_sign" place="right_margin" text="To ease thy sickness spéedilie"/>
        <mark method="pen" name="plus_sign" place="right_margin"/> <!--withall-->
        
        <mark language="EN" method="pen" name="quotation_mark" place="intext" text="none to kéepe for brauerie"/>
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="suffer none liue idlelie"/>
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="kéepe no more but néedfullie"/>
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="to raise betimes the lubberlie"/>
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="walke thy pastures vsuallie"/>
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="answere stranger ciuilie"/>
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="learne how foe to pacifie"/>
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="kéepe thy touch substanciallie"/>
        
        <mark language="EN" method="pen" name="bracket" place="intext" text="To make thy bandes aduisedly, &amp; com not bound through suerty"/>
        <mark method="pen" name="plus_sign" place="intext"/> <!--age-->
        <mark language="EN" method="pen" name="bracket" place="intext" text="To ease thy sicknes spéedilie, er helpe be past recouerie."/>
        
        
        
        
