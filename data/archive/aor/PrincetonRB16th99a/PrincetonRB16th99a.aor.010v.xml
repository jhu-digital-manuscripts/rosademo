<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    
    <page filename="PrincetonRB16th99a.010v.tif" pagination="[10v]" reader="Harvey"/>
    <annotation>
        
        <marginalia hand="Italian">
            <language ident="EN">
                <position book_orientation="0" place="head">
                    <marginalia_text>Borrowing, &amp; Lending.</marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <underline language="EN" method="pen" text="Who liuing but lends, and" type="straight"/>
        <underline language="EN" method="pen" text="buieng and selling, might" type="straight"/>
        <underline language="EN" method="pen" text="thou keepest, thy thouch for" type="straight"/>
        <underline language="EN" method="pen" text="Who quick be to borow, and slow be to" type="straight"/>
        <underline language="EN" method="pen" text="their credit is naught, go they neuer" type="straight"/> 
        <underline language="EN" method="pen" text="By shifting and borrowing, who so" type="straight"/>
        <underline language="EN" method="pen" text="Then lay to liue warily, and wisely to" type="straight"/>
        <underline language="EN" method="pen" text="for prodigall liuers, haue seldom" type="straight"/>
        <underline language="EN" method="pen" text="the foole at the bottom" type="straight"/>
        <underline language="EN" method="pen" text="wife, at the brim" type="straight"/>
        <underline language="EN" method="pen" text="Who careth, nor spareth, till spent" type="straight"/>
        <underline language="EN" method="pen" text="bobbing, not robbing, be fearefull" type="straight"/>
        <underline language="EN" method="pen" text="Where welthines floweth, no friendship" type="straight"/>
        <underline language="EN" method="pen" text="whom pouertie pincheth, hath friendship" type="straight"/>
        <underline language="EN" method="pen" text="happie is he, by example" type="straight"/>
        <underline language="EN" method="pen" text="take heede by the fall, of a mischieued" type="straight"/>
        <underline language="EN" method="pen" text="breaketh his credit, or cracketh" type="straight"/>
        <underline language="EN" method="pen" text="trust such with a suretie" type="straight"/>
        <underline language="EN" method="pen" text="Account it wel sold, that is justlie" type="straight"/>
        <underline language="EN" method="pen" text="count it wel bought, that" type="straight"/>
        <underline language="EN" method="pen" text="Leaue Princes affaires vndeskanted" type="straight"/>
        <underline language="EN" method="pen" text="keepe thy selfe out, of the Magistrates clawes" type="straight"/>
        
        <symbol name="JC" place="head"/>
        
        <symbol name="Bisected_circle" place="left_margin"/>
        
        <mark language="EN" method="pen" name="bracket" place="left_margin" text="At some time to borow, account it no shame, if iustlie thou keepest, thy touch for the same: Who quick be to borow, and slow be to paie, their credit is naught, go they neuer so gaie."/>
        <mark language="EN" method="pen" name="plus_sign" place="left_margin" text="their credit is naught, go they neuer"/>
        <mark method="pen" name="plus_sign" place="left_margin"/>
        <mark language="EN" method="pen" name="bracket" place="left_margin" text="Where welthines floweth, no friendship can lack, whom pouertie pincheth, hath friendship as slack: Then happie is he, by example that can, take heede by the fall, of a mischieued man."/>
        <mark language="EN" method="pen" name="plus_sign" place="left_margin" text="whom pouertie pincheth, hath friendship"/>
        <mark method="pen" name="plus_sign" place="left_margin"/>
        <mark language="EN" method="pen" name="plus_sign" place="left_margin" text="take heede by the fall, of a mischieued"/>
        <mark language="EN" method="pen" name="plus_sign" place="left_margin" text="trust such with a suretie"/>
        <mark language="EN" method="pen" name="bracket" place="left_margin" text="Account it wel sold, that is justlie well paid. and count it wel bought, that is neuer denaid: But yet here is tone, here is tother, doth best, for buier and seller, for quiet and rest."/>
        <mark language="EN" method="pen" name="plus_sign" place="left_margin" text="keepe thy selfe out, of the Magistrates clawes"/>
        
        <mark method="pen" name="plus_sign" place="right_margin"/>
        <mark language="EN" method="pen" name="plus_sign" place="right_margin" text="trust such with a suretie"/>
        <mark language="EN" method="pen" name="plus_sign" place="right_margin" text="Account it wel sold, that is justlie well paid"/>
        <mark method="pen" name="plus_sign" place="right_margin"/>
        <mark method="pen" name="double_vertical_bar" place="right_margin"/>
        
        <mark method="pen" name="dot" place="intext"/> <!--spend,-->
        <mark method="chalk" name="vertical_bar" place="intext"/>
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="suretie"/>
        <mark method="chalk" name="vertical_bar" place="intext"/>
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="Princes affaires"/>
        <mark method="pen" name="dot" place="intext"/> <!--on-->
        
        <mark method="pen" name="plus_sign" place="tail"/>
        
    </annotation>
