<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    
    <page filename="PrincetonRB16th99a.011v.tif" pagination="[11v]" reader="Harvey"/>
    <annotation>
        
        <underline language="EN" method="pen" text="seeketh reuengement" type="straight"/>
        <underline language="EN" method="pen" text="quiet nor safetie" type="straight"/>
        <underline language="EN" method="pen" text="of wilfulnes trieth" type="straight"/>
        <underline language="EN" method="pen" text="striue for a coxcome" type="straight"/>
        <underline language="EN" method="pen" text="thriue as a daw" type="straight"/>
        <underline language="EN" method="pen" text="To hunters and haukers," type="straight"/>
        <underline language="EN" method="pen" text="milde answere" type="straight"/>
        <underline language="EN" method="pen" text="where a mans better, wil open a gap" type="straight"/>
        <underline language="EN" method="pen" text="resist not with rudenes for" type="straight"/>
        <underline language="EN" method="pen" text="world, for a churle that" type="straight"/>
        <underline language="EN" method="pen" text="keepe that is his owne" type="straight"/>
        <underline language="EN" method="pen" text="as of curtesie smels" type="straight"/>
        <underline language="EN" method="pen" text="fauor and friendship, where" type="straight"/>
        <underline language="EN" method="pen" text="from gadding, but when" type="straight"/>
        <underline language="EN" method="pen" text="and fasting daie" type="straight"/>
        <underline language="EN" method="pen" text="what custome thou keepest" type="straight"/>
        <underline language="EN" method="pen" text="let others" type="straight"/>
        <underline language="EN" method="pen" text="their tithing, be" type="straight"/>
        <underline language="EN" method="pen" text="vnto Godward, not" type="straight"/>
        <underline language="EN" method="pen" text="ill tithers, ill thriuers, most" type="straight"/>
        <underline language="EN" method="pen" text="Paie weekelie thy workman" type="straight"/>
        <underline language="EN" method="pen" text="paie quarterlie seruants" type="straight"/>
        <underline language="EN" method="pen" text="Giue garment to such, as deserue" type="straight"/>
        <underline language="EN" method="pen" text="Beware raskabilia, slothfull" type="straight"/>
        <underline language="EN" method="pen" text="purloiners and filchers, that" type="straight"/>
        <underline language="EN" method="pen" text="lubbers" type="straight"/>
        <underline language="EN" method="pen" text="roules in expences, but" type="straight"/>
        <underline language="EN" method="pen" text="Good wife, and" type="straight"/>
        <underline language="EN" method="pen" text="good seruant, good laborer, earneth" type="straight"/>
        <underline language="EN" method="pen" text="Good friend, and good neighbor" type="straight"/>
        <underline language="EN" method="pen" text="hartilie welcome" type="straight"/>
        
        <symbol name="JC" place="head"/>
        
        <mark language="EN" method="pen" name="plus_sign" place="left_margin" text="milde answere"/>
        <mark language="EN" method="pen" name="plus_sign" place="left_margin" text="resist not with rudenes for"/>
        <mark language="EN" method="pen" name="plus_sign" place="left_margin" text="ill tithers, ill thriuers, most"/>
        <mark language="EN" method="pen" name="plus_sign" place="left_margin" text="purloiners and filchers, that"/>
        <mark method="pen" name="plus_sign" place="left_margin"/>
        <mark language="EN" method="pen" name="plus_sign" place="left_margin" text="good seruant, good laborer, earneth"/>
        
        <mark language="EN" method="pen" name="plus_sign" place="right_margin" text="seeketh reuengement"/>
        <mark language="EN" method="pen" name="plus_sign" place="right_margin" text="as of curtesie smels"/>
        <mark language="EN" method="pen" name="plus_sign" place="right_margin" text="and fasting daie"/>
        <mark language="EN" method="pen" name="plus_sign" place="right_margin" text="Giue garment to such, as deserue"/>
        <mark language="EN" method="pen" name="bracket" place="right_margin" text="Good wife, and good children, are worthie to eate, good seruant, good laborer, earneth their meate. Good friend, and good neighbor, that fellowlie gest: with hartilie welcome, should haue of the best."/>
        
        <mark method="pen" name="dot" place="intext"/> <!--saie,-->
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="gadding"/>
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="slothfull"/>
        <mark method="chalk" name="vertical_bar" place="intext"/>
        <mark method="pen" name="dot" place="intext"/> <!--eate,-->
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="earneth"/>
        
    </annotation>
