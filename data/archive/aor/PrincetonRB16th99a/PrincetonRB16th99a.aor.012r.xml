    <annotation>
        
        <underline language="EN" method="pen" text="Depart not with al that thou hast to thy childe" type="straight"/>
        <underline language="EN" method="pen" text="much lesse vnto" type="straight"/>
        <underline language="EN" method="pen" text="greatest preferment, that" type="straight"/>
        <underline language="EN" method="pen" text="learning and nurture, to traine" type="straight"/>
        <underline language="EN" method="pen" text="though left as a" type="straight"/>
        <underline language="EN" method="pen" text="consumeth to nothing, as block in" type="straight"/>
        <underline language="EN" method="pen" text="fruites of thy faith, to heauen aforehand" type="straight"/>
        <underline language="EN" method="pen" text="maketh thy store, with his blessing to swim" type="straight"/>
        <underline language="EN" method="pen" text="to get riches, by sea and by land" type="straight"/>
        <underline language="EN" method="pen" text="ventreth his life, in his enimies hand" type="straight"/> 
        <underline language="EN" method="pen" text="setteth his soule vpon sixe or on seauen" type="straight"/>
        <underline language="EN" method="pen" text="fearing nor caring, for hell nor for heauen" type="straight"/>
        <underline language="EN" method="pen" text="pincheth and spareth, and pineth his life" type="straight"/>
        <underline language="EN" method="pen" text="vp bags, for to leaue to his wife" type="straight"/>
        <underline language="EN" method="pen" text="open the chest" type="straight"/>
        <underline language="EN" method="pen" text="can sooth hir, and" type="straight"/>
        <underline language="EN" method="pen" text="away" type="straight"/>
        <underline language="EN" method="pen" text="takes part of Gods benefits" type="straight"/>
        <underline language="EN" method="pen" text="lessons approoued" type="straight"/>
        <underline language="EN" method="pen" text="ye note" type="straight"/>
        <underline language="EN" method="pen" text="saue and auantage ye, many a grote" type="straight"/>
        <underline language="EN" method="pen" text="can follow" type="straight"/>
        <underline language="EN" method="pen" text="occasion found" type="straight"/>
        <underline language="EN" method="pen" text="euerie lesson, may saue ye a pound" type="straight"/>
        
