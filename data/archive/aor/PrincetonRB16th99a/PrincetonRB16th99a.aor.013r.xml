        <underline language="EN" method="pen" text="Fresh herring plentie, Mihell" type="straight"/>
        <underline language="EN" method="pen" text="fatted Crones, and" type="straight"/>
        <underline language="EN" method="pen" text="such old things" type="straight"/>
        <underline language="EN" method="pen" text="doe laie, for" type="straight"/>
        <underline language="EN" method="pen" text="porke and souse" type="straight"/>
        <underline language="EN" method="pen" text="sprats and spurlings" type="straight"/>
        <underline language="EN" method="pen" text="make good cheere" type="straight"/>
        <underline language="EN" method="pen" text="Christmas comes, but once a yeere" type="straight"/>
        <underline language="EN" method="pen" text="thriftie doe, as doe they should" type="straight"/>
        <underline language="EN" method="pen" text="causes good, so many waies" type="straight"/>
        <underline language="EN" method="pen" text="Embrings wel, and fasting" type="straight"/>
        <underline language="EN" method="pen" text="lawe commands" type="straight"/>
        <underline language="EN" method="pen" text="land doth will" type="straight"/>
        <underline language="EN" method="pen" text="Sea doth wish" type="straight"/>
        <underline language="EN" method="pen" text="Spare sometime flesh" type="straight"/>
        <underline language="EN" method="pen" text="and feede of fish" type="straight"/>
        <underline language="EN" method="pen" text="fish is scant, and" type="straight"/>
        <underline language="EN" method="pen" text="fruite of trées" type="straight"/>
        <underline language="EN" method="pen" text="that want, with butter and chéese" type="straight"/>
        <underline language="EN" method="pen" text="of windes" type="straight"/>
        <underline language="EN" method="pen" text="all the times" type="straight"/>
        <underline language="EN" method="pen" text="North winds send" type="straight"/>
        <underline language="EN" method="pen" text="South winds" type="straight"/>
        <underline language="EN" method="pen" text="East windes we" type="straight"/>
        <underline language="EN" method="pen" text="West winds blow" type="straight"/>
        <underline language="EN" method="pen" text="North east is" type="straight"/>
        <underline language="EN" method="pen" text="South east not" type="straight"/>
        <underline language="EN" method="pen" text="North west is" type="straight"/>
        <underline language="EN" method="pen" text="South west" type="straight"/>
