    <page filename="PrincetonRB16th99a.019r.tif" pagination="19[r]" reader="Harvey" signature="F.j."/>
    <annotation>
        
        <underline language="EN" method="pen" text="late" type="straight"/>
        <underline language="EN" method="pen" text="soone is as" type="straight"/>
        <underline language="EN" method="pen" text="pick cleane, séede wheat" type="straight"/>
        <underline language="EN" method="pen" text="Good ground" type="straight"/>
        <underline language="EN" method="pen" text="choice séede to" type="straight"/>
        <underline language="EN" method="pen" text="lustily thwack" type="straight"/>
        <underline language="EN" method="pen" text="soweth ill séed" type="straight"/>
        <underline language="EN" method="pen" text="furrow for water" type="straight"/>
        <underline language="EN" method="pen" text="Each soile" type="straight"/>
        <underline language="EN" method="pen" text="no vaine" type="straight"/>
        <underline language="EN" method="pen" text="euerie graine" type="straight"/>
        <underline language="EN" method="pen" text="Naught proue" type="straight"/>
        <underline language="EN" method="pen" text="naught craue" type="straight"/>
        <underline language="EN" method="pen" text="naught uenter" type="straight"/>
        <underline language="EN" method="pen" text="naught haue" type="straight"/>
        
        <underline language="EN" method="pen" text="A rottenly mould" type="straight"/>
        <underline language="EN" method="pen" text="is land woorth gould" type="straight"/>
        <underline language="EN" method="pen" text="Land, all forlorne" type="straight"/>
        <underline language="EN" method="pen" text="not good for corne" type="straight"/>
        <underline language="EN" method="pen" text="Land barren doth" type="straight"/>
        <underline language="EN" method="pen" text="small strawe, short eare" type="straight"/>
        <underline language="EN" method="pen" text="best graine" type="straight"/>
        <underline language="EN" method="pen" text="most flower" type="straight"/>
        <underline language="EN" method="pen" text="Grosse corne" type="straight"/>
        <underline language="EN" method="pen" text="much bran" type="straight"/>
        <underline language="EN" method="pen" text="the baker doth ban" type="straight"/>
        <underline language="EN" method="pen" text="Som woodland may" type="straight"/>
        <underline language="EN" method="pen" text="thrée crops he may" type="straight"/>
        <underline language="EN" method="pen" text="First barlie" type="straight"/>
        <underline language="EN" method="pen" text="pease" type="straight"/>
        <underline language="EN" method="pen" text="then wheat, if" type="straight"/>
        
        <mark language="EN" method="pen" name="plus_sign" place="tail" text="naught haue"/>
        
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Too late doth kill, too soone is as ill."/>
        <mark method="chalk" name="vertical_bar" place="intext"/>
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Good ground doth craue, choice séede to haue."/>
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Who soweth in raine, hath wéed to his paine. But woorse shall he spéed, that soweth ill séed."/>
        <mark method="pen" name="plus_sign" place="intext"/>
        <mark method="pen" name="dash" place="intext"/> <!--furrow for water.-->
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Each soile no vaine, for euerie graine."/>
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Naught proue, naught craue, naught venter, naught haue."/>
        
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="All grauell and sand, is not the best land. A rottenly mould,"/>
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Land, all forlorne, not good for corne."/>
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Land barren doth beare, small strawe, short eare."/>
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Tis tride ery hower, best graine most flower."/>
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Grosse corne much bran, the baker doth ban."/>
        <mark method="pen" name="plus_sign" place="intext"/>
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Som woodland may crake, thrée crops he may take."/>
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="thrée crops he may"/>
        
    </annotation>
</transcription>
