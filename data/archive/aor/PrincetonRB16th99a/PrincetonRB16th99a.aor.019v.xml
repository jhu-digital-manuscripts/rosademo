    <page filename="PrincetonRB16th99a.019v.tif" pagination="[19v]" reader="Harvey"/>
    <annotation>
        
        <underline language="EN" method="pen" text="Two crops" type="straight"/>
        <underline language="EN" method="pen" text="and away" type="straight"/>
        <underline language="EN" method="pen" text="must champion" type="straight"/>
        <underline language="EN" method="pen" text="Where barlie" type="straight"/>
        <underline language="EN" method="pen" text="laie wheat to" type="straight"/>
        <underline language="EN" method="pen" text="What champion knowes" type="straight"/>
        <underline language="EN" method="pen" text="First barlie" type="straight"/>
        <underline language="EN" method="pen" text="pease" type="straight"/>
        <underline language="EN" method="pen" text="fallow for wheat" type="straight"/>
        <underline language="EN" method="pen" text="Fat peasefed swine" type="straight"/>
        <underline language="EN" method="pen" text="for drouer is fine." type="straight"/>
        <underline language="EN" method="pen" text="Each diuers soile" type="straight"/>
        <underline language="EN" method="pen" text="hath diuers toile" type="straight"/>
        <underline language="EN" method="pen" text="countries" type="straight"/>
        <underline language="EN" method="pen" text="that some refuse" type="straight"/>
        <underline language="EN" method="pen" text="wheat" type="straight"/>
        <underline language="EN" method="pen" text="where water" type="straight"/>
        <underline language="EN" method="pen" text="Sowe pease or dredge" type="straight"/>
        <underline language="EN" method="pen" text="belowe in that redge" type="straight"/>
        <underline language="EN" method="pen" text="Learne soone to get" type="straight"/>
        <underline language="EN" method="pen" text="good quickset." type="straight"/>
        
        <underline language="EN" method="pen" text="Hog measeled" type="straight"/>
        <underline language="EN" method="pen" text="With peasebolt" type="straight"/>
        <underline language="EN" method="pen" text="Old corne" type="straight"/>
        <underline language="EN" method="pen" text="so kept as" type="straight"/>
        <underline language="EN" method="pen" text="by sloes well" type="straight"/>
        <underline language="EN" method="pen" text="Kéepe sloes vpon" type="straight"/>
        <underline language="EN" method="pen" text="for flixe of thy" type="straight"/>
        <underline language="EN" method="pen" text="Of vergis be" type="straight"/>
        <underline language="EN" method="pen" text="poore cattel," type="straight"/>
        <underline language="EN" method="pen" text="By malt ill" type="straight"/>
        <underline language="EN" method="pen" text="small profit is" type="straight"/>
        <underline language="EN" method="pen" text="Rie straw vp stack" type="straight"/>
        <underline language="EN" method="pen" text="Wheat straw, drie" type="straight"/>
        <underline language="EN" method="pen" text="Wheat chaffe, lay" type="straight"/>
        <underline language="EN" method="pen" text="handsome a bin" type="straight"/>
        <underline language="EN" method="pen" text="for chaffe to lie in" type="straight"/>
        
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Two crops and away, must champion say."/>
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Each diuers soile, hath diuers toile."/>
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Some countries vse, that some refuse."/>
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Learne soone to get, a good quickset."/>
        
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Old corne worth gold, so kept as it shold."/>
        <mark method="pen" name="plus_sign" place="intext"/>
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Kéepe sloes vpon bow, fr flire of thy cow."/>
        
    </annotation>
