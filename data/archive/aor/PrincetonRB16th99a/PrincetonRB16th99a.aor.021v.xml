    <annotation>
        
        <underline language="EN" method="pen" text="breadcorne" type="straight"/>
        <underline language="EN" method="pen" text="drinkcorne, such" type="straight"/>
        <underline language="EN" method="pen" text="Good land that is seuerall, crops may" type="straight"/>
        <underline language="EN" method="pen" text="Ton taketh his season, as commoners" type="straight"/>
        <underline language="EN" method="pen" text="a good fallow to make" type="straight"/>
        <underline language="EN" method="pen" text="thereon barlie" type="straight"/>
        <underline language="EN" method="pen" text="to sowe pease" type="straight"/>
        <underline language="EN" method="pen" text="to sowe wheat" type="straight"/>
        <underline language="EN" method="pen" text="then fallow againe" type="straight"/>
        <underline language="EN" method="pen" text="or lie lay for" type="straight"/>
        <underline language="EN" method="pen" text="lay on more compas" type="straight"/>
        <underline language="EN" method="pen" text="and fallow agen" type="straight"/>
        <underline language="EN" method="pen" text="land be vnlustie, the crop" type="straight"/>
        <underline language="EN" method="pen" text="lose ye your cost, to your coresie" type="straight"/>
        <underline language="EN" method="pen" text="and land (ouerburdened) is cleane out of hart" type="straight"/>
        <underline language="EN" method="pen" text="(Just by) ye may comfort, with compas at" type="straight"/>
        <underline language="EN" method="pen" text="far off ye must comfort, with fauor and" type="straight"/>
        <underline language="EN" method="pen" text="codware be" type="straight"/>
        <underline language="EN" method="pen" text="bent of the market place, serue thee not" type="straight"/>
        <underline language="EN" method="pen" text="hogs vp a fatting, to drouer to" type="straight"/>
        <underline language="EN" method="pen" text="of a fallow, enricheth" type="straight"/>
        
