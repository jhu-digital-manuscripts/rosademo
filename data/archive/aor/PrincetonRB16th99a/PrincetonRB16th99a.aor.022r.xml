    <annotation>
        
        <underline language="EN" method="pen" text="broken vpland" type="straight"/>
        <underline language="EN" method="pen" text="with water opprest" type="straight"/>
        <underline language="EN" method="pen" text="much doonged" type="straight"/>
        <underline language="EN" method="pen" text="for wheat is" type="straight"/>
        <underline language="EN" method="pen" text="winter, annoieth" type="straight"/>
        <underline language="EN" method="pen" text="sowe otes" type="straight"/>
        <underline language="EN" method="pen" text="else bullimong" type="straight"/>
        <underline language="EN" method="pen" text="peason, or runciuals, fitches or tere" type="straight"/>
        <underline language="EN" method="pen" text="Sowe acornes" type="straight"/>
        <underline language="EN" method="pen" text="sowe hawe and rie with" type="straight"/>
        <underline language="EN" method="pen" text="Cunnie, may enter to crop" type="straight"/>
        <underline language="EN" method="pen" text="is in daunger, of loosing his top" type="straight"/>
        <underline language="EN" method="pen" text="pescods delighteth" type="straight"/>
        <underline language="EN" method="pen" text="greener thy peason, and warmer the roome" type="straight"/>
        <underline language="EN" method="pen" text="the layer" type="straight"/>
        <underline language="EN" method="pen" text="more plenty they come" type="straight"/>
        <underline language="EN" method="pen" text="plow vp" type="straight"/>
        <underline language="EN" method="pen" text="delue vp" type="straight"/>
        <underline language="EN" method="pen" text="speedy quickset, for a fence" type="straight"/>
        <underline language="EN" method="pen" text="the porkling to fat" type="straight"/>
        <underline language="EN" method="pen" text="taken in season" type="straight"/>
        <underline language="EN" method="pen" text="ratling, or swelling, get once to the" type="straight"/>
        <underline language="EN" method="pen" text="euer thing fat is, againe if it fall" type="straight"/>
        <underline language="EN" method="pen" text="fatter the better" type="straight"/>
        <underline language="EN" method="pen" text="to sell or to kil" type="straight"/>
        <underline language="EN" method="pen" text="to continue, make" type="straight"/>
        <underline language="EN" method="pen" text="dieth, go burie or burne" type="straight"/>
        <underline language="EN" method="pen" text="tainting of ground" type="straight"/>
        
        <underline language="EN" method="pen" text="Hastings or fullams" type="straight"/>
        <underline language="EN" method="pen" text="rearing" type="straight"/>
        
        <mark method="pen" name="plus_sign" place="right_margin"/>
        <mark language="EN" method="pen" name="plus_sign" place="right_margin" text="is in daunger, of loosing his top"/>
        <mark language="EN" method="pen" name="vertical_bar" place="right_margin" text="Who pescods delighteth, to haue with the furst, if now he do sowe them, I thinke it not wurst. The greener thy peason, and warmer the roome: more lusty the layer, more plenty they come."/>
        <mark language="EN" method="pen" name="plus_sign" place="right_margin" text="euer thing fat is, againe if it fall"/>
        <mark language="EN" method="pen" name="bracket" place="right_margin" text="What euer thing fat is, againe if it fall, thou ventrest the thing, and the fatnes withall. The fatter the better, to sell or to kil: but not to continue, make proofe if ye wil."/>
