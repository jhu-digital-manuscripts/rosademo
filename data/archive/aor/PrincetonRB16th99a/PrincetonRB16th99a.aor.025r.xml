        <underline language="EN" method="pen" text="Lay compas vp handsomly" type="straight"/>
        <underline language="EN" method="pen" text="More compas it maketh" type="straight"/>
        <underline language="EN" method="pen" text="horsekeeper daily, forgetteth" type="straight"/>
        <underline language="EN" method="pen" text="Make hillocks" type="straight"/>
