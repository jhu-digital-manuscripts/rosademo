    <annotation>
        
        <underline language="EN" method="pen" text="plough vp thy hedlond" type="straight"/>
        <underline language="EN" method="pen" text="delue it with spade" type="straight"/>
        <underline language="EN" method="pen" text="vpon hillocks" type="straight"/>
        <underline language="EN" method="pen" text="now trench it" type="straight"/>
        <underline language="EN" method="pen" text="trench not a yard, from another" type="straight"/>
        <underline language="EN" method="pen" text="filled, with muck by" type="straight"/>
        <underline language="EN" method="pen" text="couer with mould, for a" type="straight"/>
        <underline language="EN" method="pen" text="Foule priuies are now" type="straight"/>
        <underline language="EN" method="pen" text="buried in garden, in trenches alowe" type="straight"/>
        <underline language="EN" method="pen" text="very many things, better to growe" type="straight"/>
        <underline language="EN" method="pen" text="chimney all sootie, would" type="straight"/>
        <underline language="EN" method="pen" text="chimney and sootie, if fier once take" type="straight"/>
        <underline language="EN" method="pen" text="burning and breaking, soone mischeefe may make" type="straight"/>
