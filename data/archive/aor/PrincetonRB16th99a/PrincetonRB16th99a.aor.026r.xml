    <annotation>
        
        <underline language="EN" method="pen" text="labour, no sweate" type="straight"/>
        <underline language="EN" method="pen" text="labour, for heate." type="straight"/>
        <underline language="EN" method="pen" text="Fat hog (er ye kill" type="straight"/>
        <underline language="EN" method="pen" text="ye doo spill it." type="straight"/>
        <underline language="EN" method="pen" text="ore in" type="straight"/>
        
        <underline language="EN" method="pen" text="sée brawne, sod tender." type="straight"/>
        <underline language="EN" method="pen" text="Ill bread, and ill drinke" type="straight"/>
        <underline language="EN" method="pen" text="makes many, ill thinke" type="straight"/>
        <underline language="EN" method="pen" text="Both meate, and" type="straight"/>
        <underline language="EN" method="pen" text="ill dressed, halfe lost" type="straight"/>
        <underline language="EN" method="pen" text="may chéere, when" type="straight"/>
        <underline language="EN" method="pen" text="charged man" type="straight"/>
        <underline language="EN" method="pen" text="must chéere as he can" type="straight"/>
        
        <underline language="EN" method="pen" text="to dike and to hedge" type="straight"/>
        <underline language="EN" method="pen" text="with thy beetle &amp; wedge" type="straight"/>
        <underline language="EN" method="pen" text="sliuers doo well" type="straight"/>
        <underline language="EN" method="pen" text="be letted, and freat bellie" type="straight"/>
        <underline language="EN" method="pen" text="wheele barrow also" type="straight"/>
        <underline language="EN" method="pen" text="cattle their fodder, in plot drie" type="straight"/>
        <underline language="EN" method="pen" text="with thy wennels, together" type="straight"/>
        <underline language="EN" method="pen" text="lurched by others" type="straight"/>
        
        <mark language="EN" method="pen" name="vertical_bar" place="left_margin" text="No labour, no sweate, go labour, for heate."/>
        
        <mark language="EN" method="pen" name="plus_sign" place="right_margin" text="ill dressed, halfe lost"/>
        <mark language="EN" method="pen" name="plus_sign" place="right_margin" text="must chéere as he can"/>
        
        <mark method="pen" name="dot" place="intext"/> <!--sweate, go-->
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="labour, for heate."/>
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Fat hog (er ye kill it) or else ye doo spill it."/>
        
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Ill bread, and ill drinke, makes many, ill thinke. Both meate, and cost, ill dressed, halfe lost."/>
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Who hath wherewithall, may chéere, when he shall. But charged man, must chéere as he can."/>
        
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="cattle their fodder, in plot drie"/>
        
        <mark language="EN" method="pen" name="plus_sign" place="tail" text="lurched by others"/>
        
    </annotation>
