    <page filename="PrincetonRB16th99a.032v.tif" pagination="[32v]" reader="Harvey"/>
    <annotation>
        
        <underline language="EN" method="pen" text="Haithorne and prim" type="straight"/>
        <underline language="EN" method="pen" text="clothes trim" type="straight"/>
        
        <underline language="EN" method="pen" text="mindfull of rearing, in hope of a gaine" type="straight"/>
        <underline language="EN" method="pen" text="profit shall giue thee, reward for" type="straight"/>
        <underline language="EN" method="pen" text="calfe" type="straight"/>
        <underline language="EN" method="pen" text="his lamb" type="straight"/>
        <underline language="EN" method="pen" text="neate, and a sheepe" type="straight"/>
        <underline language="EN" method="pen" text="reare vp, a pig in his" type="straight"/>
        <underline language="EN" method="pen" text="cheaper his bacon, and sweeter" type="straight"/>
        <underline language="EN" method="pen" text="veale, pig and lamb being froth" type="straight"/>
        <underline language="EN" method="pen" text="in a weeke, go to bed without broth" type="straight"/>
        <underline language="EN" method="pen" text="greedie in" type="straight"/>
        <underline language="EN" method="pen" text="shortly" type="straight"/>
        <underline language="EN" method="pen" text="wilfull to kill, and vnskilfull" type="straight"/>
        <underline language="EN" method="pen" text="looke for no foison, I tell thee before" type="straight"/>
        
        <mark language="EN" method="pen" name="vertical_bar" place="left_margin" text="Be mindfull of rearing, in hope of a gaine, dame profit shall giue thee, reward for thy paine."/>
        <mark language="EN" method="pen" name="vertical_bar" place="left_margin" text="And he that can reare vp, a pig in his house: hath cheaper his bacon, and sweeter his souse."/>
        <mark method="pen" name="plus_sign" place="left_margin"/>
        <mark language="EN" method="pen" name="vertical_bar" place="left_margin" text="Be greedie in spending, and careles to saue, and shortly be needie, and readie to craue. Be wilfull to kill, and vnskilfull to store: and looke for no foison, I tell thee before."/>
        
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="rearing"/>
        <mark method="pen" name="plus_sign" place="intext"/> <!--being froth-->
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="foison"/>
  
    </annotation>
