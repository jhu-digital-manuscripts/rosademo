<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    
    <page filename="PrincetonRB16th99a.036r.tif" pagination="36[r]" reader="Harvey" signature="ij."/>
    <annotation>
        
        <underline language="EN" method="pen" text="LAy compas" type="straight"/>
        <underline language="EN" method="pen" text="euer ye plow" type="straight"/>
        <underline language="EN" method="pen" text="doong heapes alowe" type="straight"/>
        <underline language="EN" method="pen" text="barlie to growe" type="straight"/>
        <underline language="EN" method="pen" text="séedes" type="straight"/>
        <underline language="EN" method="pen" text="Moone doth growe" type="straight"/>
        <underline language="EN" method="pen" text="pease not too" type="straight"/>
        <underline language="EN" method="pen" text="plough ye set" type="straight"/>
        <underline language="EN" method="pen" text="sowen, sore noieth" type="straight"/>
        <underline language="EN" method="pen" text="ripe, hog stroieth" type="straight"/>
        <underline language="EN" method="pen" text="Some prouender" type="straight"/>
        <underline language="EN" method="pen" text="oxen that" type="straight"/>
        <underline language="EN" method="pen" text="hay, and not strawe" type="straight"/>
        <underline language="EN" method="pen" text="stéeres ye" type="straight"/>
        <underline language="EN" method="pen" text="mixe strawe" type="straight"/>
        <underline language="EN" method="pen" text="carting, ill tillage" type="straight"/>
        <underline language="EN" method="pen" text="makes som to flie village" type="straight"/>
        <underline language="EN" method="pen" text="cattle aright" type="straight"/>
        <underline language="EN" method="pen" text="kéepe them in plight" type="straight"/>
        
        <underline language="EN" method="pen" text="Cut vines and osier" type="straight"/>
        <underline language="EN" method="pen" text="plash hedge of enclosier" type="straight"/>
        <underline language="EN" method="pen" text="strike" type="straight"/>
        <underline language="EN" method="pen" text="of deluing mowles." type="straight"/>
        <underline language="EN" method="pen" text="Such hillocks in" type="straight"/>
        <underline language="EN" method="pen" text="lay leauelled plaine" type="straight"/>
        <underline language="EN" method="pen" text="Cow little giueth" type="straight"/>
        <underline language="EN" method="pen" text="that hardly liueth" type="straight"/>
        
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="LAy compas ynow, er euer ye plow."/>
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Place doong heapes alowe, more barlie to growe."/>
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Fine séedes then sowe, whilst Moone doth growe."/>
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Late sowen, fore noieth, late ripe, hog stroieth."/>
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="To oxen that drawe, give hay, and not strawe."/>
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="stéeres ye"/>
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="mixe strawe"/>
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Much carting, ill tillage, makes som to flie village."/>
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Use cattle aright, to kéepe them in plight."/>
        
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Cut vines and osier, plash hedge of enclosier."/>
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="of deluing mowles"/>
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="of deluing mowles. Such hillocks in vaine, lay leauelled plaine."/>
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Cow little giueth, that hardly liueth."/>
        
    </annotation>
</transcription>
