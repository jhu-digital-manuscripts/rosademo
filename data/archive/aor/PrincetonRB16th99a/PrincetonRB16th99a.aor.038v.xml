    <annotation>
        
        <underline language="EN" method="pen" text="Good bearing" type="straight"/>
        <underline language="EN" method="pen" text="climes vp to the top" type="straight"/>
        <underline language="EN" method="pen" text="Kéepe hop from sunne" type="straight"/>
        <underline language="EN" method="pen" text="and hop is vndunne" type="straight"/>
        <underline language="EN" method="pen" text="A scraper" type="straight"/>
        <underline language="EN" method="pen" text="Of euerie suite" type="straight"/>
        <underline language="EN" method="pen" text="graffe daintie fruite." type="straight"/>
        <underline language="EN" method="pen" text="Graffe good fruite" type="straight"/>
        <underline language="EN" method="pen" text="or graffe not at all." type="straight"/>
        
        <underline language="EN" method="pen" text="Sowe euen thy" type="straight"/>
        <underline language="EN" method="pen" text="with plentifull hand" type="straight"/>
        <underline language="EN" method="pen" text="By sowing in" type="straight"/>
        <underline language="EN" method="pen" text="is little to get" type="straight"/>
        <underline language="EN" method="pen" text="Now rowle" type="straight"/>
        <underline language="EN" method="pen" text="Best searching" type="straight"/>
        <underline language="EN" method="pen" text="the best waie finds" type="straight"/>
        <underline language="EN" method="pen" text="Good tilth brings" type="straight"/>
        <underline language="EN" method="pen" text="euill tilture, wéedes" type="straight"/>
        <underline language="EN" method="pen" text="Yoong plants soone" type="straight"/>
        <underline language="EN" method="pen" text="that growes too drie" type="straight"/>
        
        <mark language="EN" method="pen" name="vertical_bar" place="left_margin" text="Good bearing hop, climes vp to the top."/>
        <mark language="EN" method="pen" name="vertical_bar" place="left_margin" text="Of euerie suite, graffe daintie fruite. Graffe good fruite all, or graffe not at all."/>
        
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Sowe euen thy land, with plentifull hand."/>
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="By sowing in wet, is little to get."/>
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Best searching minds, the best waie finds."/>
        <mark language="EN" method="pen" name="vertical_bar" place="intext" text="Good tilth brings séedes, euill tilture, wéedes."/>
        
    </annotation>
