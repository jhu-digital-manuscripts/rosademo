    <annotation>
        
        <marginalia anchor_text="trim vp their house, and to furnish their pot" hand="English_secretary">
            <language ident="EN">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>rather Houswifery, then Husbandry.</marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <underline language="EN" method="pen" text="sowing and setting, good huswiues delight" type="straight"/>
        <underline language="EN" method="pen" text="trim vp their house, and to furnish their pot" type="straight"/>
        <underline language="EN" method="pen" text="nature of flowers, dame Physick" type="straight"/>
        <underline language="EN" method="pen" text="falling or lieng, full South" type="straight"/>
        <underline language="EN" method="pen" text="southwest" type="straight"/>
        <underline language="EN" method="pen" text="by tillage is lightly" type="straight"/>
        <underline language="EN" method="pen" text="garden with orchard, and hopyard" type="straight"/>
        <underline language="EN" method="pen" text="without cost, be assured of weedes" type="straight"/>
        <underline language="EN" method="pen" text="garden be suer, thou loosest thy seedes" type="straight"/>
        <underline language="EN" method="pen" text="digging, remoouing, and weeding" type="straight"/>
        <underline language="EN" method="pen" text="herbe the more holesome, and greater" type="straight"/>
        <underline language="EN" method="pen" text="thing, or gather, the Moone in" type="straight"/>
        <underline language="EN" method="pen" text="sowe in encreasing, or giue" type="straight"/>
        <underline language="EN" method="pen" text="cunning with dible" type="straight"/>
        <underline language="EN" method="pen" text="rake" type="straight"/>
        <underline language="EN" method="pen" text="mattock" type="straight"/>
        <underline language="EN" method="pen" text="and spade" type="straight"/>
        <underline language="EN" method="pen" text="and by leauell, trim garden is made." type="straight"/>
        <underline language="EN" method="pen" text="soweth too lateward, hath" type="straight"/>
        <underline language="EN" method="pen" text="too soone, little better" type="straight"/>
        <underline language="EN" method="pen" text="the season, so diuers" type="straight"/>
        <underline language="EN" method="pen" text="laier, helpe practise and wit" type="straight"/>
        
        <underline language="EN" method="pen" text="Gardening" type="straight"/>
        <underline language="EN" method="pen" text="To know good land" type="straight"/>
        
