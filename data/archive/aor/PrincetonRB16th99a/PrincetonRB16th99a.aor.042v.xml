<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    
    <page filename="PrincetonRB16th99a.042v.tif" pagination="[42v]" reader="Harvey"/>
    <annotation>
        
        <underline language="EN" method="pen" text="leekes are" type="straight"/>
        <underline language="EN" method="pen" text="spareth the milchcow" type="straight"/>
        <underline language="EN" method="pen" text="both otemell" type="straight"/>
        <underline language="EN" method="pen" text="bread to" type="straight"/>
        <underline language="EN" method="pen" text="haue husbandlie" type="straight"/>
        <underline language="EN" method="pen" text="them, and cram them, till purse doe lack" type="straight"/>
        <underline language="EN" method="pen" text="spoone meate, no bellifull, labourers" type="straight"/>
        <underline language="EN" method="pen" text="Kill crowe, pie and cadow, rooke, buzard and rauen" type="straight"/>
        
        <underline language="EN" method="pen" text="One daie" type="straight"/>
        <underline language="EN" method="pen" text="spred compas ynow" type="straight"/>
        <underline language="EN" method="pen" text="First, bark go and sell" type="straight"/>
        <underline language="EN" method="pen" text="timber ye fell" type="straight"/>
        
        <underline language="EN" method="pen" text="Crab trée" type="straight"/>
        <underline language="EN" method="pen" text="for plough to" type="straight"/>
        <underline language="EN" method="pen" text="Small commons" type="straight"/>
        <underline language="EN" method="pen" text="yéelds cattell ill fare" type="straight"/>
        <underline language="EN" method="pen" text="Some champions" type="straight"/>
        <underline language="EN" method="pen" text="waspe doth with bée" type="straight"/>
        
        <mark language="EN" method="pen" name="bracket" place="left_margin" text="Though neuer so much, a good huswife doth care, that such as doe labour, haue husbandlie fare. Yet feed them, and cram them, till purse doe lack chinke: no spoone meate, no bellifull, labourers thinke."/>
        
        <mark language="EN" method="pen" name="bracket" place="intext" text="Crab trée preserue, for plough to serue."/>
        <mark language="EN" method="pen" name="bracket" place="intext" text="Small commons, &amp; bare, yéelds cattell ill fare."/>
        
        
    </annotation>
</transcription>
