    <page filename="PrincetonRB16th99a.043r.tif" pagination="43[r]" reader="Harvey" signature="M.j."/>
    <annotation>
        
        <underline language="EN" method="pen" text="to the Deuill" type="straight"/>
        <underline language="EN" method="pen" text="shepherd is euill" type="straight"/>
        <underline language="EN" method="pen" text="Man cow" type="straight"/>
        <underline language="EN" method="pen" text="wife dairie guides" type="straight"/>
        
        <underline language="EN" method="pen" text="Slut Cisley vntaught" type="straight"/>
        <underline language="EN" method="pen" text="hath whitemeat naught" type="straight"/>
        <underline language="EN" method="pen" text="Such Mistris, such Nan" type="straight"/>
        <underline language="EN" method="pen" text="such Maister, such Man" type="straight"/>
        
        <underline language="EN" method="pen" text="Sweete April showers" type="straight"/>
        <underline language="EN" method="pen" text="spring Maie flowers" type="straight"/>
        
        <underline language="EN" method="pen" text="April be dripping, then" type="straight"/>
        <underline language="EN" method="pen" text="ye, of meadow for hay" type="straight"/>
        <underline language="EN" method="pen" text="fennes be vndrowned, there cheapest ye may" type="straight"/>
        <underline language="EN" method="pen" text="count best" type="straight"/>
        <underline language="EN" method="pen" text="the best cheape, wheresoeuer" type="straight"/>
        
        <mark language="EN" method="pen" name="vertical_bar" place="right_margin" text="Slut Cisley vntaught, hath whitemeat naught."/>
        <mark language="EN" method="pen" name="bracket" place="right_margin" text="Such Mistris, such Nan, such Maister, such Man."/>
        <mark language="EN" method="pen" name="plus_sign" place="right_margin" text="fennes be vndrowned, there cheapest ye may"/>
        <mark method="pen" name="plus_sign" place="right_margin"/>
        <mark method="pen" name="plus_sign" place="right_margin"/>
        
        <mark language="EN" method="pen" name="bracket" place="intext" text="All goes to the Deuill, where shephers is euill."/>
        <mark language="EN" method="pen" name="bracket" place="intext" text="Man cow prouides, wife dairie guides."/>
        
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="Such Mistris, such Nan"/>
        
    </annotation>
