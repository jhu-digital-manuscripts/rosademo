<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    
    <page filename="PrincetonRB16th99a.047v.tif" pagination="[47v]" reader="Harvey"/>
    <annotation>
        
        <underline language="EN" method="pen" text="twifallow thy land" type="straight"/>
        <underline language="EN" method="pen" text="plow thou with the furst" type="straight"/>
        <underline language="EN" method="pen" text="Twifallow" type="straight"/>
        <underline language="EN" method="pen" text="get tumbrell" type="straight"/>
        <underline language="EN" method="pen" text="compas that fallow, as soone" type="straight"/>
        <underline language="EN" method="pen" text="skilfull bestowit," type="straight"/>
        <underline language="EN" method="pen" text="dripped and shadowd, with bushes" type="straight"/>
        <underline language="EN" method="pen" text="Rare plots full of galles, if ye" type="straight"/>
        <underline language="EN" method="pen" text="to lay to their bones" type="straight"/>
        <underline language="EN" method="pen" text="To gras with" type="straight"/>
        <underline language="EN" method="pen" text="some medow" type="straight"/>
        <underline language="EN" method="pen" text="neither their mothers" type="straight"/>
        <underline language="EN" method="pen" text="Where water is plentie" type="straight"/>
        <underline language="EN" method="pen" text="barth to sit warme" type="straight"/>
        <underline language="EN" method="pen" text="looke well vnto" type="straight"/>
        <underline language="EN" method="pen" text="Pinch neuer thy wennels, of water" type="straight"/>
        <underline language="EN" method="pen" text="Sommer" type="straight"/>
        <underline language="EN" method="pen" text="in frost" type="straight"/>
        <underline language="EN" method="pen" text="lack" type="straight"/>
        <underline language="EN" method="pen" text="drinke" type="straight"/>
        <underline language="EN" method="pen" text="much, ouerlay not" type="straight"/>
        <underline language="EN" method="pen" text="thy cattel, be lustie and" type="straight"/>
        <underline language="EN" method="pen" text="pinch them of pasture" type="straight"/>
        <underline language="EN" method="pen" text="lift at their tailes, er an Winter" type="straight"/>
        
        <underline language="EN" method="pen" text="Twifallowing." type="straight"/>
        <underline language="EN" method="pen" text="Carie out" type="straight"/>
        <underline language="EN" method="pen" text="grasse" type="straight"/>
        <underline language="EN" method="pen" text="Ouerlay" type="straight"/>
        <underline language="EN" method="pen" text="not thy" type="straight"/>
        
        <mark language="EN" method="pen" name="plus_sign" place="left_margin" text="Twifallowing"/>
        <mark language="EN" method="pen" name="plus_sign" place="left_margin" text="Ouerlay"/>
        
        <mark language="EN" method="pen" name="bracket" place="left_margin" text="What wisedome for profit, aduiseth vnto: that husband and huswife, must willingly do."/>
        <mark language="EN" method="pen" name="plus_sign" place="left_margin" text="looke well vnto"/>
        <mark language="EN" method="pen" name="bracket" place="left_margin" text="For coueting much, ouerlay not thy ground, and then shall thy cattel, be lustie and sound."/>
        
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="Pinch neuer thy wennels, of water"/>
        <mark language="EN" method="pen" name="plus_sign" place="intext" text="much, ouerlay not"/>
        
    </annotation>
</transcription>
