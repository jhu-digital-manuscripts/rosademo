        <underline language="EN" method="pen" text="Winter, make" type="straight"/>
        <underline language="EN" method="pen" text="carman and collier, harps" type="straight"/>
        <underline language="EN" method="pen" text="let patch sleepe a snatch," type="straight"/>
        <underline language="EN" method="pen" text="Though sleeping one" type="straight"/>
        <underline language="EN" method="pen" text="not hob growthed," type="straight"/>
        <underline language="EN" method="pen" text="knowledge of stilling" type="straight"/>
        <underline language="EN" method="pen" text="pretie feat" type="straight"/>
        <underline language="EN" method="pen" text="as the gilloflower" type="straight"/>
        <underline language="EN" method="pen" text="ladies and gentils, for whom" type="straight"/>
        <underline language="EN" method="pen" text="oxe fro thy cow" type="straight"/>
        <underline language="EN" method="pen" text="cow be deceiued, by" type="straight"/>
        <underline language="EN" method="pen" text="want of a calfe" type="straight"/>
        <underline language="EN" method="pen" text="and a cow to wax" type="straight"/>
        
        <underline language="EN" method="pen" text="Wash shéep for to share" type="straight"/>
        <underline language="EN" method="pen" text="Though fléese ye" type="straight"/>
        <underline language="EN" method="pen" text="patches make" type="straight"/>
        
        <underline language="EN" method="pen" text="Share lambes no whit" type="straight"/>
        <underline language="EN" method="pen" text="meadow be growne" type="straight"/>
