        <underline language="EN" method="pen" text="barnroome haue" type="straight"/>
        <underline language="EN" method="pen" text="yardroome as" type="straight"/>
        <underline language="EN" method="pen" text="houels" type="straight"/>
        <underline language="EN" method="pen" text="or rikes, they" type="straight"/>
        <underline language="EN" method="pen" text="of breadcorne," type="straight"/>
        <underline language="EN" method="pen" text="to, for mouse and for raine" type="straight"/>
        <underline language="EN" method="pen" text="for turfe and for sedge, for to bake" type="straight"/>
        <underline language="EN" method="pen" text="charcole and sea cole, as" type="straight"/>
        <underline language="EN" method="pen" text="tallwood and billet, as" type="straight"/>
        <underline language="EN" method="pen" text="be fooles" type="straight"/>
        <underline language="EN" method="pen" text="handsome haue storehouse, for trinkets and" type="straight"/>
        <underline language="EN" method="pen" text="all in good order, fast" type="straight"/>
        <underline language="EN" method="pen" text="what euer is needfull, to find by and by" type="straight"/>
        <underline language="EN" method="pen" text="houses and barnes, would" type="straight"/>
        <underline language="EN" method="pen" text="things amended, er haruest come" type="straight"/>
        <underline language="EN" method="pen" text="Things thus set in order, in quiet" type="straight"/>
        <underline language="EN" method="pen" text="shall further thy haruest, and pleasure" type="straight"/>
        <underline language="EN" method="pen" text="bushes and thorne, with the shrubs" type="straight"/>
        <underline language="EN" method="pen" text="woodsere" type="straight"/>
        <underline language="EN" method="pen" text="sommer, cut downe" type="straight"/>
        <underline language="EN" method="pen" text="danger in woodsere, let hacking alone" type="straight"/>
        <underline language="EN" method="pen" text="brembles &amp; brakes" type="straight"/>
        <underline language="EN" method="pen" text="longer now standing" type="straight"/>
        <underline language="EN" method="pen" text="worse to be mowne" type="straight"/>
        
        <underline language="EN" method="pen" text="husbandlie storhouse." type="straight"/>
        
