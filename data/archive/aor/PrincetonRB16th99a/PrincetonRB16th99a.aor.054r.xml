        <underline language="EN" method="pen" text="hawine is" type="straight"/>
        <underline language="EN" method="pen" text="strawe," type="straight"/>
        <underline language="EN" method="pen" text="and rake it, and sets it on" type="straight"/>
        <underline language="EN" method="pen" text="binds it, and sets it on shocks" type="straight"/>
        <underline language="EN" method="pen" text="standing by dallops" type="straight"/>
        <underline language="EN" method="pen" text="early in morning" type="straight"/>
        <underline language="EN" method="pen" text="barlie is raked" type="straight"/>
        <underline language="EN" method="pen" text="of such raking," type="straight"/>
        <underline language="EN" method="pen" text="scatring of barlie, is seene to" type="straight"/>
        <underline language="EN" method="pen" text="conscience" type="straight"/>
        <underline language="EN" method="pen" text="taken in time, is a husbandly" type="straight"/>
        <underline language="EN" method="pen" text="beaten with showers" type="straight"/>
        <underline language="EN" method="pen" text="turning and tossing, they shed as they lie" type="straight"/>
        <underline language="EN" method="pen" text="weather be faire, and tidie thy graine" type="straight"/>
        <underline language="EN" method="pen" text="speedily carrege, for feare of a raine" type="straight"/>
        <underline language="EN" method="pen" text="and showers, deceiueth a menie" type="straight"/>
        <underline language="EN" method="pen" text="lingering lubbers, loose many a penie" type="straight"/>
        
        <underline language="EN" method="pen" text="Tithe of rakings." type="straight"/>
        <underline language="EN" method="pen" text="Lingring" type="straight"/>
        
