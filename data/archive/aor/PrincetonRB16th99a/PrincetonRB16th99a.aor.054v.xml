    <page filename="PrincetonRB16th99a.054v.tif" pagination="[54v]" reader="Harvey"/>
    <annotation>
        
        <underline language="EN" method="pen" text="In gouing at haruest" type="straight"/>
        <underline language="EN" method="pen" text="selfe on a mow" type="straight"/>
        <underline language="EN" method="pen" text="Stack pease" type="straight"/>
        <underline language="EN" method="pen" text="it for rowen, til" type="straight"/>
        <underline language="EN" method="pen" text="haruest time, haruest" type="straight"/>
        <underline language="EN" method="pen" text="make all togither, good cheere" type="straight"/>
        <underline language="EN" method="pen" text="out the black boule, of bleith" type="straight"/>
        <underline language="EN" method="pen" text="them be merie, all haruest" type="straight"/>
        <underline language="EN" method="pen" text="better thou thriuest, the gladder am I" type="straight"/>
        <underline language="EN" method="pen" text="carrie out compas" type="straight"/>
        <underline language="EN" method="pen" text="it on heape, in the" type="straight"/>
        <underline language="EN" method="pen" text="Whose compas is rotten," type="straight"/>
        <underline language="EN" method="pen" text="spred as it should" type="straight"/>
        <underline language="EN" method="pen" text="ladder may" type="straight"/>
        
        <underline language="EN" method="pen" text="Peasestack" type="straight"/>
        <underline language="EN" method="pen" text="gleaning" type="straight"/>
        
        <mark language="EN" method="pen" name="bracket" place="left_margin" text="In haruest time, haruest folke, seruants and all, should make all togither, good cheere in the hall. And fill out the black boule, of bleith to their song: and let them be merie, all haruest time long."/>
        <mark method="pen" name="plus_sign" place="left_margin"/>
        
    </annotation>
