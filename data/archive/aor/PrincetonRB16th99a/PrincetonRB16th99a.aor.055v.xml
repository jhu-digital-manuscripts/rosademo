    <annotation>
        
        <marginalia hand="Italian">
            <language ident="EN">
                <position book_orientation="0" place="head">
                    <marginalia_text>Buiing, &amp; selling. shopping, &amp; shanging.</marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <underline language="EN" method="pen" text="One aker well compassed, passeth some three" type="straight"/>
        <underline language="EN" method="pen" text="barne shall at haruest, declare it to thee" type="straight"/>
        <underline language="EN" method="pen" text="riding" type="straight"/>
        <underline language="EN" method="pen" text="prices of vittels, the yeere" type="straight"/>
        <underline language="EN" method="pen" text="what to be selling," type="straight"/>
        <underline language="EN" method="pen" text="what to be buieng, to bring" type="straight"/>
        <underline language="EN" method="pen" text="Though buieng and selling, doth" type="straight"/>
        <underline language="EN" method="pen" text="chopping and changing, I cannot" type="straight"/>
        <underline language="EN" method="pen" text="theefe and his marrow" type="straight"/>
        <underline language="EN" method="pen" text="rich in his bargaining, needes" type="straight"/>
        <underline language="EN" method="pen" text="buier and seller, full far is he sought" type="straight"/>
        <underline language="EN" method="pen" text="who buieth at first hand" type="straight"/>
        <underline language="EN" method="pen" text="who at the next" type="straight"/>
        <underline language="EN" method="pen" text="At first hand he" type="straight"/>
        <underline language="EN" method="pen" text="paieth all downe" type="straight"/>
        <underline language="EN" method="pen" text="at second, that hath" type="straight"/>
        <underline language="EN" method="pen" text="At third hand he" type="straight"/>
        <underline language="EN" method="pen" text="buieth of trust" type="straight"/>
        <underline language="EN" method="pen" text="his hand who" type="straight"/>
        <underline language="EN" method="pen" text="to buie it the cheaper, haue chinkes in" type="straight"/>
        <underline language="EN" method="pen" text="Touch kept is" type="straight"/>
        <underline language="EN" method="pen" text="is paie and dispatch him, er euer" type="straight"/>
        <underline language="EN" method="pen" text="fortie good crones" type="straight"/>
        
        <underline language="EN" method="pen" text="at first hand" type="straight"/>
        <underline language="EN" method="pen" text="Readie monie" type="straight"/>
        <underline language="EN" method="pen" text="best cheape" type="straight"/>
        
        <mark method="pen" name="plus_sign" place="head"/>
        
        <mark language="EN" method="pen" name="bracket" place="left_margin" text="This lesson is learned, by riding about, the prices of vittels, the yeere thorough out. Both what to be selling, and what to refraine: and what to be buieng, to bring in againe."/>
