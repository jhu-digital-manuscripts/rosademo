    <annotation>
        
        <underline language="EN" method="pen" text="thriftie that teacheth" type="straight"/>
        <underline language="EN" method="pen" text="timelie to trauerse, the thing that thou triue" type="straight"/>
        <underline language="EN" method="pen" text="trustie" type="straight"/>
        <underline language="EN" method="pen" text="to thee" type="straight"/>
        <underline language="EN" method="pen" text="trustily thriftines, trowleth" type="straight"/>
        <underline language="EN" method="pen" text="trauell, to tarie the tide" type="straight"/>
        <underline language="EN" method="pen" text="thankfull thy talent" type="straight"/>
        <underline language="EN" method="pen" text="teacheth, thy time to transpose" type="straight"/>
        <underline language="EN" method="pen" text="twise to thee teached" type="straight"/>
        <underline language="EN" method="pen" text="teach twentie times ten" type="straight"/>
        <underline language="EN" method="pen" text="trade, thou" type="straight"/>
        <underline language="EN" method="pen" text="vnder, sir hobbard de hoy," type="straight"/>
        <underline language="EN" method="pen" text="let lustie, laie wisely to wiue" type="straight"/>
        <underline language="EN" method="pen" text="laie now, or else neuer to thriue" type="straight"/>
        <underline language="EN" method="pen" text="make sure for terme of thy life," type="straight"/>
        <underline language="EN" method="pen" text="saue somewhat for children and wife" type="straight"/>
        <underline language="EN" method="pen" text="Who looseth their youth, shall rue it in age" type="straight"/>
        
        <underline language="EN" method="pen" text="Thee" type="straight"/>
        
