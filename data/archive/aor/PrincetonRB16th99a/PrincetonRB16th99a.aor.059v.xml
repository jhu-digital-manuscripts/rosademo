        <underline language="EN" method="pen" text="homeward, or outward (like" type="straight"/>
        <underline language="EN" method="pen" text="take them, what" type="straight"/>
        <underline language="EN" method="pen" text="hold they togither like burs" type="straight"/>
        <underline language="EN" method="pen" text="For commons, these commoners" type="straight"/>
