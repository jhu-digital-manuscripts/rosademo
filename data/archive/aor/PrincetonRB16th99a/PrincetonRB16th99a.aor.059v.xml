        <underline language="EN" method="pen" text="with thy pease" type="straight"/>
        <underline language="EN" method="pen" text="Now stealeth" type="straight"/>
        <underline language="EN" method="pen" text="he craue" type="straight"/>
        <underline language="EN" method="pen" text="he coosen and cog" type="straight"/>
        <underline language="EN" method="pen" text="The oxboy" type="straight"/>
