        <underline language="EN" method="pen" text="grasse and of corne" type="straight"/>
        <underline language="EN" method="pen" text="neuer so well for to" type="straight"/>
        <underline language="EN" method="pen" text="by night or by daie, he will" type="straight"/>
        <underline language="EN" method="pen" text="orchard vnrobbed" type="straight"/>
