    <annotation>
        <marginalia hand="Italian">
            <language ident="EN">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>This, &amp; the next, two notable sonets, aswel for stile, as matter.</marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <underline language="EN" method="pen" text="poore at enclosing doo grutch" type="straight"/>
        <underline language="EN" method="pen" text="should haue but too much" type="straight"/>
        <underline language="EN" method="pen" text="againe nothing at all" type="straight"/>
        <underline language="EN" method="pen" text="If order might" type="straight"/>
        <underline language="EN" method="pen" text="what were to the feuerall ground" type="straight"/>
        <underline language="EN" method="pen" text="an enuious" type="straight"/>
        <underline language="EN" method="pen" text="naughtie neighbour" type="straight"/>
        <underline language="EN" method="pen" text="cumbersome fetches" type="straight"/>
        <underline language="EN" method="pen" text="friendship like Judas that carried the purse" type="straight"/>
        <underline language="EN" method="pen" text="His head is a storehouse, with quarrels full fraught" type="straight"/>
        <underline language="EN" method="pen" text="memorie pregnant" type="straight"/>
        <underline language="EN" method="pen" text="mouth full of venim, his" type="straight"/>
        <underline language="EN" method="pen" text="lips out of frame" type="straight"/>
        <underline language="EN" method="pen" text="tonge a false witnes" type="straight"/>
        <underline language="EN" method="pen" text="eies be promooters" type="straight"/>
        <underline language="EN" method="pen" text="ears be as spials, alarum to" type="straight"/>
        <underline language="EN" method="pen" text="hands be as tyrants" type="straight"/>
        <underline language="EN" method="pen" text="feete at thine elbow, as serpent" type="straight"/>
        <underline language="EN" method="pen" text="breast full of rancor, like" type="straight"/>
        <underline language="EN" method="pen" text="Canker to freat" type="straight"/>
        <underline language="EN" method="pen" text="hart like a Lion" type="straight"/>
        <underline language="EN" method="pen" text="gate like a sheepebiter, fleering aside" type="straight"/>
        <underline language="EN" method="pen" text="looke like a coxcombe, vp puffed" type="straight"/>
        <underline language="EN" method="pen" text="made of brasse" type="straight"/>
        <underline language="EN" method="pen" text="iesture like Dauus" type="straight"/>
        <underline language="EN" method="pen" text="brag as Thersites, with" type="straight"/>
        <underline language="EN" method="pen" text="in his furie, shall swell" type="straight"/>
        
        <mark language="EN" method="pen" name="quotation_mark" place="left_margin" text="If order might therein be found:"/>
        <mark language="EN" method="pen" name="quotation_mark" place="left_margin" text="what were to the feuerall ground?"/>
