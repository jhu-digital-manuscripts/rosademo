<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    
    <page filename="PrincetonRB16th99a.067v.tif" pagination="[67v]" reader="Harvey"/>
    <annotation>
        
        <underline language="EN" method="pen" text="Good vsage" type="straight"/>
        <underline language="EN" method="pen" text="makes huswife to shine" type="straight"/>
        <underline language="EN" method="pen" text="chance, to be readie at hand" type="straight"/>
        <underline language="EN" method="pen" text="that all thing in season, be huswifelie fed." type="straight"/>
        <underline language="EN" method="pen" text="Dame practise is she" type="straight"/>
        <underline language="EN" method="pen" text="childe to be ciuill, keepe seruant in awe" type="straight"/>
        <underline language="EN" method="pen" text="Who careles" type="straight"/>
        <underline language="EN" method="pen" text="Occasion doe" type="straight"/>
        <underline language="EN" method="pen" text="where, a respect to" type="straight"/>
        <underline language="EN" method="pen" text="of thy life, any slander" type="straight"/>
        <underline language="EN" method="pen" text="doo knowe, though" type="straight"/>
        <underline language="EN" method="pen" text="will abrode, when a mischiefe" type="straight"/>
        <underline language="EN" method="pen" text="loue of they" type="straight"/>
        <underline language="EN" method="pen" text="Use friendly thy" type="straight"/>
        <underline language="EN" method="pen" text="hath thy friendship" type="straight"/>
        <underline language="EN" method="pen" text="Strike nothing" type="straight"/>
        <underline language="EN" method="pen" text="Take heede" type="straight"/>
        <underline language="EN" method="pen" text="by like malice, be bid to like" type="straight"/>
        
        <mark language="EN" method="pen" name="bracket" place="left_margin" text="Haue euerie wehere, a respect to thy waies, that none of thy life, any slander may raies. What many doo knowe, though a time it be hid, at length will abrode, when a mischiefe shall bid."/>
        <mark language="EN" method="pen" name="plus_sign" place="left_margin" text="by like malice, be bid to like"/>
        
    </annotation>
</transcription>
