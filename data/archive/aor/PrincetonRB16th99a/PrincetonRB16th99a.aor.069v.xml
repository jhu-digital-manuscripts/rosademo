    <page filename="PrincetonRB16th99a.069v.tif" pagination="[69v]" reader="Harvey"/>
    <annotation>
        
        <underline language="EN" method="pen" text="eie in a corner, who vseth to" type="straight"/>
        <underline language="EN" method="pen" text="reuealeth a drab, and preuenteth a knaue" type="straight"/>
        <underline language="EN" method="pen" text="Let hollie wand" type="straight"/>
        <underline language="EN" method="pen" text="will clicket, make" type="straight"/>
        <underline language="EN" method="pen" text="hir wicket, seeme easie" type="straight"/>
        <underline language="EN" method="pen" text="mendeth" type="straight"/>
        <underline language="EN" method="pen" text="so naught will be naught" type="straight"/>
        <underline language="EN" method="pen" text="Fight seldome" type="straight"/>
        <underline language="EN" method="pen" text="vse not to" type="straight"/>
        <underline language="EN" method="pen" text="pay home" type="straight"/>
        <underline language="EN" method="chalk" text="pay home when thou fightest" type="straight"/>
        <underline language="EN" method="pen" text="As order" type="straight"/>
        <underline language="EN" method="pen" text="error is hell" type="straight"/>
        <underline language="EN" method="pen" text="better a lawe" type="straight"/>
        <underline language="EN" method="pen" text="Than subiects in awe" type="straight"/>
        <underline language="EN" method="pen" text="lesse of thy counsell" type="straight"/>
        <underline language="EN" method="pen" text="dutie the better, such seruants shall" type="straight"/>
        
        <mark method="pen" name="plus_sign" place="left_margin"/>
        <mark language="EN" method="pen" name="bracket" place="left_margin" text="An eie in a corner, who vseth to haue, reuealeth a drab, and preuenteth a knaue."/>
        <mark language="EN" method="pen" name="plus_sign" place="left_margin" text="so naught will be naught"/>
        <mark method="pen" name="plus_sign" place="left_margin"/>
        <mark language="EN" method="pen" name="bracket" place="left_margin" text="As order is heauenly, where quiet is had, so error is hell, or a mischiefe as bad."/>
        <mark language="EN" method="pen" name="bracket" place="left_margin" text="The lesse of thy counsell, thy seruants doe knowe, Their dutie the better, such seruants shall showe."/>

        <mark language="EN" method="pen" name="plus_sign" place="intext" text="heauenly"/>
        <mark method="pen" name="plus_sign" place="intext"/>
        
    </annotation>
