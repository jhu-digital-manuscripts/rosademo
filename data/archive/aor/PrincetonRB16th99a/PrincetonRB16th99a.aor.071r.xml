    <annotation>
        
        <underline language="EN" method="pen" text="Good droie woorth" type="straight"/>
        <underline language="EN" method="pen" text="Good droie" type="straight"/>
        <underline language="EN" method="pen" text="needfull is truelie, than some in" type="straight"/>
