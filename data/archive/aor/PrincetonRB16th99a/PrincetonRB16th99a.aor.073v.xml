    <annotation>
        
        <underline language="EN" method="pen" text="Where twaine" type="straight"/>
        <underline language="EN" method="pen" text="serued with" type="straight"/>
        <underline language="EN" method="pen" text="knaues in a companie" type="straight"/>
        <underline language="EN" method="pen" text="worser they" type="straight"/>
        <underline language="EN" method="pen" text="lackey to" type="straight"/>
        <underline language="EN" method="pen" text="seruant thy" type="straight"/>
        <underline language="EN" method="pen" text="leaue ianting thy nag" type="straight"/>
        <underline language="EN" method="pen" text="make lackey of" type="straight"/>
        <underline language="EN" method="pen" text="lug in wood" type="straight"/>
        <underline language="EN" method="pen" text="Where pullen vse" type="straight"/>
        <underline language="EN" method="pen" text="to pearch in" type="straight"/>
        <underline language="EN" method="pen" text="two legged foxes, keepe watches" type="straight"/>
        <underline language="EN" method="pen" text="thing at quiet" type="straight"/>
        <underline language="EN" method="pen" text="Take heede it" type="straight"/>
        <underline language="EN" method="pen" text="True pittie is meedefull" type="straight"/>
        <underline language="EN" method="pen" text="clothes in garden" type="straight"/>
        <underline language="EN" method="pen" text="no trinkets" type="straight"/>
        <underline language="EN" method="pen" text="doore leaue vnbolted, for" type="straight"/>
        <underline language="EN" method="pen" text="Vse mirth" type="straight"/>
        <underline language="EN" method="pen" text="good woorde" type="straight"/>
        <underline language="EN" method="pen" text="At bed" type="straight"/>
        <underline language="EN" method="pen" text="and at boorde" type="straight"/>
        <underline language="EN" method="pen" text="make him good" type="straight"/>
        <underline language="EN" method="pen" text="merrie togither, while" type="straight"/>
        <underline language="EN" method="pen" text="At bed" type="straight"/>
        <underline language="EN" method="pen" text="and at boord," type="straight"/>
        <underline language="EN" method="pen" text="euer God sendeth, be" type="straight"/>
        <underline language="EN" method="pen" text="No brawling" type="straight"/>
        <underline language="EN" method="pen" text="No ielousie" type="straight"/>
        <underline language="EN" method="pen" text="taunts before seruants" type="straight"/>
        <underline language="EN" method="pen" text="iarring too loude" type="straight"/>
        
        <mark method="pen" name="plus_sign" place="left_margin"/>
        <mark language="EN" method="pen" name="plus_sign" place="left_margin" text="Make lackey to trudge, Make seruant thy drudge."/>
        <mark language="EN" method="pen" name="plus_sign" place="left_margin" text="two legged foxes, keepe watches"/>
        <mark language="EN" method="pen" name="bracket" place="left_margin" text="No clothes in garden, no trinkets without, no doore leaue vnbolted, for feare of a dout."/>
        <mark method="pen" name="plus_sign" place="left_margin"/>
        <mark language="EN" method="pen" name="bracket" place="left_margin" text="Vse mirth and good woorde, At bed and at boorde."/>
        <mark language="EN" method="pen" name="bracket" place="left_margin" text="At bed and at boord, howsoeuer befall, what euer God sendeth, be merrie withall."/>
        <mark language="EN" method="pen" name="plus_sign" place="left_margin" text="No brawling make, No ielousie take."/>
        
        <mark language="EN" method="pen" name="plus_sign" place="right_margin" text="leaue ianting thy nag"/>
        
    </annotation>
