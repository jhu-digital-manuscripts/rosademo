    <annotation>
        
        <underline language="EN" method="pen" text="As fransie" type="straight"/>
        <underline language="EN" method="pen" text="and heresie roueth" type="straight"/>
        <underline language="EN" method="pen" text="iealousie leadeth" type="straight"/>
        <underline language="EN" method="pen" text="Yong children and chickens" type="straight"/>
        <underline language="EN" method="pen" text="seruant at table" type="straight"/>
        <underline language="EN" method="pen" text="No lurching" type="straight"/>
        <underline language="EN" method="pen" text="snatching" type="straight"/>
        <underline language="EN" method="pen" text="no striuing" type="straight"/>
        <underline language="EN" method="pen" text="what worke in the morning ech seruant" type="straight"/>
        <underline language="EN" method="pen" text="Bandog that" type="straight"/>
        <underline language="EN" method="pen" text="bones and thy scraps" type="straight"/>
        <underline language="EN" method="pen" text="Make keies to be" type="straight"/>
        <underline language="EN" method="pen" text="keies to be keepers" type="straight"/>
        <underline language="EN" method="pen" text="let drousie go" type="straight"/>
        <underline language="EN" method="pen" text="least knaue in the darke to his marrow do creepe" type="straight"/>
        <underline language="EN" method="pen" text="Keepe keies as thy life" type="straight"/>
        <underline language="EN" method="pen" text="Feare candle good" type="straight"/>
        <underline language="EN" method="pen" text="Such keies lay" type="straight"/>
        <underline language="EN" method="pen" text="dairie" type="straight"/>
        <underline language="EN" method="pen" text="of buttrie" type="straight"/>
        <underline language="EN" method="pen" text="cubboord" type="straight"/>
        <underline language="EN" method="pen" text="chest" type="straight"/>
        
        <symbol language="EN" name="SS" place="right_margin" text="Keepe keies as thy life;"/>
        
        <mark language="EN" method="pen" name="plus_sign" place="left_margin" text="Keepe keies as thy life"/>
        <mark language="EN" method="pen" name="bracket" place="left_margin" text="Such keies lay vp safe, ere ye take ye to rest, of dairie, of buttrie, of cubboord and chest."/>
        
        <mark language="EN" method="pen" name="plus_sign" place="right_margin" text="let drousie go"/>
        <mark method="pen" name="plus_sign" place="right_margin"/>
