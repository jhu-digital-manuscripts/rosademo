    <annotation>
        
        <underline language="EN" method="pen" text="tread down the serpents head" type="straight"/>
        <underline language="EN" method="pen" text="attonement" type="straight"/>
        <underline language="EN" method="pen" text="betray to false dissembling Jewes" type="straight"/>
        <underline language="EN" method="pen" text="vnto Pilat being Judge" type="straight"/>
        <underline language="EN" method="pen" text="Condemned and tormented was" type="straight"/>
        <underline language="EN" method="pen" text="pearcing wounds" type="straight"/>
        <underline language="EN" method="pen" text="that Pascall lambe" type="straight"/>
