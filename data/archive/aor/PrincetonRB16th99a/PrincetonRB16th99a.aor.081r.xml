        <underline language="EN" method="pen" text="this did rise" type="straight"/>
        <underline language="EN" method="pen" text="on earth to his elect, times oft in" type="straight"/>
        <underline language="EN" method="pen" text="heaven ascend he" type="straight"/>
        <underline language="EN" method="pen" text="respect vnto his death" type="straight"/>
        <underline language="EN" method="pen" text="founded trump" type="straight"/>
        <underline language="EN" method="pen" text="noise all flesh shall dread" type="straight"/>
        <underline language="EN" method="pen" text="returne with" type="straight"/>
        <underline language="EN" method="pen" text="Come, come, ye good to" type="straight"/>
        <underline language="EN" method="pen" text="Hence, hence to hell, you workers" type="straight"/>
