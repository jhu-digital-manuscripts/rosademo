<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    
    <page filename="PrincetonRB16th99a.088v.tif" pagination="[88v]" reader="Harvey"/>
    <annotation>
        
        <underline language="EN" method="pen" text="The yonger sonne" type="straight"/>
        <underline language="EN" method="pen" text="glad to seeke, from creeke" type="straight"/>
        <underline language="EN" method="pen" text="In world is set, ynough to get" type="straight"/>
        <underline language="EN" method="pen" text="where and whan, that scarsely" type="straight"/>
        <underline language="EN" method="pen" text="By learning some" type="straight"/>
        <underline language="EN" method="pen" text="By ship and plough, some" type="straight"/>
        <underline language="EN" method="pen" text="some so wiue, that trim" type="straight"/>
        <underline language="EN" method="pen" text="Youth hardnes taught" type="straight"/>
        <underline language="EN" method="pen" text="knowlege wraught" type="straight"/>
        <underline language="EN" method="pen" text="do prooue, to shift and shooue" type="straight"/>
        <underline language="EN" method="pen" text="make sawsie" type="straight"/>
        <underline language="EN" method="pen" text="to fetch, a Tiburne" type="straight"/>
        <underline language="EN" method="pen" text="rampish" type="straight"/>
        <underline language="EN" method="pen" text="garment trim" type="straight"/>
        <underline language="EN" method="pen" text="the summe" type="straight"/>
        <underline language="EN" method="pen" text="end it is, that noted is" type="straight"/>
        <underline language="EN" method="pen" text="if it bide, with vertue" type="straight"/>
        <underline language="EN" method="pen" text="deserueth fame" type="straight"/>
        
        <underline language="EN" method="pen" text="Hardnes" type="straight"/>
        <underline language="EN" method="pen" text="worst" type="straight"/>
        <underline language="EN" method="pen" text="Cocking of youth" type="straight"/>
        <underline language="EN" method="pen" text="Not pride" type="straight"/>
        <underline language="EN" method="pen" text="but welth in age" type="straight"/>
        
        <mark language="EN" method="pen" name="plus_sign" place="left_margin" text="By learning some, to riches come,"/>
        <mark language="EN" method="pen" name="plus_sign" place="left_margin" text="The end it is, that noted is,"/>
        
        <mark language="EN" method="pen" name="plus_sign" place="right_margin" text="do prooue, to shift and shooue,"/>
        
        <mark method="chalk" name="vertical_bar" place="intext"/>

    </annotation>
