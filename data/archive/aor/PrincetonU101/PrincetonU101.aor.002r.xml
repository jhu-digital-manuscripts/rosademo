    <annotation>
    	<!-- Marginalia -->
    	<marginalia hand="Italian">
    		<language ident="LA">
    			<position book_orientation="0" place="head">
    				<marginalia_text>
    					Imbelles Dama[e], quid nisi pra[e]da sumus?
    				</marginalia_text>
    				<X-ref book_title="Epigrams" language="LA" person="Martial" text="Imbelles Damae, quid nisi praeda sumus?"/>
    			</position>
    		</language>
    		<translation>
    			"Helpless deer, what are we but prey?"
    		</translation>
    	</marginalia>
    	
    	<underline language="EN" method="pen" text="he feleth himselfe naturally" type="straight"/>
    	<underline language="EN" method="pen" text="aduaunceme[n]t of a Kingdome, or Common" type="straight"/>
    	<underline language="EN" method="pen" text="no one thing to be" type="straight"/>
    	<underline language="EN" method="pen" text="profitable, necessarye, or more honourable" type="straight"/>
    	<underline language="EN" method="pen" text="knowledge of seruice in warre, and déedes of armes;" type="straight"/>
    	<underline language="EN" method="pen" text="ambicion of the worlde" type="straight"/>
    	<underline language="EN" method="pen" text="the defence of the swoorde is not alwayes" type="straight"/>
    	<underline language="EN" method="pen" text="Gréekes, being occupied about trifling matters" type="straight"/>
    	<underline language="EN" method="pen" text="pleasure in resiting of Comedies," type="straight"/>
    	<underline language="EN" method="pen" text="neglecting Martial feates" type="straight"/>
    	<underline language="EN" method="pen" text="to Philip king of Macedonia, father to Alexander" type="straight"/>
    	<underline language="EN" method="pen" text="Great, to oppresse &amp; to bring them in seruitude" type="straight"/>
    	    	
