    			<position book_orientation="0" place="intext">
    				<marginalia_text>
    					[The] hardy Education of [the] antiquity,
    				</marginalia_text>
    			</position>
    			<position book_orientation="0" place="right_margin">
    				<marginalia_text>
    					to make them aswell maruelous passiue, as exeeding actiue. 
    				</marginalia_text>
    			</position>
    		</language>
    	</marginalia>

	<underline language="EN" method="pen" text="antiquitie in harde things, then in the delicate" type="straight"/>
    	<underline language="EN" method="pen" text="much detested the liuing in ease" type="straight"/>
    	<underline language="EN" method="pen" text="a louer of the same hardenesse of" type="straight"/>
    	<underline language="EN" method="pen" text="bée able in person, nor in those of his sonnes" type="straight"/>
    	<underline language="EN" method="pen" text="in the hottest day of Sommer being naked" type="straight"/>
    	<underline language="EN" method="pen" text="himselfe vpon the Sande; or" type="straight"/>
    	<underline language="EN" method="pen" text="Winter in the" type="straight"/>
    	<underline language="EN" method="pen" text="coldest monethes vpon the snowe" type="straight"/>
    	<underline language="EN" method="pen" text="nourishe" type="straight"/>
    	<underline language="EN" method="pen" text="them to sléepe in the open ayre," type="straight"/>
    	<underline language="EN" method="pen" text="feete naked" type="straight"/>
    	<underline language="EN" method="pen" text="washe them selues in the colde" type="straight"/>
    	<underline language="EN" method="pen" text="bée able to abide muche" type="straight"/>
    	<underline language="EN" method="pen" text="make them to loue lesse life" type="straight"/>
    	<underline language="EN" method="pen" text="to feare" type="straight"/>
    	<underline language="EN" method="pen" text="lesse death" type="straight"/>
    	<underline language="EN" method="pen" text="wilde beast" type="straight"/>
    	<underline language="EN" method="pen" text="then as a man" type="straight"/>
    	<underline language="EN" method="pen" text="with peason and beanes" type="straight"/>
    	<underline language="EN" method="pen" text="despise golde" type="straight"/>
    	<underline language="EN" method="pen" text="bée praised of few" type="straight"/>
    	<underline language="EN" method="pen" text="followed of none" type="straight"/>

