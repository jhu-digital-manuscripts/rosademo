    <annotation>
    
	<underline language="EN" method="pen" text="being an art, wherby men" type="straight"/>
    	<underline language="EN" method="pen" text="liue honestly" type="straight"/>
    	<underline language="EN" method="pen" text="of a common weale: or of a Kingdome" type="straight"/>
    	<underline language="EN" method="pen" text="purposing alwayes" type="straight"/>
    	<underline language="EN" method="pen" text="thereby, it is requisite for him to be rauening" type="straight"/>
    	<underline language="EN" method="pen" text="deceyptfull, violente, and to haue manye qualities, the whiche" type="straight"/>
    	<underline language="EN" method="pen" text="of necessitie maketh him not good: nor" type="straight"/>
    	<underline language="EN" method="pen" text="thinke that there is no peace" type="straight"/>
    	<underline language="EN" method="pen" text="preuayle in the time of warre" type="straight"/>
    	<underline language="EN" method="pen" text="bée able to kéepe them selues" type="straight"/>
    	
