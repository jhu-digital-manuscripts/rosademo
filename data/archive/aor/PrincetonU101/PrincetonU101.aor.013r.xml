	<underline language="EN" method="pen" text="kingdomes whiche" type="straight"/>
    	<underline language="EN" method="pen" text="not their absolute Empire vnto their king, sauing" type="straight"/>
    	<underline language="EN" method="pen" text="this place onelye" type="straight"/>
    	<underline language="EN" method="pen" text="quicke deliberation is necessarye" type="straight"/>
    	<underline language="EN" method="pen" text="principall power ought" type="straight"/>
    	<underline language="EN" method="pen" text="to do any thing without counsell" type="straight"/>
    	<underline language="EN" method="pen" text="to be feared, whiche counsell" type="straight"/>
    	<underline language="EN" method="pen" text="in time of peace" type="straight"/>
    	<underline language="EN" method="pen" text="warre, because" type="straight"/>
    	<underline language="EN" method="pen" text="also of a king those ought" type="straight"/>
    	<underline language="EN" method="pen" text="which take the warre for their art" type="straight"/>
    	<underline language="EN" method="pen" text="of armies without" type="straight"/>
    	<underline language="EN" method="pen" text="are the foote men" type="straight"/>
    	<underline language="EN" method="pen" text="men in time of peace maye be content" type="straight"/>
    	<underline language="EN" method="pen" text="owne trades" type="straight"/>
    	<underline language="EN" method="pen" text="he ruinate" type="straight"/>
    	<underline language="EN" method="pen" text="more perillous men, then those, which make the warre" type="straight"/>
    	<underline language="EN" method="pen" text="as their arte" type="straight"/>
    	<underline language="EN" method="pen" text="alwayes to make" type="straight"/>
    	<underline language="EN" method="pen" text="paye them alwaies" type="straight"/>
    	<underline language="EN" method="pen" text="in perill" type="straight"/>
    	<underline language="EN" method="pen" text="they take not from him" type="straight"/>
    	<underline language="EN" method="pen" text="make" type="straight"/>
    	<underline language="EN" method="pen" text="paye them alwaies" type="straight"/>
    	<underline language="EN" method="pen" text="were wise &amp; good" type="straight"/>
    	<underline language="EN" method="pen" text="séeing the time did not varye" type="straight"/>
    	<underline language="EN" method="pen" text="chaunged the men" type="straight"/>
    	<underline language="EN" method="pen" text="in xv. yeres alwayes, they" type="straight"/>
    	<underline language="EN" method="pen" text="renewed them" type="straight"/>
