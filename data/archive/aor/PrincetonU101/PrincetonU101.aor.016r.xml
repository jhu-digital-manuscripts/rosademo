    <annotation>

	<underline language="EN" method="pen" text="those that are" type="straight"/>
    	<underline language="EN" method="pen" text="the worst of a Prouince" type="straight"/>
    	<underline language="EN" method="pen" text="will serue, whose customes ca[n]not" type="straight"/>
    	<underline language="EN" method="pen" text="true &amp; good seruice" type="straight"/>
    	<underline language="EN" method="pen" text="you, so many of suche men, as come" type="straight"/>
    	<underline language="EN" method="pen" text="nomber, that you haue" type="straight"/>
    	<underline language="EN" method="pen" text="them" type="straight"/>
    	<underline language="EN" method="pen" text="matter beyng" type="straight"/>
    	<underline language="EN" method="pen" text="choise is" type="straight"/>
    	<underline language="EN" method="pen" text="haue neede" type="straight"/>
    	<underline language="EN" method="pen" text="chosen men, but hired Souldiours" type="straight"/>
    	<underline language="EN" method="pen" text="disorder the armies of Italie, are" type="straight"/>
    	<underline language="EN" method="pen" text="do not hire any by" type="straight"/>
    	<underline language="EN" method="pen" text="chuse them of their owne subiectes" type="straight"/>
    	<underline language="EN" method="pen" text="the chosen" type="straight"/>
