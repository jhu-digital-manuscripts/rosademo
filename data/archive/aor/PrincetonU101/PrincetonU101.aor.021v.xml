    <annotation>

	<underline language="EN" method="pen" text="mée twentye young men of" type="straight"/>
    	<underline language="EN" method="pen" text="what rule I ought" type="straight"/>
    	<underline language="EN" method="pen" text="lesse errour" type="straight"/>
    	<underline language="EN" method="pen" text="to take them al, to arme them" type="straight"/>
    	<underline language="EN" method="pen" text="exercise them, being not able to knowe" type="straight"/>
    	<underline language="EN" method="pen" text="practising them with exercise" type="straight"/>
    	<underline language="EN" method="pen" text="those of moste spirite, and of moste life: whiche" type="straight"/>
    	<underline language="EN" method="pen" text="case a fewe" type="straight"/>
    	<underline language="EN" method="pen" text="bindeth the[m] only in the ydle dayes, to assemble together" type="straight"/>
    	<underline language="EN" method="pen" text="exercise them" type="straight"/>
    	<underline language="EN" method="pen" text="to pay the lesse" type="straight"/>
    	<underline language="EN" method="pen" text="kéepe them more obedient" type="straight"/>
    	<underline language="EN" method="pen" text="ordinaunce of so few, which may be" type="straight"/>
    	<underline language="EN" method="pen" text="v. thousande men" type="straight"/>
    	
