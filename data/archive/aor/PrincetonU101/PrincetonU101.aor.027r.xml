<page filename="PrincetonU101.027r.tif" pagination="xxiii[r]" reader="Harvey" signature="[Fiii[r]]"/>
    <annotation>

	<underline language="EN" method="pen" text="pain to carry them" type="straight"/>
    	<underline language="EN" method="pen" text="with accustoming the bodye to diseases" type="straight"/>
    	<underline language="EN" method="pen" text="hardening it, to be able" type="straight"/>
    	<underline language="EN" method="pen" text="in thinges accustomed" type="straight"/>
    	<underline language="EN" method="pen" text="suffer no griefe" type="straight"/>
    	<underline language="EN" method="pen" text="co[n]strayned, to fight with footemen" type="straight"/>
    	<underline language="EN" method="pen" text="and with horse" type="straight"/>
    	<underline language="EN" method="pen" text="cannot either sustain the horses" type="straight"/>
    	<underline language="EN" method="pen" text="notwithstanding néede to feare the footemen, which be better armed" type="straight"/>
    	<underline language="EN" method="pen" text="in the Dutchemen" type="straight"/>
    	<underline language="EN" method="pen" text="to ouercome the horses" type="straight"/>
    	<underline language="EN" method="pen" text="disaduantage, when they fight with men" type="straight"/>
    	<underline language="EN" method="pen" text="ordeyned" type="straight"/>
    	<underline language="EN" method="pen" text="armed as" type="straight"/>
    	<underline language="EN" method="pen" text="Romanes could ouercome the men" type="straight"/>
    	<underline language="EN" method="pen" text="the horses" type="straight"/>
    	<underline language="EN" method="pen" text="Dutchemen onely the horses" type="straight"/>
    	<underline language="EN" method="pen" text="the Romane footemen" type="straight"/>
    	<underline language="EN" method="pen" text="innumerable horses" type="straight"/>
    	<underline language="EN" method="pen" text="haue béene ouercome of men on foote" type="straight"/>
    	<underline language="EN" method="pen" text="defaulte that" type="straight"/>
    	<underline language="EN" method="pen" text="vantage that the enemie hath had in" type="straight"/>
    	<underline language="EN" method="pen" text="defaulte" type="straight"/>
    	<underline language="EN" method="pen" text="the one of these twoo thinges" type="straight"/>
    	<underline language="EN" method="pen" text="suche, as shoulde arme them better" type="straight"/>
    	<underline language="EN" method="pen" text="haue taken the straungers maners, and shoulde haue lefte their owne" type="straight"/>

    	<mark language="EN" method="pen" name="hash" place="right_margin" text="accustoming the bodye to diseases"/>
    	<mark language="EN" method="pen" name="plus_sign" place="right_margin" text="thinges accustomed"/>
    	<mark language="EN" method="pen" name="plus_sign" place="right_margin" text="to ouercome the horses"/>
    	<mark method="pen" name="plus_sign" place="right_margin"/>
    	<mark method="pen" name="bracket" place="right_margin"/>
    	<mark language="EN" method="pen" name="plus_sign" place="right_margin" text="the one of these twoo thinges"/>
    	
