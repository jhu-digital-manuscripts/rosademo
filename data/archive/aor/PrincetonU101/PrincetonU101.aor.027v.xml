    <annotation>
    	<!-- Underlines -->
	<underline language="EN" method="pen" text="haue chaunsed" type="straight"/>
    	<underline language="EN" method="pen" text="men on foote prepared" type="straight"/>
    	<underline language="EN" method="pen" text="of the vauntage, which the same" type="straight"/>
    	<underline language="EN" method="pen" text="sixe thousande horse" type="straight"/>
    	<underline language="EN" method="pen" text="footemen, went" type="straight"/>
    	<underline language="EN" method="pen" text="them, he was repulsed with his most great" type="straight"/>
    	<underline language="EN" method="pen" text="as a prudent man, knewe straight" type="straight"/>
    	<underline language="EN" method="pen" text="puissaunce of the enemies weapons" type="straight"/>
    	<underline language="EN" method="pen" text="the horses they" type="straight"/>
    	<underline language="EN" method="pen" text="debilitie" type="straight"/>
    	<underline language="EN" method="pen" text="against those on foote so appointed" type="straight"/>
    	<underline language="EN" method="pen" text="to finde" type="straight"/>
    	<underline language="EN" method="pen" text="them, hee" type="straight"/>
    	<underline language="EN" method="pen" text="men of armes, to alight from their horse, and" type="straight"/>
    	<underline language="EN" method="pen" text="fighting with them, he slue them all" type="straight"/>
    	<underline language="EN" method="pen" text="three thousande: the" type="straight"/>
    	<underline language="EN" method="pen" text="casting their weapons" type="straight"/>
    	<underline language="EN" method="pen" text="grounde, yeelded" type="straight"/>
    	<underline language="EN" method="pen" text="were vnarmed, to defende them" type="straight"/>
    	<underline language="EN" method="pen" text="to offende, the Pike and the swoorde" type="straight"/>
    	<underline language="EN" method="pen" text="these weapons, and with their orders" type="straight"/>
    	<underline language="EN" method="pen" text="bee well armed, to defende" type="straight"/>
    	<underline language="EN" method="pen" text="men of armes of Carminuola" type="straight"/>
    	<underline language="EN" method="pen" text="swoorde, and in their orders to finde" type="straight"/>
    	<underline language="EN" method="pen" text="difficultie, then to come neere to the Suizzers" type="straight"/>
    	<underline language="EN" method="pen" text="maye retche them with the swoorde" type="straight"/>

    	
