    <annotation>
    	<!-- Marginalia -->
    	<marginalia hand="Italian">
    		<language ident="LA">
    			<position book_orientation="0" place="left_margin">
    				<marginalia_text>
    					{ Gallicus Equitatus:
    					Hispanicus peditatus: approbatissimi.
    					In peroratione Principis machia.
    				</marginalia_text>
    				<person name="Machiavelli"/>
    				<book title="Il Principe"/>
    			</position>
    		</language>
    		<translation>
    			French cavalry:
    			Spanish infantry: most approved.
    			In the conclusion of Machiavelli's Prince.
    		</translation>
    	</marginalia>
    	<marginalia hand="Italian">
    		<language ident="EN">
    			<position book_orientation="0" place="tail">
    				<marginalia_text>
    					The best &amp; surest arming of footemen.
    				</marginalia_text>
    			</position>
    		</language>
    	</marginalia>
    	
    	<!-- Underlines -->
	<underline language="EN" method="pen" text="finde, Consaluo, who" type="straight"/>
    	<underline language="EN" method="pen" text="his men of armes" type="straight"/>
    	<underline language="EN" method="pen" text="thousand Dutcheme[n]" type="straight"/>
    	<underline language="EN" method="pen" text="Dutchmen incountered with their Pikes lowe" type="straight"/>
    	<underline language="EN" method="pen" text="opened the power of Spaniardes" type="straight"/>
    	<underline language="EN" method="pen" text="meane of their bucklers and of the agiletie of" type="straight"/>
    	<underline language="EN" method="pen" text="might retche them with the" type="straight"/>
    	<underline language="EN" method="pen" text="the death, almost of all them, and the victorye" type="straight"/>
    	<underline language="EN" method="pen" text="many Dutchmen" type="straight"/>
    	<underline language="EN" method="pen" text="slaine in the bataile of Rauenna" type="straight"/>
    	<underline language="EN" method="pen" text="the Spanish" type="straight"/>
    	<underline language="EN" method="pen" text="them within a swoords length" type="straight"/>
    	<underline language="EN" method="pen" text="destroyed them all" type="straight"/>
    	<underline language="EN" method="pen" text="horsemen" type="straight"/>
    	<underline language="EN" method="pen" text="Dutchemen on foote, had not" type="straight"/>
    	<underline language="EN" method="pen" text="succoured: not" type="straight"/>
    	<underline language="EN" method="pen" text="Spaniardes close together, brought themselues into a safe place" type="straight"/>
    	<underline language="EN" method="pen" text="good power" type="straight"/>
    	<underline language="EN" method="pen" text="withstande the horses" type="straight"/>
    	<underline language="EN" method="pen" text="not to haue" type="straight"/>
    	<underline language="EN" method="pen" text="men on foote" type="straight"/>
    	<underline language="EN" method="pen" text="proceedeth of the armours, and of the order" type="straight"/>
    	<underline language="EN" method="pen" text="Dutchemens weapons" type="straight"/>
    	<underline language="EN" method="pen" text="one halfe, shoulde bee appointed" type="straight"/>
    	<underline language="EN" method="pen" text="other halfe like the Dutchemen" type="straight"/>
    	<underline language="EN" method="pen" text="sixe thousande footemen" type="straight"/>
    	<underline language="EN" method="pen" text="haue three thousande" type="straight"/>
    	<underline language="EN" method="pen" text="Targaettes, after the Romane" type="straight"/>
    	<underline language="EN" method="pen" text="thousande Pikes" type="straight"/>
    	<underline language="EN" method="pen" text="thousande Harkebutters" type="straight"/>
    	<underline language="EN" method="pen" text="Dutche fashion" type="straight"/>
    	<underline language="EN" method="pen" text="place the Pikes, either in the front" type="straight"/>
    	<underline language="EN" method="pen" text="feare most the horses" type="straight"/>
    	<underline language="EN" method="pen" text="the Targaettes and swoordes" type="straight"/>
    	<underline language="EN" method="pen" text="backe to the Pikes, and to winne the battaile" type="straight"/>

    	<underline language="EN" method="pen" text="Hovv to arme men" type="straight"/>
    	<underline language="EN" method="pen" text="vveapons to appoint" type="straight"/>
    	<underline language="EN" method="pen" text="Romaine maner" type="straight"/>
    	<underline language="EN" method="pen" text="Dutche fashion" type="straight"/>
    	
