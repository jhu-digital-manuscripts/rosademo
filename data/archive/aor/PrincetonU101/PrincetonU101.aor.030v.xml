    <annotation>
    	<!-- Underlines -->
	<underline language="EN" method="pen" text="that a hardye man" type="straight"/>
    	<underline language="EN" method="pen" text="vpon a vile horse" type="straight"/>
    	<underline language="EN" method="pen" text="coward vpon a good" type="straight"/>
    	<underline language="EN" method="pen" text="euil matching of stomackes" type="straight"/>
    	<underline language="EN" method="pen" text="that a bande" type="straight"/>
    	<underline language="EN" method="pen" text="footemen, susteyneth all violence of horses" type="straight"/>
    	<underline language="EN" method="pen" text="horse is a beast, that hath sense" type="straight"/>
    	<underline language="EN" method="pen" text="perils, and with an ill will, will" type="straight"/>
    	<underline language="EN" method="pen" text="what force maketh" type="straight"/>
    	<underline language="EN" method="pen" text="be greater, whiche keepeth" type="straight"/>
    	<underline language="EN" method="pen" text="spurre maketh them" type="straight"/>
    	<underline language="EN" method="pen" text="swoorde, or the Pike, keepeth" type="straight"/>
    	<underline language="EN" method="pen" text="bande of footemen to bee most safe" type="straight"/>
    	<underline language="EN" method="pen" text="inuinsible for horses" type="straight"/>
    	<underline language="EN" method="pen" text="heate, with" type="straight"/>
    	<underline language="EN" method="pen" text="furious to incounter, who that woulde" type="straight"/>
    	<underline language="EN" method="pen" text="vpon the poincte of the Pike, either" type="straight"/>
    	<underline language="EN" method="pen" text="them, he will tourne on the right" type="straight"/>
    	<underline language="EN" method="pen" text="horse against a wall: you shall" type="straight"/>
    	<underline language="EN" method="pen" text="euer furie he come withall, will strike against" type="straight"/>
    	<underline language="EN" method="pen" text="to fight with the Suizzers" type="straight"/>
    	<underline language="EN" method="pen" text="alighted, and made euery man alight on foote" type="straight"/>
    	<underline language="EN" method="pen" text="from the arayes, the horses" type="straight"/>
    	<underline language="EN" method="pen" text="flie" type="straight"/>
    	<underline language="EN" method="pen" text="these naturall" type="straight"/>
    	<underline language="EN" method="pen" text="impedimentes, which" type="straight"/>
    	<underline language="EN" method="pen" text="wayes, which haue for horse the moste" type="straight"/>
    	<underline language="EN" method="pen" text="maye bee" type="straight"/>
    	
    	<underline language="EN" method="pen" text="men maie" type="straight"/>
    	<underline language="EN" method="pen" text="themselues from horsemen" type="straight"/>

    	
