    <annotation>
    	<!-- Underlines -->
	<underline language="EN" method="pen" text="first the armies" type="straight"/>
    	<underline language="EN" method="pen" text="neglected" type="straight"/>
    	<underline language="EN" method="pen" text="the orders" type="straight"/>
    	<underline language="EN" method="pen" text="the kingdomes" type="straight"/>
    	<underline language="EN" method="pen" text="matter of exercises, I saye" type="straight"/>
    	<underline language="EN" method="pen" text="make good armies" type="straight"/>
    	<underline language="EN" method="pen" text="hardened the" type="straight"/>
    	<underline language="EN" method="pen" text="strong, swift, and handsome" type="straight"/>
    	<underline language="EN" method="pen" text="stand in the orders" type="straight"/>
    	<underline language="EN" method="pen" text="to obey to signes" type="straight"/>
    	<underline language="EN" method="pen" text="to soundes" type="straight"/>
    	<underline language="EN" method="pen" text="to the voice of the capitayne" type="straight"/>
    	<underline language="EN" method="pen" text="knowe, standing, to retire them" type="straight"/>
    	<underline language="EN" method="pen" text="forwardes, both fighting, &amp; marching to maintayne" type="straight"/>
    	<underline language="EN" method="pen" text="with all serious" type="straight"/>
    	<underline language="EN" method="pen" text="was neuer" type="straight"/>
    	<underline language="EN" method="pen" text="the fierce and disordered men" type="straight"/>
    	<underline language="EN" method="pen" text="much more weaker, then the fearfull that are ordered" type="straight"/>
    	<underline language="EN" method="pen" text="order driueth" type="straight"/>
    	<underline language="EN" method="pen" text="men feare" type="straight"/>
    	<underline language="EN" method="pen" text="disorder abateth" type="straight"/>
    	<underline language="EN" method="pen" text="in the ordering of their men" type="straight"/>
    	<underline language="EN" method="pen" text="a principall" type="straight"/>
    	<underline language="EN" method="pen" text="member, the which though" type="straight"/>
    	<underline language="EN" method="pen" text="litle varied with the nomber" type="straight"/>
    	<underline language="EN" method="pen" text="between sixe &amp; viij." type="straight"/>
    	<underline language="EN" method="pen" text="Legion" type="straight"/>
    	<underline language="EN" method="pen" text="Grekes, a Fallange" type="straight"/>
    	<underline language="EN" method="pen" text="Frencheman, Caterua" type="straight"/>
    	<underline language="EN" method="pen" text="the Suizzers" type="straight"/>
    	<underline language="EN" method="pen" text="warfare, keepe" type="straight"/>
    	<underline language="EN" method="pen" text="the maine battaile" type="straight"/>
    	<underline language="EN" method="pen" text="deuided it, according to their" type="straight"/>
    	<underline language="EN" method="pen" text="name moste knowen" type="straight"/>
    	<underline language="EN" method="pen" text="according to the" type="straight"/>
    	<underline language="EN" method="pen" text="aunciente, &amp; to the orders nowe a daies" type="straight"/>
    	<underline language="EN" method="pen" text="to ordayne" type="straight"/>
    	
    	<underline language="EN" method="pen" text="knovvledge" type="straight"/>

