<page filename="PrincetonU101.035v.tif" pagination="xxxi[v]" reader="Harvey" signature="[Hiii[v]]"/>
    <annotation>
    	<!-- Underlines -->
    	<underline language="EN" method="pen" text="I maye retire" type="straight"/>
    	<underline language="EN" method="pen" text="to succeede in my" type="straight"/>
    	<underline language="EN" method="pen" text="alwayes fight with boldnesse" type="straight"/>
    	<underline language="EN" method="pen" text="my succour at hande" type="straight"/>
    	<underline language="EN" method="pen" text="not be afraide, for" type="straight"/>
    	<underline language="EN" method="pen" text="the same, which may giue the victory to my" type="straight"/>
    	<underline language="EN" method="pen" text="is made of newe" type="straight"/>
    	<underline language="EN" method="pen" text="olde armie is, they be also" type="straight"/>
    	<underline language="EN" method="pen" text="knewe from their infanrie, the order of their armies" type="straight"/>
    	<underline language="EN" method="pen" text="continually did" type="straight"/>
    	<underline language="EN" method="pen" text="continuall exercises" type="straight"/>
    	<underline language="EN" method="pen" text="all the same multitude" type="straight"/>
    	<underline language="EN" method="pen" text="in the day of battaile" type="straight"/>
    	<underline language="EN" method="pen" text="profitable" type="straight"/>
    	<underline language="EN" method="pen" text="because they all knewe, howe to" type="straight"/>
    	<underline language="EN" method="pen" text="the orders, and to fight keeping the same" type="straight"/>
    	<underline language="EN" method="pen" text="armies of newe men" type="straight"/>
    	<underline language="EN" method="pen" text="together, to fighte straighte" type="straight"/>
    	<underline language="EN" method="pen" text="power to fighte, when neede" type="straight"/>
    	<underline language="EN" method="pen" text="without these exercises, as" type="straight"/>
    	<underline language="EN" method="pen" text="is made nothing" type="straight"/>
    	<underline language="EN" method="pen" text="orders being" type="straight"/>
    	<underline language="EN" method="pen" text="with double" type="straight"/>
    	<underline language="EN" method="pen" text="industrie and labour" type="straight"/>
    	<underline language="EN" method="pen" text="shewe them" type="straight"/>
    	<underline language="EN" method="pen" text="Capitaynes haue trauailed, without" type="straight"/>
    	<underline language="EN" method="pen" text="the wayes, with the which" type="straight"/>
    	<underline language="EN" method="pen" text="affection, which I beare to these" type="straight"/>
    	<underline language="EN" method="pen" text="seeing they be not put" type="straight"/>
    		
    	<!-- Marks -->
    	<mark language="EN" method="pen" name="hash" place="left_margin" text="to succeede in my"/>
    	<mark method="pen" name="hash" place="left_margin"/>
