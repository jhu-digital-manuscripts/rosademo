    	<!-- Underlines -->
	<underline language="EN" method="pen" text="all the common weales" type="straight"/>
    	<underline language="EN" method="pen" text="left not anye waye to vertue" type="straight"/>
    	<underline language="EN" method="pen" text="that vertuous men began to be" type="straight"/>
    	<underline language="EN" method="pen" text="the vertue being" type="straight"/>
    	<underline language="EN" method="pen" text="[the] same was corrupted" type="straight"/>
    	<underline language="EN" method="pen" text="the worlde came to be corrupted" type="straight"/>
    	<underline language="EN" method="pen" text="Scithian people" type="straight"/>
    	<underline language="EN" method="pen" text="come to spoile the same" type="straight"/>
    	<underline language="EN" method="pen" text="extinguished the vertue of other" type="straight"/>
    	<underline language="EN" method="pen" text="not howe to maintaine their" type="straight"/>
    	<underline language="EN" method="pen" text="same Empire was deuided into" type="straight"/>
    	<underline language="EN" method="pen" text="vertue is not renued" type="straight"/>
    	<underline language="EN" method="pen" text="againe the orders" type="straight"/>
    	<underline language="EN" method="pen" text="because the maner of liuing" type="straight"/>
    	<underline language="EN" method="pen" text="hauing respecte to the Christian Religion" type="straight"/>
    	<underline language="EN" method="pen" text="not the same necessitie to men, to defende them selues" type="straight"/>
    	<underline language="EN" method="pen" text="whiche in olde times was for that" type="straight"/>
    	<underline language="EN" method="pen" text="the men ouercome in warre, either were killed" type="straight"/>
    	<underline language="EN" method="pen" text="remayned perpetuall slaues, where" type="straight"/>
    	<underline language="EN" method="pen" text="The townes" type="straight"/>
    	<underline language="EN" method="pen" text="rased" type="straight"/>
    	<underline language="EN" method="pen" text="inhabiters" type="straight"/>
    	<underline language="EN" method="pen" text="dispersed" type="straight"/>
    	<underline language="EN" method="pen" text="fewe bee killed, none is kept long in prison" type="straight"/>
    	<underline language="EN" method="pen" text="that with facelitie, they are set at libertie" type="straight"/>
    	<underline language="EN" method="pen" text="Cities also, which a thousand times haue rebelled, are not destroyed, the men whereof, are left a lone" type="straight"/>
    	<underline language="EN" method="pen" text="their gods, so that the greatest hurt that is feared" type="straight"/>
    	<underline language="EN" method="pen" text="but a taske" type="straight"/>
    	<underline language="EN" method="pen" text="muche, that men will not submit" type="straight"/>
    	
