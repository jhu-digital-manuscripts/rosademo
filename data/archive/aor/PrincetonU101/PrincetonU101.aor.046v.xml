    	<!-- Underlines -->
	<underline language="EN" method="pen" text="the Hastati" type="straight"/>
    	<underline language="EN" method="pen" text="retired by a litle, and litle, by the" type="straight"/>
    	<underline language="EN" method="pen" text="together with" type="straight"/>
    	<underline language="EN" method="pen" text="al in the rarenesse of the orders of the" type="straight"/>
    	<underline language="EN" method="pen" text="altogether on a heape began againe the fight" type="straight"/>
    	<underline language="EN" method="pen" text="was no more remedy" type="straight"/>
    	<underline language="EN" method="pen" text="remained no more waies to renue" type="straight"/>
    	<underline language="EN" method="pen" text="The horses stoode on" type="straight"/>
    	<underline language="EN" method="pen" text="likenesse of two winges to a body" type="straight"/>
    	<underline language="EN" method="pen" text="with [the] enemies horses" type="straight"/>
    	<underline language="EN" method="pen" text="they rescued [the] fotemen" type="straight"/>
    	<underline language="EN" method="pen" text="of renuing the[m] selues three times, is almost" type="straight"/>
    	<underline language="EN" method="pen" text="to ouercome" type="straight"/>
    	<underline language="EN" method="pen" text="fortune must three times forsake" type="straight"/>
    	<underline language="EN" method="pen" text="to haue so much strength, that" type="straight"/>
    	<underline language="EN" method="pen" text="times he may ouercome" type="straight"/>
    	<underline language="EN" method="pen" text="not" type="straight"/>
    	<underline language="EN" method="pen" text="maner of renuing the[m] selues" type="straight"/>
    	<underline language="EN" method="pen" text="in rescuing the one the other" type="straight"/>
    	<underline language="EN" method="pen" text="not to retire the one order within the other, as the" type="straight"/>
    	<underline language="EN" method="pen" text="to enter the one man into the place" type="straight"/>
    	<underline language="EN" method="pen" text="foremoste sixe, mighte" type="straight"/>
    	<underline language="EN" method="pen" text="their Launces, the whiche" type="straight"/>
    	<underline language="EN" method="pen" text="Sarisse, were so long" type="straight"/>
    	<underline language="EN" method="pen" text="woundes fell" type="straight"/>
    	<underline language="EN" method="pen" text="the same man" type="straight"/>
    	<underline language="EN" method="pen" text="that was behinde in the seconde ranke, and in" type="straight"/>
    	<underline language="EN" method="pen" text="thirde, and thus" type="straight"/>
    	
