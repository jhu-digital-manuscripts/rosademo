    <annotation>
    	<!-- Underlines -->
	<underline language="EN" method="pen" text="rankes behinde, restored the faultes of those afore" type="straight"/>
    	<underline language="EN" method="pen" text="firste rankes" type="straight"/>
    	<underline language="EN" method="pen" text="consumed the last" type="straight"/>
    	<underline language="EN" method="pen" text="might sooner be consumed" type="straight"/>
    	<underline language="EN" method="pen" text="broken, for that" type="straight"/>
    	<underline language="EN" method="pen" text="the grosse bodye, made it more immooueable" type="straight"/>
    	<underline language="EN" method="pen" text="vsed at the beginning" type="straight"/>
    	<underline language="EN" method="pen" text="the Falangi" type="straight"/>
    	<underline language="EN" method="pen" text="order their Legio[n]s like vnto" type="straight"/>
    	<underline language="EN" method="pen" text="this order pleased them not" type="straight"/>
    	<underline language="EN" method="pen" text="Legions into" type="straight"/>
    	<underline language="EN" method="pen" text="that is, in bandes and companies" type="straight"/>
    	<underline language="EN" method="pen" text="same bodye, should haue" type="straight"/>
    	<underline language="EN" method="pen" text="manye Capitaynes" type="straight"/>
    	<underline language="EN" method="pen" text="made of sundry partes" type="straight"/>
    	<underline language="EN" method="pen" text="by it selfe, might be gouerned" type="straight"/>
    	<underline language="EN" method="pen" text="battailes" type="straight"/>
    	<underline language="EN" method="pen" text="maners of the Falangi, as wel in ordering it grosse" type="straight"/>
    	<underline language="EN" method="pen" text="in rescuing the one the other" type="straight"/>
    	<underline language="EN" method="pen" text="the maine battailes, the one to" type="straight"/>
    	<underline language="EN" method="pen" text="of the other" type="straight"/>
    	<underline language="EN" method="pen" text="[the] the first retiring" type="straight"/>
    	<underline language="EN" method="pen" text="maye bee receyued of [the] second" type="straight"/>
    	<underline language="EN" method="pen" text="to succour the one [the] other" type="straight"/>
    	<underline language="EN" method="pen" text="battaile before" type="straight"/>
    	<underline language="EN" method="pen" text="the same on the" type="straight"/>
    	<underline language="EN" method="pen" text="make forewarde, &amp; succour it" type="straight"/>
    	<underline language="EN" method="pen" text="thirde maine battaile, they" type="straight"/>
    	<underline language="EN" method="pen" text="but distant" type="straight"/>
    	<underline language="EN" method="pen" text="twoo maine battailes being" type="straight"/>
    	<underline language="EN" method="pen" text="that marcheth" type="straight"/>
    	<underline language="EN" method="pen" text="auoide the iustling of the one" type="straight"/>
    	<underline language="EN" method="pen" text="grosse multitude, cannot bee receuyed" type="straight"/>
    	<underline language="EN" method="pen" text="litle bodies being" type="straight"/>
    	
    	<underline language="EN" method="pen" text="Suizzers vse in their maine" type="straight"/>

