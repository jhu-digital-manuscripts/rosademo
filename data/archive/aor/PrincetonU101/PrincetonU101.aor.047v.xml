    	<underline language="EN" method="pen" text="fought with the Grekes Falangi" type="straight"/>
    	<underline language="EN" method="pen" text="were consumed of them" type="straight"/>
    	<underline language="EN" method="pen" text="that the kinde" type="straight"/>
