    <annotation>
    	<!-- Underlines -->
	<underline language="EN" method="pen" text="this inconuenience, put after" type="straight"/>
    	<underline language="EN" method="pen" text="three rankes" type="straight"/>
    	<underline language="EN" method="pen" text="ranke of Halberdes" type="straight"/>
    	<underline language="EN" method="pen" text="make roome to the" type="straight"/>
    	<underline language="EN" method="pen" text="not yet so" type="straight"/>
    	<underline language="EN" method="pen" text="as suffiseth" type="straight"/>
    	<underline language="EN" method="pen" text="our Pikes afore" type="straight"/>
    	<underline language="EN" method="pen" text="Targaettes behinde" type="straight"/>
    	<underline language="EN" method="pen" text="of the fight, they open the" type="straight"/>
    	<underline language="EN" method="pen" text="molest the footemen" type="straight"/>
    	<underline language="EN" method="pen" text="together, and that" type="straight"/>
    	<underline language="EN" method="pen" text="and swoords succeede, which" type="straight"/>
    	<underline language="EN" method="pen" text="ordeyne the armie to fighte the fielde" type="straight"/>
    	<underline language="EN" method="pen" text="in an ordinarye" type="straight"/>
    	<underline language="EN" method="pen" text="Romane" type="straight"/>
    	<underline language="EN" method="pen" text="they call a Consull armie" type="straight"/>
    	<underline language="EN" method="pen" text="then twoo Legions of Romane Citezens" type="straight"/>
    	<underline language="EN" method="pen" text="sixe hundred horse, and about aleuen thousande footemen" type="straight"/>
    	<underline language="EN" method="pen" text="many more footemen" type="straight"/>
    	<underline language="EN" method="pen" text="and horsemen, whiche" type="straight"/>
    	<underline language="EN" method="pen" text="them from" type="straight"/>
    	<underline language="EN" method="pen" text="&amp; considerates" type="straight"/>
    	<underline language="EN" method="pen" text="one, the right horne" type="straight"/>
    	<underline language="EN" method="pen" text="left horne: nor" type="straight"/>
    	<underline language="EN" method="pen" text="these aiding" type="straight"/>
    	<underline language="EN" method="pen" text="no[m]ber of the footemen of their" type="straight"/>
    	<underline language="EN" method="pen" text="that the nomber" type="straight"/>
    	<underline language="EN" method="pen" text="those horse" type="straight"/>
    	<underline language="EN" method="pen" text="be more then theirs" type="straight"/>
    	<underline language="EN" method="pen" text="of xxij. thousand footemen, &amp; about twoo" type="straight"/>
    	<underline language="EN" method="pen" text="good horse, a Consul executed all affaires" type="straight"/>
    	<underline language="EN" method="pen" text="to all enterprises" type="straight"/>
    	<underline language="EN" method="pen" text="against a greater" type="straight"/>
    	<underline language="EN" method="pen" text="twoo Consulles ioyned together" type="straight"/>
    	<underline language="EN" method="pen" text="with twoo armies" type="straight"/>
    	<underline language="EN" method="pen" text="principall actes" type="straight"/>
    	<underline language="EN" method="pen" text="to march" type="straight"/>
    	<underline language="EN" method="pen" text="to incampe" type="straight"/>
    	<underline language="EN" method="pen" text="to fight" type="straight"/>
    	<underline language="EN" method="pen" text="put their Legions in the middest, for" type="straight"/>
    	<underline language="EN" method="pen" text="the same power, wherein they most trusted" type="straight"/>
    	<underline language="EN" method="pen" text="shoulde bee moste vnited" type="straight"/>
    		
    	<underline language="EN" method="pen" text="vvas in a Counsulles armie" type="straight"/>
