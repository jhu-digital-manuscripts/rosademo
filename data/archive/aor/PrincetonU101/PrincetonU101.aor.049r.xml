    	<underline language="EN" method="pen" text="other in the middest" type="straight"/>
    	<underline language="EN" method="pen" text="the last ranke" type="straight"/>
    	<underline language="EN" method="pen" text="a Tergiductore, whome" type="straight"/>
    	<underline language="EN" method="pen" text="the heade of the armie" type="straight"/>
    	<underline language="EN" method="pen" text="Veliti extraordinarye, whiche" type="straight"/>
