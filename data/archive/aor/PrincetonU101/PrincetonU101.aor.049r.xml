    	<underline language="EN" method="pen" text="length CL" type="straight"/>
    	<underline language="EN" method="pen" text="The extraordinarie Pikes, I woulde" type="straight"/>
    	<underline language="EN" method="pen" text="on the lefte" type="straight"/>
    	<underline language="EN" method="pen" text="distaunt from them fifteene yardes" type="straight"/>
    	<underline language="EN" method="pen" text="making Cxliij." type="straight"/>
    	<underline language="EN" method="pen" text="to a ranke" type="straight"/>
    	<underline language="EN" method="pen" text="they may impale with their length, all the left side of the ten battails" type="straight"/>
    	<underline language="EN" method="pen" text="fourtie rankes to keepe the carriages, and the vnarmed, which" type="straight"/>
    	<underline language="EN" method="pen" text="remayne in the taile of" type="straight"/>
    	<underline language="EN" method="pen" text="armie" type="straight"/>
    	<underline language="EN" method="pen" text="Peticaptaynes, and the Centurions" type="straight"/>
