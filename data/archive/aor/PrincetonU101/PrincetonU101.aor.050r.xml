    <annotation>
    	<marginalia anchor_text="The artillerie of the armie" hand="Italian">
    		<language ident="EN">
    			<position book_orientation="0" place="head">
    				<marginalia_text>
    					The Canons, &amp; other Artillery.
    				</marginalia_text>
    			</position>
    		</language>
    	</marginalia>
    	
    	<!-- Underlines -->
	<underline language="EN" method="pen" text="The artillerie of the armie" type="straight"/>
    	<underline language="EN" method="pen" text="suffiseth ten" type="straight"/>
    	<underline language="EN" method="pen" text="Cannons, for the winning" type="straight"/>
    	<underline language="EN" method="pen" text="whose shotte" type="straight"/>
    	<underline language="EN" method="pen" text="passe fiftie pounde" type="straight"/>
    	<underline language="EN" method="pen" text="more for defence of the campe, then" type="straight"/>
    	<underline language="EN" method="pen" text="the battaile" type="straight"/>
    	<underline language="EN" method="pen" text="other artillerie, should" type="straight"/>
    	<underline language="EN" method="pen" text="of ten, then" type="straight"/>
    	<underline language="EN" method="pen" text="fifteene pounde" type="straight"/>
    	<underline language="EN" method="pen" text="afore" type="straight"/>
    	<underline language="EN" method="pen" text="on the front of all the armie, if sometime" type="straight"/>
    	<underline language="EN" method="pen" text="it by the flancke in a sure" type="straight"/>
    	<underline language="EN" method="pen" text="of the enemie be in daunger" type="straight"/>
    	<underline language="EN" method="pen" text="armie thus ordered" type="straight"/>
    	<underline language="EN" method="pen" text="fighting, vse" type="straight"/>
    	<underline language="EN" method="pen" text="the order of" type="straight"/>
    	<underline language="EN" method="pen" text="Falangi" type="straight"/>
    	<underline language="EN" method="pen" text="order of the Romane Legions: for" type="straight"/>
    	<underline language="EN" method="pen" text="the fronte, bee Pikes" type="straight"/>
    	<underline language="EN" method="pen" text="men bee set in" type="straight"/>
    	<underline language="EN" method="pen" text="rankes, after such" type="straight"/>
    	<underline language="EN" method="pen" text="Falangi, restore the firste ranckes, with those" type="straight"/>
    	<underline language="EN" method="pen" text="charged so sore, that they" type="straight"/>
    	<underline language="EN" method="pen" text="constrayned to breake the orders, and to retire themselues" type="straight"/>
    	<underline language="EN" method="pen" text="the voide places of the seconde" type="straight"/>
    	<underline language="EN" method="pen" text="which they haue behinde them, &amp; vnite" type="straight"/>
    	<underline language="EN" method="pen" text="selues with" type="straight"/>
    	<underline language="EN" method="pen" text="making a new force, withstande" type="straight"/>
    	<underline language="EN" method="pen" text="verie same maner, retire" type="straight"/>
    	<underline language="EN" method="pen" text="seconde time, and the third fight" type="straight"/>
    	<underline language="EN" method="pen" text="there is to renue them" type="straight"/>
    	<underline language="EN" method="pen" text="concerning the strength of the armie" type="straight"/>
    	<underline language="EN" method="pen" text="cannot be ordayned a more stronger" type="straight"/>
    	<underline language="EN" method="pen" text="one &amp; the other borne therof" type="straight"/>
    	<underline language="EN" method="pen" text="exceedingly" type="straight"/>
    	<underline language="EN" method="pen" text="replenished, both with heades, and weapons" type="straight"/>
    	<underline language="EN" method="pen" text="remayneth weake, other then the part behinde" type="straight"/>
    	<underline language="EN" method="pen" text="vnarmed, and" type="straight"/>
    	<underline language="EN" method="pen" text="flanckes impaled with the extraordinarie" type="straight"/>
    	<underline language="EN" method="pen" text="anye parte assaulte it" type="straight"/>
    	<underline language="EN" method="pen" text="shall not finde it" type="straight"/>
    	<underline language="EN" method="pen" text="appointed" type="straight"/>
    	<underline language="EN" method="pen" text="the hinder parte can not be assaulted" type="straight"/>
    	<underline language="EN" method="pen" text="bee an enemie, that hath so" type="straight"/>
    		
    	<underline language="EN" method="pen" text="many canons is requisite" type="straight"/>
    	<underline language="EN" method="pen" text="artillerie ought to be" type="straight"/>
    	<underline language="EN" method="pen" text="that vvere ordered" type="straight"/>
    	<underline language="EN" method="pen" text="aboue" type="straight"/>
    	<underline language="EN" method="pen" text="vse the Grekes" type="straight"/>
    	<underline language="EN" method="pen" text="Romane" type="straight"/>
    		
