    <annotation>
    	<!-- Marginalia -->
    	<marginalia anchor_text="The description of a battaile that is a fighting." hand="Italian">
    		<language ident="EN">
    			<position book_orientation="0" place="head">
    				<marginalia_text>
    					A visible, &amp; liuely Description of A most valiant, &amp;
    					victorious Battail.
    				</marginalia_text>
    			</position>
    		</language>
    	</marginalia>
    	<marginalia hand="Italian">
    		<language ident="EN">
    			<position book_orientation="0" place="tail">
    				<marginalia_text>
    					The seueral causes, &amp; reasons of euery Particular,
    					ar expressly declarid in [the] militar Analysis following.
    				</marginalia_text>
    			</position>
    		</language>
    	</marginalia>
    	
    	<!-- Underlines -->
	<underline language="EN" method="pen" text="to kepe the enemie at a baye, and to" type="straight"/>
    	<underline language="EN" method="pen" text="people sayde of him" type="straight"/>
    	<underline language="EN" method="pen" text="not heare the artillery" type="straight"/>
    	<underline language="EN" method="pen" text="Durs haue already" type="straight"/>
    	<underline language="EN" method="pen" text="extraordinarie Veliti, issuing out of" type="straight"/>
    	<underline language="EN" method="pen" text="the lighte horsemen" type="straight"/>
    	<underline language="EN" method="pen" text="moste speedelye" type="straight"/>
    	<underline language="EN" method="pen" text="moste meruailous furie, and greatest crye" type="straight"/>
    	<underline language="EN" method="pen" text="assault the enemie" type="straight"/>
    	<underline language="EN" method="pen" text="whose artillerie" type="straight"/>
    	<underline language="EN" method="pen" text="ouer the heades" type="straight"/>
    	<underline language="EN" method="pen" text="shoote the seconde time, the Veliti, &amp; our horsemen" type="straight"/>
    	<underline language="EN" method="pen" text="nowe gotten it" type="straight"/>
    	<underline language="EN" method="pen" text="for to defende" type="straight"/>
    	<underline language="EN" method="pen" text="neither oure ordinaunce" type="straight"/>
    	<underline language="EN" method="pen" text="the enemies, can anye more do their" type="straight"/>
    	<underline language="EN" method="pen" text="much vertue, strength and agilitie oure" type="straight"/>
    	<underline language="EN" method="pen" text="men fighteth" type="straight"/>
    	<underline language="EN" method="pen" text="exercise, which" type="straight"/>
    	<underline language="EN" method="pen" text="confidence, that they" type="straight"/>
    	<underline language="EN" method="pen" text="marcheth in good order" type="straight"/>
    	<underline language="EN" method="pen" text="to giue the charge" type="straight"/>
    	<underline language="EN" method="pen" text="our artillerie, whiche to" type="straight"/>
    	<underline language="EN" method="pen" text="Capitayn incourageth them" type="straight"/>
    	<underline language="EN" method="pen" text="sheweth them the" type="straight"/>
    	<underline language="EN" method="pen" text="the Veliti and light horsemen" type="straight"/>
    	<underline language="EN" method="pen" text="beholde how the armies be affronted" type="straight"/>
    	<underline language="EN" method="pen" text="with howe muche valiauntnesse" type="straight"/>
    	<underline language="EN" method="pen" text="much silence" type="straight"/>
    	<underline language="EN" method="pen" text="the Captayne co[m]maundeth the men" type="straight"/>
    	<underline language="EN" method="pen" text="[that] they sustayn, &amp; not charge" type="straight"/>
    	<underline language="EN" method="pen" text="howe our lighte" type="straight"/>
    	<underline language="EN" method="pen" text="horsemen be" type="straight"/>
    	<underline language="EN" method="pen" text="giue the charge on" type="straight"/>
    	<underline language="EN" method="pen" text="enemies, Harkebutters" type="straight"/>
    	
    	<underline language="EN" method="pen" text="a battaile that is a fighting" type="straight"/>
    		
