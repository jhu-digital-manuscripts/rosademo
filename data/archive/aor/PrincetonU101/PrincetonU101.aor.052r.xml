    	<underline language="EN" method="pen" text="be retched" type="straight"/>
    	<underline language="EN" method="pen" text="cannot finde a way: to defende the[m]" type="straight"/>
    	<underline language="EN" method="pen" text="some meane" type="straight"/>
    		
    	<underline language="EN" method="pen" text="demau[n]ded, co[n]cerning the shoting" type="straight"/>
    		
