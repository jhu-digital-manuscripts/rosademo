    	<underline language="EN" method="pen" text="enemie" type="straight"/>
    	<underline language="EN" method="pen" text="that to purpose" type="straight"/>
    	<underline language="EN" method="pen" text="a peece of ordinaunce hurte you not, it is" type="straight"/>
    	<underline language="EN" method="pen" text="to stande where it can not retche" type="straight"/>
    	<underline language="EN" method="pen" text="behinde a wall, or behinde a banke" type="straight"/>
    	<underline language="EN" method="pen" text="that can withholde it" type="straight"/>
    	<underline language="EN" method="pen" text="the one and the other be moste strong" type="straight"/>
    	<underline language="EN" method="pen" text="fielde, cannot stande" type="straight"/>
