    	<underline language="EN" method="pen" text="manye dispraise the armours" type="straight"/>
    	<underline language="EN" method="pen" text="orders of the aunciente" type="straight"/>
    	<underline language="EN" method="scoring" text="armies, arguing" type="straight"/>
    	<underline language="EN" method="pen" text="vnprofitable, hauing respect to the furie of the" type="straight"/>
    	<underline language="EN" method="pen" text="artillerie: because, this breaketh the orders, and passeth" type="straight"/>
    	<underline language="EN" method="pen" text="armours in such wise, that" type="straight"/>
    	<underline language="EN" method="pen" text="foolishenesse to make an order, which cannot" type="straight"/>
