    <annotation>
    	<!-- Underlines -->
	<underline language="EN" method="pen" text="those thinges, which can not be withstode, there must bee giuen way, as" type="straight"/>
    	<underline language="EN" method="pen" text="made to the Eliphants" type="straight"/>
    	<underline language="EN" method="pen" text="to the carres full of hookes" type="straight"/>
    	<underline language="EN" method="pen" text="ordered" type="straight"/>
    	<underline language="EN" method="pen" text="wonne a battaile after" type="straight"/>
    	<underline language="EN" method="pen" text="impossible, that an armie thus ordered" type="straight"/>
    	<underline language="EN" method="pen" text="armed, should not ouercome at" type="straight"/>
    	<underline language="EN" method="pen" text="should bee ordayned, as they" type="straight"/>
    	<underline language="EN" method="pen" text="adayes" type="straight"/>
    	<underline language="EN" method="pen" text="but one" type="straight"/>
    	<underline language="EN" method="pen" text="no Targaettes" type="straight"/>
    	<underline language="EN" method="pen" text="vnarmed, that they cannot" type="straight"/>
    	<underline language="EN" method="pen" text="their battailes by flancke" type="straight"/>
    	<underline language="EN" method="pen" text="the armie thinne" type="straight"/>
    	<underline language="EN" method="pen" text="behinde the other, hauing" type="straight"/>
    	<underline language="EN" method="pen" text="they do it confusedly, and" type="straight"/>
    	<underline language="EN" method="pen" text="three names" type="straight"/>
    	<underline language="EN" method="pen" text="into three companies" type="straight"/>
    	<underline language="EN" method="pen" text="vawarde" type="straight"/>
    	<underline language="EN" method="pen" text="battaile" type="straight"/>
    	<underline language="EN" method="pen" text="rerewarde" type="straight"/>
    	<underline language="EN" method="pen" text="to marche" type="straight"/>
    	<underline language="EN" method="pen" text="to distinguish the" type="straight"/>
    	<underline language="EN" method="pen" text="battaile, they binde" type="straight"/>
    	<underline language="EN" method="pen" text="all to the" type="straight"/>
    	<underline language="EN" method="pen" text="brunte, and to the first" type="straight"/>
    	<underline language="EN" method="pen" text="your horsemen were repulced" type="straight"/>
    	<underline language="EN" method="pen" text="they retired to the" type="straight"/>
    	<underline language="EN" method="pen" text="ride of them" type="straight"/>
    	<underline language="EN" method="pen" text="they withstoode, and draue" type="straight"/>
    	<underline language="EN" method="pen" text="maye withstande" type="straight"/>
    	<underline language="EN" method="pen" text="but in a grosse" type="straight"/>
    	<underline language="EN" method="pen" text="the heade" type="straight"/>
    	<underline language="EN" method="pen" text="fiue ranckes of Pikes" type="straight"/>
    	<underline language="EN" method="pen" text="seuen, so that" type="straight"/>
    	
    	<underline language="EN" method="pen" text="rule against" type="straight"/>
    	<underline language="EN" method="pen" text="as cannot be" type="straight"/>
    	
