    	<underline language="EN" method="pen" text="this advertismente" type="straight"/>
    	<underline language="EN" method="pen" text="Souldiour obstinate to fighte" type="straight"/>
    	<underline language="EN" method="pen" text="sende home any of their substaunce" type="straight"/>
    	<underline language="EN" method="pen" text="any place, till the warre" type="straight"/>
    	<underline language="EN" method="pen" text="flying saue their life" type="straight"/>
    	<underline language="EN" method="pen" text="saueth not them their goodes" type="straight"/>
    	<underline language="EN" method="pen" text="the loue whereof" type="straight"/>
    	<underline language="EN" method="pen" text="make men obstinate in" type="straight"/>
