    	<underline language="EN" method="pen" text="them to be vnworthie" type="straight"/>
    	<underline language="EN" method="pen" text="an oration for the" type="straight"/>
    	<underline language="EN" method="pen" text="to" type="straight"/>
    	<underline language="EN" method="pen" text="them of their cowardnesse, and" type="straight"/>
    	<underline language="EN" method="pen" text="them ashamed" type="straight"/>
    	<underline language="EN" method="pen" text="ashamed, to tell them that you will fight alone" type="straight"/>
