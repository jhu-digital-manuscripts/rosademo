    	<underline language="EN" method="pen" text="all the army must be" type="straight"/>
    	<underline language="EN" method="pen" text="heades thereof" type="straight"/>
    	
    	<underline language="EN" method="pen" text="courage soldiours" type="straight"/>
    	<underline language="EN" method="pen" text="make the soldiour most obstinately" type="straight"/>
    	

