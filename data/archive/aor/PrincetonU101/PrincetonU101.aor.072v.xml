<page filename="PrincetonU101.072v.tif" pagination="[lxviii[v]]" reader="Harvey"/>
    <annotation>
    	<!-- Underlines -->
	<underline language="EN" method="pen" text="to be made to" type="straight"/>
    	<underline language="EN" method="pen" text="marching, to obserue all this order" type="straight"/>
    	<underline language="EN" method="pen" text="without" type="straight"/>
    	<underline language="EN" method="pen" text="sure from all the tumultes of the countrie" type="straight"/>
    	<underline language="EN" method="pen" text="other prouision" type="straight"/>
    	<underline language="EN" method="pen" text="the tumultuarye assaultes" type="straight"/>
    	<underline language="EN" method="pen" text="tumultuous people, will come" type="straight"/>
    	<underline language="EN" method="pen" text="of the swoorde, or Pikes poincte" type="straight"/>
    	<underline language="EN" method="pen" text="men out of order, haue feare of those that bee in araye" type="straight"/>
    	<underline language="EN" method="pen" text="with cries and rumours" type="straight"/>
    	<underline language="EN" method="pen" text="make a great assaulte" type="straight"/>
    	<underline language="EN" method="pen" text="like vnto barking curres about" type="straight"/>
    	<underline language="EN" method="pen" text="through all Fraunce, and" type="straight"/>
    	<underline language="EN" method="pen" text="Frenche tumultes, hee tooke small" type="straight"/>
    	<underline language="EN" method="pen" text="haue playners &amp; labourers afore, who" type="straight"/>
    	<underline language="EN" method="pen" text="bee garded of those" type="straight"/>
    	<underline language="EN" method="pen" text="horsemen, that are sent" type="straight"/>
    	<underline language="EN" method="pen" text="viewe the countrie" type="straight"/>
    	<underline language="EN" method="pen" text="order may march" type="straight"/>
    	<underline language="EN" method="pen" text="ten mile the day, and shal" type="straight"/>
    	<underline language="EN" method="pen" text="ynough to incampe" type="straight"/>
    	<underline language="EN" method="pen" text="and suppe before Sunne going downe" type="straight"/>
    	<underline language="EN" method="pen" text="that ordinaryly" type="straight"/>
    	<underline language="EN" method="pen" text="an army may march twentie mile" type="straight"/>
    	<underline language="EN" method="pen" text="set in order" type="straight"/>
    	<underline language="EN" method="pen" text="cannot growe sodainly: for that" type="straight"/>
    	<underline language="EN" method="pen" text="commeth with his pace" type="straight"/>
    	<underline language="EN" method="pen" text="inough, to set thy selfe in" type="straight"/>
    	<underline language="EN" method="pen" text="to fighte" type="straight"/>
    	
    	<underline language="EN" method="pen" text="made plaine" type="straight"/>
    	<underline language="EN" method="pen" text="marche in order" type="straight"/>
    	<underline language="EN" method="pen" text="miles a daie" type="straight"/>
    	<underline language="EN" method="pen" text="marche in battaile raie" type="straight"/>
    	<underline language="EN" method="pen" text="incampe before sun set" type="straight"/>

