    				<marginalia_text>
    					Ironia Militaris.
    				</marginalia_text>
    			</position>
    		</language>
    		<translation>
    			Military irony.
    		</translation>
    	</marginalia>
    	
    	<!-- Underlines -->
	<underline language="EN" method="pen" text="to geue tyme to them to fyght wyth hym" type="straight"/>
    	<underline language="EN" method="pen" text="fained, that he would lodge there" type="straight"/>
    	<underline language="EN" method="pen" text="caused trenches to be made, and" type="straight"/>
    	<underline language="EN" method="pen" text="pauilions to be erected" type="straight"/>
    	<underline language="EN" method="pen" text="certayne horsemen" type="straight"/>
    	<underline language="EN" method="pen" text="countrey for forredge" type="straight"/>
    	<underline language="EN" method="pen" text="that he incamped" type="straight"/>
    	<underline language="EN" method="pen" text="also incamped, and" type="straight"/>
    	<underline language="EN" method="pen" text="partes, to prouyde" type="straight"/>
    	<underline language="EN" method="pen" text="beyng aware, passed the riuer" type="straight"/>
    	<underline language="EN" method="pen" text="hauyng no brydge, haue" type="straight"/>
    	<underline language="EN" method="pen" text="deuyded it, and" type="straight"/>
    	<underline language="EN" method="pen" text="turned behynde theyr backes" type="straight"/>
    	<underline language="EN" method="pen" text="becomynge shallower, wyth" type="straight"/>
    	<underline language="EN" method="pen" text="bee swyft, purposyng" type="straight"/>
    	<underline language="EN" method="pen" text="men to passe" type="straight"/>
    	<underline language="EN" method="pen" text="theyr strongest horses on the higher syde, [that] they may" type="straight"/>
    	<underline language="EN" method="pen" text="parte be lower [that] may succour the men, yf any" type="straight"/>
    	<underline language="EN" method="pen" text="[that] bee verie deepe" type="straight"/>
    	<underline language="EN" method="pen" text="bridges" type="straight"/>
    	<underline language="EN" method="pen" text="botes" type="straight"/>
    	<underline language="EN" method="pen" text="wyth barrelles" type="straight"/>
    	<underline language="EN" method="pen" text="armie wherewyth to be able" type="straight"/>
    	<underline language="EN" method="pen" text="make all these" type="straight"/>
    	<underline language="EN" method="pen" text="standing agaynst" type="straight"/>
    	<underline language="EN" method="pen" text="other banke" type="straight"/>
    	<underline language="EN" method="pen" text="of Cesar" type="straight"/>
    	<underline language="EN" method="pen" text="banke of" type="straight"/>
    	<underline language="EN" method="pen" text="passage beyng" type="straight"/>
    	<underline language="EN" method="pen" text="Vergintorige" type="straight"/>
    	<underline language="EN" method="pen" text="Frenche man, the" type="straight"/>
    	<underline language="EN" method="pen" text="dayes a longe the riuer, and the" type="straight"/>
    	<underline language="EN" method="pen" text="incamping in a woddie place, apte to hide men" type="straight"/>
    	<underline language="EN" method="pen" text="legion three cohortes" type="straight"/>
    	<underline language="EN" method="pen" text="them to tarye in the" type="straight"/>
    	<underline language="EN" method="pen" text="soone as he was departed, they" type="straight"/>
    	<underline language="EN" method="pen" text="shoulde caste ouer a bridge, and should" type="straight"/>
    	
    	<underline language="EN" method="pen" text="policie to passe ouer" type="straight"/>
    	<underline language="EN" method="pen" text="riuer" type="straight"/>
    	<underline language="EN" method="pen" text="vvithout a bridge" type="straight"/>
    	<underline language="EN" method="pen" text="A policie of Cesar to passe a riuer" type="straight"/>

