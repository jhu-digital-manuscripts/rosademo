    <annotation>
    	<!-- Marginalia -->
    	<marginalia hand="Italian">
    		<language ident="LA">
    			<position book_orientation="0" place="right_margin">
    				<marginalia_text>
    					Strategematicè.
    				</marginalia_text>
    			</position>
    		</language>
    		<translation>
    			Strategically.
    		</translation>
    	</marginalia>
    	<marginalia hand="Italian">
    		<language ident="LA">
    			<position book_orientation="0" place="right_margin">
    				<marginalia_text>
    					hypocriticè,
    					et ironicè.
    				</marginalia_text>
    			</position>
    		</language>
    		<translation>
    			Hypocritically and deceptively.
    		</translation>
    	</marginalia>
    	<marginalia hand="Italian">
    		<language ident="LA">
    			<position book_orientation="0" place="tail">
    				<marginalia_text>
    					Tiberij Ca[e]saris perpetua Ironia. aliud semper	
    					pra[e]tendere; aliud intendere. Vulpina Simulatio.
    				</marginalia_text>
    				<person name="Tiberius"/>
    			</position>
    		</language>
    		<translation>
    			The perpetual Deception of Tiberius Caesar. Always claim one thing,
    			intend to do another. Foxlike simulation.
    		</translation>
    	</marginalia>
    	
    	<!-- Underlines -->
	<underline language="EN" method="pen" text="remayning in the fielde, and in" type="straight"/>
    	<underline language="EN" method="pen" text="suspected Townes" type="straight"/>
    	<underline language="EN" method="pen" text="whome it is good to be assured" type="straight"/>
    	<underline language="EN" method="pen" text="enemies to ouercome them" type="straight"/>
    	<underline language="EN" method="pen" text="to this demonstration, and to passe" type="straight"/>
    	<underline language="EN" method="pen" text="difficultie with the same" type="straight"/>
    	<underline language="EN" method="pen" text="men, or many people to dooe a thing, which" type="straight"/>
    	<underline language="EN" method="pen" text="breake downe the wall of their citie" type="straight"/>
    	<underline language="EN" method="pen" text="into exile many of them" type="straight"/>
    	<underline language="EN" method="pen" text="beguile them in suche wise" type="straight"/>
    	<underline language="EN" method="pen" text="one beleue not" type="straight"/>
    	<underline language="EN" method="pen" text="succouring not the" type="straight"/>
    	<underline language="EN" method="pen" text="all to be" type="straight"/>
    	<underline language="EN" method="pen" text="vnto all" type="straight"/>
    	<underline language="EN" method="pen" text="same, which" type="straight"/>
    	<underline language="EN" method="pen" text="in one selfe daye" type="straight"/>
    	<underline language="EN" method="pen" text="beleuing to be alone, to whome" type="straight"/>
    	<underline language="EN" method="pen" text="not to remedie it: and" type="straight"/>
    	<underline language="EN" method="pen" text="withoute tumulte" type="straight"/>
    	<underline language="EN" method="pen" text="commaundement to be of euery man executed" type="straight"/>
    	<underline language="EN" method="pen" text="the fidelitie of" type="straight"/>
    	<underline language="EN" method="pen" text="ouercome them at vnawares" type="straight"/>
    	<underline language="EN" method="pen" text="coolour thy" type="straight"/>
    	<underline language="EN" method="pen" text="better, then to counsell with them of some purpose" type="straight"/>
    	<underline language="EN" method="pen" text="thine, desiring their aide, and to seem to" type="straight"/>
    	<underline language="EN" method="pen" text="other enterprise" type="straight"/>
    	<underline language="EN" method="pen" text="thy minde farre from" type="straight"/>
    	<underline language="EN" method="pen" text="not thincke on their owne defence" type="straight"/>
    	<underline language="EN" method="pen" text="purposest to hurte them" type="straight"/>
    	<underline language="EN" method="pen" text="commoditie, to be able easely to satisfie" type="straight"/>
    	<underline language="EN" method="pen" text="some, that vsed to aduertise thy" type="straight"/>
    	<underline language="EN" method="pen" text="deuises" type="straight"/>
    	<underline language="EN" method="pen" text="then to commen with them of those thinges, that thou wilt not" type="straight"/>
    	<underline language="EN" method="pen" text="that thou wilte doo, to kepe secrete" type="straight"/>
    	
    	<underline language="EN" method="pen" text="me[n] to do such" type="straight"/>
    	<underline language="EN" method="pen" text="thi[n]g as" type="straight"/>
    	<underline language="EN" method="pen" text="&amp; hurtfull" type="straight"/>
    	<underline language="EN" method="pen" text="ouercome me[n] at vnvvares" type="straight"/>
    	<underline language="EN" method="pen" text="doing of such as vse to aduertise" type="straight"/>
    	<underline language="EN" method="pen" text="enemie" type="straight"/>
    	<underline language="EN" method="pen" text="thy proceadinges" type="straight"/>
    	
