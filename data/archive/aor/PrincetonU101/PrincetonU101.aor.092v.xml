    	<!-- Underlines -->
	<underline language="EN" method="pen" text="doubtest, to hide" type="straight"/>
    	<underline language="EN" method="pen" text="some enterprise in hande" type="straight"/>
    	<underline language="EN" method="pen" text="easyly thou" type="straight"/>
    	<underline language="EN" method="pen" text="beguile and oppresse him" type="straight"/>
    	<underline language="EN" method="pen" text="to deminishe thy" type="straight"/>
    	<underline language="EN" method="pen" text="not to deminish" type="straight"/>
    	<underline language="EN" method="pen" text="to maintaine the signes, &amp; the orders" type="straight"/>
    	<underline language="EN" method="pen" text="the very same fires, and" type="straight"/>
    	<underline language="EN" method="pen" text="thou werte ingrossed" type="straight"/>
    	<underline language="EN" method="pen" text="not to increase" type="straight"/>
    	<underline language="EN" method="pen" text="the lodginges" type="straight"/>
    	<underline language="EN" method="pen" text="keeping secrete" type="straight"/>
    	<underline language="EN" method="pen" text="alwaies beene most profitable" type="straight"/>
    	<underline language="EN" method="pen" text="woulde doe" type="straight"/>
    	<underline language="EN" method="pen" text="his sherte knew therof, he" type="straight"/>
    	<underline language="EN" method="pen" text="beeluest thou to" type="straight"/>
    	<underline language="EN" method="pen" text="alone not to here the trumpet" type="straight"/>
    	<underline language="EN" method="pen" text="the secretes of thy enemie" type="straight"/>
    	<underline language="EN" method="pen" text="sende ambassadours, and" type="straight"/>
    	<underline language="EN" method="pen" text="seruauntes apparel, msote expertest" type="straight"/>
    	<underline language="EN" method="pen" text="taken occasion to see the" type="straight"/>
    	<underline language="EN" method="pen" text="consider his strength" type="straight"/>
    	<underline language="EN" method="pen" text="and weakenesse" type="straight"/>
    	<underline language="EN" method="pen" text="opportunitie to ouercome" type="straight"/>
    	<underline language="EN" method="pen" text="into exile one of their familiars" type="straight"/>
    	<underline language="EN" method="pen" text="knowen the deuises of his" type="straight"/>
    	<underline language="EN" method="pen" text="like secretes are" type="straight"/>
    	<underline language="EN" method="pen" text="when for this effecte" type="straight"/>
    	<underline language="EN" method="pen" text="were taken any prisoners" type="straight"/>
    	<underline language="EN" method="pen" text="the fayeth of those Frenchmen" type="straight"/>
    	<underline language="EN" method="pen" text="Lombardie, and were in" type="straight"/>
