    <annotation>
    	<!-- Underlines -->
	<underline language="EN" method="pen" text="power of the Frenchemen, of [the] Tusca[n]s" type="straight"/>
    	<underline language="EN" method="pen" text="Umbries and Sannites" type="straight"/>
    	<underline language="EN" method="pen" text="a fewe men" type="straight"/>
    	<underline language="EN" method="pen" text="noised through all his armye, that" type="straight"/>
    	<underline language="EN" method="pen" text="daye to fighte the fielde with" type="straight"/>
    	<underline language="EN" method="pen" text="that certayne of the prisoners" type="straight"/>
    	<underline language="EN" method="pen" text="afore, had occasion to runne" type="straight"/>
    	<underline language="EN" method="pen" text="taken to fighte" type="straight"/>
    	<underline language="EN" method="pen" text="nexte" type="straight"/>
    	<underline language="EN" method="pen" text="afrayde to deminish" type="straight"/>
    	<underline language="EN" method="pen" text="serueth not" type="straight"/>
    	<underline language="EN" method="pen" text="deuide the force" type="straight"/>
    	<underline language="EN" method="pen" text="but to augmente a mans" type="straight"/>
    	<underline language="EN" method="pen" text="to deuide the enemies" type="straight"/>
    	<underline language="EN" method="pen" text="him to enter into their countrie" type="straight"/>
    	<underline language="EN" method="pen" text="him take many townes" type="straight"/>
    	<underline language="EN" method="pen" text="same garnisons" type="straight"/>
    	<underline language="EN" method="pen" text="might thereby diminishe" type="straight"/>
    	<underline language="EN" method="pen" text="this waye hauing made him" type="straight"/>
    	<underline language="EN" method="pen" text="assaulted and ouercome" type="straight"/>
    	<underline language="EN" method="pen" text="into one prouince" type="straight"/>
    	<underline language="EN" method="pen" text="woulde haue inuaded an other" type="straight"/>
    	<underline language="EN" method="pen" text="diligence, that sodainlye entring" type="straight"/>
    	<underline language="EN" method="pen" text="was not doubted that they woulde" type="straight"/>
    	<underline language="EN" method="pen" text="firste wonne it: before the enemie coulde haue" type="straight"/>
    	<underline language="EN" method="pen" text="not sure, whether" type="straight"/>
    	<underline language="EN" method="pen" text="tourne backe, to the" type="straight"/>
    	<underline language="EN" method="pen" text="not to forsake" type="straight"/>
    	<underline language="EN" method="pen" text="place, to succour the other" type="straight"/>
    	<underline language="EN" method="pen" text="defendeth neither the one" type="straight"/>
    	<underline language="EN" method="pen" text="nor the other" type="straight"/>
    	<underline language="EN" method="pen" text="growe sedition or discorde" type="straight"/>
    	<underline language="EN" method="pen" text="arte howe to extinguishe" type="straight"/>
    	<underline language="EN" method="pen" text="the heades of" type="straight"/>
    	<underline language="EN" method="pen" text="mayest first haue oppressed" type="straight"/>
    	
    	<underline language="EN" method="pen" text="staied his enemies" type="straight"/>
    	<underline language="EN" method="pen" text="to incou[n]ter a legion of" type="straight"/>
    	<underline language="EN" method="pen" text="to deuide his force" type="straight"/>
    	<underline language="EN" method="pen" text="enemies countrie" type="straight"/>
    	<underline language="EN" method="pen" text="he be avvare" type="straight"/>
    	<underline language="EN" method="pen" text="reforme sedicion and" type="straight"/>
    	

