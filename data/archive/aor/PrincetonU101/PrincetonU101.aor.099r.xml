    <annotation>
    	<!-- Marginalia -->
    	<marginalia anchor_text="a citie thus builded, to be altogether inuincible" hand="Italian">
    		<language ident="EN">
    			<position book_orientation="0" place="intext">
    				<marginalia_text>
    					A City altogither inuincible.
    				</marginalia_text>
    			</position>
    		</language>
    	</marginalia>
    	
    	<!-- Underlines -->
	<underline language="EN" method="pen" text="is most difficulte" type="straight"/>
    	<underline language="EN" method="pen" text="the capacitie" type="straight"/>
    	<underline language="EN" method="pen" text="difficultie, that is in" type="straight"/>
    	<underline language="EN" method="pen" text="comming neere it, the wall being strong and concaued" type="straight"/>
    	<underline language="EN" method="pen" text="after to go vp a breache" type="straight"/>
    	<underline language="EN" method="pen" text="ruine, which giueth thee most great" type="straight"/>
    	<underline language="EN" method="pen" text="a citie thus builded, to be altogether inuincible" type="straight"/>
    	<underline language="EN" method="pen" text="ditche within" type="straight"/>
    	<underline language="EN" method="pen" text="also withoute, shoulde" type="straight"/>
    	<underline language="EN" method="pen" text="bee without doubte" type="straight"/>
    	<underline language="EN" method="pen" text="make one ditche" type="straight"/>
    	<underline language="EN" method="pen" text="bettter within then without" type="straight"/>
    	<underline language="EN" method="pen" text="that water shoulde" type="straight"/>
    	<underline language="EN" method="pen" text="herein bee diuers" type="straight"/>
    	<underline language="EN" method="pen" text="ditches full of water" type="straight"/>
    	<underline language="EN" method="pen" text="thee from mines" type="straight"/>
    	<underline language="EN" method="pen" text="the Ditches without water" type="straight"/>
    	<underline language="EN" method="pen" text="more difficulte the filling of them" type="straight"/>
    	<underline language="EN" method="pen" text="them without water" type="straight"/>
    	<underline language="EN" method="pen" text="For ditches with water, haue been" type="straight"/>
    	<underline language="EN" method="pen" text="the Winter to be frosen" type="straight"/>
    	<underline language="EN" method="pen" text="make easy the winning" type="straight"/>
    	<underline language="EN" method="pen" text="to Mirandola, when Pope" type="straight"/>
    	<underline language="EN" method="pen" text="from mines" type="straight"/>
    	<underline language="EN" method="pen" text="make it so deepe, that he" type="straight"/>
    	<underline language="EN" method="pen" text="digge lower, should" type="straight"/>
    	<underline language="EN" method="pen" text="Fortresses also" type="straight"/>
    	<underline language="EN" method="pen" text="haue the like difficultie" type="straight"/>
    	<underline language="EN" method="pen" text="One thing I will earnestly" type="straight"/>
    	<underline language="EN" method="pen" text="defendeth a citie: and that" type="straight"/>
    	<underline language="EN" method="pen" text="that he make no Bulwarkes without" type="straight"/>
    	<underline language="EN" method="pen" text="buildeth the Fortresse" type="straight"/>
    	<underline language="EN" method="pen" text="not anye refuge place in them" type="straight"/>
    	<underline language="EN" method="pen" text="the firste wall being loste, maye retire" type="straight"/>
    	
    	<underline language="EN" method="pen" text="A drie diche is moste" type="straight"/>
    	<underline language="EN" method="pen" text="building &amp; defending of a Tourne" type="straight"/>
    	
