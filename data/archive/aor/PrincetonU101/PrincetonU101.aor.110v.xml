    <annotation>
    	<!-- Marginalia -->
    	<marginalia hand="Italian">
    		<language ident="EN">
    			<position book_orientation="0" place="left_margin">
    				<marginalia_text>
    					What dominion, &amp; autority requisite, to
    					leuy, train, &amp; refourm an Army.
    				</marginalia_text>
    			</position>
    		</language>
    	</marginalia>
    	
    	<!-- Underlines -->
	<underline language="EN" method="pen" text="make good and" type="straight"/>
    	<underline language="EN" method="pen" text="deserue muche more praise" type="straight"/>
    	<underline language="EN" method="pen" text="with old armies" type="straight"/>
    	<underline language="EN" method="pen" text="were Pelopida" type="straight"/>
    	<underline language="EN" method="pen" text="Epaminonda" type="straight"/>
    	<underline language="EN" method="pen" text="Tullus Hostillius" type="straight"/>
    	<underline language="EN" method="pen" text="Philip of" type="straight"/>
    	<underline language="EN" method="pen" text="Cirus king" type="straight"/>
    	<underline language="EN" method="pen" text="Graccus a Romane" type="straight"/>
    	<underline language="EN" method="pen" text="driuen first to" type="straight"/>
    	<underline language="EN" method="pen" text="armies good, and" type="straight"/>
    	<underline language="EN" method="pen" text="fighte with them" type="straight"/>
    	<underline language="EN" method="pen" text="as well through their prudence" type="straight"/>
    	<underline language="EN" method="pen" text="hauing subiectes whome" type="straight"/>
    	<underline language="EN" method="pen" text="otherwise possible" type="straight"/>
    	<underline language="EN" method="pen" text="good and full of all excellencie" type="straight"/>
    	<underline language="EN" method="pen" text="in a strau[n]ge countrie, full of men corrupted, not vsed" type="straight"/>
    	<underline language="EN" method="pen" text="any honest obedience" type="straight"/>
    	<underline language="EN" method="pen" text="brought to passe" type="straight"/>
    	<underline language="EN" method="pen" text="how to gouerne" type="straight"/>
    	<underline language="EN" method="pen" text="to make it, and after" type="straight"/>
    	<underline language="EN" method="pen" text="commaunde" type="straight"/>
    	<underline language="EN" method="pen" text="bee those Princes" type="straight"/>
    	<underline language="EN" method="pen" text="subiectes ynough, may haue" type="straight"/>
    	<underline language="EN" method="pen" text="commaunded, nor" type="straight"/>
    	<underline language="EN" method="pen" text="commaunde, but to armies of straungers" type="straight"/>
    	<underline language="EN" method="pen" text="bounde to other, and not" type="straight"/>
    	<underline language="EN" method="pen" text="possibile, or no, to introduce any" type="straight"/>
    	<underline language="EN" method="pen" text="when coulde I make one" type="straight"/>
    	<underline language="EN" method="pen" text="souldiours" type="straight"/>
    	<underline language="EN" method="pen" text="weare more" type="straight"/>
    	<underline language="EN" method="pen" text="armour then" type="straight"/>
    	<underline language="EN" method="pen" text="beare their owne meate for twoo or three" type="straight"/>
    	<underline language="EN" method="pen" text="mattocke" type="straight"/>
    	<underline language="EN" method="pen" text="them to digge" type="straight"/>
    	<underline language="EN" method="pen" text="many howers armed, in fained" type="straight"/>
    	<underline language="EN" method="pen" text="in the very thing in deede" type="straight"/>
    	<underline language="EN" method="pen" text="they abstayne from playe" type="straight"/>
  
