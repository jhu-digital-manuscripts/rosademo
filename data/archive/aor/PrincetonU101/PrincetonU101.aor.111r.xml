    <annotation>
    	<!-- Marginalia -->
    	<marginalia hand="Italian">
    		<language ident="EN">
    			<position book_orientation="0" place="right_margin">
    				<marginalia_text>
    					[The] inordinate corruption, &amp; dissolutenes, 
    					of ower Sowldiours nowadayes.
    				</marginalia_text>
    			</position>
    		</language>
    	</marginalia>
    	<marginalia hand="Italian">
    		<language ident="EN">
    			<position book_orientation="0" place="right_margin">
    				<marginalia_text>
    					He must haue souerain autority, 
    					&amp; imperium in Belluas, that 
    					shoold reforme so manifould disorders.
    				</marginalia_text>
    			</position>
    		</language>
    	</marginalia>
    	<marginalia hand="Italian">
    		<language ident="EN">
    			<position book_orientation="0" place="right_margin">
    				<marginalia_text>
    					[The] Common blasphemy, &amp; Atheisme, 
    					of common Sowldiours; aswell faithles, 
    					as otherwise orderles. 
    					Howbeit Gain, &amp; Praise, may pra[e]uaile much, 
    					euen with [the] Vilest, and dissolutest Creatures. 
    					especially when [the] Captain shal play his 
    					own part lustely &amp; Valiantly.
    				</marginalia_text>
    			</position>
    		</language>
    	</marginalia>
    	<marginalia hand="Italian"> <!--JG: Link to printed text-->
    		<language ident="EN">
    			<position book_orientation="0" place="tail">
    				<marginalia_text>
    					Suizzers, &amp; Spaniards, better Sowldiours, then Italians.
    				</marginalia_text>
    			</position>
    		</language>
    	</marginalia>
