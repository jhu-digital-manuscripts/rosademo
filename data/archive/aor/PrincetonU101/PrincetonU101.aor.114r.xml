<page filename="PrincetonU101.114r.tif" reader="Harvey" signature="Eii[r]"/>
    <annotation>
    	<!-- Underlines -->
	<underline language="EN" method="pen" text="an ordinary battaile or band of fowre hundred and fiftie men" type="straight"/>
    	<underline language="EN" method="pen" text="is redoubled by" type="straight"/>
    	<underline language="EN" method="pen" text="parte the fiue ranckes of Pike" type="straight"/>
    	<underline language="EN" method="pen" text="the foremoste" type="straight"/>
    	<underline language="EN" method="pen" text="placed beehinde" type="straight"/>
    	<underline language="EN" method="pen" text="come to assaulte them at theyr backes" type="straight"/>
    	<underline language="EN" method="pen" text="to fighte on the flancke" type="straight"/>
    	<underline language="EN" method="pen" text="with twoo hornes" type="straight"/>
    	<underline language="EN" method="pen" text="a voyde place in the middeste" type="straight"/>
    	<underline language="EN" method="pen" text="an armye appoincted to fighte the battaile with the enemies" type="straight"/>
    	<underline language="EN" method="pen" text="the verye same is" type="straight"/>
    	<underline language="EN" method="pen" text="plainelier set foorthe in" type="straight"/>
    	<underline language="EN" method="pen" text="other twoo figures nexte" type="straight"/>
    	<underline language="EN" method="pen" text="forme of a fowre square armie: as in" type="straight"/>
    	<underline language="EN" method="pen" text="a fowresquare fashion" type="straight"/>
    	<underline language="EN" method="pen" text="to the ordinarye forme, to fighte a" type="straight"/>
    	<underline language="EN" method="pen" text="manner of incamping" type="straight"/>

    	<!-- Marks -->
    	<mark language="EN" method="pen" name="plus_sign" place="intext" text="theyr backes"/>
