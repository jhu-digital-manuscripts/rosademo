        <marginalia anchor_text="" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Argume[n]tu[m] p[er]accommodum.</marginalia_text>
                </position>
            </language>
            <translation>Very suitable argument</translation>
        </marginalia>
        
        <marginalia anchor_text="Ponentur tame[n] omnia co[m]munia, deinde ex personaru[m] co[m]paratione, &amp; indicij ordine, &amp; silentio              repudiatae argumenta ducentur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Exemplu[m]</marginalia_text>
                </position>
            </language>
            <translation>Example</translation>
        </marginalia>
        
        <marginalia anchor_text="pars expostionis, pro nobis, pars contra nos" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Cu[m] pars pro nobis pars co[n]tra nos.</marginalia_text>
                </position>
            </language>
            <translation>Because part for us and part against us.</translation>
        </marginalia>
        
        <marginalia anchor_text="si plura sint quae noce[n]t" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Si plura noce[n]t.</marginalia_text>
                </position>
            </language>
            <translation>If many are hamful.</translation>
        </marginalia>
        
        <marginalia anchor_text="si plura proderunt" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Si plura proderu[n]t</marginalia_text>
                </position>
            </language>
            <translation>If many are beneficial.</translation>
        </marginalia>
        
        <marginalia anchor_text="q[uam] excursion: nec unquam debeit esse nisi breuis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Nulla excursio esset in narratione</marginalia_text>
                </position>
            </language>
            <translation>No digression in the narrative.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>NARRATIO</marginalia_text>
                </position>
            </language>
            <translation>Narrative.</translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="Ponentur tame[n] omnia co[m]munia, deinde ex personaru[m] co[m]paratione, &amp; indicij ordine, &amp; silentio              repudiatae argumenta ducentur. Ne" type="straight"/>
        <underline language="LA" method="pen" text="Quod si pars expositionis, pro nobis, pars contra nos erit, miscenda sit, an separa[n]da" type="straight"/>
        <underline language="LA" method="pen" text="quia nisi distinxerimus, uerendum est, ne bona nostra permixitis malis inquinentur. Illa" type="straight"/>
        <underline language="LA" method="pen" text="alienae personae" type="straight"/>
        
