    <annotation>
        
        <marginalia anchor_text="propter quae partitione non semper est utendum." hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Quapropter no[n] semp[er] partitione utendu[m] est</marginalia_text>
                </position>
            </language>
            <translation>Why a partition is not always used.</translation>
        </marginalia>
        
        <marginalia anchor_text="mouendum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Movere.</marginalia_text>
                </position>
            </language>
            <translation>To move.</translation>
        </marginalia>
        
        <marginalia anchor_text="iucunda partitio est: ut quid quaq[ue] de re dicturi sumus, ordine appareat. At si unum crimen uarie defenderimus, superuacua" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Si vnu[m] crime[n] defe[n]derimus sup[er]vacua partitio est.</marginalia_text> 
                </position>
            </language>
            <translation>If we defend one crime, a parition is superfluous.</translation> 
        </marginalia>
        
        <underline language="LA" method="pen" text="Non enim solum oratoris est docere, sed plus eloquentia circa mouendum ualet" type="straight"/>
        <underline language="LA" method="pen" text="trans mare fuisse" type="straight"/>
        
