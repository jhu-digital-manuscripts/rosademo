        <marginalia anchor_text="" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Qatuor pra[e]dicabilia finitio[n]i subiecta.</marginalia_text>
                </position>
            </language>
            <translation>The four things that can be predicated on a subject definition.</translation> <!-- M.B. Not sure of praedicabilia and the phrasing of this translation. -->
        </marginalia>
        
        <marginalia anchor_text="" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>A genere ad spe[cie]m affirmatiue no[n] valet sed negatiue</marginalia_text>
                </position>
            </language>
            <translation>From genus to species is affirmative but non valid when negative.</translation> <!-- M.B. Not sure of this translation? --> <!-- FSF: I can't think of a better way to be honest -->
        </marginalia>
        
        <marginalia anchor_text="species firman probationem habet generis, infirmam refutationem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Spe[cie]s firma[m] probatione[m], infirma[m] refutationem habet</marginalia_text>
                </position>
            </language>
            <translation>Species offer stong proof but a weak refutation.</translation>
        </marginalia>
        
        <marginalia anchor_text="Praeterea finimus aut ui, sicut superiora, aut ἐτυμoλoγίᾳ," hand="Italian"> <!-- M.B. Translate Greek marginalia later. -->
