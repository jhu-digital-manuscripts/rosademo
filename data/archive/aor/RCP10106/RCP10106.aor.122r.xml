        <underline language="LA" method="pen" text="Itaq[ue] in hoc genere" type="straight"/>
        <underline language="LA" method="pen" text="ut apud Aeschinem Socraticu[m] male respondit Aspasia Xenophontis uxor, quod Cicero his uerbis transfert: Dic mihi quaeso Xenophontis uxor, si uicina" type="straight"/>
        <underline language="LA" method="pen" text="male enim responderat, se malle alienum aurum" type="straight"/>
        <underline language="LA" method="pen" text="esse simile minus" type="straight"/>
        <underline language="LA" method="pen" text="aliquid plus" type="straight"/>
        <underline language="LA" method="pen" text="Et dißimilibus inesse simile" type="straight"/>
        <underline language="LA" method="pen" text="similibus dißimile" type="straight"/>
        <underline language="LA" method="pen" text="argume[n]ta duci ex iure simili, ut" type="straight"/>
        
        <drawing book_orientation="0" method="pen" place="right_margin" type="florilegium"/>
        
