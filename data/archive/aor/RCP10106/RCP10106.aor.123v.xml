        <underline language="LA" method="pen" text="si non possumt ualere quia magna non sunt, ualebu[n]t quia multa sunt, quae" type="straight"/>
        <underline language="LA" method="pen" text="etiamsi non ut fulmine, tamen ut grandine" type="straight"/>
        <underline language="LA" method="pen" text="si no[n] nudos &amp; uelut carne spoliatos artus ostenderint" type="straight"/>

