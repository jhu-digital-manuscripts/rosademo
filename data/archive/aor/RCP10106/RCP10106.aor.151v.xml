                    <marginalia_text>Pra[e]conis Vox.</marginalia_text>
                </position>
            </language>
            <translation>The voice of the herald.</translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="uirtute dici potest, paulùm impendamus" type="straight"/>
