    <annotation>
        
        <marginalia anchor_text="neque perfectus orator sine hac uirtute dici potest" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Perfectus orator sine altercatione eße no[n] p[otes]t</marginalia_text>
