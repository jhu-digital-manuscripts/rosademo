    <page filename="RCP10106.152v.tif" pagination="304" reader="John Dee"/> 
    <annotation>
        
        <marginalia anchor_text="Nam culpam, praesertim deprehe[n]sam, pertinaciter tueri, culpa altera est." hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>Culpa[m], presertim deprehensam, p[er]tinaciter tueri, culpa altera est</marginalia_text>
                </position>
            </language>
            <translation>To defend a crime, obstinately, especially after being caught, amounts to a second crime.</translation>
        </marginalia>
        
        <marginalia anchor_text="Illud uix saltem praecipiendum uidetur, ne turdibus &amp; clamosus tantu[m] sit altercator, &amp; quales facti sunt, wui literas nesciu[n]t." hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Infantis</marginalia_text>
                </position>
            </language>
            <translation>Inarticulate.</translation>
        </marginalia>
        
        <marginalia anchor_text="improbatis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Improbitas</marginalia_text>
                </position>
            </language>
            <translation>Dishonesty.</translation>
        </marginalia>
        
        <marginalia anchor_text="iudex" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Iudex respici[en]dus</marginalia_text>
                </position>
            </language>
            <translation>The judge is to be respected.</translation>
        </marginalia>
        
        <marginalia anchor_text="Faciunt hoc medici quoq[ue] ut remedia perinde perseuerent adhibere, uel definant, ut illa recipi uel respui uident." hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>A simili</marginalia_text>
                </position>
            </language>
            <translation>A resemblance.</translation>
        </marginalia>
        
