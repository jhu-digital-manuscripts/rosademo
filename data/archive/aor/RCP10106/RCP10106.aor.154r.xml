        <marginalia anchor_text="Sed ut opera extruentibus satis non est, saxa, atq[ue] materia[m], &amp; caetera aedificanti utilia congere, nisi dispone[n]dis eis collocandisq[ue] artificum              manus adhibeaturL suc in dicendo quamlibet abundans rerum copia, cumulu[m] tantum habeat, atq[ue] congestum, nisi illas eade[m] dispositio in ordine[m[ digestas, atq[ue] inter se co[m]mißas              deuinxerit." hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Similie</marginalia_text>
                </position>
            </language>
            <translation>Similar</translation>
        </marginalia>
        
        <marginalia anchor_text="eade[m] dispositio in ordine[m] digestas, atq[ue] inter se co[m]mißas deuinxerit. Nec immeritò secunda quinque partium posita est, cu[m] sube ea prior              nihil colloce[n]tur." hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Dispositio iure secundum locu[m] obtinet.</marginalia_text>
                </position>
            </language>
            <translation>Disposition is the law that takes second place.</translation>
        </marginalia>
        
        <marginalia anchor_text="Neq[ue] enim, quanquàm fusis omnibus membris, statua sit, nisi colloce[n]tur. Et siquam in corporibus nostris, aliorum' ue animalium parte[s] permutes atq[ue]              transferas, licet habeat eadem omnia, prodigium sit tamen." hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Similitudines.</marginalia_text>
                </position>
            </language>
            <translation>Similarities.</translation>
        </marginalia>
        
        <marginalia anchor_text="qui ipsam rerum natura[m] stare ordine putant" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Ip[s]a Natura ordine stat</marginalia_text>
                </position>
            </language>
            <translation>Nature itself depends on order to remain.</translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="modò in orando, sed in omni uita prius consilio: frustra'q[ue] sine eo tradi caeteras artes, plus'q[ue] uel sine doctrina prudentiam,              quàm sine prudentia facere doctrinam. Apparet" type="straight"/>
        <underline language="LA" method="pen" text="copia, cumulu[m] tantum habeat, atq[ue] congestum" type="straight"/>
