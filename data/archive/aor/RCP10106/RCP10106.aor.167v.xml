         <!-- M.B. Greek marginalia to be captured later. -->
        
        <marginalia anchor_text="Lis est omnis aut de praemio, aut de poena, aut de qua[n]titate" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>Lis omnis aut de pra[e]mio aut et poena aut de quantiate</marginalia_text>
                </position>
            </language>
            <translation>Every dispute involves a reward, or a penalty or the amount of these.</translation>
        </marginalia>
        
        <marginalia anchor_text="Est autem qualitas alia de summo genere, atq[ue] ea quide[m] non simplex." hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Qualitatis Diuisio</marginalia_text>
                </position>
            </language>
            <translation>The division of quality.</translation>
        </marginalia>
        
        <marginalia anchor_text="suasorijs" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Suasoria[e]</marginalia_text>
                </position>
            </language>
            <translation>Persuasive.</translation>
        </marginalia>
        
        <marginalia anchor_text="ac non facie[n]da, appetenda, uitanda" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Facie[n]da &amp; no[n] appete[n]da, vita[n]da</marginalia_text>
                </position>
            </language>
            <translation>Should be sought or avoided.</translation>
        </marginalia>
        
        <marginalia anchor_text="Demo[n]stratiuae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>demonstratiua[e]</marginalia_text>
                </position>
            </language>
            <translation>Demonstrative.</translation>
        </marginalia>
        
        <marginalia anchor_text="primu[m] genus causae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Primu[m] genus caus[a]e</marginalia_text>
                </position>
            </language>
            <translation>First type of causes.</translation>
        </marginalia>
        
        <marginalia anchor_text="secundum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>secundu[m]</marginalia_text>
                </position>
            </language>
            <translation>Second.</translation>
        </marginalia>
        
        <marginalia anchor_text="κατ’ ἀντίληψιν" hand="Italian">
            <language ident="EL">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>κατ’ ἀντίληψιν</marginalia_text>
                </position>
            </language>
            <translation>By objection.</translation>
        </marginalia>
        
        <marginalia anchor_text="Iustum omne continetur natura, uel co[n]stitutione" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Iustu[m] o[mn]e Natura co[n]stitutione</marginalia_text>
                </position>
            </language>
            <translation>All justice [depends on] nature [or] convention.</translation>
        </marginalia>
        
