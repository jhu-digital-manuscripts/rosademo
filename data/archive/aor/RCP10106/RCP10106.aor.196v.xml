        <underline language="LA" method="pen" text="appositum" type="straight"/>
        <underline language="LA" method="pen" text="sequens" type="straight"/>
        <underline language="LA" method="pen" text="Namq[ue] illis satis est co[n]uenire uerbo, cui apponitur" type="straight"/>
        <underline language="LA" method="pen" text="Apud oratore[m], nisi aliquid efficitur, redundat. Tum autem efficitur" type="straight"/>
        <underline language="LA" method="pen" text="O' scelus abomina[n]dum, O' deformem libidinem" type="straight"/>
        <underline language="LA" method="pen" text="nuda sit, &amp; uelut incompta oratio" type="straight"/>
        <underline language="LA" method="pen" text="enim semper ut id, quod ist appositum, si à proprio diuiseris, per se significet, &amp; faciat antonomasiam" type="straight"/>
        <underline language="LA" method="pen" text="quam inuersionem" type="straight"/>
        <underline language="LA" method="pen" text="etiam interim" type="straight"/>
        <underline language="LA" method="pen" text="Totus'q[ue] etiam ille Horatij locus, quo nauim pro republica, fluctuum tempestates pro bellis ciuilibus, portum pro pace" type="straight"/>
        
