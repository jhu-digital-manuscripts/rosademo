        <underline language="LA" method="pen" text="ita" type="straight"/>
        <underline language="LA" method="pen" text="satisdare iuberet" type="straight"/>
        <underline language="LA" method="pen" text="Decernit. quam aequu[m], nihil dico: vnu[m] hoc dico, nouum: &amp; hoc ipsum tacuisse mallem" type="straight"/>
        <underline language="LA" method="pen" text="sponsionem cum Sex. Neuio facere" type="straight"/>
        <underline language="LA" method="pen" text="no[n] necesse esse famam alterius in iudiciu[m] venire" type="straight"/>
        <underline language="LA" method="pen" text="sponsionem porrò se istiusmodi faceret" type="straight"/>
        <underline language="LA" method="pen" text="solent homine nobiles" type="straight"/>
        <underline language="LA" method="pen" text="seu recte seu perperam facere coeperunt, ita in vtroque excellunt" type="straight"/>
        <underline language="LA" method="pen" text="vt nemo nostro loco natus assequi posset" type="straight"/>
        <underline language="LA" method="pen" text="aut sponsionem iubet facere" type="straight"/>
        <underline language="LA" method="pen" text="Conturbatus sanè discedit" type="straight"/>
        <underline language="LA" method="pen" text="optio ta[m] misera támque iniqua daretur" type="straight"/>
        <underline language="LA" method="pen" text="se capitis damnaret" type="straight"/>
        <underline language="LA" method="pen" text="si sponsione[m] fecisset" type="straight"/>
        <underline language="LA" method="pen" text="spes esset" type="straight"/>
        <underline language="LA" method="pen" text="in hoc summa iudicii causáque tota consistit" type="straight"/>
        <underline language="LA" method="pen" text="Cum maiores ita co[n]stituerint, vt qui pro capite diceret, is posteriore loco diceret" type="straight"/>
        <underline language="LA" method="pen" text="ea ingenia co[n]uerti ad perniciem, quae antea versabantur in salute atque uxilio ferendo" type="straight"/>
        <underline language="LA" method="pen" text="hesterno die fecerunt" type="straight"/>
        <underline language="LA" method="pen" text="in ius adducerent" type="straight"/>
        <underline language="LA" method="pen" text="nisi tu quod esset tuum ius" type="straight"/>
        <underline language="LA" method="pen" text="nostru[m] ius contra illos obtineremus" type="straight"/>
        <underline language="LA" method="pen" text="ita sine iniuria potentia[m] leuem atque inopem esse arbitrantur" type="straight"/>
        <underline language="LA" method="pen" text="ne dicendo tempus absuma[m]" type="straight"/>
        <underline language="LA" method="pen" text="nec illud mihi arrogabo, me posse causam commodius demonstrare, quàm antea demonstrata sit: neque tame[n] tam multa" type="straight"/>
        <underline language="LA" method="pen" text="faciam, propterea quòd &amp; ab illo qui antè dixit, informata iam causa est: &amp; à me qui neque excogitare, neque pronu[n]ciare multa possum,              breuitas postulatur, quae mihimetipsi amicissima est" type="straight"/>
        <underline language="LA" method="pen" text="totam causae meae dictione[m] certas in partes diuidam" type="straight"/>
        <underline language="LA" method="pen" text="quod tibi natura dat vt semper possis, id mihi causa dat" type="straight"/>
        <underline language="LA" method="pen" text="hodie possim" type="straight"/>
        
