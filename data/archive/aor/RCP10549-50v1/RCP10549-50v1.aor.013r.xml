        <marginalia anchor_text="iugulare ciuem ne iure quidem quisquam bonus vult" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Nullus bonus, iure, ciuem iugulare vult
                    </marginalia_text>
                </position>
            </language>
            <translation>
                No honourable man want to put a citizen to death.
            </translation>
        </marginalia>
        
