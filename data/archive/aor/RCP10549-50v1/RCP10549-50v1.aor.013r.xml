            </translation>
        </marginalia>
        
        <marginalia anchor_text="scurra" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Prou[er]biu[m] de scurra
                    </marginalia_text>
                </position>
            </language>
            <translation>
