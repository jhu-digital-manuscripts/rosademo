            </translation>
        </marginalia>
        
        <marginalia anchor_text="alteri scientes inco[m]modarint" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        inco[m]modare alteri
                    </marginalia_text>
                </position>
            </language>
            <translation>
