            </translation>
        </marginalia>
        
        <marginalia anchor_text="ad hanc ratione[m] extremam necessariam" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        r[ati]o extrema necessaria
                    </marginalia_text>
                </position>
            </language>
            <translation>
