    <annotation> 
        
        <marginalia anchor_text="non enim, que[m]admodum putatis, omnia sunt in eloquentia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        No[n] tu[n]c quemadmodu[m] putatis o[mn]ia su[n]t in eloquentia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                'For eloquence does not, as you imagine, decide everything' [Loeb 240, 89].
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
