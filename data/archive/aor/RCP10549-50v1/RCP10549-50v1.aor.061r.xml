    <annotation> 
        
        <marginalia anchor_text="legum dandarum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        dare leges
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To provide laws.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="haberent inter se controuersias" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        habere int[er] se co[n]trov[er]sias
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To have internal disputes.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="leges conscriberet" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        co[n]scribere leges
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To pass laws.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="leges Halesinis dedit: in quibus multa sanxit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        lex sancita
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A ratified law.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="pueri" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Pueri
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Boys.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="cautum est" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Cautu[m] est.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                It is provided.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="demortuus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        demortuus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Obsolete.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="neminem isto Praetore Senatore[m] fieri potuisse, nisi qui isti pecunia[m] dedisset" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        senator verre pr[a]etore sine pecunia fieri no[n] possit.
                    </marginalia_text>
                    <person name="Verres"/>
                </position>
            </language>
            <translation>
                Senator Verres cannot be made praetor without money.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="sortitò capi" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Sortitò capi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To obtain by lot.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="amplissimu[m] sacerdotiu[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Sacerdotiu[m] ampliss[imum].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A very large number of priests.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="sacerdote[m] maximum creari" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Sacerdote[m] max. creari.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To appoint a high-priest.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Climachias cognomine" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Climachias cogno[m]i[n]e
                    </marginalia_text>
                    <person name="Artemo Climachias"/>
                </position>
            </language>
            <translation>
                With the surname Climachias.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Herodotus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Heredotus
                    </marginalia_text>
                    <person name="Herodotus"/>
                </position>
            </language>
            <translation>
                Herodotus.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="legum dandarum" type="straight"/>
        <underline language="LA" method="pen" text="in quibus multa sanxit de aetate hominu[m], ne quis minor triginta annis natu: de quaestu, quem qui fecisset, ne legeretur: de censu, de caeteris rebus. quae omnia" type="straight"/>
        <underline language="LA" method="pen" text="te istum Praetorem, &amp; nostrorum magistratuum authoritate, &amp; Halesinorum" type="straight"/>
        <underline language="LA" method="pen" text="&amp; pueri annorum senûmque denûm Senatorium nomen nundinati sunt" type="straight"/>
        <underline language="LA" method="pen" text="&amp; eadem illa sancta sunt, &amp; hoc amplius" type="straight"/>
        <underline language="LA" method="pen" text="Iste qui omnia iura precio exaequasset: omniúmque rerum delectum, atque discrimen pecunia sustulisset" type="straight"/>
        <underline language="LA" method="pen" text="fit, vt precio nouus vincat" type="straight"/>
        <underline language="LA" method="pen" text="Nolite ex pectare dum omnes obea[m] oratione mea ciuitates: hoc vno co[m]plector crimine omnia: neminem" type="straight"/>
        <underline language="LA" method="pen" text="hoc idem tra[n]sfero in magistratus, curationes, sacerdotia, quibus in rebus non solum hominu[m]" type="straight"/>
        <underline language="LA" method="pen" text="sortitò capi" type="straight"/>
        <underline language="LA" method="pen" text="Fit clamor maximus" type="straight"/>
        <underline language="LA" method="pen" text="cum id vniuersis indignum, atque nefarium" type="straight"/>
        
