<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v1.063v.tif" pagination="106" reader="John Dee"/>
    <annotation> 
        
        <marginalia anchor_text="quid te futurum est" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        quid te futuru[m] est
                    </marginalia_text>
                </position>
            </language>
            <translation>
                What becomes of you?
            </translation>
        </marginalia>
        
        <marginalia anchor_text="cum à Praetore prohibere[n]tur, à quatuor Quaestoribus impedire[n]tur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        prohiberi impediri
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be hindered, to be impeded.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="legatus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        legatus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Deputy.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="nimium tenuiter Siculorum erga te voluntatis argumenta colligere" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        nimiu[m] tenuiter argume[n]ta colligere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To collect arguments in too great detail.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="monstri simile" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        monstri simile
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Like a monstrosity.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="apud omnes Graecos hoc mos est" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        apud o[mn]es hic mos est
                    </marginalia_text>
                </position>
            </language>
            <translation>
                For all here it is custom.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="propè soli" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        propè soli
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Almost single-handed.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="praeter caeteros" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        p[rae]t[er] ceteros
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Besides others.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="quem ipsum hominem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        que[m] ip[su]m ho[m]i[ne]m
                    </marginalia_text>
                </position>
            </language>
            <translation>
                That man himself.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="magna res est" type="straight"/>
        <underline language="LA" method="pen" text="quid te futurum" type="straight"/>
        <underline language="LA" method="pen" text="tame[n] id more potius suo, quàm merito tuo facere viderentur" type="straight"/>
        <underline language="LA" method="pen" text="vt multo maluerint de suo more decedere, quàm de tuis moribus non dicere?" type="straight"/>
        <underline language="LA" method="pen" text="Co[n]fitendum est igitur tibi necessario" type="straight"/>
        <underline language="LA" method="pen" text="omnium minas, atque omnia pericula prae salute sua leuia duxerunt" type="straight"/>
        <underline language="LA" method="pen" text="vt posset multas istius, &amp; varias iniurias" type="straight"/>
        <underline language="LA" method="pen" text="quàm apertissime vobis, planissiméque" type="straight"/>
        <underline language="LA" method="pen" text="numerare omnes no[n] est necesse" type="straight"/>
        <underline language="LA" method="pen" text="quàm multi, &amp; quàm multa, priore actione dixerunt" type="straight"/>
        <underline language="LA" method="pen" text="denique hoc in hac causa intelligent, hoc animo esse Siculos: vt si in istum animaduersum non sit, sibi relinquendas domos, ac sedes suas: &amp; ex Sicilia decedendum" type="straight"/>
        <underline language="LA" method="pen" text="nimium tenuiter Siculorum erga te coluntatis argumenta colligere" type="straight"/>
        <underline language="LA" method="pen" text="ecquo de homine" type="straight"/>
        <underline language="LA" method="pen" text="Graecis monstri simile" type="straight"/>
        <underline language="LA" method="pen" text="hic mos est" type="straight"/>
        <underline language="LA" method="pen" text="copias, acerimúmque eius impetum, à moenibus, litoribus, classibúsque suis" type="straight"/>
        <underline language="LA" method="pen" text="quem ipsum hominem cuperent euertere, eius effigem, simulacrúmque seruare" type="straight"/>
        <underline language="LA" method="pen" text="Videtis igitur consuetudinem, religionémque Graecorum, quae monimenta hostium in bello ipso soleat defendere, eam summa" type="straight"/>
        
