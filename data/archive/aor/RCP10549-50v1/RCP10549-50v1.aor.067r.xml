    <page filename="RCP10549-50v1.067r.tif" pagination="113" reader="John Dee" signature="hh.i."/>
    <annotation> 
        
        <marginalia anchor_text="pars virilis postulat" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        pars virilis postulat
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The share of a man demands.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Odistis [...] despictis [...] contemnitis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        odistis despicitis co[n]temnitis ]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                You hated, despised, contempted.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="omnia cum summo dedecore ac turpiditudine, tum singulari stultitia, atque inhumanitate oblita" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        o[mn]ia oblita [ su[m]mo dedecore turpitudine singulari stultitia inhumanitate.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Everything is defiled by extreme indecency, vileness, exceptional stupidity, coarseness.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="cubicularii" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Cubicularij
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Bed-chamber servants.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="viuere in pecunia tenui" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        viuere in pecunia tenui
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To live with little means.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="vestro marte" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        vestro marte
                    </marginalia_text>
                    <person name="Mars"/>
                </position>
            </language>
            <translation>
                By your campaign.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="caeteris antecellit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        c[a]eteris antecellit
                    </marginalia_text>
                </position>
            </language>
            <translation>
                It surpasses the others.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Nemine[m] vestru[m] praeterit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        nemine[m] v[est]r[u]m preterit
                    </marginalia_text>
                </position>
            </language>
            <translation>
                You must be aware [Loeb 293, 15].
            </translation>
        </marginalia>
        
        <marginalia anchor_text="alimur, ac sustinemur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        ali. sustineri.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be fed, to be sustained.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="stipe[n]diariu[m]" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        stipe[n]diariu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Stipend.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="tua verbera, cum secures, cum cruces ad ciuium Ro. supplicia fixas recordetur?" type="straight"/>
