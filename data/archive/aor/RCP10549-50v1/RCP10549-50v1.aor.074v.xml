    <annotation> 
        
        <marginalia anchor_text="agris, atque vrbibus expleti, atque saturati cum hoc cumolo quaestus decedere[n]t" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        explet[us] saturat[us] vrbib[us] agris ] cu[m] hoc cumulo quest[us] decedere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To retire finished and stuffed, from the cities and lands, with that pile of profit.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="vexauit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        vexare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To shake.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="nobilissimam ciuitate[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Ciuitas nobiliss[ima]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The most famous city.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="agros Iudices ferè optimos ac noblissimos" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        ager optimus nobiliss[imus]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The richest and most famous land.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="annum tertium: quòd &amp; recentissimus est" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        rece[n]tissim[us] annus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The nearest year.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="memoria tenetis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        memoria tenetis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                An official statement.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="symphonia caneret" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Symphonia canere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To sing with accompaniment.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="Sentio Iudices modera[n]dum mihi esse iam orationi meae, fugiendámque vestra[m] satietatem: non versabor in vno genere diutius, &amp; ita caetera" type="straight"/>
        <underline language="LA" method="pen" text="oratione mea tollam, vt in causa relinquam" type="straight"/>
        <underline language="LA" method="pen" text="Duarum mihi ciuitatum reliquos" type="straight"/>
        <underline language="LA" method="pen" text="Iudices ferè optimos ac nobilissimos, Ætnensium &amp; Leontinorum" type="straight"/>
        <underline language="LA" method="pen" text="vnum annum eligam, quo" type="straight"/>
        <underline language="LA" method="pen" text="Attendite Iudices" type="straight"/>
        <underline language="LA" method="pen" text="Audistis haec Iudices: quae nunc ego omnia praetereo ac relinquo." type="straight"/>
        <underline language="LA" method="pen" text="Nihil de luxuria Apronii loquor, nihil de insolentia, nihil de singulari nequitia ac turpitudine" type="straight"/>
        <underline language="LA" method="pen" text="tantum de quaestu ac lucro dicam vnius agri" type="straight"/>
        <underline language="LA" method="pen" text="breuis est oratio" type="straight"/>
        <underline language="LA" method="pen" text="dic, dic quaeso clarius" type="straight"/>
        
