        <marginalia anchor_text="immensum, atque infinitum lucrum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        lucru[m] [ i[m]mensu[m] infinitum
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Immense and infinite profit.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="nulla opera insumpta" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        nulla opera insu[m]pta
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Without any effort made.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="vt etiam si tibi hoc conceda[m] [...] verum vt ita" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        vt etia[m] tibi co[n]ceda[m] hoc veru[m] vt ita sit
                    </marginalia_text>
                </position>
            </language>
            <translation>
                As you ought to concede even that, but it is thus.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="quasi praeclaru[m] aliquid praedicare" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        q[ua]si p[rae]claru[m] aliquid p[rae]dicare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To declare something as glorious.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="inquit, omnes" type="straight"/>
        <underline language="LA" method="pen" text="nihil putas valere in iudiciis coniecturam, nihil suspicionem, nihil anteactae vitae existimatione[m], nihil bonoru[m] virorum testimonia,              nihil ciuitatum authoritates ac testimonia" type="straight"/>
        <underline language="LA" method="pen" text="non requiro: vestra[m] in hoc fidem, dignitate[m], religione[m] in iudicando non imploro" type="straight"/>
        <underline language="LA" method="pen" text="Verum hoc, vt opinor, esse vis" type="straight"/>
        <underline language="LA" method="pen" text="Negare hoc (nisi fortè negare omnia constituisti) nullo modo potes: palàm res gesta" type="straight"/>
        <underline language="LA" method="pen" text="tantum praesens lucrum nulla opera insumpta" type="straight"/>
        <underline language="LA" method="pen" text="Postremo illa quidem certe tibi praecisa defensio est, in wua tu semper omnia tua furta, atque flagitia latere posse arbitratus es" type="straight"/>
        <underline language="LA" method="pen" text="vt etiam si tibi hoc conceda[m]" type="straight"/>
        <underline language="LA" method="pen" text="verum vt ita sit" type="double"/>
        <underline language="LA" method="pen" text="Tenetur igitur iam Iudices, &amp; manifestò tenetur auaritia, cupiditas hominis, scelus, improbitas, audacia" type="straight"/>
        
