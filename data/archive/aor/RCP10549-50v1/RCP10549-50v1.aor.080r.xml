        <underline language="LA" method="pen" text="quoniam pater tam negligens, ac dissolutus est" type="straight"/>
        <underline language="LA" method="pen" text="Verum huc longius, quàm voluntas fuit, ab epistola Timarchidis digressa est oratio mea" type="straight"/>
        <underline language="LA" method="pen" text="vt possim illud probare, si velim" type="straight"/>
        <underline language="LA" method="pen" text="sic hanc rem totam administrasti" type="straight"/>
        <underline language="LA" method="pen" text="Sed ego habebo rationem authoritatis meae: meminero quo animo, quo consilio ad causam publicam accesserim: no[n] agam" type="straight"/>
        <underline language="LA" method="pen" text="nihil probari velim me dicente, quod non antè mihimetipsi probatum sit" type="straight"/>
        <underline language="LA" method="pen" text="binis centesimis" type="straight"/>
        <underline language="LA" method="pen" text="mihi autem hoc perarduum est demo[n]strare" type="straight"/>
        <underline language="LA" method="pen" text="ita vir bonus est, vt etiam si inimicissimus isti esset, tamen eius testimonio credi" type="straight"/>
        <underline language="LA" method="pen" text="aliqua firmiora aut grauiora quaere[n]da sunt?" type="straight"/>
        
