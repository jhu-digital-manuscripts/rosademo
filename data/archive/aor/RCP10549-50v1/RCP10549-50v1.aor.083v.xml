    <annotation> 
        
        <marginalia anchor_text="pecunia est Verres: vna, qua frumentu[m] tibi emeres in cellam: altera, qua frume[n]tu[m] emeres à ciuitatibus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        pecunia [ vna altera ].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                One [and] another sum of money.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="refertius erit aerarium" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Ærariu[m] referti[us].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A fuller treasury.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="inter se genere iniuriae, no[n] magnitudine pecuniae differat" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        inter se genere iniuri[a]e no[n] magnitudine differunt
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The injustices do not differ in size, but in kind amongst themselves.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Sophocles" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Sophocles
                    </marginalia_text>
                    <person name="Sophocles"/>
                </position>
            </language>
            <translation>
                Sophocles.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="magno co[n]uentu acta res est" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        magno co[n]uentu acta res est.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The matter was presented in a large gathering.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="large, liberalitérque" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        large liberaliter.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Generously, liberally.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="locus intra oceanu[m] iam nullus est, neque tam longinquus, neque tam reconditus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        locus longinquus reconditus.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A place, remote, hidden.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        histori[a]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Stories.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="authoritas" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Authoritas 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Authority.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ex his quidem hominibus qui nuper fuerunt" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        ex hijs ho[m]i[n]ib[us] q[ui] nuper fueru[n]t
                    </marginalia_text>
                </position>
            </language>
            <translation>
                From these men who were of our time.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="tum refertius" type="straight"/>
        <underline language="LA" method="pen" text="erit aerarium Populi Ro. quàm vnquam fuit. At" type="straight"/>
        <underline language="LA" method="pen" text="inter se genere iniuriae, no[n] magnitudine pecuniae differat" type="straight"/>
        <underline language="LA" method="pen" text="vtrum crimini defensio, an comitatus exilio quaeritur?" type="straight"/>
        <underline language="LA" method="pen" text="quaeda[m] omnino in te singularia, quae in nullum aliu[m] hominem" type="straight"/>
        <underline language="LA" method="pen" text="quaeda[m] tibi cu[m] multis co[m]munia" type="straight"/>
        <underline language="LA" method="pen" text="vt ego assentiar orationi, defensionem" type="straight"/>
        <underline language="LA" method="pen" text="tamen non probabo" type="straight"/>
        <underline language="LA" method="pen" text="Potius enim te da[m]nato, caeteris angustior locus improbitatis defende[n]dae relinquetur, quàm te absoluto" type="straight"/>
        <underline language="LA" method="pen" text="alii quod audacissime feceru[n]t, recte fecisse existime[n]tur" type="straight"/>
        <underline language="LA" method="pen" text="bellu[m], sed luctu[m]" type="straight"/>
        <underline language="LA" method="pen" text="In eiusmodi re ac moribus, si is qui erit adductus in iudiciu[m], cum manifestis in flagitiis tenebitur, alios eadem fecisse dicet, illi exe[m]pla no[n] deerunt,              reip. salus deerit, si improborum exemplis improbi, iudicio ac periculo liberabuntur" type="straight"/>
        <underline language="LA" method="pen" text="co[n]sumitur? quid sedetis? cur no[n] in media oratione mea co[n]surgitis atque disceditis?" type="straight"/>
        <underline language="LA" method="pen" text="autem istoru[m] audacias, ac libidines aliqua ex parte resecare? desinite dubitare vtru[m] sit" type="straight"/>
        <underline language="LA" method="pen" text="propter multos improbos vni parcere, an vnius improbi supplicio multorum improbitatem cohercere?" type="straight"/>
        <underline language="LA" method="pen" text="plurimum sole[n]t &amp; authoritatis habere ad proba[n]dum, &amp; iucu[n]ditatis ad audiendu[m]" type="straight"/>
        <underline language="LA" method="pen" text="Magna est hominu[m] authoritas, &amp; tanta, vt etia[m] delicti suspitionem tegere possit" type="straight"/>
        
