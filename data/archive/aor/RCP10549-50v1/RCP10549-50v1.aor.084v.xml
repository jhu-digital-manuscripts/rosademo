    <annotation> 
        
        <marginalia anchor_text="me[n]struis cibariis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        me[n]struu[m] cibaria
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A monthly ration.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="vberiora cibaria" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        vberiora cibaria
                    </marginalia_text>
                </position>
            </language>
            <translation>
                An abundant ration.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="suspitiones iniicere" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        inicere suspitio[n]em
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To incite suspicion.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="supplicio dignus est" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        supplicio dignus est
                    </marginalia_text>
                </position>
            </language>
            <translation>
                It merits punishment.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="do hoc vobis, &amp; co[n]cedo" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        do condedo
                    </marginalia_text>
                </position>
            </language>
            <translation>
                I give, I concede.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="reliquas spes turbetis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        reliquas spes turbare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To extinguish the remaining hope.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="auaritiae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Avaritia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Greed.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="neque id est nouu[m], neque reprehe[n]dendum" type="straight"/>
        <underline language="LA" method="pen" text="Etenim hoc dico" type="straight"/>
        <underline language="LA" method="pen" text="&amp; magna voce dico" type="straight"/>
        <underline language="LA" method="pen" text="factu[m] est, improbe factu[m]" type="straight"/>
        <underline language="LA" method="pen" text="sed do" type="straight"/>
        <underline language="LA" method="pen" text="&amp; co[n]cedo" type="straight"/>
        <underline language="LA" method="pen" text="negligite praeterita, si vultis: sed ne reliquas spes turbetis, atque omnes prouincias euertatis, id prouidete" type="straight"/>
        <underline language="LA" method="pen" text="solebat, authoritate vestra via[m] patefaciatis illustrem atque lata[m]" type="straight"/>
        <underline language="LA" method="pen" text="certe hoc quod adhuc" type="straight"/>
        <underline language="LA" method="pen" text="Improbi sunt qui precunias contra leges cogu[n]t: stulti, qui quod licere iudicatu[m] est, praetermittunt" type="straight"/>
        <underline language="LA" method="pen" text="sitis hominibus licentia[m] pecuniaru[m] eripiendarum" type="straight"/>
        <underline language="LA" method="pen" text="in quo primùm aestimationis iniquitas, atque improbitas" type="straight"/>
        <underline language="LA" method="pen" text="Videtis ia[m] profecto Iudices, hac aestimatione à vobis co[m]probata, neque modu[m] posthac auaritiae cuiusqua[m], neque poena[m] improbitatis futura[m]" type="straight"/>
        <underline language="LA" method="pen" text="co[m]missurum, vt de pecuniis repetundis co[n]demnari possit. quanta[m] enim quisque" type="straight"/>
        <underline language="LA" method="pen" text="tanta[m] licebit" type="straight"/>
        <underline language="LA" method="pen" text="Praeclaros viros existimas iudices nos habere, quos alienis peccatis concessuros putes, quo" type="straight"/>
     
