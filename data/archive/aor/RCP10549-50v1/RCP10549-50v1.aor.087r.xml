        <marginalia anchor_text="laesit grauissime" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        laesit grauiss[imè]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                It harms most severely.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="naui faciundae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        facere naue[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To make a boat.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="iniurias plurimas, contumeliásque imposuisti" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        imponere iniurias co[n]tumelias
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To wrong, to insult.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in perpetuum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        in perpetuu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Again and again.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="detrimentum fecit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        detrime[n]tu[m] facere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To suffer.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="de summa frumenti detractum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        de su[m]ma destractu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be deducted from the total.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="praedones" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Praedones
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Robbers.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="instituunt amicos" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        instituere amicos
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To make friends.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="se locis reciperent" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        recipere se.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To recover himself.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ciuitas improba antea non erat, etiam erat inimica improborum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Ciuitas no[n] improba inimica improbor[um]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A city, not wicked, the enemy of rascals.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="vacatio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        vacatio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Exemption.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="vt opinio mea fert" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        vt opinio mea fert
                    </marginalia_text>
                </position>
            </language>
            <translation>
                As I do believe.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="vacui, expertes, soluti ac liberi" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        vacuus expers ] liber solutus ]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Empty, immune, free, unbound.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Laudatum etiam vos quenquam venire" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        laudatu[m] venire
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To come praising.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ecquae ciuitas est non modo in prouinciis nostris, verum in vltimis nationibus, aut tam pote[n]s, aut tam libera, aut etiam tam immanis ac barbara" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Ciuitas [ potens libera immanis barbara ]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A powerful, free, inhumane, [and] uncivilised city.
            </translation>
        </marginalia>
        
