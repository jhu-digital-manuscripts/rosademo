    <page filename="RCP10549-50v1.091r.tif" pagination="161" reader="John Dee" signature="ll.i."/>
    <annotation>
        
        <marginalia anchor_text="ars certare videretur cum copia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        arte[m] certare cu[m] copia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The skill vies with the richness [Loeb 293, 361].
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ad amplissimi templi ornamentum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        templu[m] ampliss[imu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A most lavish temple.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in templo clarissimo" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        te[m]plu[m] clariss[imu]m
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A most splendid temple.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="dare, donare, dicare, consecrare" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        dare donare dicare co[n]secrare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To give, to present, to dedicate, to consecrate.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="habe[n] profectò in animo Capitoliu[m] sic ornare" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        habere in a[n]i[m]o capito. ornare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To have in mind to adorn the Capitol.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="interuerso regali hoc dono" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        interu[er]so dono
                    </marginalia_text>
                </position>
            </language>
            <translation>
                By a reverse present.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in Rege tam nobili" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        rex nobilis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A notable king.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="clarissimo, pulcherrimóque monimento" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        monime[n]tu[m] clariss[imu]m pulcherimu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A most famous and beautiful building.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        ] magnifice[n]tius copiosius preclarius ]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                More magnificently, more amply, more splendidly.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="vt ars certare videretur" type="straight"/>
        <underline language="LA" method="pen" text="velle illud etiam atque etiam considerare" type="straight"/>
        <underline language="LA" method="pen" text="dies vnus, alter, plures: non" type="straight"/>
        <underline language="LA" method="pen" text="os hominis" type="straight"/>
        <underline language="LA" method="pen" text="Rex maximo conuentu Syracusis in foro, ne quis forte me in crimine obscuro versari atque affingere aliquid suspitione" type="straight"/>
        <underline language="LA" method="pen" text="templo clarissimo" type="straight"/>
        <underline language="LA" method="pen" text="hoc sibi eripi miserum esse" type="straight"/>
        <underline language="LA" method="pen" text="Quae vox, quae latera, quae vires huius vnius criminis querimoniam possint sustinere?" type="straight"/>
        <underline language="LA" method="pen" text="accaepturas nationes exteras, quemadmodum huius tui facti famam in regna aliorum, atque" type="straight"/>
        <underline language="LA" method="pen" text="vltimas terras peruentura[m] putasti, cum" type="straight"/>
        <underline language="LA" method="pen" text="solius hoc esse facinus" type="straight"/>
        <underline language="LA" method="pen" text="vt te[m]pli dignitas, imperiíque nostri nome[n] desiderat" type="straight"/>
        <underline language="LA" method="pen" text="in iniuria tam acerba" type="straight"/>
        <underline language="LA" method="pen" text="Hoc loco Q. Catule te appello. loquor" type="straight"/>
        <underline language="LA" method="pen" text="nominis aeterna memoria simul cum" type="straight"/>
        <underline language="LA" method="pen" text="Tibi haec cura suscipienda, tibi haec opera sume[n]da est, vt" type="straight"/>
        <underline language="LA" method="pen" text="Audisti, &amp; audies omnes è conuentu Syracusano, qui ita dicant, sese audientibus" type="straight"/>
        
