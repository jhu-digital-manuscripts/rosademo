<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v1.092v.tif" pagination="164" reader="John Dee" signature="[ll.ii.[v]]"/>
    <annotation>
        
        <marginalia anchor_text="praecerpo fructum officii" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        precepere fructu[m] officij
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To anticipate the reward of the duty.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Est aliqua mea pars virilis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        est aliq[u]a me pars virilis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                In this right I have a share.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ciuitas sum quam ille amplam, illustrem, clarámque" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Ciuitas [ ampla illustris clara ]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A great, glorious, [and] famous community.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Cicero P. S. Affricano similis
                    </marginalia_text>
                    <person name="Cicero"/>
                    <person name="Scipio Africanus"/>
                </position>
            </language>
            <translation>
                Cicero is similar to Publius Scipio Africanus.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="inuictissimi Imperatoris" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Invi[n]ctiss[im][us] imperator
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Most invoncible commander.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="bonos" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        BONI
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Honest men.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="quàm audacter, quàm libidinose, quàm impude[n]ter" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        audacter libidiniosè impude[n]ter
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Unscrupulously, wantonly, shamelessly.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="anniuersariis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        anniuersari[us]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Yearly festival.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Ne multa" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        ne multa
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Well!
            </translation>
        </marginalia>
        
        <marginalia anchor_text="aliqua[n]to post" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        aliquanto post
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Some time later.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="Quid aut his respo[n]dere honestè" type="straight"/>
        <underline language="LA" method="pen" text="omnia in te sunt quae aut fortuna" type="straight"/>
        <underline language="LA" method="pen" text="Non praecerpo fructum officii tui, no[n] alienam mihi laudem appeto" type="straight"/>
        <underline language="LA" method="pen" text="pudoris mei, P. Scipione flore[n]tissimo adolescente viuo &amp; incolumi, me propugnatorem monimentorum P. Scipionis, defensorémque profiteri" type="straight"/>
        <underline language="LA" method="pen" text="No[n] est querendu[m] in ea ciuitate, quae propter virtute[m] omnibus nationibus imperat, virtutem plurimum posse. sit apud" type="straight"/>
        <underline language="LA" method="pen" text="Est aliqua mea pars virilis, quòd eius ciuitatis" type="straight"/>
        <underline language="LA" method="pen" text="praecipue quòd in his artibus pro mea parte" type="straight"/>
        <underline language="LA" method="pen" text="quarum ille princeps fuit, aequitate, industria, temperantia, defensione miserorum, odio" type="straight"/>
        <underline language="LA" method="pen" text="quae cognatio studioru[m] &amp; artium propemodu[m] non minus est coniuncta, quàm ista qua vos delectamini, generis &amp; nominis. Repeto" type="straight"/>
        <underline language="LA" method="pen" text="Non vereor ne hoc officiu[m] meum P. Seruilio iudici non probe[m], qui cum res maximas gesserit, monime[n]táque" type="straight"/>
        <underline language="LA" method="pen" text="vereor ne tibi" type="straight"/>
        <underline language="LA" method="pen" text="quamplurimos esse custodes monimentoru[m], &amp; putare omnes bonos alienae gloriae defensione[m], ad officiu[m] pertinere" type="straight"/>
        
