    <annotation>
        
        <marginalia anchor_text="Erat hyems summa: te[m]pestas, vt ipsum Sopatrum dicere audistis, perfrigida: imber maximus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        tempestas p[er]frigida hiems summa imber maxim[us] ] 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Very cold weather, midwinter, very strong rain.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="diuaricari ac deligari" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        diuaricari deligari
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be spread out; to be disclosed.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in aere, in imbri, in frigore" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        in [ aere imbre frigore ] 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                In the wind, rain, [and] cold.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="obriguisset" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        obrigere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To stiffen.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="vix viuus aufertur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        vix viu[us] aufertur
                    </marginalia_text>
                </position>
            </language>
            <translation>
                He was removed half dead [Loeb 293, 391].
            </translation>
        </marginalia>
        
        <marginalia anchor_text="disposite istu[m] accusare" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        dispositè accusare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To accuse in an ordered manner.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="distinguere ac separare" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        distinguere separare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To distinguish, to separate.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="non queo dicere" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        no[n] queo dicere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                I am not able to say.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="singularis significatur insolentia, superbia, contumacia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        singularis insole[n]tia superbia co[n]tumacia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Unique insolence, pride, disobedience.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in eorum locum substitutus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        in eoru[m] locu[m] substitui
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To take up their place.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="transducere" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        transducere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To bring across.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="stultitia, nequitia, inertia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        stultitia nequitia inertia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Foolishness, wickedness, ignorance.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="patibulo" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        patibulum
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Whipping block.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="qui honorem illi habuerant" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        habere honore[m] alicui
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To honour somebody.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="vinctum, astrictúmque" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        vinctus astrictus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Bound, tied.
            </translation>
        </marginalia>
        
