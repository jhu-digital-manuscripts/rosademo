            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        litterul[a]e minut[a]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Very small letters.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="religiosissimo fano" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        fanu[m] religiosiss[imu]m
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A much venerated temple.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="noctu vigilias agrent" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        noctu vigilias agere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To guard by night.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="coniunctissimo animo cum ipsi Agrigentinis viuunt ac negociantur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        coniunctiss[im]o a[n]i[m]o viuere negociari
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To live and carry business in joint spirit.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Herculis templum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Herculis templum
                    </marginalia_text>
                    <location name="Temple of Heracles, Agrigento"/>
                </position>
            </language>
            <translation>
                The temple of Heracles. 
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Herculis templum est apud Agrigentinos non longe à foro, sanè sanctum apud illos &amp; religiosum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        te[m]plu[m] sanctu[m] religiosu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A holy and venerated temple.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="nocte intempesta" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        nox intempesta
                    </marginalia_text>
                </position>
            </language>
