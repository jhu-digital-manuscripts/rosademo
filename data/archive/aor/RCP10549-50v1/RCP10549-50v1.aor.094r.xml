    <annotation>
        
        <marginalia anchor_text="vir omnibus rebus praecellentissimus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        vir o[mn]ib[us] reb[us] p[rae]celletiss[im][us]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A man, most distinguished in all things.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="temperationem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        temperatio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Regulation.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="elegantes" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        elega[n]tes
                    </marginalia_text>
                </position>
            </language>
            <translation>
                People of taste.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in toto orbe terrarum. In eo sacrario intimo" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        in toto orbe terrar[um] in eo sacrario intimo
                    </marginalia_text>
                </position>
            </language>
            <translation>
                In the entire world of lands, in the innermost chamber.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="aditus enim in id sacrarium non est viris" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        aditus no[n] est viris
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Entrance is not permitted to men.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="sacerdotes Cereris atque illius fani antistites" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        sacerdotes antistites
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A priest, a high priest.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="acerbum, indignu[m], luctuosum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        acerbu[m] indignu[m] luctuosu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Harsh, unworthy, mournful.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ficti testes" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        ficti testes
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Fictituous witnesses.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="secretò" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        secretò
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Secretly.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="oculis quidem captus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        oculis captus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Caught by the eyes.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="tametsi ne oculis quidem captus in hanc fraude[m] tam scelerata[m], ac tam nefaria[m] decidisti" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        fraus scelerata nefaria 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Desire for atrocity and crime.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="adamasti" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        adamare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To covet.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="auditione expetere" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        auditio[n]e expetere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To fall in lust with.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Melita" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Melita
                    </marginalia_text>
                    <location name="Mljet"/>
                </position>
            </language>
            <translation>
                Mljet.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="satis lato ab Sicilia mari, periculosóque" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        mare latu[m] periculosu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A wide and dangerous sea.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="vestem conficiendam" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        veste[m] co[n]ficere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To weave clothes.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="non modo breuiter de vnoquoque dicendum est, sed etiam praetereunda videntur esse permulta" type="straight"/>
        <underline language="LA" method="pen" text="vt ad maiora istius &amp; illustriora in" type="straight"/>
        <underline language="LA" method="pen" text="temperantia, sed etiam intelligentia te atque istos, qui se elegantes dici volunt, vicerit. nam" type="straight"/>
        <underline language="LA" method="pen" text="eo sacrario" type="straight"/>
        <underline language="LA" method="pen" text="sacra per mulieres ac virgines confici solent. hoc" type="straight"/>
        <underline language="LA" method="pen" text="omnibus acerbum, indignu[m], luctuosum denique" type="straight"/>
        <underline language="LA" method="pen" text="Res non procrastinatur" type="straight"/>
        <underline language="LA" method="pen" text="eóne tu seruos ad spoliandum fanum immittere ausus" type="straight"/>
        <underline language="LA" method="pen" text="quò liberos adire ne orandi quidem causa fas erat?" type="straight"/>
        <underline language="LA" method="pen" text="hísne rebus manus affere non dubitasti, à quibus etiam oculos cohibere te religionu[m] iura cogebant?" type="straight"/>
        <underline language="LA" method="pen" text="auribus tu tanta[m] cupiditatem" type="straight"/>
        <underline language="LA" method="pen" text="expetere" type="straight"/>
        <underline language="LA" method="pen" text="imò vero alia co[m]plura. ex quibus eligam spoliatione[m] nobilissimi atque antiquissimi fani, de qua" type="straight"/>
        <underline language="LA" method="pen" text="sicut adhuc fecistis" type="straight"/>
        
