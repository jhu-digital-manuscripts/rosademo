        <marginalia anchor_text="arma capere ac bellum facere" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        arma capere bellu[m] facere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To take up the weapons, make war.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ex media morte eripere ac liberare" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        ex media morte eriptere ac liberare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To snatch away and liberate from the middle of death.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="indemnatis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        inde[m]natus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Uncondemned.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="exitus exitiales" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Exitus exitiales
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Disastrous pardoning.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ruere illam Rempub." hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        ruere rempub.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To destroy the state.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="villicos, pastorésque tibi in suspitionem venisse" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        villicos pastores ] venire in suspitio[n]em
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Managers and shepherds coming under suspicion.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="grandis natu" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        grandis natu
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Young man.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="lecto tenebatur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        lecto teneri
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be held by a chosen man.
            </translation>
        </marginalia>
        
