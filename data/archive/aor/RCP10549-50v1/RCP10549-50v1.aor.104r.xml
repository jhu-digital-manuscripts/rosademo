        <marginalia anchor_text="terrestrem archipiratam" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        terrestris archipirata
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Pirate captain of dry land.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="victu ceterísque rebus quàm liberalissimè commodíssiméque habeatur" hand="English_secretary">
