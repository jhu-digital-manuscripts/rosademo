        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Repetitio &amp; quasi Epilogus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Repetition and a quasi epilogue.
            </translation>
        </marginalia>
        
        <marginalia anchor_text=" Reliqua est ea causa Iudices, quae non ia[m] recaepta, sed innata: neque deleta ad me, sed in anima, sensúque meo penitus affixa atque insita est" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        In a[n]i[m]a se[n]suq[ue] meo penitus affixa atq[ue] insita, caussa est
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The case is deeply anchored and embedded in my mind and senses.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="lictores circu[m]fistunt valentissimi" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        lictores vale[n]tissimi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Most powerful lictors.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Crudelitas
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Cruelty.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="collocauisti, vt aetati macimè lubricae atque incertae exempla" type="straight"/>
        <underline language="LA" method="pen" text="Neque haec tot, ta[n]táque dedecora dissimulatione tua, neque obliuione hominu[m]" type="straight"/>
        <underline language="LA" method="pen" text="tibi hominu[m] innocentiu[m] sanguis non modo voluptati, sed etiam quaestui fuit. haec" type="straight"/>
        <underline language="LA" method="pen" text="Satis est factu[m] Siculis, satis officio ac necessitudini Iudices, satis promisso nostro ac recepto" type="straight"/>
        <underline language="LA" method="pen" text="sed in anima, sensúque meo penitus affixa atque insita est: quae non ad sociorum salutem, sed ad ciuium Ro.              hoc est ad vniuscuiusque nostrum vitam &amp; sanguinem pertinet. in qua nolite à me, quasi dubium sit aliquid, argumenta Iudices expectare" type="straight"/>
        <underline language="LA" method="pen" text="aude hoc primùm negare, si potes. nemo" type="straight"/>
        <underline language="LA" method="pen" text="quasi enim possit esse vlla causa cur hoc cuiqua[m] ciui" type="straight"/>
        <underline language="LA" method="pen" text="sex lictores circu[m]sistunt valentissimi, &amp; pulsandos, verbera[n]dósque homines exercitatissimi: caedu[n]t acerrime virgis. denique" type="straight"/>
        
