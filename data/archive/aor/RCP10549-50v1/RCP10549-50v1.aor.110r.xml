                    <marginalia_text>
                        ciuilis disse[n]tio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Civil strife.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="mortem acerba[m], crudelémque proponeret" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
