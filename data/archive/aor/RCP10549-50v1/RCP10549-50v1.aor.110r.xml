        <underline language="LA" method="pen" text="circunretitum" type="straight"/>
        <underline language="LA" method="pen" text="Cui si etiam id quod defendit, velim concedere: tamen illa ipsa defensio non minus esse ei perniciosa, quàm mea vera accusatio debeat. quid" type="straight"/>
        <underline language="LA" method="pen" text="supplici no[n] illa dextera inuicta &amp; fidem porrexit, &amp; spem salutis ostendit" type="straight"/>
        <underline language="LA" method="pen" text="Malo, malo mehercules id, quod tu defe[n]dis, his iudicibus, Populóque Ro. quàm" type="straight"/>
        <underline language="LA" method="pen" text="id, quod ego insimulo, probari" type="straight"/>
        <underline language="LA" method="pen" text="Hîc vide quàm me sis vsurus" type="straight"/>
        <underline language="LA" method="pen" text="Graniu[m] testem produxero" type="straight"/>
        <underline language="LA" method="pen" text="refellito si poteris" type="straight"/>
        <underline language="LA" method="pen" text="meum testem deseram, tibi fauebo, te inquam adiuuabo: ostendito" type="straight"/>
        <underline language="LA" method="pen" text="nihil est quod te malim probare: nullum enim facinus, quod" type="straight"/>
        
