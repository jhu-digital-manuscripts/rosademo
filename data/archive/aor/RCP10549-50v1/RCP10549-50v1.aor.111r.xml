        <underline language="LA" method="pen" text="nulla vox alia istius miseri inter dolore[m] crepitúmque plagaru[m] audebatur" type="straight"/>
        <underline language="LA" method="pen" text="ciuitatis omnia verbera depulsurum, cruciatúmque à corpore deiecturum" type="straight"/>
        <underline language="LA" method="pen" text="O nomen dulce libertatis! O ius eximium nostrae ciuitatis! O lex Portia legésque Semproniae!              O grauiter desiderata &amp; aliquando reddita plebi Rom. tribunitia potestas! huccine" type="straight"/>
        <underline language="LA" method="pen" text="si te illius acerba imploratio &amp; vox miserabilis non inhibebat" type="straight"/>
        <underline language="LA" method="pen" text="fletu &amp; gemitu maximo commouebare? in crucem tu agere ausus es quenquam, qui se ciuem Rom. esse diceret" type="straight"/>
        <underline language="LA" method="pen" text="enim vt animi multitudinis in istum dolore &amp; odio &amp; communis periculi metu co[n]citarentur" type="straight"/>
        <underline language="LA" method="pen" text="Statui egomet mihi tum modu[m] orationi meae" type="straight"/>
        <underline language="LA" method="pen" text="id quod sapentissimè fecit, facere laetatus sum" type="straight"/>
        <underline language="LA" method="pen" text="iam exploratum est" type="straight"/>
        <underline language="LA" method="pen" text="futurum" type="straight"/>
        <underline language="LA" method="pen" text="sed secu[n]dum arbitrium tuum" type="straight"/>
        <underline language="LA" method="pen" text="Cu[m] haec omnia quae polliceor, cumulatè tuis proximis plana fecero, tum istuc ipsum tenebo quod abs te mihi datur: eo co[n]tentum me esse dicam" type="straight"/>
        <underline language="LA" method="pen" text="Hoc teneo: hîc haereo Iudices: hoc sum co[n]tentus" type="straight"/>
        <underline language="LA" method="pen" text="omitto ac negligo caetera: sua confessione induatur, ac iuguletur necesse est" type="straight"/>
        <underline language="LA" method="pen" text="tua te accuso oratione" type="straight"/>
        
