        <marginalia anchor_text="mone[n]di locus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Locus monendi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Place for advice.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ratione[m] officii co[n]fido esse omnibus iniquissimis meis persoluta[m]" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        r[ati]o[n]em officij o[mn]ib[us] esse p[er]soluta[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To have discharged the obligations of the office to all.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="temporum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Tempus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Circumstance.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Quaeret aliquis fortasse" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        queret aliquis fortasse
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Someone might perhaps ask.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="[cau]sa, probabilis mihi &amp;" type="straight"/>
        <underline language="LA" method="pen" text="isto voluisse" type="straight"/>
        <underline language="LA" method="pen" text="Sed mehercules vestra, reíque publicae causa Iudices" type="straight"/>
        <underline language="LA" method="pen" text="atque etiam, &amp; co[n]sideres quid agas, quo progrediare: que[m] hominem, &amp; qua ratione defendas. neque de illo quicquam tibi praefinio, quo minus ingenio mecum,              atque omni dicendi facultate contendas. caetera, siqua putas te occultius extra iudicium, quae ad iudicium pertinent, facere posse, siquid artificio, consilio, potentia, gratia, copiis istius moliri cogitas, magnopere censeo desistas" type="straight"/>
        <underline language="LA" method="pen" text="&amp; illa quae tentata iam, &amp; coepta ab isto sunt, à me autem peruestigata &amp; cognita, moneo vt extinguas, &amp; longius progredine" type="straight"/>
        <underline language="LA" method="pen" text="Mango tuo periculo peccabitur in hoc iudicio, maiore quàm" type="straight"/>
        <underline language="LA" method="pen" text="qua fide ego accusem, qua religione hi iudicent, qua tu ratione defendas" type="straight"/>
        <underline language="LA" method="pen" text="tantulum de recta regione deflexerit, non illa tacita existimatio, quam antea contemnere solebatis, sed veheme[n]s ac liberu[m] Po. Ro. iudiciu[m] co[n]sequetur" type="straight"/>
        <underline language="LA" method="pen" text="ea ne vera pute[n]tur, tibi maximè est" type="straight"/>
        <underline language="LA" method="pen" text="Ego mei iam ratione[m] officii co[n]fido esse omnibus iniquissimis meis persoluta[m]. na[m] istu[m] paucis horis primae actionis omnium mortalium sente[n]tiis co[n]demnaui" type="straight"/>
        <underline language="LA" method="pen" text="in rebus, tum in Rep. permagni mome[n]ti est, ratio atque inclinatio temporum. nempe" type="straight"/>
        <underline language="LA" method="pen" text="iste recreari &amp; co[n]firmari visus est, lex statim" type="straight"/>
        <underline language="LA" method="pen" text="cui legi cum vestra dignitas vehementer aduersaretur, istius spes falsa &amp; insignis impude[n]tia maximè siffragatur" type="straight"/>
        <underline language="LA" method="pen" text="tantum Resp. vulnus acceperit" type="straight"/>
        <underline language="LA" method="pen" text="aut in hoc iudicio deponere, aut diutius tacitus sustinere? non" type="straight"/>
        <underline language="LA" method="pen" text="quibus omnia Populi Ro. bene[ficia]" type="straight"/>
        
