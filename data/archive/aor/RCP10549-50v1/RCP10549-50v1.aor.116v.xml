    <annotation>
        
        <marginalia anchor_text="considerati hominis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Confiderati ho[min]is officiu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The duty of a thoughtful person.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="è co[n]suetudine recedatur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        è co[n]suetudine recedi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To withdraw from the practice.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="longe alia ratione" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        longe alia r[ati]o[n]e, atq[ue]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Far from the other reasoning, and.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="probi" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Probi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Honest.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="impude[n]ter fecisse, astute &amp; callide" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        facere { impude[n]t[er] astute callide }
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To do outragiously, shrewdly and cleverly.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="excellens ingeniu[m]" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        excellens ingeniu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                An exceptional ability.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in consuetudinem venit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        In co[n]suetudine[m] venire
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To become customary.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="qua de re iure decertari oporteret, armis non contendere" type="straight"/>
        <underline language="LA" method="pen" text="è co[n]suetudine" type="straight"/>
        <underline language="LA" method="pen" text="se, quo impudentius egerit, hoc superiore[m] discessurum" type="straight"/>
        <underline language="LA" method="pen" text="tum enim" type="straight"/>
        <underline language="LA" method="pen" text="causae spes erat posita in defensione mea: nu[n]c in confessione aduersarii" type="straight"/>
        <underline language="LA" method="pen" text="de quibus ego antea laboraba[m], ne si improbi essent, falsi" type="straight"/>
        <underline language="LA" method="pen" text="si probi existimare[n]tur, quod dixissent probarent. nunc" type="straight"/>
        <underline language="LA" method="pen" text="Veru[m]tamen cum illoru[m] causae actione[m] considero" type="straight"/>
        <underline language="LA" method="pen" text="video quid impude[n]tius dici possit" type="straight"/>
        <underline language="LA" method="pen" text="se iniecturos vobis causam delibera[n]di, &amp; iudicandi iustam mora[m] ac religione[m]" type="straight"/>
        <underline language="LA" method="pen" text="excellens ingeniu[m]" type="straight"/>
        <underline language="LA" method="pen" text="Sed cu[m] de eo iure mihi dicendu[m] sit quod" type="straight"/>
        <underline language="LA" method="pen" text="quod constitutu[m] sit à maioribus, conseruatu[m] vsque ad hoc te[m]pus: quo sublato" type="straight"/>
        <underline language="LA" method="pen" text="solum pars aliqua iuris diminuta, sed etia[m] vis ea quae iuri maxime est aduersaria, iudicio confirmata esse videatur" type="straight"/>
        
