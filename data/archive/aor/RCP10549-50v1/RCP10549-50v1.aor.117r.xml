    <annotation>
        
        <marginalia anchor_text="mala consuetudine dissolutu[m] est" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        mala co[n]suetudine dissolutu[m] 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Abrogated by this evil practice.
            </translation>
        </marginalia>
        
        <marginalia anchor_text=" nam vt quaeque res est turpissima, sic maxime &amp; maturissime" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        maturisse tardissimè
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Very speedily, very slowly.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        lex spo[n]siorius
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The law of the one giving the promise.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="facto quidem turpe" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        facto turpe
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Done by infamous conduct.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="actione co[n]fligere" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        actio[n]e co[n]fligere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To bring one's one action.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="summo loco natam &amp; probatissimam foeminam" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        femina summo loco nata probatiss[ima]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A woman, born into the highest family and highly esteemed.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="adiuncta mercatur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        adiuncta mercari
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To buy adjacent land.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="se ipse inferebat &amp; intrudebat" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        se inferre intrudere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To obtrude themselves, to push themselves into.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="mala consuetudine" type="straight"/>
