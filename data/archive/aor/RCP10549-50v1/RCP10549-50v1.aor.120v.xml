    <annotation>
        
        <marginalia anchor_text="numero distingua[n]t" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        numero distinguere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To distinguish by number.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="quida[m] penè dominus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        penè do[m]i[n]us quis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Nearly the master.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="non quò" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        no[n] quò
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Not that.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="verum quia non quibus verbis quiduqe dicatur quaeritur, sed quae res agatur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        verba &amp; res
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Word and issue.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="cogit is" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Cogit quis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Who thinks.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="verbo differre videbitur, te tamen erit vnum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        verbo differre re vnu[m] esse
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To differ in word, but to be one in matter.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ARMATISVE" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Armati qui
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Those armed.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        obiecto
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Objection.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="cauae no[n] ratione, sed vocibus pondera[n]tur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        caussa[m] vocib[us] ponderari
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Principles rather than voices count.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="arma" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Armoru[m] diuisio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Division of arms.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in ista tam frigida, tam ieiuna calumnia delitescas" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        in frigida ieiuna ] calu[m]nia delitescere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To take refuge in a poor and empty subterfuge.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        solutio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Resolution.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Verba
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Words.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="cum scuto, cum gladio impetum in me fecisset" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        facere impetu[m] cu[m] [ scuto gladio ] in alique[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To have an impact on someone with a shield or sword.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="vnus homo plures" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        vn[us] homo plures. porphy
                    </marginalia_text>
                    <person name="Porphyry of Tyre"/>
                </position>
            </language>
            <translation>
                One man is many. Porphyry of Tyre.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="verba" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Verba
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Words.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="quasi quida[m] penè dominus, hoc est alieni iuris vicarius, aut tuus colonus" type="straight"/>
        <underline language="LA" method="pen" text="ea intellecta, certe nihil ad rem pertinet quae verborum vis fit ac nominu[m]" type="straight"/>
        <underline language="LA" method="pen" text="non quò idem fit seruulus, quod familia" type="straight"/>
        <underline language="LA" method="pen" text="verum quia non quibus verbis quidque dicatur" type="straight"/>
        <underline language="LA" method="pen" text="quaeritur, sed quae res agatur, etiam vt iam longius à verbo recedamus: ab aequitate ne tantulum quide[m], si tuus seruus nullus fuerit, &amp;" type="straight"/>
        <underline language="LA" method="pen" text="qui congregat homines &amp; conuocat" type="straight"/>
        <underline language="LA" method="pen" text="coacti sunt ii qui ab aliquo sunt vnum in locum co[n]gregati" type="straight"/>
        <underline language="LA" method="pen" text="defendes homines coactos non fuisse: &amp; verbo quidem superabis. me ipso iudice, re autem ne consistes quidem vllo iudice" type="straight"/>
        <underline language="LA" method="pen" text="quod etiam  si verbo differre videbitur, re tamen erit vnum, &amp; omnibus in causis ide[m] valebit in quibus perspicitur vna atque eade[m] causa aequitatis" type="straight"/>
        <underline language="LA" method="pen" text="quos appellare vere possumus? opinor eos qui scutis, telísque parati, ornatíque sunt" type="straight"/>
        <underline language="LA" method="pen" text="verba si vale[n]t, si causae no[n] ratione, sed vocibus pondera[n]tur" type="straight"/>
        <underline language="LA" method="pen" text="arma esse suis nominibus, alia ad tegendum, alia ad nocendum: quae qui non habuerint, eos inermes fuisse vnices" type="straight"/>
        <underline language="LA" method="pen" text="frigida, tam ieiuna calumnia" type="straight"/>
        <underline language="LA" method="pen" text="sed perinde valebit qusi armatissimi fuerint, si reperie[n]tur ita parati fuisse vt vim vitae, aut corpori potuerint afferre" type="straight"/>
        <underline language="LA" method="pen" text="Atque vt magis intelligas quàm verba nihil valeant, Si" type="straight"/>
        <underline language="LA" method="pen" text="Atqui vide ne multo nunc sis impudentior" type="straight"/>
        <underline language="LA" method="pen" text="vnus homo plures esse homines" type="straight"/>
        <underline language="LA" method="pen" text="non verba veniunt in iudicium, sed ea res, cuius causa verba haec in interdictum coniecta sunt" type="straight"/>
        
