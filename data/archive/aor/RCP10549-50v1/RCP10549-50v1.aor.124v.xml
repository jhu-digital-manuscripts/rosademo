    <annotation>
        
        <marginalia anchor_text="insignia victoriae, non victoriam reportare[n]t" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        insignia victori[a]e no[n] victoria[m] reportare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To pring home trappings of victory, not victory.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="tempus, non ad obliuionem veteris belli, sed ad comparationem noui contulit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        co[n]ferre temp[us]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To devote time.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="aedificasset, ornassétque classes" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        aedificare ornare ] classes magnas
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To build and fit out a mighty fleet.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in locis disiunctissimis, maximéque diuersis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        loci disiunctiss[imi] maximè diuersi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The most disjointed frots, greatly diverse.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ancipiti conte[n]tione distracti" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        ancipite co[n]se[n]tio[n]e distracti
                    </marginalia_text>
                </position>
            </language>
            <translation>
                With the distraction of a twofold attack.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="multo plus firmamenti ac roboris habebat" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        multo plus firmame[n]ti ac roboris habere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be much more serious and substantial.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="is est exorsus orationis meae" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        is est exorsus or[ati]o[n]is me[a]e.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                That is the beginning of my speech.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="iniuriosius tractatis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        iniuriosi[us] tractari
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be treated wrongfully.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Corinthum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Corinthus
                    </marginalia_text>
                    <location name="Ancient Corinth"/>
                </position>
            </language>
            <translation>
                Corinth.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        A maiori
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Generally.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="regno expulsus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        regno expulsus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Driven out from the kingdom.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="quo etiam carent aegrius" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        carere aliq[uo] [a]egrius
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be without someone more reluctantly.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="impetus hostium repressos esse intelligunt ac retardatos" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        impetus hostium rep[re]ssi retardati
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To check and restrain the onslaught of enemies.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Laus pompei
                    </marginalia_text>
                    <person name="Pompey"/>
                </position>
            </language>
            <translation>
                Praise of Pompey.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        À maiori
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Generally.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Asia" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Asia
                    </marginalia_text>
                    <location name="Asia"/>
                </position>
            </language>
            <translation>
                Asia.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="magnitudine pastionis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        magnitudo pastionis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The extent of pastures.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="emergere è patrio regno" type="straight"/>
        <underline language="LA" method="pen" text="in Aisae luce versari" type="straight"/>
        <underline language="LA" method="pen" text="insignia victoriae, non victoriam reportare[n]t" type="straight"/>
        <underline language="LA" method="pen" text="duo fortissimi viri &amp; summi Imperatores: sed ita triumpharunt vt ille pulsus superatúsque regnaret" type="straight"/>
        <underline language="LA" method="pen" text="Veruntamen illis Imperatoribus laus est tribuenda, quòd egerunt: venia danda" type="straight"/>
        <underline language="LA" method="pen" text="quòd reliquerunt: propterea quòd ab eo bello Syllam" type="straight"/>
        <underline language="LA" method="pen" text="Mithridates autem omne reliquum tempus, non ad obliuionem veteris belli, sed ad comparationem noui contulit" type="straight"/>
        <underline language="LA" method="pen" text="Bosphoranis finitimis suis bellum inferre simulasset" type="straight"/>
        <underline language="LA" method="pen" text="bellum gerebamus" type="straight"/>
        <underline language="LA" method="pen" text="vt cum duobus in loci disiunctissimis" type="straight"/>
        <underline language="LA" method="pen" text="maximéque diuersis vno consilio, ab vnis hostium copiis bellum terra, maríque gereretur, vox ancipiti conte[n]tione distracti, de imperio dimicaretis" type="straight"/>
        <underline language="LA" method="pen" text="plus firmamenti ac roboris habebat" type="straight"/>
        <underline language="LA" method="pen" text="Pompeii diuino consilio ac singulari virtute depulsum est" type="straight"/>
        <underline language="LA" method="pen" text="Sed de Lucullo dica[m] alio loco" type="straight"/>
        <underline language="LA" method="pen" text="vera laus ei detracta oratione nostra, neque falsa afficta esse videatur. sed" type="straight"/>
        <underline language="LA" method="pen" text="is est exorsus orationis meae" type="straight"/>
        <underline language="LA" method="pen" text="era[n]t appellati superbius" type="straight"/>
        <underline language="LA" method="pen" text="totius Graeciae lumen extinctu[m] esse volueru[n]t" type="straight"/>
        <underline language="LA" method="pen" text="patiemini, qui" type="straight"/>
        <underline language="LA" method="pen" text="Videte, ne vt illis pulcherrimum fuit, ta[m]tam vobis imperii gloriam relinquere: sic" type="straight"/>
        <underline language="LA" method="pen" text="vobis turpissimu[m] sit, illud quod accaepistis, tueri &amp; conseruare non posse" type="straight"/>
        <underline language="LA" method="pen" text="regno expulsus est" type="straight"/>
        <underline language="LA" method="pen" text="Imperatorem à vobis certum deposcere" type="straight"/>
        <underline language="LA" method="pen" text="vident &amp; sentiunt hoc idem quod &amp; vos" type="straight"/>
        <underline language="LA" method="pen" text="quo etiam carent" type="straight"/>
        <underline language="LA" method="pen" text="Hi vos, quoniam libere loqui non licet, tacite rogant vt se quoque, sicut caeterarum prouinciarum socios, dignos existimetis quorum salutem tali viro commendetis" type="straight"/>
        <underline language="LA" method="pen" text="tamen ipsorum aduentus in vrbes sociorum non multum ab hostili expugnatione differant" type="straight"/>
        <underline language="LA" method="pen" text="vident, tanta temperantia, tanta mansuetudine, tanta humanitate, vt ii beatissimi esse videantur, apud quod ille diuertissime commoratur" type="straight"/>
        <underline language="LA" method="pen" text="gesserunt: quanto vos studio conuenit iniuriis prouocatos, sociorum salute[m] vnà cum imperii cestri dignitate defendere,              presertim cu[m] de vestris maximis vestigalibus agatur? nam caeteraru[m]" type="straight"/>
        <underline language="LA" method="pen" text="Asia veròtam opima est &amp; fertilis, vt &amp; vbertate agrorum &amp; varietate fructuum, &amp; magnitudine pastionis,              &amp; multitudine earum rerum quae exportan[tur]" type="straight"/>
        
