        <marginalia anchor_text="quatuor has res inesse oportere, scientia[] rei militaris, virtutem, authoritate[m], foelicitate[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
