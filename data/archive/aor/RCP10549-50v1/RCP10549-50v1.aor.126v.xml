    <annotation>
        
        <marginalia anchor_text="Siciliam adiit, Africam explorauit: inde Sardiniam [...] haec tria frumentaria subsidia Reip." hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Sicilia, Affrica, et Sardinia fueru[n]t reip. subsidia frume[n]taria
                    </marginalia_text>
                    <location name="Sicily"/>
                    <location name="Africa"/>
                    <location name="Sardinia"/>
                </position>
            </language>
            <translation>
                Sicily, Africa and Sardinia were the corn suplliers for the state.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="se cum in Italiam recaepisset" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        recipere se
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To hold back oneself.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="extrema hyeme" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        extrema su[m]ma ] hieme
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Extreme and highest winter.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Repetitio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Repetition.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        A maiore 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Generally.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="sermones ac literaae perferuntur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        sermones p[ro]ferri
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To deliver a speech.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Repetitio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Repetition.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="haec tria frumentaria subsidia" type="straight"/>
        <underline language="LA" method="pen" text="firmissimis praesidiis, classibúsque muniuit" type="straight"/>
        <underline language="LA" method="pen" text="in Italiam recaepisset" type="straight"/>
        <underline language="LA" method="pen" text="firmissimísque praesidiis adornauit" type="straight"/>
        <underline language="LA" method="pen" text="Brundisio profectus est" type="straight"/>
        <underline language="LA" method="pen" text="ad imperium Populi Romani Ciliciam adiunxit" type="straight"/>
        <underline language="LA" method="pen" text="huius imperio ac potestati se dedideru[n]t" type="straight"/>
        <underline language="LA" method="pen" text="quo bello omnes gentes ac nationes premeba[n]tur, Cn. Pompeius extrema hyeme apparauit, ineunte vere suscaepit, media aestate confecit" type="straight"/>
        <underline language="LA" method="pen" text="Ac primùm, quanta innocentia debe[n]t esse Imperatores? qua[n]ta deinde omnibus in rebus te[m]perantia? qua[n]ta facilitate? qua[n]to ingenio?              qua[n]ta humanitate? quae" type="straight"/>
        <underline language="LA" method="pen" text="Quirites, sed ea magis ex aliorum contentione, quàm ipsa per sese cognosci atque intelligi possunt" type="straight"/>
        <underline language="LA" method="pen" text="ad bellu[m] administrandu[m]" type="straight"/>
        <underline language="LA" method="pen" text="agnoscere videamini" type="straight"/>
        <underline language="LA" method="pen" text="ego autem neminem nomino. quare irasci mihi nemo poterit" type="straight"/>
        <underline language="LA" method="pen" text="Neque enim potest exercitum is continere Imperator qui seipsum non co[n]tinet, neque seuerus esse in iudicando qui alios in se seueros esse iudices non vult" type="straight"/>
        <underline language="LA" method="pen" text="tantum excellere caeteris" type="straight"/>
        <underline language="LA" method="pen" text="modo manus tanti exercitus, sed ne vestigium quidem cuiquam pacato nocuisse dicatur? Iam verò quemadmodum milites hyberne[n]t" type="straight"/>
        <underline language="LA" method="pen" text="hyemis enim, no[n] auaritiae perfugium maiores nostri in sociorum atque amicorum tectis esse voluerunt" type="straight"/>
        <underline language="LA" method="pen" text="non enim" type="straight"/>
        <underline language="LA" method="pen" text="eximia vis remigum, aut ars inaudita quaedam gubernandi, aut venti aliqui noui, tam celeriter in vltimas terras pertulerunt: sed hae res quae caeteros remorari solent,              non retardarunt. non auaritia ab instituto cursu ad praedam aliquam reuocauit" type="straight"/>
        <underline language="LA" method="pen" text="Itaque omnes quidem nunc in his locis Cn. Pompeium sicut aliquem non ex hac vrbe missum, sed de coelo delapssum intuentur. nunc denique incipiunt credere,              fuisse homines Romanos hac quondam abstinentia, quod iam nationibus exteris incredibile, ac falsò memoriae proditum videbatur. nunc imperii nostri splendor illis gentibus lucet" type="straight"/>
        <underline language="LA" method="pen" text="seruire Pop. Rp. quàm imperare aliis maluisse" type="straight"/>
        
