    <annotation>
        
        <marginalia anchor_text="haberet ex Nouia infante[m] filium" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        habere ex aliq[ua] infante[m] filiu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To have an infant boy from someone.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="abest à Larino dece[m] &amp; octo millia passuum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        abest à larino 10 &amp; 8 millia passuu[m]
                    </marginalia_text>
                    <location name="Larino"/>
                </position>
            </language>
            <translation>
                It is 18 miles from Larino.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in publico valens visus esset" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        valens visus esset publica
                    </marginalia_text>
                </position>
            </language>
            <translation>
                He was seen in public in good health.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="exequiaru[m]" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Exequia[e]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Funeral.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="confestim" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        co[n]festim. 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Immediately.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="funus iam sepulto filio fecit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        facere funus.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To hold a funeral.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Repetitio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Repetition.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="breuiter à me stritímque" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        breuit[er] strictim aceruatim
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Shortly, superficially, in heaps.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="nefarium &amp; consceleratum vultum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        vultus nefari[us] co[n]sceleratus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Countenance, wicked and guilt-stained.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="quisquam" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        et est quisqua[m]?
                    </marginalia_text>
                </position>
            </language>
            <translation>
                And who is it?
            </translation>
        </marginalia>
        
        <marginalia anchor_text="amita" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Amita
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Paternal aunt.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="aditu[m] sibi aliis sceleribus antèmuniuit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        aditu[m] munire
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To prepare.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="grauida Auria" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        auria grauida
                    </marginalia_text>
                    <person name="Auria"/>
                </position>
            </language>
            <translation>
                The pregnant Auria.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="appropinquare partus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        partum appropinquare.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The birth is approaching
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in ipsa significatione" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        in ip[s]a significatio[n]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                In the meaning itself.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="rebus in causa iudicióque patefactis, quis est qui illum obsolui potuisse arbitraretur? Atque haec parua sunt, cognoscite reliqua" type="straight"/>
        <underline language="LA" method="pen" text="vt non aliquando" type="straight"/>
        <underline language="LA" method="pen" text="condemnatum" type="straight"/>
        <underline language="LA" method="pen" text="Primùm" type="straight"/>
        <underline language="LA" method="pen" text="viri sui sanguine redundantem reformidat" type="straight"/>
        <underline language="LA" method="pen" text="nam cum haberet ex Nouia infante[m]" type="straight"/>
        <underline language="LA" method="pen" text="mater nihil mali misera suspicans mittit. ille" type="straight"/>
        <underline language="LA" method="pen" text="cum hora vndecima in publico valens visus esset, ante nocte[m] mortuus, &amp; postridie antequa[m] luceret, combustus est" type="straight"/>
        <underline language="LA" method="pen" text="&amp; ibi de integro funus iam sepulto filio" type="straight"/>
        <underline language="LA" method="pen" text="nec mirum, quae se no[n] nuptialibus donis, sed filioru[m] funeribus delinita[m] videret" type="straight"/>
        <underline language="LA" method="pen" text="his tantis sceleribus breuiter à me demo[n]stratis" type="straight"/>
        <underline language="LA" method="pen" text="tandem igitur animo fuisse illos arbitramini, quibus his de rebus non modo audiendu[m] fuit, verum etia[m] iudicandu[m]" type="straight"/>
        <underline language="LA" method="pen" text="potestis: de eo, qui &amp;" type="straight"/>
        <underline language="LA" method="pen" text="quem leges exilio, natura morte multauit" type="straight"/>
        <underline language="LA" method="pen" text="auditis cu[m] ea quae copiosissime dici possunt, breuiter à me strictímque dicuntur" type="straight"/>
        <underline language="LA" method="pen" text="nefarium &amp; consceleratum vultum intuebantur: de" type="straight"/>
        <underline language="LA" method="pen" text="à P. Cannutio homine eloquentissimo" type="straight"/>
        <underline language="LA" method="pen" text="Aceruatim iam reliqua Iudices dicam, vt ad ea quae propriora huius causae &amp; adiunctiora sunt, perueniam" type="straight"/>
        <underline language="LA" method="pen" text="hominem sceleratissimum &amp; nocentissimu[m]" type="straight"/>
        <underline language="LA" method="pen" text="aditu[m] sibi aliis sceleribus antè muniuit" type="straight"/>
        <underline language="LA" method="pen" text="iam appropinquare partus videretur" type="straight"/>
        <underline language="LA" method="pen" text="testame[n]túmque mutare cuperet" type="straight"/>
        <underline language="LA" method="pen" text="in ipsa significatione huius" type="straight"/>
        <underline language="LA" method="pen" text="muliere[m], nepartu eius ab haereditate fraterna excluderetur, necauit. fratris aute[m] liberos prius vita priuauit, quàm illi hanc" type="straight"/>
        
