        <underline language="LA" method="pen" text="partum sibi ipsa medicamentis abegisset" type="straight"/>
        <underline language="LA" method="pen" text="capitalis esse damnatam" type="straight"/>
        <underline language="LA" method="pen" text="siquidem illa cu[m] suo corpori vim attulisset, seipsam cruciauit" type="straight"/>
        <underline language="LA" method="pen" text="est qui in vno corpore plures necaret" type="straight"/>
        <underline language="LA" method="pen" text="quoad pareret, habitaret: diligentiámque adhiberet, vt id quod concaeperat seruaret, vt saluum parere posset" type="straight"/>
        <underline language="LA" method="pen" text="Quid de Oppianico suspicatus sit, videtis. quid iudicarit, obscurum non" type="straight"/>
        <underline language="LA" method="pen" text="Quid Oppianicus fecerit cognoscite: vt illum" type="straight"/>
        <underline language="LA" method="pen" text="merces abortionis" type="straight"/>
        <underline language="LA" method="pen" text="illam, quam in aluo co[m]mendatam à viro continebat, victa auaritia sceleri Oppianici vendidit" type="straight"/>
        <underline language="LA" method="pen" text="Attendite" type="straight"/>
        <underline language="LA" method="pen" text="erant enim non matrimonii dignitate, sed sceleris societate co[n]iunctae" type="straight"/>
        <underline language="LA" method="pen" text="Fuit Aiulius quidam Larinas perdita nequitia &amp; summa egestate, arte quada[m] praeditus ad libidines adolescentuloru[m] excitandas acco[m]modata. qui" type="straight"/>
        <underline language="LA" method="pen" text="&amp; fortunas eius patrias expugnare posse" type="straight"/>
        <underline language="LA" method="pen" text="facilius in solitudine, perfici rem eiusmodi commodius in tubra posse arbitrati sunt" type="straight"/>
        <underline language="LA" method="pen" text="Romae vixerint, quibus conuiuiis, quibus flagitiis, quantis &amp; quàm profusis sumptibus, non modo conscio, sed etiam co[n]uiua &amp; adiutore Oppianico, longum est mihi dicere, presertim ad alia properanti" type="straight"/>
        <underline language="LA" method="pen" text="conualescit" type="straight"/>
        <underline language="LA" method="pen" text="atque ille continuo nullo teste, nullo indice, rece[n]tis maleficii co[n]scientia perterritus, omnia" type="straight"/>
        
