            </translation>
        </marginalia>
        
        <marginalia anchor_text="collo obtorto" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Collo obtorto
                    </marginalia_text>
                </position>
            </language>
