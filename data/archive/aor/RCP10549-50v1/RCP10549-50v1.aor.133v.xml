            </language>
            <translation>
                Titus Accius.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Extat memoria" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        extat memoria
                    </marginalia_text>
                </position>
            </language>
            <translation>
                There is living memory.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="redargue me, si mentior" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        redargue me, si me[n]tior.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Contradict me if I am wrong.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="occurra[m] expectationi" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        occurrere expectatio[n]i
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To meet the impatience.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="à vobis sic audior" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        a vobis sic audior
                    </marginalia_text>
                </position>
            </language>
            <translation>
                I hear from you.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="obloqui" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        obloqui
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To interrupt.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="eloquentia, sed defensionis impude[n]tia co[m]moueri" type="straight"/>
        <underline language="LA" method="pen" text="Posteaquam de re coepit dicere, ad ea quae erant in causa addebat etiam ipse noua quaeda[m] vulnera. hoc" type="straight"/>
        <underline language="LA" method="pen" text="&amp; cu[m] illa verba grauissima ex intimo artificio depro[m]psisset" type="straight"/>
        <underline language="LA" method="pen" text="Respicite Iudices hominum fortunas, respicite dubios variósq[ue] casus, respicite C. Fabritii senectute[m]:              cum hoc REPICITE orna[n]dae orationis causa saepe dixisset, respexit ipse: at C." type="straight"/>
        <underline language="LA" method="pen" text="&amp; collo obtorto" type="straight"/>
        <underline language="LA" method="pen" text="deinde legis ci &amp; sententiis iudicum est condemnatus" type="straight"/>
        <underline language="LA" method="pen" text="Quòd so hoc videtis, quod iam hac omni oratione patefactum est, illo iudicio" type="straight"/>
        <underline language="LA" method="pen" text="Extat memoria" type="straight"/>
        <underline language="LA" method="pen" text="redargue me, si mentior" type="straight"/>
        <underline language="LA" method="pen" text="multa dici possunt quare ita necesse fuerit iudicari. sed ego occurra[m] expectationi vestrae Iudices" type="straight"/>
        <underline language="LA" method="pen" text="etsi à vobis sic audior, vt nu[m]quam benignius, neque attentius quenquam auditum putem:              tamen vocat me aliò iandudum tacita vestra expectatio quae mihi obloqui videtur" type="straight"/>
        
