        <underline language="LA" method="pen" text="qui quoda[m] odore suspitionis Stalenum corruptu[m] esse sensisset" type="straight"/>
        <underline language="LA" method="pen" text="summa omnium expectatio quidna[m] sente[n]tiae ferre[n]t leues ac nu[m]marii iudices" type="straight"/>
        <underline language="LA" method="pen" text="qui neque absoluere hominum nocentissimum, possent, neque eum de quo esset orta suspitio, pecunia oppugnatum, re illa incognita primo" type="straight"/>
        
