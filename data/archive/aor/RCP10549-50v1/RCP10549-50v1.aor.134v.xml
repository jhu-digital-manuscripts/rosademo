    <annotation>
        
        <marginalia anchor_text="versare mentem" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        versare me[n]tem
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To turn one's mind.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="initium facit à Bulbo" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Initiu[m] facit à bulbo
                    </marginalia_text>
                </position>
            </language>
            <translation>
                He began with Bulbus.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="quo voles, inquit, sequar" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        q[uo] voles sequar
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Where you wish to follow.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="co[n]ditor totius negotii" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        co[n]ditor toti[us] negotij
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The founder of the whole business.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="nationis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Natio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A people.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="contrahit fro[n]tem" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        co[n]trahere fro[n]te[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To wrinkle the forehead.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Manarat sermo" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        sermone[m] manare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To leak a rumour.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="quoda[m] odore suspitionis Stalenum corruptu[m] esse sensisset" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        corruptu[m] odore q[uo]da suspitio[n]is se[n]tire aliquem
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To discern by an air of suspicion that someone was bribed.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="conuitium C. Iunio iudici quaestionis maximum fecit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        co[n]vitiu[m] facere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To make angry noise.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="co[n]surgitur in co[n]silium" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        co[n]surgitur in co[n]siliu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To stand up in council.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="iniectus est hominibus scrupulus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        scrupulus iniectus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Made uneasy.
            </translation>
        </marginalia>
        
