        <marginalia anchor_text="His de causis C. Iunius condemnatus est Iudices, leuissimis &amp; infirmissimis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        caussa leuissima infirmissima
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A case, most trivial and unsubstantial.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="exlegem esse Syllam, aut causam pecuniae publicae contemptam" hand="English_secretary">
            <language ident="LA">
