    <page filename="RCP10549-50v1.139v.tif" pagination="258" reader="John Dee" signature="[rr.i.[v]]"/>
    <annotation>
        
        <marginalia anchor_text="quàm iuste? quàm sapienter? quàm diligenter?" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        iuste sapie[n]ter diligenter ]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Justly, wisely, diligently.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="pueri paruuli" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        puer paruulus.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A little boy.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="mare quod sua natura tranquillum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Mare sua natura tra[n]quillu[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A sea, calm by its nature.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Comparatio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Comparison.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="turpiter penè praeterii" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        turpit[er] penè p[rae]terij.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To my shame, I will go past.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="perinde quasi" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        perinde q[ua]si
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Just as if.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="M. Antonium aiu[n]t solitu[m] esse dicere, idcirco se nullam vnquam orationem scripsisse" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Quin Marc[us] antoni[us] nu[n]qua[m] or[ati]o[n]em scripsit 
                    </marginalia_text>
                    <person name="Mark Antony"/>
                </position>
            </language>
            <translation>
                In fact, Mark Antony never wrote his speeches.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="facile illis ipsis iudicibus &amp; falsae suspitioni &amp; inuidiae" type="straight"/>
        <underline language="LA" method="pen" text="Vnum etia[m] est quod me maxime conturbat: cui loco respondere posse vix videor" type="straight"/>
        <underline language="LA" method="pen" text="honestissimi videlicet" type="straight"/>
        <underline language="LA" method="pen" text="cum vir optimus &amp; homo" type="straight"/>
        <underline language="LA" method="pen" text="Senatus decreuerat ad illud inuidiae praesens incendiu[m] restinguendum, id" type="straight"/>
        <underline language="LA" method="pen" text="vt mare quod sua natura tranquillum sit, ventorum vi agitari atque turbari: sic &amp; Populum Ro. sua sponte esse placatum, hominum seditiosorum vocibus,              vt violentissimis tempestatibus concitari" type="straight"/>
        <underline language="LA" method="pen" text="perinde quasi" type="straight"/>
        <underline language="LA" method="pen" text="Ego vero siquid eiusmodi dixi, neque cognitum co[m]memoraui, neque pro testimonio dixi: &amp; illa oratio potius te[m]poris mei,              quàm iudicii &amp; authoritatis fuit" type="straight"/>
        <underline language="LA" method="pen" text="Sed errat veheme[n]ter, siquis" type="straight"/>
        <underline language="LA" method="pen" text="nostris quas in" type="straight"/>
        <underline language="LA" method="pen" text="omnes" type="straight"/>
        <underline language="LA" method="pen" text="Nam si causae ipsae pro se loqui possent, nemo adhiberet oratorem. nunc" type="straight"/>
        <underline language="LA" method="pen" text="Homine[m] ingeniosum M. Antonium aiu[n]t solitu[m] esse dicere, idcrico se nullam vnquam orationem scripsisse, vt siquid aliquando quod non opus esset,              ab se esset dictum, posset se negare dixisse. perinde quasi quid à nobis dictum aut actum sit, id nisi literis mandaueri[mus]" type="straight"/>
        
