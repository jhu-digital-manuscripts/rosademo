    <annotation>
        
        <marginalia anchor_text="leuia genere ipso" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        leuia genere ip[s]o
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Essentially trivial.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="veneno esse sublatu[m]" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        tolli veneno
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be removed by poison.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="intestatum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        intestatus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Intestate.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in mulso" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        mulsu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Mead.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="intercaepisse" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        intercapere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To intercept.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="intercaeptio poculi" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        interceptio poculi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Interception of the cup.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="epoto poculo" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        epoto poculo
                    </marginalia_text>
                </position>
            </language>
            <translation>
                By having drunk up the cup.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="homines, inimicos suos morte affici" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Cur ho[m]i[n]es inimicos morte afficiunt
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Why people kill their enemies.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="quo de homine nihil etia[m] nunc dicere nobis est necesse" type="straight"/>
        <underline language="LA" method="pen" text="annos octo meditati accusatores collegeru[n]t, quàm leuia genere ipso, quàm falsa re, quàm breuia responsu" type="straight"/>
        <underline language="LA" method="pen" text="quàm paucis verbis haec causa perorari potuerit, &amp; quàm multa à me dicta sint, quae ad huius voluntate[m] maxime ad vestru[m] iudicium minime pertinerent" type="straight"/>
        <underline language="LA" method="pen" text="Opportune adest homo summa fide &amp; omni" type="straight"/>
        <underline language="LA" method="pen" text="adolesce[n]ti pudentissimo, &amp; in primis honesto Equiti Ro. data[m]" type="straight"/>
        <underline language="LA" method="pen" text="intercaepisse, bibisse" type="straight"/>
        <underline language="LA" method="pen" text="hoc ego si sic agere[m], tanquam mihi crime[n] esset diluendum, haec pluribus verbis dicere[m], quae nu[n]c paucis percurrit oratio mea" type="straight"/>
        <underline language="LA" method="pen" text="Multa sunt quae dici possunt, sed non co[m]mittam vt videar non dice[n]do voluisse dicere" type="straight"/>
        <underline language="LA" method="pen" text="magnum &amp; impude[n]s mandaciu[m]. perspicite" type="straight"/>
        <underline language="LA" method="pen" text="crudior venisset" type="straight"/>
        <underline language="LA" method="pen" text="paulisper exurge, &amp; perfer hunc dolorem commemorationis necessariae, in qua ego diutius non morabor: quoniam quod" type="straight"/>
        <underline language="LA" method="pen" text="tuus calamitatem &amp; falsum crimen" type="straight"/>
        <underline language="LA" method="pen" text="Sed homines, inimicos suos morte affici volunt, vel quòd metuunt, vel quòd oderunt: quo tandem" type="straight"/>
        
