                    <marginalia_text>
                        domina
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Mistress.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="constituitur quaestio" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        questio[n]em co[n]stitui
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To hold an enquiry.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in quaestionem" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        in questio[n]em postulare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To enquire once again.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in quaestionem tulit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
