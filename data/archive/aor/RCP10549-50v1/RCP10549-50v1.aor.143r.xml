            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Crudelitas ac furtu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Cruelty and theft.
            </translation>
        </marginalia>
        
