                    <marginalia_text>
                        Tortor
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Torturer.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="tormenta ipsa defessa" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Torme[n]ta defessa
                    </marginalia_text>
                </position>
