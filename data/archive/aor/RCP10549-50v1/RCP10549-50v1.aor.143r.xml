            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        furtu[m] co[m]moueri in alique[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The theft falls on someone.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="exectio" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        exectio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The cutting out.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="aduncam ex omni parte dentium, &amp; tortuosam venire serrulam" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        serrula adu[n]ca ex omni parte de[n]tiu[m] tortuosa
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A little saw, curved from all sides with dents and crooked.
            </translation>
        </marginalia>
        
