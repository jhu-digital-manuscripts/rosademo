        <marginalia anchor_text="solicitationes seruorum &amp; minis &amp; promissis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        solicitatioes seruoru[m] minis promissis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Overtures to the slaves, by threat and promisses.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="conscelerata exectio linguae" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        co[n]scelerata exectio lingu[a]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Dishonourable cutting out of the tongue.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="propter vicinitatem" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        propter vicinitate[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Because of the vicinity.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Nemo erat illorum penè dicam" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        nemo erat illoru[m] penè dica[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                No one of them was there, I almost say.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="fidem precio labefactare" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        fide[m] precio labefactare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To undermine loyalty with a bribe.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Deus malis aduersatur
                    </marginalia_text>
                </position>
            </language>
            <translation>
                God is against evil.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        pare[n]s pro liberis arguatur
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A parent arguing for their childs.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Petitio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Petition.
            </translation>
        </marginalia>
        
