    <page filename="RCP10549-50v1.147r.tif" pagination="273" reader="John Dee" signature="ss.i."/>
    <annotation>
        
        <marginalia anchor_text="perlongo interuallo propè memoriae" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        p[er]longo interuallo memori[a]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                After a very long period of memory.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="sine repulsa Consules facti sunt" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        sine repulsa co[n]sule[m] fieri
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be made consul without rejection.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Cicero primus consul fuit
                    </marginalia_text>
                    <person name="Cicero"/>
                </position>
            </language>
            <translation>
                Cicero was first consul.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="tam insigne, tam singulare vestrum beneficium" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        beneficiu[m] insigne singulare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A favour, remarkable and extraordinary.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="cogitationes, quae mihi nullam partem neque diurnae neque nocturnae" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Cogitatio[n]es nocturn[a]e diurn[a]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Thoughts at day and night.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        propositio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Proposal.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="esse locus huic voci" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        locu[m] esse huic voci
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be a place for this voice.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="de tantis vestris beneficiis nullo modo possum" type="straight"/>
        <underline language="LA" method="pen" text="&amp; eum locum, quem nobilitas praesidiis firmatum, atque omni ratione obuallatum tenebat, me duce rescidistis, virtutíque" type="straight"/>
        <underline language="LA" method="pen" text="vt vester honos ad mei temporis diem petitus, non ad alienae petitionis occasionem intercaeptus, nec" type="straight"/>
        <underline language="LA" method="pen" text="Sed tame[n] magnificentius, atque ornatius esse illo nihil potest, quòd meis comitiis non tabellam indicem tacitae libertatis, sed vocem viuam prae vobis,              indicem vestrarum erga me voluntatum" type="straight"/>
        <underline language="LA" method="pen" text="Hoc ego tam insigne, tam singulare vestrum beneficium Quirites, cum ad animi mei fructum, atque laetitiam duco esse permagnum, tum ad curam,              sollicitudinémque multo maius" type="straight"/>
        <underline language="LA" method="pen" text="Versantur enim Quirites in animo meo multae, &amp; graues cogitationes, quae mihi nullam partem neque diurnae neque nocturnae quietis impartiunt" type="straight"/>
        <underline language="LA" method="pen" text="cui errato nulla venia, recte facto exigua laus, &amp; ab inuitis expressa proponitur" type="straight"/>
        <underline language="LA" method="pen" text="omnia potius perpetienda esse duco: quàm non ita gerendum Consulatum, vt in omnibus meis factis, atque co[n]siliis, vestru[m] de me factu[m],              consiliúmque laudetur" type="straight"/>
        <underline language="LA" method="pen" text="vt nobilissimis hominibus lo[n]ge praeponerer: no[n] &amp; in hoc magistratu" type="straight"/>
        <underline language="LA" method="pen" text="sed mihi ad huiusce verbi vim, &amp; interpretationem vehementer opus est vestra sapientia" type="straight"/>
        <underline language="LA" method="pen" text="qui cum populi non solum commoda, verumetiam salutem" type="straight"/>
        <underline language="LA" method="pen" text="plena[m] sollicitudinis" type="straight"/>
        <underline language="LA" method="pen" text="timoris: in qua nihil erat mali, nihil aduersi, quod non boni metuerent, improbi expectarent" type="straight"/>
        <underline language="LA" method="pen" text="Omnia turbulenta consilia contra hunc Reipublicae statum, &amp; contra vestrum" type="straight"/>
        
