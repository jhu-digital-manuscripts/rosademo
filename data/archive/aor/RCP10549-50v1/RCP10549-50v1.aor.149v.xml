        <underline language="LA" method="pen" text="Verbum mihi deest Quirites, cum ego hanc potestatem regiam" type="straight"/>
        <underline language="LA" method="pen" text="cur hoc tam est obscurum atque" type="straight"/>
        <underline language="LA" method="pen" text="fortasse propter" type="straight"/>
        <underline language="LA" method="pen" text="magna potestas permittitur. nisi fortè nihil est aequum vos de eorum audacia suspicari, quorum cupiditati nimium angustus" type="straight"/>
        <underline language="LA" method="pen" text="terrarum esse videatur" type="straight"/>
        <underline language="LA" method="pen" text="intelligo: sed attendite animos ad ea quae consequuntur: hunc quasi gradum" type="straight"/>
        <underline language="LA" method="pen" text="atque aditum ad caetera iactum intelligetis" type="straight"/>
        <underline language="LA" method="pen" text="longum putauit, &amp; timuit nequid praeteriret. adscripsit" type="straight"/>
        <underline language="LA" method="pen" text="Pergamum" type="straight"/>
        <underline language="LA" method="pen" text="vtrum oratio ad eius rei discaeptationem deerit: an cum idem &amp; disseret" type="straight"/>
        <underline language="LA" method="pen" text="falsum" type="straight"/>
        <underline language="LA" method="pen" text="quid, quòd disputari contrà nullo pacto potest? quoniam statutum" type="straight"/>
        <underline language="LA" method="pen" text="vobis est &amp; iudicatum" type="straight"/>
        <underline language="LA" method="pen" text="vrbs &amp; natura &amp; situ, &amp; descriptio[ne]" type="straight"/>
        
