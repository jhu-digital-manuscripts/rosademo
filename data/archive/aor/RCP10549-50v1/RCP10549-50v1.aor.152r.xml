<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v1.152r.tif" pagination="283" reader="John Dee"/>
    <annotation>
        
        <marginalia anchor_text="horreba[n]t, vestra[m] vim metuebant, mentionem legis Agrariae pertimescebant" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        horrere metuere p[er]timescere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To shudder, to dread, to tremble.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="carme[n] hic Tribunus ple. non vobis, sed sibi intus canit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Carme[n] sibi intus canere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To sing a song for oneself.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="inuidiam deponere" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        inuidia[m] deponere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To put down hate.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="propter vastitatem" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        vastitas
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Devastation.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ingenti pecunia vobis inducetur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        inge[n]ti pecunia induci
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be charged a huge sum.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="adducentur pecuniae magnitudine" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        adduci pecuni[a]e magnitudi[n]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be adduced a great amount of money.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="diligentiam maiorum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Diligentia maiorum
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The diligence of the ancestors.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="coerceas? opprimas? redigas" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        coercere opprimere redigere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To coerce, to crush, to rule it.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="rogabuntur atque orabu[n]tur, vt agros partim" type="straight"/>
        <underline language="LA" method="pen" text="atque hoc carme[n] hic Tribunus ple." type="straight"/>
        <underline language="LA" method="pen" text="pecuniam condere" type="straight"/>
        <underline language="LA" method="pen" text="maiorum vestrorum sanguine &amp; sudore" type="straight"/>
        <underline language="LA" method="pen" text="quoquo precio coemptus erit, tamen ingenti pecunia" type="straight"/>
        <underline language="LA" method="pen" text="sterilitatem incultum" type="straight"/>
        <underline language="LA" method="pen" text="quasi de aliqua sentina, ac non de optimorum ciuiu[m] genere loqueretur. Vos" type="straight"/>
        <underline language="LA" method="pen" text="retinete istam possessione[m] gratiae, libertatis, suffragioru[m], dignitatis, vrbis, fori, ludorum, festorum dierum, caeterorum omnium co[m]modorum" type="straight"/>
        <underline language="LA" method="pen" text="vectigalibus abalienatis, sociis vexatis, regibus atque omnibus gentibus exinanitis, illi pecunias habebunt, vos agros no[n] habebitis. Facile" type="straight"/>
        <underline language="LA" method="pen" text="Reipub. partibus, est opraeprecium diligentiam" type="straight"/>
        <underline language="LA" method="pen" text="recordari, qui colonias sic idoneis in locis contra" type="straight"/>
        <underline language="LA" method="pen" text="suum praesidium" type="straight"/>
        <underline language="LA" method="pen" text="vestris possint collocare" type="straight"/>
        
