        <marginalia anchor_text="refricare obductam iam Reipub. cicatrice[m]" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        refricare obducta[m] Reip. 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To reopen an wound of the state.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="interrex" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
