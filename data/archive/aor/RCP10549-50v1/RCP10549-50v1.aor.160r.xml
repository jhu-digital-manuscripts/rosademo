                    <marginalia_text>
                        Conflare invidiam
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To kindle hate.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Aquilam illam argentea[m]" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Aquila arge[n]tea
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A silver eagle.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="bacchabere" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        bacchari
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To rave.
            </translation>
