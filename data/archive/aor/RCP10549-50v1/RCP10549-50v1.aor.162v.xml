        <underline language="LA" method="pen" text="id quod stultissimum" type="straight"/>
        <underline language="LA" method="pen" text="aut deduci de sententia" type="straight"/>
        <underline language="LA" method="pen" text="deinde magnos animos esse in bonis viris" type="straight"/>
        <underline language="LA" method="pen" text="magnam concordiam, maximam multitudine[m], magnas praeterea copias militum, deos denique immortales huic inuicto populo, clarissimo imperio,              pulcherrimae vrbi, contra ta[n]tam vim sceleris praesentis auxilium esse laturos" type="straight"/>
        <underline language="LA" method="pen" text="aetate iam confectu[m], exercitatione robustum: quo ex" type="straight"/>
        <underline language="LA" method="pen" text="repentinísque pecuniis sumptuosius, insolentiúmque iactarunt" type="straight"/>
        <underline language="LA" method="pen" text="pecudes quidem mihi passurae esse videantur" type="straight"/>
        <underline language="LA" method="pen" text="&amp; turbulentum" type="straight"/>
        <underline language="LA" method="pen" text="partim male gerendo negocio, partim etiam sumptibus in vetere aere alieno" type="straight"/>
        <underline language="LA" method="pen" text="qui homines primùm si stare non possunt, corruant" type="straight"/>
        <underline language="LA" method="pen" text="viuere honeste no[n] possunt, perire turpiter velint: aut" type="straight"/>
        <underline language="LA" method="pen" text="omnium facinorosoru[m], quos ego" type="straight"/>
        <underline language="LA" method="pen" text="&amp; pereant sanè in latrocinio, quoniam sunt ita multi, vt eos carcer capere no[n]" type="straight"/>
        <underline language="LA" method="pen" text="proprium est Catilinae, de eius delectu, imò verò de complexu eius ac sinu, quos pexo capillo nitidos, aut imberbes, aut bene barbatos videtis,              manicatis, &amp; talaribus tunicis, velis amictos non togis" type="straight"/>
        <underline language="LA" method="pen" text="coenis expromitur" type="straight"/>
        <underline language="LA" method="pen" text="in his gregibus omnes aleatores, omnes adulteri, omnes impuri, impudicíque versantur. hi pueri tam lepidi, ac delicati, non solum amare &amp;,              neque cantare &amp; psallere, sed etiam sicas vibrare, &amp; spargere venena didicerunt: qui nisi exeunt, nisi pereunt, etiam si Catilina perierit, scitote hoc in Rep. seminarium Catilinarium futurum" type="straight"/>
        <underline language="LA" method="pen" text="atque pruinas ac niues perferent?" type="straight"/>
        <underline language="LA" method="pen" text="quòd in conuiuiis nudi saltare didiceru[n]t" type="straight"/>
        
