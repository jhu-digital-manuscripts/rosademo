    <page filename="RCP10549-50v1.169r.tif" pagination="317" reader="John Dee"/>
    <annotation>
        
        <marginalia anchor_text="summam mihi superbiae, crudelitatísque famam inussisset" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        inurere su[m]ma fama[m] superbi[a]e crudelitatis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To brand with the greatest fame of pride and cruelty.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="astuti &amp; ingrati" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Astut[us] &amp; Ingrat[us]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Clever and ungrateful.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="quem contra" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        que[m] co[n]tra
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Against him.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Saltatorem" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        saltator
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Dancer.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="conuiciatoris" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        co[n]viciator
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Abuser.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="sed ne concedi quidem potest vt amicis nostris accusantibus, non etiam alienissimos defendamus" type="straight"/>
        <underline language="LA" method="pen" text="Nam cum praemia mihi tanta pro hac industra sint data, qua[n]ta antea nemini: labores per quos ea caeperis, cu[m] adeptus sis, deponere,              esset hominis &amp; astuti &amp; ingrati" type="straight"/>
        <underline language="LA" method="pen" text="quem contra" type="straight"/>
        <underline language="LA" method="pen" text="cum tui fontes vel inimicis tuis pateant, nostros riuulos etiam amicis putes clausos esse oportere" type="straight"/>
        <underline language="LA" method="pen" text="existimarem nefarium si amico, crudelem si misero, superbum si Consuli defuissem" type="straight"/>
        <underline language="LA" method="pen" text="quod da[n]dum est amicitiae, large dabitur à me, vt tecum agam Serui non secus" type="straight"/>
        <underline language="LA" method="pen" text="quod tribuendu[m] est officio, fidei, religioni, id ita moderabor, vt meminerim me co[n]tra amici studiu[m] pro amici periculo dicere" type="straight"/>
        <underline language="LA" method="pen" text="illa, quae grauissima esse debebat, ita fuit infirma &amp; leuis, vt illos lex magis quaedam accusatoria, quàm vera maledicendi facultas de vita L. Murenae dicere" type="straight"/>
        <underline language="LA" method="pen" text="ex qua laus familiae, memoria generi, honos &amp; gloria nomini constituta est" type="straight"/>
        <underline language="LA" method="pen" text="in hisce rebus loci est, quòd omnia" type="straight"/>
        
