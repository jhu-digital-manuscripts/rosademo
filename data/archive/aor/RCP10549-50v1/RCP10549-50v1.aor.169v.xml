    <annotation>
        
        <marginalia anchor_text="arripere maledictum ex triuio" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        arripere maledictu[m] ex triuio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To pick a piece of dirt from the street corner.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="nemo enim ferè saltat sobrius" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Nemo ferè saltat sobrius
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Almost no one sober dances.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="saltatio" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Saltatio vnde
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Enjoyment therefrom.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="iacta sunt fundamenta defensionis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        iacta su[n]t fundame[n]ta defe[n]sionis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The foundations of his defense have been laid.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        à genere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Generally.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="hominibus literatis &amp; historicis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        ho[m]i[n]es litterati historici
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Men of letters and antiquarians.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="annalium" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Annales
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Ancient documents.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ignobilitas generis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Ignobilitas generis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Lack of nobility.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="claustra ista nobilitatis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        claustra nobilitatis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Protected nobility.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="prouinciam tacitam &amp; quietam. tu illam, cui cum Quaestores sortiuntur, etiam acclamari solet, Hostiensem, non tam gratiosa &amp; illustrem, quàm negociosam &amp; molestam" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        proui[n]cia [ tacita quieta gratiosa illustris ] negociosa molesta
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A province, quiet, peaceful, kind, famous, occupied and troublesome.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="nemo enim ferè saltat sobrius, nisi forte insanit, neque in solitudine, neque in conuiuio moderato atque honesto. intempestiui conuiuii, amoeni loci, multarum delitiarum comes est extrema, saltatio" type="straight"/>
        <underline language="LA" method="pen" text="in eo te vmbram" type="straight"/>
        <underline language="LA" method="pen" text="nullum petulans dictum in vita" type="straight"/>
        <underline language="LA" method="pen" text="fundamenta defensionis. nondum" type="straight"/>
        <underline language="LA" method="pen" text="quo constituto facilior est mihi aditus ad contentionem dignitatis, quae pars altera fuit accusationis" type="straight"/>
        <underline language="LA" method="pen" text="Patria cognosco esse ista in L. Murena, atque ita paria, vt neque ipse dignitate vinci potuerit, neque te dignitate superarit" type="straight"/>
        <underline language="LA" method="pen" text="hominibus literatis &amp; historicis est notior" type="straight"/>
        <underline language="LA" method="pen" text="itaque non ex sermone hominum recenti, sed ex annalium vetustate erue[n]da est memoria nobilitatis tuae" type="straight"/>
        <underline language="LA" method="pen" text="memoriam propè intermortua[m] generis sui, virtute renouare" type="straight"/>
        <underline language="LA" method="pen" text="Cum verò ego tanto interuallo claustra ista nobilitatis refregissem" type="straight"/>
        <underline language="LA" method="pen" text="non magis nobilitati quàm virtuti pateret" type="straight"/>
        <underline language="LA" method="pen" text="superaui tame[n] dignitate Catilinam" type="straight"/>
        <underline language="LA" method="pen" text="Omittamus igitur de genere dicere, cuius est magna in vtrque dignitas" type="straight"/>
        <underline language="LA" method="pen" text="videamus" type="straight"/>
        <underline language="LA" method="pen" text="non est respondendum" type="straight"/>
        <underline language="LA" method="pen" text="omnia" type="straight"/>
        
