    <page filename="RCP10549-50v1.173r.tif" pagination="325" reader="John Dee" signature="xx.iii."/>
    <annotation>
        
        <marginalia anchor_text="duo corpora esse Reipublicae" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Duo corpora republi.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Two bodies of the state.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Congemuit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Congemescere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To groan.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="satis seuere pro rei indignitate" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        satis seuere p[ro] rei [dig - deleted] indignitate
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Severe enough for the indignity of the matter.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="perpurgatus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        perpurgatus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Cleansed.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="desultorius" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        desultorius
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Very superficially.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="vnum debile infirmo capite, alterum firmum sine capite. huic" type="straight"/>
        <underline language="LA" method="pen" text="seuere pro rei indignitate decreuit" type="straight"/>
        <underline language="LA" method="pen" text="etenim sciebam Catilinam non latus aut ventrem, sed caput &amp; collum solere petere" type="straight"/>
        <underline language="LA" method="pen" text="Magna est autem comitiis Consularibus" type="straight"/>
        <underline language="LA" method="pen" text="repentina voluntatum inclinatio, praesertim cum incubuit ad virum bonu[m] &amp; multis aliis adiume[n]tis petitionis ornatu[m]" type="straight"/>
        <underline language="LA" method="pen" text="Nunc mihi tertius ille locus est orationis de ambitus criminibus, perpurgatus ab iis, qui ante me dixerunt: à me quoniam ita Murena voluit, retractandus. quo" type="straight"/>
        <underline language="LA" method="pen" text="ingenioso &amp; bono, Seruio" type="straight"/>
        <underline language="LA" method="pen" text="homini in omni" type="straight"/>
        <underline language="LA" method="pen" text="sed pauca quae meum animum repente mouerunt, prius de L. Murenae fortuna conquerar" type="straight"/>
        <underline language="LA" method="pen" text="vt non queam satis, neque communem omnium nostrum conditionem, neque huius euentum fortunámque miserari. qui primùm" type="straight"/>
        <underline language="LA" method="pen" text="continuis familiae maiorúmque suoru[m] vnum asce[n]dere gradum dignitatis conatus" type="straight"/>
        <underline language="LA" method="pen" text="venit in periculum, ne &amp; ea quae relicta, &amp; haec quae ab ipso parta sunt, amittat" type="straight"/>
        <underline language="LA" method="pen" text="propter studiu[m] nouae laudis etiam in veteris" type="straight"/>
        <underline language="LA" method="pen" text="Quae cum sunt grauia Iudices, tum illud acerbissimum est, quod habet eos accusatores, non qui odio inimicitiarum ad accusandum,              sed qui studio accusandi ad inimicitias descenderent. nam" type="straight"/>
        <underline language="LA" method="pen" text="nobis erat in hac ciuitate natus, vt eius opes &amp; ingenium" type="straight"/>
        <underline language="LA" method="pen" text="vix cuiquam inimicus esse deberet" type="straight"/>
        <underline language="LA" method="pen" text="quasi desultorius in quadrigarum curriculum incurrere" type="straight"/>
        
