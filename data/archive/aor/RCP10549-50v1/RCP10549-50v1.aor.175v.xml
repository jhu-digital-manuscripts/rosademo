    <annotation>
        
        <marginalia anchor_text="in exitu iam est meus co[n]sulatus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        co[n]sulatu[m] esse in exitu
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The consulship is at its end.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="intonuit vox perniciosa" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        intonuit vox perniciosa
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The sinister voice thunders.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="vigilantem Consulem" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        vigilans co[n]sul
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A vigilant consul.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ne sufficiatur Consul" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        ne suffriciatur consul.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Lest a consul is elected.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="non tibi, sed patriae natus es" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        No[n] nobis sed patri[a]e nati
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be born not for our, but the country's sake.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in discrimen extremum venimus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        in discrime[n] extremu[m] venire
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To come to the end of a road [Loeb 324, 291].
            </translation>
        </marginalia>
        
