        <underline language="LA" method="pen" text="casus est enim in capiendo, locus, euentus, occasio. defendendi facilis est cautio, no[n] solum latibulis occultorum locorum,              sed etiam tempestatum moderatione &amp; co[n]uersione" type="straight"/>
        <underline language="LA" method="pen" text="ciuitates proportione in prouincias descripsisset" type="straight"/>
        <underline language="LA" method="pen" text="Non refert vero" type="straight"/>
        <underline language="LA" method="pen" text="fratris mei laude delector, sed" type="straight"/>
        <underline language="LA" method="pen" text="Citat" type="straight"/>
        <underline language="LA" method="pen" text="vnus Asclepiades" type="straight"/>
        <underline language="LA" method="pen" text="etiámne praecone[m] me[n]tiri coegisti" type="straight"/>
        <underline language="LA" method="pen" text="quas ego no[n] solu[m] propter longitudine[m], sed eita[m] propter turpissima[m] obscoenitatem verboru[m] praetereundas puto. dixit" type="straight"/>
        <underline language="LA" method="pen" text="fortuna egens, vita turpis, existimatione damnatus, impude[n]tia atque audacia fretus, sine tabulis, sine authore legerit, id nos quasi" type="straight"/>
        <underline language="LA" method="pen" text="quide[m] laudationis iactura exoptanda nobis" type="straight"/>
        <underline language="LA" method="pen" text="Flacci vita, &amp; digitas nititur" type="straight"/>
        <underline language="LA" method="pen" text="mihi quod haec causa maxime postulat, nulla[m] grauitate[m], nulla[m] consta[n]tiam, nullu[m] firmu[m] in Graecis hominibus consiliu[m],              nulla[m] denique esse testimonii fide[m]. nisi vero hactenus ista formula testimonii atq[ue] orationis tuae describi ac distingui potest, vt Flacco absenti aliquid ciuitates tribuisse dicantur" type="straight"/>
        <underline language="LA" method="pen" text="Equidem in minimus rebus saepe res magnas vidi Iudices deprehe[n]di ac teneri, vt in hoc Asclepiade" type="straight"/>
        <underline language="LA" method="pen" text="obsignata erat creta illa Asiatica" type="straight"/>
        
