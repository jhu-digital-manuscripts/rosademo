        <underline language="LA" method="pen" text="Satísne vobis Iudices, videor ad singulos testes accedere, neque vt primò constituera[m], tantu[m]modo cu[m] vniuerso genere co[n]fligere" type="straight"/>
        <underline language="LA" method="pen" text="Itaque etsi teneo quid sit dicere paratus, nihil tame[n] contra disputabo, prius quàm dixerit: totum enim co[n]uertet, atque alia finget" type="straight"/>
        <underline language="LA" method="pen" text="seruet quod parauit, &amp; ego me ad id quod attulerit, integrum co[n]seruabo" type="straight"/>
        <underline language="LA" method="pen" text="eam ciuitate[m], in quam ego multa &amp; magna officia" type="straight"/>
        <underline language="LA" method="pen" text="quae si ciuitas" type="straight"/>
        <underline language="LA" method="pen" text="magnifica &amp; gloriosa" type="straight"/>
        <underline language="LA" method="pen" text="iam inde à patre, atque" type="straight"/>
        <underline language="LA" method="pen" text="quasi ministrator" type="straight"/>
        <underline language="LA" method="pen" text="subiiciens, quid in suos ciues ciuitatémque" type="straight"/>
        <underline language="LA" method="pen" text="nihil enim illo homine leuius, nihil egentius, nihil inquinatius" type="straight"/>
        <underline language="LA" method="pen" text="remittant spiritus, co[m]primant animos suos, sedent arrogantiam, fateantur in Meandrii persona esse expressam faciem ciuitatis" type="straight"/>
        <underline language="LA" method="pen" text="domi protere[n]dum esse &amp; co[n]culcandum putaueru[n]t" type="straight"/>
        <underline language="LA" method="pen" text="esse in eo testimonio, cuius author inue[n]tus est nemo" type="straight"/>
        <underline language="LA" method="pen" text="qua[m]obrem ista ciuitas neque seuere Flaccu[m]" type="straight"/>
        <underline language="LA" method="pen" text="hinc totu[m] odiu[m], hinc omnis offensio" type="straight"/>
        <underline language="LA" method="pen" text="vulnus dice[n]do refricuisset" type="straight"/>
        <underline language="LA" method="pen" text="vt princeps principu[m]" type="straight"/>
        <underline language="LA" method="pen" text="cuius linguae quasi flabello seditionis illa tum est ege[n]tium co[n]cio ve[n]tilata" type="straight"/>
        <underline language="LA" method="pen" text="istum dolore[m], querelásque" type="straight"/>
        <underline language="LA" method="pen" text="cognoscite" type="straight"/>
        
