    <annotation>
        
        <marginalia anchor_text="in duplum iret" hand="English_secretary"> 
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        in duplu[m] ire
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To advance to the double.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="stari" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        stari
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To maintain.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="se retulit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        referre se
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To take oneself off to.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="quae si ciuitas" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        que si ciuitas
                    </marginalia_text>
                </position>
            </language>
            <translation>
                If this city.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="nónne esset puditu[m]" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        nònne esset puditu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Was it not deplorable.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="iam inde à parte, atque maioribus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        ia[m] inde a patre atq[ue] maiorib[us]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To his father and forebearers as before.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ministrator" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        ministrator
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Servant.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Repetitio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Repetition.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="protere[n]dum esse &amp; co[n]culcandum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        proterere co[n]culcare ] 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To trample on and to tread underfoot.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="vulnus dice[n]do refricuisset" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        vulnus dice[n]do refricare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To reopen a wound by talking.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="princeps principu[m]" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        princeps principu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The leader of leaders.
            </translation>
        </marginalia>
        
