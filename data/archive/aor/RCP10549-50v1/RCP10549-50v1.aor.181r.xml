    <annotation>
        
        <marginalia anchor_text="no[n] enim omnia quae dolemus, eade[m] iure queri possumus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        Non enim o[mn]ia qu[a]e dolem[us] eadem iure queri possum[us]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                For we cannot reasonably complain about everything that we do not like [Loeb 324, 505].
            </translation>
        </marginalia>
        
        <marginalia anchor_text="grauis, locuples, ornata ciuitas" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Ciuitas grauis locuples ornata
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A city, responsible, wealthy and distinguished.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="multis pòst annis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        multis pòst annis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                After many years.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="leuitas propria Graecoru[m]" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        leuitas gra[e]cor[um]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The shallowness of the Greeks.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Ciuitas grauiss[im]a modertaiss[im]a
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A city, most responsible and controlled.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        à co[n]trario
                    </marginalia_text>
                </position>
            </language>
            <translation>
                On the contrary.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="quantu[m] in ipsis fuit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        qua[n]tu[m] in ip[s]is fuit
                    </marginalia_text>
                </position>
            </language>
            <translation>
                As much as it was in their power.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Athenienses" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Athenienses
                    </marginalia_text>
                </position>
            </language>
            <translation>
                People from Athens.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Positio athenaru[m]
                    </marginalia_text>
                    <location name="Athens"/>
                </position>
            </language>
            <translation>
                The position of Athens.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Lacedaemonii" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Laceda[e]monij
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Spartans.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="videro. sed primu[m] illud tenebo" type="straight"/>
        <underline language="LA" method="pen" text="spoliatam se dicit, quòd id non habet, quod eius no[n] fuit, quid hoc impudentius" type="straight"/>
        <underline language="LA" method="pen" text="amissum est enim praeter spem, quod erat spe deuoratu[m] lucru[m]" type="straight"/>
        <underline language="LA" method="pen" text="no[n] enim omnia quae dolemus, eade[m] iure queri possumus" type="straight"/>
        <underline language="LA" method="pen" text="quo loco etia[m] atq[ue] etia[m] facite vt recordemini quae sit temeritas multitudinis, quae leuitas propria Graecoru[m], quid in co[n]cione seditiosa valeat oratio" type="straight"/>
        <underline language="LA" method="pen" text="atque obsidet rostra vindex temeritatis, &amp; moderatrix officii Curia" type="straight"/>
        <underline language="LA" method="pen" text="vt amicitiam Pop. Rom. fidem suam, iura omnia officii, humanitatísque violarent, quàm vt filium testimonio laederent, cuius patre[m] armis pellendum" type="straight"/>
        <underline language="LA" method="pen" text="quos enim hostes haec familia contempsit, nunqua[m] eosdem testes pertimescet" type="straight"/>
        <underline language="LA" method="pen" text="quae quidem à me si vt dice[n]da sunt, dicere[n]tur, grauius agerem Iudices, quàm adhuc egi, quanta[m]" type="straight"/>
        <underline language="LA" method="pen" text="reuocarem animos vestros ad Mithridatici belli memoria[m]" type="straight"/>
        <underline language="LA" method="pen" text="Liceat haec nobis, si obliuisci no[n] possumus, at tacere" type="straight"/>
        <underline language="LA" method="pen" text="quantu[m] in ipsis fuit" type="straight"/>
        <underline language="LA" method="pen" text="In hac igitur vrbe se iactant qua[m] oderu[n]t, apud eos quos inuiti vident, in ea Rep. ad quam opprimendam non animus eis, sed vires defuerunt. aspiciant" type="straight"/>
        <underline language="LA" method="pen" text="de quorum vrbis positione" type="straight"/>
        <underline language="LA" method="pen" text="pulchritudine[m] etiam inter deos certame[n] fuisse proditu[m] est. quae vetustate ea est, vt ipsa ex sese suos ciues genuisse dicatur:              &amp; eoru[m] eade[m] terra pare[n]s, altrix, patria dicatur. authoritate aute[m] tanta est, vt iam fractu[m] propè ac debilitatu[m] Graeciae nome[n] huius vrbis laude nitatur." type="straight"/>
        <underline language="LA" method="pen" text="cuius ciuitatis spectata ac nobilitata virtus, no[n] solu[m] natura coroborata, veru[m]etiam disciplina putatur: qui soli toto orbe terraru[m] septinge[n]tos iam annos" type="straight"/>
        
