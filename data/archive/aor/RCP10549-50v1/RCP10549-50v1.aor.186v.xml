    <annotation>
        
        <marginalia anchor_text="attulerint neruos &amp; industriam" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        afferre [ neruos industriam
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To bring vigour and drive.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="excutient tibi istam verboru[m] iactione[m]" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        excutere verboru[m] iactio[n]em
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To shake off throwing of words.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ex somno saepe excitabunt" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        ex somno excitare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To wake up from sleep.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Regium
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Royal.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="replices annalium memoriam" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        replicare animaliu[m] memoria[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To search through historical records.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        no[n] su[m] nescius
                    </marginalia_text>
                </position>
            </language>
            <translation>
                I do not know.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="efferatur immoderatione verborum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        efferri immoderatio[n]e v[er]bor[um]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be carried away by exaggerated language.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="cum suppressa voce de scelere P. Lentuli, de audacia coniuratorum omnium dixisset" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        dicere supp[re]ssa voce
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To speak in a low voice.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="queribu[n]da voce" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        queribunda vox
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Plaintive voice.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="mihi crede, excutient tibi istam verboru[m] iactatione[m]" type="straight"/>
