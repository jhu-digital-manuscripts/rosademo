    <annotation>
        
        <marginalia anchor_text="difficile est enim tacere cum doleas" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        Difficile est tacere cu[m] doleas
                    </marginalia_text>
                </position>
            </language>
            <translation>
                It is hard to be silent when you are hurt.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="oratoris vitium" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        vitium oratio[n]is
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The vice of speech.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Adestote omnes animis, qui adestis corporibus, " hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        adestote o[mn]es a[nim]is qui adestis corporibus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                All those gathered here, give me your attention.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Erigite mentes aurésque" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        errigere [ me[n]tes aures ]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Alert your minds and ears.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="occasum interitúmque Reipublicae" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        occasus interritus Reip
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The destruction and ruin of the state.
            </translation>
        </marginalia>

        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Contubernalis in co[n]sulatu
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Comrad in consulship.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="secum facere" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        secu[m] facere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To do with them.
            </translation>
        </marginalia>
        
