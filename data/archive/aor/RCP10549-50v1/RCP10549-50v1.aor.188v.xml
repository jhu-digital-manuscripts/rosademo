    <annotation>
        
        <marginalia anchor_text="At accusat C. Cornelii filius, idémque valere debet, ac si pater indicaret" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Eade[m] est r[ati]o accusatio[n]is filij &amp; patris
                    </marginalia_text>
                </position>
            </language>
            <translation>
                This is the reasoning for the accusation of son and father.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="tectus praesidio firmo amicoru[m]" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        tectus pr[a]esidio firmo amicoru[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Protected by a stout guard of friends {Loeb 324, 365].
            </translation>
        </marginalia>
        
        <marginalia anchor_text="copias, &amp; conatum repressi" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        reprimere copias conatu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To suppress forces and an attempt.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="informat" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        informare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To prime.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="adumbratum iudicium" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        adu[m]bratu[m] iudiciu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Flimsy judgement.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Catilinae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Coniuratio Catilin[a]e
                    </marginalia_text>
                    <person name="Lucius Sergius Catilina"/>
                </position>
            </language>
            <translation>
                Catilina's conspiracy.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="quae nox omnium temporum coniurationis acerrima fuit atque acerbissima" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        nox acerrima acerbiss[im]a
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Night, most critical and alarming.
            </translation>
        </marginalia>

        <marginalia anchor_text="cum prima luce Consulem salutatu[m] veniret" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        cu[m] prima luce venire
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To come with the first light.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ordinarentur, instituerentur, pararentur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        ordinari institui parari
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be arranged, to be set up, to be prepared.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Neapoli" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Neapolis
                    </marginalia_text>
                    <location name="Naples"/>
                </position>
            </language>
            <translation>
                Naples.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="precibus à P. Sylla petit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        pra[e]cibus petere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To beg.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ad ferramenta" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        ferrame[n]ta
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Weapons.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="aes alienum eiusde[m] dissolutu[m]" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        a[e]s alienu[m] dissoluere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To pay off debts.
            </translation>
        </marginalia>
        
