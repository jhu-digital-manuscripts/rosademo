        <underline language="LA" method="pen" text="eius voces, eius minae ferebantur, eius aspectus" type="straight"/>
        <underline language="LA" method="pen" text="P. Sylla hoc importunissimo tum honoris, tum etiam calamitatis socio atque" type="straight"/>
