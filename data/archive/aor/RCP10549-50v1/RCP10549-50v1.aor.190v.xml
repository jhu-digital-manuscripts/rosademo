        <underline language="LA" method="pen" text="facillime perspici, subito flecti, singíque no[n] posse" type="straight"/>
        <underline language="LA" method="pen" text="à me timide modicéque dicetur" type="straight"/>
        <underline language="LA" method="pen" text="si cum caeteri de nobis file[n]t, non etiam nosmetipsi tacemus" type="straight"/>
        <underline language="LA" method="pen" text="vt nobis libertate[m] retinere liceat, si minus liceat dignitate[m]" type="straight"/>
        <underline language="LA" method="pen" text="audaci, at" type="straight"/>
        <underline language="LA" method="pen" text="quid est quamobre[m]" type="straight"/>
