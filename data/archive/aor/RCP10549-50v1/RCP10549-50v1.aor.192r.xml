        <underline language="LA" method="pen" text="quod non modo à consuetudine iudiciorum, veru[m]etiam à forensi sermone abhorreat" type="straight"/>
        <underline language="LA" method="pen" text="huic reo" type="straight"/>
        <underline language="LA" method="pen" text="vti propè nouo quodam &amp; inusitato genere dice[n]di" type="straight"/>
