        </marginalia>
        
        <marginalia anchor_text="prospera, aequabilis, perpetuáque fortuna" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        fortuna p[ro]spera [a]equabilis p[er]petua
                    </marginalia_text>
                </position>
            </language>
