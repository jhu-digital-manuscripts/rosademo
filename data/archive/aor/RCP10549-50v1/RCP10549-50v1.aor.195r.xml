    <page filename="RCP10549-50v1.195r.tif" pagination="369" reader="John Dee" signature="aaa.i."/>
    <annotation>
        
        <marginalia anchor_text="cum primùm licuit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        cu[m] primu[m] licuit
                    </marginalia_text>
                </position>
            </language>
            <translation>
                As it was principally permitted.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ad meam perniciem vocem suam co[m]munibus hostibus praebuisset" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        p[rae]bere voce[m] co[mmun]ib[us] hostib[us] ad mea[m] p[er]niciem
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To lend one's voice to the foes of the state for my ruin [Loeb 158, 113].
            </translation>
        </marginalia>
        
        <marginalia anchor_text="querelas Senatus, luctum bonorum, Italiae gemitum pertulerunt" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        p[er]ferre qu[a]erelas luctum gemitum
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To endure the protests, distress and grief.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="conscribi, centuriaríque" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        co[n]scribi centuriari
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Enrolled and divided in centuries.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="boni" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Boni.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The good.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="impunitas" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        impunitas
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Freedom from punishment.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Repub. exterminata" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Repub exterminata.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The state banished.
            </translation>
        </marginalia>
        
