    <annotation>
        
        <marginalia anchor_text="me quatuor omnino genera hominum violarunt" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Quatuor genera ho[m]i[nu]m qui Cice. violarunt
                    </marginalia_text>
                    <person name="Cicero"/>
                </position>
            </language>
            <translation>
                Four kind of people that outrage Cicero.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="penes ipsos" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        penes eos.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                In the power of those.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="vlciscar" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        vltio Cice. in inimicos
                    </marginalia_text>
                    <person name="Cicero"/>
                </position>
            </language>
            <translation>
                Cicero's revenge against his enemies.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="exaequari" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        ex[a]equari bonis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To regard as equal to the good.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Odiu[m]" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Odium
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Hatred.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        infra p[ro] plancio 424.55
                    </marginalia_text>
                    <book title="Pro Plancio"/>
                    <internal_ref>
                        <target ref="RCP10549-50v1:0453" text="424.55"/>
                    </internal_ref>
                </position>
            </language>
            <translation>
                See Pro Plancio, [page] 424.55.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="dum anima superero mea" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        du[m] a[n]i[m]a sup[er]ero mea
                    </marginalia_text>
                </position>
            </language>
            <translation>
                As long as survive with my breath.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="inimicorum iniurias, crudelitatémque persequar" type="straight"/>
        <underline language="LA" method="pen" text="Etenim vlcisce[n]dae iniuriae facilior" type="straight"/>
        <underline language="LA" method="pen" text="remunerandi: propterea quò superiorem esse contra improbos, minus est negocii, quàm bonis exaequari. tum etia[m] ne tam necessariu[m] quide[m] est,              male meritis quàm optime meritis referre quod debeas" type="straight"/>
        <underline language="LA" method="pen" text="bene meritos quin colas, nec exorari fas est, neque id" type="straight"/>
        <underline language="LA" method="pen" text="neque aequu[m] est te[m]pore &amp; die memoria[m] beneficii definire" type="straight"/>
        <underline language="LA" method="pen" text="vlciscendo remissior fuit, mox aperte" type="straight"/>
        <underline language="LA" method="pen" text="propterea quò pecuniam qui retinet, no[n] dissoluit: qui reddidit, non habet. gratiam, &amp; qui retulit, habet: &amp; habet, dissoluit" type="straight"/>
        <underline language="LA" method="pen" text="beneficii cola[m] beneuolentia sempiterna, non solu[m] dum" type="straight"/>
        <underline language="LA" method="pen" text="sed etia[m] cum mortuo monimenta vestri in me beneficii permanebunt. In referenda" type="straight"/>
        <underline language="LA" method="pen" text="erit infixa animo meo sempiterna" type="straight"/>
        <underline language="LA" method="pen" text="qui apud me deorum immortalium vim &amp; numen tenetis, tum" type="straight"/>
        <underline language="LA" method="pen" text="Quae enim tanta potest existere vbertas ingenii, quae tanta dicendi copia, quod tam diuinum atque incredibile genus orationis,              quo quisquam possit vestra in nos vniuersa promerita, non dicam complecti orando, sed percensere numerando? qui" type="straight"/>
        <underline language="LA" method="pen" text="qui denique nosmetipsos nobis" type="straight"/>
        
