    <annotation>
        
        <marginalia anchor_text="optandum quidem est homini, imortalitatem" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Immortalitas no[n] est optanda
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Immortality is not to be desired.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="pestifer annus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        annus [ fatalis pestifer
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A fatale and destructive year.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Rempublicam lacerare" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        lacerare re[m]publica[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To slander the state.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="legúmque metuentes" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        legu[m] metue[n]tes
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Afraid of the laws.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Cn. Pompeius" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Cn. Pompeius
                    </marginalia_text>
                    <person name="Pompey"/>
                </position>
            </language>
            <translation>
                Gn. Pompeius.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="P. Lentulus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        P Lentulus
                    </marginalia_text>
                    <person name="Cornelius Lentulus Spinther"/>
                </position>
            </language>
            <translation>
                Cornelius Lentulus Spinther.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="atque in hac omniu[m] terrarum arce collocati sumus" type="straight"/>
        <underline language="LA" method="pen" text="Itaque P. C. quod ne optandam quidem est homini, immortalitatem quanda[m] per vos adepti videmur" type="straight"/>
        <underline language="LA" method="pen" text="cu[m] vi, ferro, metu, minis obsessi teneremini" type="straight"/>
        <underline language="LA" method="pen" text="maxime fidelem, &amp; minime" type="straight"/>
        <underline language="LA" method="pen" text="lacerare non" type="straight"/>
        <underline language="LA" method="pen" text="perfectum est" type="straight"/>
        <underline language="LA" method="pen" text="àm patriae malueram esse fatalem" type="straight"/>
        <underline language="LA" method="pen" text="nunqua[m] dubitastis mea[m] salutem cu[m] communi salute coniungere" type="straight"/>
        <underline language="LA" method="pen" text="anni caligine &amp; tenebris luce[m]" type="straight"/>
        <underline language="LA" method="pen" text="gloria, rebus gestis, Cn. Pompeius omnium gentium, omnium seculoru[m], omnis memoriae facile princeps, tuto se venire in Senatum arbitraretur" type="straight"/>
        <underline language="LA" method="pen" text="Respub. crue[n]taretur" type="straight"/>
        <underline language="LA" method="pen" text="sed flumine sanguinis intercludendum putauerunt" type="straight"/>
        <underline language="LA" method="pen" text="mutum forum, elinguem curiam tacitam &amp; fractam ciuitatem videbatis" type="straight"/>
        <underline language="LA" method="pen" text="volita[n]tes, magistratuum tecta oppugnata, deorum templa inflammata, summi viri &amp; clarissimi Consulis fasces fractos,              fortissimi atque optimi viri Tribuni plebis sanctissimum corpus non solum tactum ac violatum manu, sed vulneratum ferro confectúmque vidistis" type="straight"/>
        <underline language="LA" method="pen" text="Reliqui fuerunt, quos neque terror, nec vis, nec spes, nec metus, nec promissa, nec minae, nec tela, nec faces à vestra authoritate,              à Populi Romani dignitate, à mea salute depellerent." type="straight"/>
        <underline language="LA" method="pen" text="Princeps P. Lentulus parens, ac deus nostrae vitae, fortunae, memoriae, nominis, hoc specimen virtutis, hoc indicium animi, hoc lumen Consulatus sui fore putauit,              si me mihi, si meis, si vobis, si Reipublicae" type="straight"/>
        
