    <page filename="RCP10549-50v1.197r.tif" pagination="373" reader="John Dee" signature="aaa.iii."/>
    <annotation>
        
        <marginalia anchor_text="praeclarum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Caput p[rae]claru[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                An astounding clause.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="pedibus iret" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        pedibus ire.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To walk.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Contraria circa ide[m] versantur.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                They turned against these.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="non saepe vnum Consulem improbum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Numq[uam] duo co[n]sules imp[ro]bi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Never two wicked consuls.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="perenne ac proprium manere" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        perenne ac p[ro]p[ri]u[m] manere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To remain continual and proper.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="me[n]tes angustae, humiles, prauae, oppletae tenebris ac sordibus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Mentes angust[a]e humiles praua[e] opplet[a]e tenebris ac sordibus.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Minds, narrow, cringing and depraved, filled with darkness and filth.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="rem non minus strenue, quàm postea publicam confecisset" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        co[n]ficere rem
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To complete a matter.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="detrime[n]to" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        detrimentum
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Defeat.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="cincinnatus ganeo" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Cincinnatus ganneo
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A curled debauchee.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Consul imperiosus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Powerful consul.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="cum praeclarum caput recitaretur" type="straight"/>
        <underline language="LA" method="pen" text="P. Lentulus Consul fuit! quo quanto maius dedissetis, si superiore anno fuisset! nec enim eguissem medicina" type="straight"/>
        <underline language="LA" method="pen" text="causam semper fore firmissima[m] dicere solebat" type="straight"/>
        <underline language="LA" method="pen" text="perenne ac proprium manere potuisset" type="straight"/>
        <underline language="LA" method="pen" text="me[n]tes angustae, humiles, prauae, oppletae tenebris ac sordibus, nomen" type="straight"/>
        <underline language="LA" method="pen" text="qui me duo sceleratissimi" type="straight"/>
        <underline language="LA" method="pen" text="non Co[n]sules, sed latrones" type="straight"/>
        <underline language="LA" method="pen" text="enim vllam vllius boni spem haberet in eo, cuius primum tempus aetatis palàm fuisset ad omnes libidines diuulgatu[m]?              qui ne à sanctissima quidem parte corporis potuisset hominu[m] impura[m] intemperantiam propulsare" type="straight"/>
        <underline language="LA" method="pen" text="egestate[m] &amp; luxuriam domestico lenocinio suste[n]tauit" type="straight"/>
        <underline language="LA" method="pen" text="profecto egestate &amp; improbitate" type="straight"/>
        <underline language="LA" method="pen" text="se ad lenonis impurissimi pedes abiecissent, quo vultu cincinnatus" type="straight"/>
        <underline language="LA" method="pen" text="ille vnguentis oblitus, cum toga praetexta, quam omnes" type="straight"/>
        <underline language="LA" method="pen" text="irrisit squallore[m] vestrum &amp; luctu[m] grauissimae ciuitatis" type="straight"/>
        <underline language="LA" method="pen" text="vestrum malum gemeretis, nihil diceret" type="straight"/>
        <underline language="LA" method="pen" text="ne aperte incommoda patriae lugeretis, ediceret" type="straight"/>
        <underline language="LA" method="pen" text="vini, somni, stupri plenus, madenti coma, co[m]posito ca[pillo]" type="straight"/>
 
