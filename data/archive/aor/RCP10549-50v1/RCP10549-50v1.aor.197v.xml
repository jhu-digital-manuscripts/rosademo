        <underline language="LA" method="pen" text="cum incultum, horridu[m], moestúmque vidisses, etiam" type="straight"/>
        <underline language="LA" method="pen" text="agrestem &amp;" type="straight"/>
        <underline language="LA" method="pen" text="tamen libidinosum &amp; perditum non dubitares" type="straight"/>
        <underline language="LA" method="pen" text="si in foro co[n]stitisses, nihil crederes interesse, sine sensu, sine sapore, elingue[m], tardu[m], inhumanum" type="straight"/>
