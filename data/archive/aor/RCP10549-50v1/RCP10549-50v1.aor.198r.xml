        <marginalia anchor_text="iuxta, ac si meus frater esset" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        iuxta ac si me[us] f[rate]r esset
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Close to me, as if he had been my brother.
            </translation>
