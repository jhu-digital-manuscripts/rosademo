        <marginalia anchor_text="priuata[m] offensionem obliterauerunt" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        p[ri]uata[m] offe[n]sio[n]em oblitterare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To efface an unofficial estrangement.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="separatim" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
