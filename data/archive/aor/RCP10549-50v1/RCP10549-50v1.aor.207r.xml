    <annotation>
        
        <marginalia anchor_text="Memortem timuisse? turpe est" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        Turpe est timere mortem
                    </marginalia_text>
                </position>
            </language>
            <translation>
                It is wrong to fear death.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="res post natos homines pulcherrima" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        res post natos ho[m]i[n]es pulcherima
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The most beautiful thing after the creation of man.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="duplicata gloria" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        duplicata gloria.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Doubled glory.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="dixi sublatius" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        dicere sublatius
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To speak under compulsion.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="fundatissimae familiae" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        familia fundatiss[im]a
                    </marginalia_text>
                </position>
            </language>
            <translation>
                An very well-established family.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="duritiam" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        duritia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Hardship.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="dolenter" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        dolenter.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Painfully.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="communis humanitas" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        co[mmun]is humanitas
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Universal humanity.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="Aliud" type="straight"/>
        <underline language="LA" method="pen" text="sed erat res post natos homines" type="straight"/>
        <underline language="LA" method="pen" text="Me mortem timuisse?" type="straight"/>
        <underline language="LA" method="pen" text="de me dixi sublatius asciscendae laudis causa" type="straight"/>
        <underline language="LA" method="pen" text="inficiari me esse hominem, &amp; communem naturae sensum repudiare?" type="straight"/>
        <underline language="LA" method="pen" text="complexu suoru[m], disturbari tecta, diripi fortunas, patriae denique causa patriam ipsam amittere, spoliari populi Rom. beneficiis amplissimis,              praecipitrari ex altissimo dignitatis gradu, videre praetextatos inimicos nondum morte complorata arbitria petentes funeris: haec omnia subire co[n]seruandorum" type="straight"/>
        <underline language="LA" method="pen" text="dolenter" type="straight"/>
        <underline language="LA" method="pen" text="communis humanitas" type="straight"/>
        <underline language="LA" method="pen" text="Nam qui ea quae nunquam chara atque iucunda duxit, aequo animo Reipub. causa deserit, nulla[m] beneuole[n]tiam insignem in Rempub. declarat.              qui autem ea relinquit Reip. causa, à quibus cum summo dolore diuellitur, ei chara patria est, cuius salutem charitati anteponit suoru[m]" type="straight"/>
        <underline language="LA" method="pen" text="Quare disru[m]patur licet ista furia, atque audiat haec ex me, quoniam lacessiuit" type="straight"/>
        <underline language="LA" method="pen" text="nam si vos me in meis aedibus collocatis, id quod in omni mea causa semper studiis, consiliis, authoritatibus, sententiísque fecistis:              video me planè, ac sentio restitu[m]. Sin" type="straight"/>
        <underline language="LA" method="pen" text="sed etiam monimentum praebet inimico doloris mei, sceleris sui, publice calamitatis: quis erit qui hunc" type="straight"/>
        
