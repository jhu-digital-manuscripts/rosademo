        <marginalia anchor_text="domu[m] meam euersam non ab inimico meo, sed ab hoste co[m]muni, &amp; eodem extructam, &amp; positam in oculis ciuitatis [...] complanata [...] publicata [...]              dubus lucis co[n]cestita" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        dom[us] eu[er]sa extructa posita in oculis ciuitatis co[m]planata. publicata duob[us] lucis co[n]vestita
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A house, ruined, raised, placed in the public eye, confiscated, lit by two lights. 
            </translation>
        </marginalia>
        
