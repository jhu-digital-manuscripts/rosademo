                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Rex duplex
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A double king.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="cerimonias" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Cerimoni[a]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Ceremonies.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="inaedificatm sacellum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        In[a]edificatu[m] Sacellu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A walled-up shrine.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="armamentarium" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        armame[n]tariu[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Armory.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="est curationis meae" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        est curatio[n]is me[a]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                It is in my care.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="lege tamen naturae" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Lex natur[a]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The law of nature.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="maximum &amp; sanctissimum Dianae sacellum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        sacellu[m] maximu[m] sanctiss[imu]m
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The greatest and most sacred shrine.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="sacrificia gentilicia" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        sacrificia gentilicia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                National sacrifice.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="sacella suffossa, incensa, inaedificata, oppressa, summa denique turpitudine esse foedata" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        sacella suffossa incensa in[a]edificata oppressa fo[e]data.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Shrines, undermined, inflamed, walled up, overthrown and defiled.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="sacra illa tua facientem" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        facere sacra
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To make something sacred.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ius legatorum cum hominum praesidio munitum sit, tum etiam diuino iure esse vallatum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        ius manitu[m] vallatu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Law, defended and fortified.
            </translation>
        </marginalia>
        
