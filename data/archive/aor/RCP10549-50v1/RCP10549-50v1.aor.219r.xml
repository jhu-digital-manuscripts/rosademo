        <marginalia anchor_text="tractus celeberrimus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        tractus celeberimus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A very crowded district.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="aspera &amp; montuosa &amp; &amp; fidelis &amp; simplex &amp; fautrix suoru[m] regio" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Regio aspera mo[n]tuosa fidelis simplex fautrix suor[um]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A region, inflicted, hilly, dependable, simple, friendly to them.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Publicanorum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Publicani
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Farmers.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="vnicè diligebatur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        vnicè diligi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be chosen singularly.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="vir amplissimus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        vir ampliss[im][us]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The greatest man.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="rogatio" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Rogatio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A proposed measure.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="tecto recaeperu[n]t" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        tecto recipere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To receive shelter.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="inedia fluctibúsque recrearu[n]t" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        fessu[m] inedia fluctib[us] recreare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To repose his body tired of starvation and commotion.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="nauigiu[m]" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        navigiu[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A vessel.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="viaticum congresserunt" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        viaticu[m] co[n]gerere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To collect for the cost of voyage.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="aeterna in laude versantur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        a[e]t[er]na laude v[er]sari
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To enjoy eternal glory.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="omni virtute &amp; laude par" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        o[mn]i v[ir]tute laude par
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Equal to all virtue and praise.
            </translation>
        </marginalia>
        
