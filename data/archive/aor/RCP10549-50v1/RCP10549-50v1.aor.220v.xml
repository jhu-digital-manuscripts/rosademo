        <underline language="LA" method="pen" text="mirabor te his armis" type="straight"/>
        <underline language="LA" method="pen" text="cur hoc sibi oneris" type="straight"/>
        <underline language="LA" method="pen" text="officiosam amicitiam nomine inquinas criminoso" type="straight"/>
