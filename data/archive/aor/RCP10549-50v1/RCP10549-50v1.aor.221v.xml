    <annotation>
        
        <marginalia anchor_text="prouidentem antè &amp; praecauentem" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        p[ro]uidens pr[a]ecaue[n]s
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Forewarned and forearmed.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="coitio facta sit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        coitio facta est.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Collusion happened.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in angustum venirent" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        in angustu[m] venire
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To come close.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="restrictos &amp; tenaces" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        restrictus. tenax.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Restricted, restrained.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in idem crimen vocandum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        vocare in crime[n].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To call to judgement.
            </translation>
        </marginalia>
        
        <marginalia anchor_text=" caluit re recenti, nu[n]c in causa refrixit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Caluit in re rece[n]ti nu[n]c in c[aus]a refrixit
                    </marginalia_text>
                </position>
            </language>
            <translation>
                It deceived the recent issue, now during the case it has cooled down.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Repetitio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Repetitio.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="nimium retinens Equistris iuris &amp; libertatis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        nimiu[m] retinens iuris &amp; liberta.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Excessibely retainment of right and freedom.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="neque necesse est me id persequivoce, quod vos me[n]te videatis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        no[n] necesse est me id p[er]seq[ui] voce quod vos mente videatis.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                It is not necessary that with my voice I set forth what you see with your mind.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ne disseminato dispersóque sermoni, fortunas nnocentium subiiciendas" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        subicere fortunas disseminato dispersóq[ue] sermone.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To subject oneself to the fate spread and dispersed by rumour.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Maledictum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Maledictum
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Insult.
            </translation>
        </marginalia>
        
