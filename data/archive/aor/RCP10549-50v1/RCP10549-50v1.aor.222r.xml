            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="Vigilandum est semper, multae insidiae sunt bonis" type="straight"/>
        <underline language="LA" method="pen" text="Etenim honorum gradus summis hominibus &amp; infimis sunt pares, gloriae dispares" type="straight"/>
        <underline language="LA" method="pen" text="honorum populi finis est" type="straight"/>
        <underline language="LA" method="pen" text="iam octingenti" type="straight"/>
        <underline language="LA" method="pen" text="qui ciuitatem dominatu regio liberauit" type="straight"/>
        <underline language="LA" method="pen" text="quibus re bene gesta" type="straight"/>
        <underline language="LA" method="pen" text="omnes enim istiusmodi artes in iis reprehenduntur, qui cum professi fuerint, satisfacere non possunt, non in his qui abfuisse ab his studiis confitentur" type="straight"/>
        <underline language="LA" method="pen" text="non linguae volubilitas" type="straight"/>
        <underline language="LA" method="pen" text="nihil in eo nisi frugalitatem, laborem, vigilantiam esse curamus" type="straight"/>
        <underline language="LA" method="pen" text="facile patior id te agere multis verbis, quod ad iudicium non pertineat: &amp; id te accusantem tandiu dicere, quod ego defensor sine periculo possim confiteri" type="straight"/>
        <underline language="LA" method="pen" text="homines vnquam aliud Romae, nisi de" type="straight"/>
        
