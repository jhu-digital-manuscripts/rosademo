            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        re bene gesta
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Good deed.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="artes in iis reprehenduntur, qui cum professi" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Artiu[m] professores.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Those professing the arts.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in candidato, non linguae volubilitas, non ars, non scientia requiri" hand="English_secretary">
            <language ident="LA">
