        <marginalia anchor_text="stomacha[n]s fastidiose" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        stomachans fastidiose
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Disdainfully boiling with rage.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="aures hebetiores, oculos acres atque acutos habere" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        habere { aures hebetiones oculos acres, atq[ue] acutos
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To have blunt ears, sharp and piercing eyes.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="habitaui in oculis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        habitare in oculis.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To live in the public eye.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="pressi eorum neminem à congressu" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        premere à co[n]gressu.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To press by a contest.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ocium quidem vnqua[m]ociosum" hand="English_secretary">
