    <annotation>
        
        <marginalia anchor_text="in locos causam coniicere" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        in locos caussa[m] co[n]ijcere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To throw together the reason and the places.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="miserabilores epilogos possem dicere" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        miserabiliores epilogos dicere.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To give a very miserable peroration.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="declamatore" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Declamator.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Declaimer.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="subaccusare." hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        subaccusare.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To put under a charge.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="furialis illa vox" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        furialis vox
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A frenzied voice.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="no[n] discaepta[n]do decertandu[m]" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        no[n] discepta[n]do decerta[n]du[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be decided not by debating.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="improbos" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Improbi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Wicked.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="Sed hoc nescio qhomodo frequenter in me congessisti, sanéque in eo creber fuisti, te iccirco in locos causam coniicere voluisse, ne ego mea consuetudine aliquid de thensis misericordiae causa dicere[m], quod" type="straight"/>
        <underline language="LA" method="pen" text="nam mihi eripuisti ornamentu[m] orationis meae" type="straight"/>
        <underline language="LA" method="pen" text="lege ex isto ambitu traxisse" type="straight"/>
        <underline language="LA" method="pen" text="non vobis videor cu[m] aliquo declamatore, no[n] causa laboris &amp; fori" type="straight"/>
        <underline language="LA" method="pen" text="Nam quòd in eo me reprehendisti, quòd nimium multos defenderim: vtinam &amp; tu, qui potes, &amp; caeteri qui defugiunt, vellent me labore hoc leuare. sed sit vestra" type="straight"/>
        <underline language="LA" method="pen" text="Sed sunt haec leuiora, illa vero grauiora atque magna" type="straight"/>
        <underline language="LA" method="pen" text="cum quid sentirent, nesciretur: furialis illa vox nefariis stupris, religiosis altaribus effoeminata, secu[m]" type="straight"/>
        <underline language="LA" method="pen" text="cui quide[m] belli intestini &amp; vastitatis metus inferebatur" type="straight"/>
        <underline language="LA" method="pen" text="vinci autem improbos à bonis, fateor fuisse praeclarum, si" type="straight"/>
        <underline language="LA" method="pen" text="Consulibus. Nihil amplius dico, nisi illud Victoriae nostrae graues aduersarios paratos, interitus nullos vltores esse videcam. Hisce ego auxiliis" type="straight"/>
        <underline language="LA" method="pen" text="tu id in me reprehendis, quòd Q. Metello" type="straight"/>
        
