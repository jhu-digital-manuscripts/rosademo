    <annotation>
        
        <marginalia anchor_text="Decurionésque" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Decurio[n]es.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Decurions.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="commendationis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        co[m]mendatio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Entrusting.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ad vrbe[m] confestim incredibili celeritate aduolauit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        ad vrbe[m] co[n]festim incredibili celeritate aduolauit.
                    </marginalia_text>
                    <location name="Rome"/>
                </position>
            </language>
            <translation>
                He immediately hastened to Rome with incredible speed.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="militum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Milites
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Soldiers.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="stimulos admouerit homini fortassis studioso victoriae" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        admouere stimulos studiosus victori[a]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To bring up incentives, eager for victory.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="mirificus vsus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        mirific[us] vsus.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Amazing skill.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ad excitandum Antonium, cohortandum, impellendum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        excitare cohortare impellere.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To excite, to encourage, to persuade.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="è pruina Apennini, atque è niuibus illis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        emergere è pruina niuibus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To emerge from the frost and snow.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="contento studi, cursúque veniamus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        venire co[n]tento studio cursuq[ue]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To come with content, eagerness and direction.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="dolentius deplorandum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        dolentius deplorare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To lament with sorrow.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="ad vrbe[m] confestim incredibili celeritate aduolauit. Atque vt illius te[m]poris attrocitate[m] recordari" type="straight"/>
        <underline language="LA" method="pen" text="ad timoris praeteriti cogitationem excitate" type="straight"/>
        <underline language="LA" method="pen" text="sed hoc breue dica[m]" type="straight"/>
        <underline language="LA" method="pen" text="pruina Apennini, atque è niuibus illis emersisset" type="straight"/>
        <underline language="LA" method="pen" text="Macedoniae" type="straight"/>
        <underline language="LA" method="pen" text="vidi vestigia non pressa leuiter ad exigui praedicationem temporis, sed fixa ad memoria[m] illius prouinciae sempiternam. Verum haec ita praetereamus, vt tamen intue[n]tes &amp; respectantes relinquamus" type="straight"/>
        <underline language="LA" method="pen" text="quia ipse ad sese iandudu[m] vocat, &amp; quodammodo absorbet oratione[m] mea[m], contento studio, cursúque veniamus" type="straight"/>
        <underline language="LA" method="pen" text="non defensionem modo criminum videretur continere, sed etiam memoria digna vti, &amp; Reipublicae capessendae authoritatem disciplinámque praescribere" type="straight"/>
        <underline language="LA" method="pen" text="si non subtilius disputandum, at certe dolentius deplorandum. Qua in oratione si asperius in quosdam homines inuehi vellem" type="straight"/>
        <underline language="LA" method="pen" text="concederet, vt eos quorum sceleris furore violatus essem, vocis libertate" type="straight"/>
        <underline language="LA" method="pen" text="Sed vt agam moderate, &amp; huic potius tempori seruia[m], quàm dolori meo, siqui occulte à salute nostra dissentiunt, lateant: siqui fecerunt aliquid aliquando, atque iidem nunc tacent &amp; quiescunt, nos quoque simus obliti: siqui se offerunt insolenter &amp; insecta[n]tur, quoad ferri potuerunt perferemus: neque que[m]quam offendet oratio mea, nisi qui se ita obtulerit, vt in eum non inuasisse, sed incurrisse videamur" type="straight"/>
        <underline language="LA" method="pen" text="Sestii dicere incipiam, me totum superioris anni Reipub. naufragium exponere. in quo colligendo" type="straight"/>
        
