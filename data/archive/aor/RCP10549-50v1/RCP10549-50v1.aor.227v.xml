        <marginalia anchor_text="animus eius vultu, flagitia parietibus tegebantur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        A[n]i[m]us ei[us] vultu, flagitia parietib[us] tegebantur.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                His heart was concealed by his face, his shameful acts by the walls of his house.
            </translation>
        </marginalia>
        
