    <page filename="RCP10549-50v1.228v.tif" pagination="436" reader="John Dee" signature="[eee.ii.[v]]"/>
    <annotation>
        
        <marginalia anchor_text="voce ac sente[n]tia sua co[m]probauerunt" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        voce &amp; sententia co[m]probare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To approve by voice and vote.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="obnunciaret" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        obnu[n]ciare.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To announce.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="relegarentur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        relegari
                    </marginalia_text>
                </position>
            </language>
            <language ident="EN">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        to be banished
                    </marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <marginalia anchor_text="vrbem nondum excisam &amp; euersam, sed iam captam atque oppressam" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        vrbs excisa. euersa. capta. oppressa.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The city, annihilated, overthrown, captured, subdued.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="despicatissimi hominis furori, co[n]temptissimorum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        despicatiss[imus] co[n]temptiss[imus].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Most despiciple and despised.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="nullius animi, nullius consilii fuisse" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        esse nulli[us] a[nim]i nulli[us] co[n]silij
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be of no courage, of no judgement.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="iurare" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        iurare.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To swear.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in causa populari si non moderate, at certe populariter, abstinentérque versato" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        In caussa populari versari. { moderate populariter abstine[n]ter.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To maneuver in the popular case moderately, in the people's interest, and with self restraint.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="causam Senatus palàm, Equester ordo acerrime, cunta Italia publice, omnes boni proprie enixéque suscaeperant" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        suscipere caussa[m]. ] palàm acerrime publice proprie enixe
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To assume the case, openly, most keenly, publicly, properly, earnestly. 
            </translation>
        </marginalia>
        
        <marginalia anchor_text="victore exercitu" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        victor exerritus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A wandering victor.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="voce ac sente[n]tia" type="straight"/>
        <underline language="LA" method="pen" text="ad vim, ad manus, ad caedem, ad direptionem incitarentur" type="straight"/>
        <underline language="LA" method="pen" text="terrore ac metu multos, plures etiam spe &amp; promissis tenebat" type="straight"/>
        <underline language="LA" method="pen" text="alii nominatim relegarentur" type="straight"/>
        <underline language="LA" method="pen" text="Expona[m] enim hodierno die Iudices, omnem ratione[m] facti &amp;" type="straight"/>
        <underline language="LA" method="pen" text="studio audiendi, nec verò huic tantae multitudini, quanta mea memoria nunqua[m] vllo in iudicio fuit, deero" type="straight"/>
        <underline language="LA" method="pen" text="iurare" type="straight"/>
        <underline language="LA" method="pen" text="si non moderate, at certe populariter, abstinentérque" type="straight"/>
        <underline language="LA" method="pen" text="quáeque no[n] modo ad singulare[m] meam gloria[m], sed ad commune[m] salutem omniu[m] ciuium, &amp; propè gentiu[m] pertinerent: ea" type="straight"/>
        
