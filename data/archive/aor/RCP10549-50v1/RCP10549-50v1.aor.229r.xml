    <annotation>
        
        <marginalia anchor_text="testamentario" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Testame[n]tarius
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A forger of wills.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="vim vi repulsam" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        vim vi repellere licet.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                One may repell force by force.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="M. Crassus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        M Crassus
                    </marginalia_text>
                    <person name="Marcus Licinius Crassus"/>
                </position>
            </language>
            <translation>
                Marcus Licinius Crassus.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="quocum mihi omnes erant amicitiae necessitudines" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        q[uo]cu[m] o[mn]es era[n]t amiciti[a]e necessitudines
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To whom there are all ties of friendship.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="labefactari à Praetoribus, infirmari à Senatu" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        labefactari infirmari.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be shaken, to be weakened.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="cautior" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        cautior.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                More cautious.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Senatum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Senatus.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The senate.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="quid reliquiarum restaret" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        quid reliq[ui]ar[um] restare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To withstand what is left.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="plaga mihi insanabilis, illi mortifera" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        plaga insanabilis motifera.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A wound, incurable and fatal. 
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="sed cum scurrarum locupletu[m] scorto" type="straight"/>
        <underline language="LA" method="pen" text="sororis adultero" type="straight"/>
