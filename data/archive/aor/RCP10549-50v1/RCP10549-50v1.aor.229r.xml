        <underline language="LA" method="pen" text="cum venefico, cum testamentario, cum sicario, cum latrone. quos homines" type="straight"/>
        <underline language="LA" method="pen" text="illa furia" type="straight"/>
        <underline language="LA" method="pen" text="erant amicitiae" type="straight"/>
        <underline language="LA" method="pen" text="qui à me nullo meo merito alienus esse" type="straight"/>
        <underline language="LA" method="pen" text="me vero non mouit illius oratio, sed eorum taciturnitas, in quos illa oratio tam improba conferebatur: qui tum, quanquam ob" type="straight"/>
        <underline language="LA" method="pen" text="suáque sibi propiora esse pericula quàm mea loquebantur" type="straight"/>
        <underline language="LA" method="pen" text="quem maxime homines ignari veritatis, mihi esse iratum putabunt, erat" type="straight"/>
        <underline language="LA" method="pen" text="Haec ergo cum viderem (neque enim era[n]t occulta) Senatum, sine quo ciuitas stare non posset, omnino de ciuitate esse sublatum" type="straight"/>
        <underline language="LA" method="pen" text="quid reliquiarum" type="straight"/>
        <underline language="LA" method="pen" text="ac no[n] accipienda plaga mihi insanabilis" type="straight"/>
        
