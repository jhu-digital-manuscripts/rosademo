        <marginalia anchor_text="no[n] labefactata[m] Remp. te Tribuno, neque conquassatam ciuitate[m], sed capta[m] hanc vrbe[m] atque euersam" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
