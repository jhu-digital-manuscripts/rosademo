    <annotation>
        
        <marginalia anchor_text="honores habueru[n]t" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        habere honores.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To have honours.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="iecisse fundamenta defensionis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        iacere fundame[n]ta defensio[n]is
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To lay the foundations of the defence.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="profluxi [...] dimanauit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        profluere dimanare ]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To emanate, to flow in different directions.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="vocibus maledictísque celebratum est [...] peruulgata" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        vocib[us] o[mn]i[u]m celebratu[m]. p[er]uulgatu[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Celebrated and spread by everybody.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        ACCVSATIO. MALEDICTIO. 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Accusation, slander.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="forma &amp; species fuit liberalis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        forma spes ] liberalis.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Honorable figure and hope.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="conuitium: si facetius, vrbanitas" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Convitium Vrbanitas.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Noise, elegance.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Illud tame[n] te esse admonitu[m] volo: primùm qualis es, tale[m] te esse existimes" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Illud te esse admonitu[m] velim qualis es tale[m] te existimes.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                I should like, however, to give you a hint in order that all may form a correct view of you [Loeb 447, 415].
            </translation>
        </marginalia>
        
        <marginalia anchor_text="M. Caelii" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        M Ca[e]lius
                    </marginalia_text>
                    <person name="Marcus Caelius Rufus"/>
                </position>
            </language>
            <translation>
                Marcus Caelius Rufus.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in illo aetatis flore" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        in illo a[e]tatis flore.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                In his early youth.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="artibus honestissimis erudiretur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        artib[us] honestiss[im]is erudiri.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be trained in most honourable pursuits.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="esse hunc in illius amicis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        esse in amicis alicui[us]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be among someone's adherents.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="secutus est annus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        secut[us] est annus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A year followed.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ea in alteru[m] ne dicas, quae cu[m] tibi falso respo[n]sa sint, erubescas" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Ea ne in alteru[m] dicas qu[a]e cu[m] tibi falso responsa sint, erubescas
                    </marginalia_text>
                </position>
            </language>
            <translation>
                'You should not bring charges against another which you would blush to hear brought falsely against yourself' [Loeb 447, 415].
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="tacitus facile ipse respondet" type="straight"/>
        <underline language="LA" method="pen" text="Videor mihi iecisse fundamenta defensionis meae" type="straight"/>
        <underline language="LA" method="pen" text="Equide[m] vt ad me reuertar, ab his fontibus profluxi ad hominu[m] fama[m]:              &amp; meus hic fore[n]sis labor vitáeque ratio dimanauit ad existimatione[m] hominu[m] paulo latius comme[n]datione ac iudicio meorum" type="straight"/>
        <underline language="LA" method="pen" text="maledicta peruulgata" type="straight"/>
        <underline language="LA" method="pen" text="Sed aliud est maledicere, aliud accusare. accusatio crimen desiderat, rem vt definiat, homine[m] vt notet, argumento probet,              teste confirmet. maledictio aute[m] nihil habet propositi praeter contumeliam: quae si petula[n]tius iactatur, conuitium: si facetius, vrbanitas nominatur. quam" type="straight"/>
        <underline language="LA" method="pen" text="neque enim decebat, neque aetas illa postulabat, neque id, quod animaduertere poteratis,              pudor patiebatur optimi adolesce[n]tis in tali illu[m] oratione versari Velle[m] aliquis ex vobis robustioribus hu[n]c maledice[n]di locu[m] suscaepisset:              aliquanto liberius &amp; fortius &amp; magis more nostro refutaremus ista[m] maledice[n]di lice[n]tiam" type="straight"/>
        <underline language="LA" method="pen" text="aga[m] leuius, quòd &amp; pudor tuus moderatur orationi meae, &amp; meum erga te parentémq[ue] tuum beneficiu[m] tueri debeo" type="straight"/>
        <underline language="LA" method="pen" text="es, tale[m] te esse existimes: vt quantu[m] à rerum turpitudine abes, tantu[m] te à verboru[m] libertate seiungas.              Deinde vt ea in alteru[m] ne dicas, quae cu[m] tibi falso respo[n]sa sint, erubescas" type="straight"/>
        <underline language="LA" method="pen" text="qui isti aetati non possit quàm velit petulanter" type="straight"/>
        <underline language="LA" method="pen" text="suspitione, at non sine argume[n]to maledicere" type="straight"/>
        <underline language="LA" method="pen" text="Sed istaru[m] partiu[m] culpa est eoru[m]" type="straight"/>
        <underline language="LA" method="pen" text="laus pudoris tui, quòd ea te inuitu[m] dicere videbamus: ingenii, quòd ornate politéque dixisti" type="straight"/>
        <underline language="LA" method="pen" text="cu[m] artibus honestissimis" type="straight"/>
        <underline language="LA" method="pen" text="illius amicis" type="straight"/>
        
