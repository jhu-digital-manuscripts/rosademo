<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v1.246v.tif" pagination="472" reader="John Dee" signature="[ggg.iiii.[v]]"/>
    <annotation>
        
        <marginalia anchor_text="neque principiu[m] inuenire, neque euoluere exitu[m] possum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        neq[ue] principiu[m] invenire neq[ue] euoluere exitu[m] possu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                I can neither invent the origin nor unravel the end.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="innoce[n]tiae" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Innoce[n]tia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Innocence.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ex inimica, ex infami, ex crudeli, ex facinorosa, ex libidinosa domo" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Dom[us] inimica infamis crudelis facinorosa libidinosa ]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A house, hostile, infamous, cruel, wicked and lustful.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in co[n]tentione ponatur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        In co[n]tentio[n]e poni.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To compare.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="temeraria, procax, irata mulier" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Mulier temeraria p[ro]cax irata ]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A woman, reckless, frivolous and angry.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="grauis, sapiens, moderatúsque vir" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        vir grauis sapiens moderat[us]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A man, severe, wise and moderate.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="abstraheretur è sine, gremióque patriae" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        abstrahi è sinu grenio'q[ue] patri[a]e.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be snatched from the bosom and embrace of the country.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ille moriens" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Q Metellus moriens
                    </marginalia_text>
                    <person name="Quintus Caecilius Metellus Celer"/>
                </position>
            </language>
            <translation>
                The death of Quintus Caecilius Metellus Celer.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="communis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        patris co[mmun]is.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Of the common father.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="an aliqua[m] voce[m] putatis ipsam pro se causam &amp; veritate[m] posse" type="straight"/>
        <underline language="LA" method="pen" text="innoce[n]tiae defensio: haec ipsius causae oratio, haec vna vox veritatis. in" type="straight"/>
        <underline language="LA" method="pen" text="sermonis, loci, temporis" type="straight"/>
        <underline language="LA" method="pen" text="dubitanda in co[n]tentione" type="straight"/>
        <underline language="LA" method="pen" text="cuius ego neque principiu[m] inuenire, neque euoluere exitu[m] possum" type="straight"/>
        <underline language="LA" method="pen" text="si caetera inimica oratione" type="straight"/>
        <underline language="LA" method="pen" text="sceleribus maximis aut conniuetis, aut" type="straight"/>
        <underline language="LA" method="pen" text="è sinu, gremióque" type="straight"/>
        <underline language="LA" method="pen" text="oppressa mens esset, extremum sensum ad memoriam Reipublicae reseruabat" type="straight"/>
        <underline language="LA" method="pen" text="intuens flentem significabat interruptis ac morientibus vocibus, quanta impenderet procella vrbi, quanta tempestas ciuitati: &amp; cum parietem saepe feriens eum,              qui cum Q. Catulo fuerat ei communis, crebro Catulum, saepe me, saepissime Rempublicam nominabat" type="straight"/>
        <underline language="LA" method="pen" text="quàm spoliari suo praesidio cum patriam, tum" type="straight"/>

