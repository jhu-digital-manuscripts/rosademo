<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v1.250r.tif" pagination="478" reader="John Dee" signature="[ggg.viii.]"/>
    <annotation>
        
        <marginalia anchor_text="pauloane interrupta est oratio mea" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        paulo ante interupta est or[ati]o mea
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A little before my speech was interrupted.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="exemplo" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Exempla
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Examples.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Quis plenior inimicorum fuit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        quis plenior inimicor[um]?
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Who has more enemies?
            </translation>
        </marginalia>
        
        <marginalia anchor_text="si idem extrema persequitur, qui inchoauit, iam omnia perfecta videamus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        si ide[m] extrema p[er]sequit[ur] q[ui] inchoauit, ia[m] o[mn]ia p[er]fecta videam[us].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                If he who began [the operations] follows it to the end, then we will see them all accomplished.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="instauratas [...] renouatas" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        instaurare renovare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To renew, to restore.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="inimicitias depono" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        deponere inimicitias.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To lay aside enmities.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="exempla [...] petenda" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        petere exemplu[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To seek a precedent.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="celebrantur aures" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        aures celebrari
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To frequent the ears.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="tela vndique esse intenta" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        tela esse intenta
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Spears are pointed.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="amicus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Amicitia 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Friendship.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="bellum indixi atque intuli" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        bellu[m] indicere inferre co[n]ficere 479.
                    </marginalia_text>
                    <internal_ref>
                        <target ref="RCP10549-50v1:0508" text="479"/> <!--JG: 'bellu[m] conficere posses', in printed text, more or less to the right of the marginal note 'differe tempus'-->
                    </internal_ref>
                </position>
            </language>
            <translation>
                To declare, wage and cause war.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="qua pauloante interrupta" type="straight"/>
        <underline language="LA" method="pen" text="affectum videmus, &amp; vere vt dicam penè confectu[m]" type="straight"/>
        <underline language="LA" method="pen" text="vt si idem extrema persequitur, qui inchoauit, iam omnia perfecta videamus. si succeditur" type="straight"/>
        <underline language="LA" method="pen" text="maximi" type="straight"/>
        <underline language="LA" method="pen" text="non solum memoria &amp; testimonio, sed etiam annalium literis, &amp; summi poetae voce laudatus est, quòd" type="straight"/>
        <underline language="LA" method="pen" text="Atque vt vetera, quae sunt innumerabilia, mitta[m], tuus pater" type="straight"/>
        <underline language="LA" method="pen" text="Multa praetereo, quòd intueor cora[m] haec lumina atque ornamenta Reip. P. Seruilium, &amp; M. Lucullum" type="straight"/>
        <underline language="LA" method="pen" text="cuius literis, fama, nunciis, celebrantur aures quotidie meae nouis nominibus gentium, nationum, locorum" type="straight"/>
        <underline language="LA" method="pen" text="Caesare reducit, reconciliat, restituit in gratiam" type="straight"/>
        <underline language="LA" method="pen" text="nemini ego possum esse bene merenti de Rep. no[n] amicus" type="straight"/>
        <underline language="LA" method="pen" text="me in amicos inflammare potuit, inimicis placare non possit? quod mihi" type="straight"/>
        
