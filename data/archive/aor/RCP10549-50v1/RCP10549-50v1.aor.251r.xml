<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v1.251r.tif" pagination="481" reader="John Dee" signature="hhh.i."/>
    <annotation>
        
        <marginalia anchor_text="Cn. Pompeii" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Cn Pompeius
                    </marginalia_text>
                    <person name="Gnaeus Pompeius"/>
                </position>
            </language>
            <translation>
                Gnaeus Pompeius.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in Prouincias [...] recisas, partim repressas" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        prouincia[e] reris[a]e repress[a]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Provinces, cut off, driven back.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="gens [...] subacta [...] domita [...] pacata" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        ge[n]tes subact[a]e domit[a]e pacat[a]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                People, subjugated, subdued, pacified.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="influentes in Italiam Gallorum maximas copias repressit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        influententes in Italia[m] Gallo maximas copias rep[re]ssit
                    </marginalia_text>
                    <location name="Italy"/>
                    <location name="Gaul"/>
                </position>
            </language>
            <translation>
                He drove back vast hordes that streaned into Italy from Gaul.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="domui [...] contentus [...] parêre assuefecit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        parêre assuefacere frangere domare co[n]serere co[m]pellere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To obey, to accustom, to dishearten, to subdue, to engage, to force.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="bellandum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        bellare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To fight.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in nostram ditionem esse redigendam" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        in n[ost]ram ditione[m] redigere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To bring under our sway.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="à gentibus, aut inimicis huc Imperio, aut infidis, aut incognitis, aut certe immanibus &amp; barbaris &amp; bellicosis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Gentes inimic[a]e infid[a]e incognit[a]e immanes barbare bellicos[a]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The peoples, hostile, faithless, unknown, brutal, uncivilised, belligerent.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Restitimus semper lacessiti" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Resistim[us] lacesiti
                    </marginalia_text>
                </position>
            </language>
            <translation>
                We withstood when provoked.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Alpibus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Alpes
                    </marginalia_text>
                    <location name="Alps"/>
                </position>
            </language>
            <translation>
                The Alps.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="impolitae vero res, &amp; acerbae si erunt relictae (qua[m]quam sunt abscissae)" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        res impolita acerba relicta abscissa
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A matter, unpolished, rough, abandoned, torn.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="reuirescent" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        reuirescere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To revive.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in eius tutela" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        in ei[us] tutela
                    </marginalia_text>
                </position>
            </language>
            <translation>
                In his guardianship.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="vrbes sedésque penetrauit. Modo" type="straight"/>
        <underline language="LA" method="pen" text="quas nationes nemo vnquam fuit quin frangi, domaríque cuperet: nemo" type="straight"/>
        <underline language="LA" method="pen" text="Restitimus semper lacessiti" type="straight"/>
        <underline language="LA" method="pen" text="Alpibus Italiam munierat antè natura non sine aliquo diuino" type="straight"/>
        <underline language="LA" method="pen" text="Nam illae sente[n]tiae virorum clarissimorum minime probandae sunt, quorum alter vlteriorem Galliam decernit cum Syria, alter" type="straight"/>
        
