    <annotation>
        
        <marginalia anchor_text="extremu[m] principium" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        extremu[m] principiu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The limit of the beginning.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="de authoritate" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Authoritas
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Authority.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Audiui hoc de parente meo puer" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        audiui hoc de pare[n]te meo puer
                    </marginalia_text>
                </position>
            </language>
            <translation>
                As a boy, I heard this from my father.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="remoueret oculos, &amp; se totum auerteret" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        remouere oculos se totu[m] auertere.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To avert the gaze and turn away completely.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="sanctè grauitérque vixisset" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        sanctè grauiter viuere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To live piously and seriously.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="omnia cum plena laudis, tum propria Cornelii, nec in his rebus crimen est vllum" type="straight"/>
        <underline language="LA" method="pen" text="nisi honos" type="straight"/>
        <underline language="LA" method="pen" text="Est enim aliquid quod non oporteat, etiam si licet: quicquid vero non licet, certe non oportet. Hîc" type="straight"/>
        <underline language="LA" method="pen" text="terrarum: tot victorias bellicas, quot sunt in reru[m] natura genera belloru[m]? an" type="straight"/>
        <underline language="LA" method="pen" text="etia[m] ipse casus euentúsque rerum, no[n] duces, sed comites eius consilioru[m]" type="straight"/>
        <underline language="LA" method="pen" text="ita summa fortuna cum summa virtute certauit" type="straight"/>
        <underline language="LA" method="pen" text="cui patriae salus dulcior quàm conspetus fuit" type="straight"/>
        <underline language="LA" method="pen" text="uitate decedere quàm de sententia maluit: hoc" type="straight"/>
        <underline language="LA" method="pen" text="veritate fidem esse constrictam" type="straight"/>
        <underline language="LA" method="pen" text="mutae regiones imploro, &amp; sola terrarum vltimarum: vos maria, portus, insulae, litoráque. quae est enim ora, quae sedes, qui locus, in quo non extent huius cum fortituinis, tum vero humanitatis, tum animi, tum consilii impressa vestigia?" type="straight"/>
        <underline language="LA" method="pen" text="inaudita grauitate, virtute, constantia praeditum foedera scie[n]tem neglixisse, violasse, rupisse, dicere audebit?" type="straight"/>
