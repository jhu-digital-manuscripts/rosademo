            <language ident="EN">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        a garland of oken leues vsed to be giue[n] to them [that] has saued a cytizen in battayle: e... . <!-- FSF: two word missing --> <!--JG: Can't figure out last word. 'eliot'??-->
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Civic crown.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ludi Compitalitii" hand="English_secretary"> 
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Ludi co[m]pitalitij, 
                    </marginalia_text>
                </position>
            </language>
            <language ident="EN">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        called also
                    </marginalia_text>
                </position>
            </language>
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        ludi liberales
                    </marginalia_text>
                </position>
            </language>
            <language ident="EN">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
