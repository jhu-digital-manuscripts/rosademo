        <marginalia anchor_text="sit sanè Fors domina campi" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Sit fors domina Campi.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Let chance be the mistress of the field.
            </translation>
        </marginalia>
        
