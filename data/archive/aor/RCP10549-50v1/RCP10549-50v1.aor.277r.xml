        <marginalia anchor_text="aliquid clementer, mansuete, iuste, moderate, sapienter" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>factu[m] cleme[n]ter mansuete, iuste, moderate, sapienter</marginalia_text>
                </position>
            </language>
            <translation>An 'act of mercy, of kindliness, of justice, of moderation, and of wisdom' [Loeb 252, 429].</translation>
        </marginalia>
        
        <marginalia anchor_text="pectus memoria effodit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>memoria pect[us] effodit</marginalia_text>
                </position>
            </language>
