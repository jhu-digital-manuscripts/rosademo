    <page filename="RCP10549-50v1.289r.tif" pagination="557" reader="John Dee" signature="[mmm.vii.]"/>
    <annotation>
        
        <marginalia anchor_text="depulsórque" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>depulsor</marginalia_text>
                </position>
            </language>
            <translation>One who repels.</translation>
        </marginalia>
        
        <marginalia anchor_text="Cicerone[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Accusatio Antonij in Ciceronem.</marginalia_text>
                    <person name="Mark Antony"/>
                    <person name="Cicero"/>
                </position>
            </language>
            <translation>Anthony's accusation against Cicero.</translation>
        </marginalia>
        
        <marginalia anchor_text="quid enim interest inter suasorem facti &amp; probatorem?" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Quid interest inter suasore[m] facti &amp; probatorem?</marginalia_text>
                </position>
            </language>
            <translation>'What difference is there between one who advises an action and one who approves of it?' [Loeb 189, 83].</translation>
        </marginalia>
        
        <marginalia anchor_text="stillante[m] prae se pugione[m] tulit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>stillante[m] pugione[m] ferre.</marginalia_text>
                </position>
            </language>
            <translation>To carry a dripping dagger.</translation>
        </marginalia>
        
        <marginalia anchor_text="edormi crapula[m] inquam &amp; exhala" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>edormi crapula[m] &amp; exhala</marginalia_text>
                </position>
            </language>
            <translation>Sleep off the intoxication and breathe out.</translation>
        </marginalia>
        
        <marginalia anchor_text="siquide[m] est atrocius patriae parente[m], quàm suu[m] occidere" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Atrocius est patri[a]e paren[n]te[m] qua[m] suu[m] occidere</marginalia_text>
                </position>
            </language>
            <translation>'It is a more atrocious crime to kill the “father of the fatherland” than one’s own parent' [Loeb 189, 85].</translation>
        </marginalia>
        
        <marginalia anchor_text="tertium nihil" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>nihil mediu[m] tertium</marginalia_text>
                </position>
            </language>
            <translation>No middle [nor] third [possibility].</translation>
        </marginalia>
        
        <marginalia anchor_text="distinctius dicuntur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>distinctius dici.</marginalia_text>
                </position>
            </language>
            <translation>To be said more lucidly.</translation>
        </marginalia>
        
        <marginalia anchor_text="retexo oratione[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>retexere or[ati]o[n]em</marginalia_text>
                </position>
            </language>
            <translation>To cancel a speech.</translation>
        </marginalia>
        
        <marginalia anchor_text="in equu[m] Troianu[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Equus troianus.</marginalia_text>
                </position>
            </language>
            <translation>The Trojan Horse.</translation>
        </marginalia>
        
        <marginalia anchor_text="affari" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>affari.</marginalia_text>
                </position>
            </language>
            <translation>To address.</translation>
        </marginalia>
        
        <marginalia anchor_text="ascribe me tale[m] in numerum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>ascribe in numeru[m]</marginalia_text>
                </position>
            </language>
            <translation>Insert in number.</translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="interest inter suasorem facti &amp; probatorem" type="straight"/>
        <underline language="LA" method="pen" text="fieri, an gaudea[m] factum" type="straight"/>
        <underline language="LA" method="pen" text="Etenim omnes boni, quantu[m] in ipsis fuit, Caesare[m] occiderunt" type="straight"/>
        <underline language="LA" method="pen" text="stupore[m] hominis, vel dica[m] pecudis" type="straight"/>
        <underline language="LA" method="pen" text="statuendu[m] tibi esse, vtru[m] illi, qui ista[m] rem gesseru[n]t, homicidii rei sint, an vindices liberatis? atte[n]de" type="straight"/>
        <underline language="LA" method="pen" text="cogiationemque sobrii hominis ad punctu[m] te[m]poris suscipe" type="straight"/>
        <underline language="LA" method="pen" text="siquide[m] est atrocius patriae parente[m], quàm suu[m] occidere" type="straight"/>
        <underline language="LA" method="pen" text="no[n] igitur homicidae" type="straight"/>
        <underline language="LA" method="pen" text="tertium nihil" type="straight"/>
        <underline language="LA" method="pen" text="Sed tamen haec est summa co[n]clusionis meae, quonia[m] scelere à te liberati sunt, ab eodem te amplissimis praemiss dignissimi iudicati sunt.              Itaque iam retexo oratione[m] mea[m]" type="straight"/>
        <underline language="LA" method="pen" text="in equu[m] Troianu[m]" type="straight"/>
        <underline language="LA" method="pen" text="quam tu in me vis concitare" type="straight"/>
        <underline language="LA" method="pen" text="cu[m] laude no[m] compare[m]" type="straight"/>
        <underline language="LA" method="pen" text="quae eoru[m] gloria[m] no[n] immortalitatis memoria prosequantur" type="straight"/>
        
