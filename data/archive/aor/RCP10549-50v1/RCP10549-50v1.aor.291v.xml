    <annotation>
        
        <marginalia anchor_text="tam impius, tam demens, tam diis hominibúsque hostis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>impius domens dijs ho[m]i[ni]b[u]sq[ue] hostis</marginalia_text>
                </position>
            </language>
            <translation>Impious. Out of one's mind. The enemy to gods and men.</translation>
        </marginalia> 
        
        <marginalia anchor_text="inuasit in eius viri fortunas; copias cu[m] se subito ingurgitauisset" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>invadere in fortunas ingurgitare in copias</marginalia_text>
                </position>
            </language>
            <translation>To lay hands on the posession. To plunge into the wealth [Loeb 189, 119].</translation>
        </marginalia> 
        
        <marginalia anchor_text="Male parta, male dilabu[n]tur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Male parta, male dilabu[n]t[ur]</marginalia_text>
                </position>
            </language>
            <translation>'Ill gotten is ill squandered' [Loeb 189, 119].</translation>
        </marginalia> 
        
        <marginalia anchor_text="Quae Charybdis tam vorax?" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Que Charibdis ta[m] vorax?</marginalia_text>
                </position>
            </language>
            <translation>'Was ever a Charybdis so ravenous?' [Loeb 189, 119].</translation>
        </marginalia> 
        
        <marginalia anchor_text="oceanus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Oceanus.</marginalia_text>
                </position>
            </language>
            <translation>The ocean.</translation>
        </marginalia> 
        
        <marginalia anchor_text="atque id" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>atq[ue] id.</marginalia_text>
                </position>
            </language>
            <translation>And that.</translation>
        </marginalia> 
        
        <marginalia anchor_text="fecit diuortiu[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>facere diuortium.</marginalia_text>
                </position>
            </language>
            <translation>To divorce.</translation>
        </marginalia> 
        
        <marginalia anchor_text="hoc est dicere" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>hoc est dicere.</marginalia_text>
                </position>
            </language>
            <translation>That is to say.</translation>
        </marginalia> 
        
        <marginalia anchor_text="antesignanus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>antesignanus</marginalia_text>
                </position>
            </language>
            <translation>'A commando' [Loeb 189, 123].</translation>
        </marginalia> 
        
        <underline language="LA" method="pen" text="miseru[m] me, consumptis enim lachrymis, tamen infixus animo haeret dolor" type="straight"/>
        <underline language="LA" method="pen" text="no[n] te execrandum Pop. Rom. non detestabilem, non omnes tibi deos, omnes homines &amp; esse inimicos, &amp; futuros" type="straight"/>
        <underline language="LA" method="pen" text="modo ege[n]s, repente diues" type="straight"/>
        <underline language="LA" method="pen" text="que[m], Male parta, male dilabu[n]tur: incredibile, ac simile portenti est" type="straight"/>
        <underline language="LA" method="pen" text="oceanus mediusfidius, vix videtur" type="straight"/>
        <underline language="LA" method="pen" text="dissipatas, tam distantibus in locis positas, tam cito absorbere potuisse" type="straight"/>
        <underline language="LA" method="pen" text="mimae: domus erat aleatoribus referta, plena ebriorum" type="straight"/>
        <underline language="LA" method="pen" text="potabatur, atque id locis pluribus: suggerebantur" type="straight"/>
        <underline language="LA" method="pen" text="Quamobre[m] desinite mirari haec tam celeriter esse consumpta. non" type="straight"/>
        <underline language="LA" method="pen" text="quanuis enim sine mente, sine sensu sis, vt es: tamen" type="straight"/>
        <underline language="LA" method="pen" text="verò te vnqua[m] neque vigilantem, neque in somnis credo me[n]te posse consistere. necesse est, quanuis sis, vt es, violentus &amp; furens" type="straight"/>
        <underline language="LA" method="pen" text="clarus" type="straight"/>
        <underline language="LA" method="pen" text="Consul &amp; impudicissimus, &amp; Consul &amp; homo nequissimus" type="straight"/>
        <underline language="LA" method="pen" text="gustaras ciuilem sanguinem, vel potius" type="straight"/>
        
