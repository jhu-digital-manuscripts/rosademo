    <annotation>
        
        <marginalia anchor_text="laete atque insolenter tulit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>ferre l[a]ete insole[n]ter</marginalia_text>
                </position>
            </language>
            <translation>To receive joyfully. [To receive] insolently.</translation>
        </marginalia> 
        
        <marginalia anchor_text="ad posteritatis etiam memoria[m] pertinere" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>ad posteritatis memoria[m] p[er]tinere</marginalia_text>
                </position>
            </language>
            <translation>To concern the memory of posterity.</translation>
        </marginalia> 
        
        <marginalia anchor_text="quin honore[m] mortuo, cui iniuria[m] viuo fecimus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>facere honore[m].</marginalia_text>
                </position>
            </language>
            <translation>To bestow honour.</translation>
        </marginalia> 
        
        <marginalia anchor_text="excusationi obstitistis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>excusatio[n]i obsistere</marginalia_text>
                </position>
            </language>
            <translation>To withstand an excuse.</translation>
        </marginalia> 
        
        <marginalia anchor_text="exequi ma[n]data" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>exeq[ui] mandata</marginalia_text>
                </position>
            </language>
            <translation>To follow orders.</translation>
        </marginalia> 
        
        <marginalia anchor_text="vita enim mortuoru[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Vita mortuoru[m]</marginalia_text>
                </position>
            </language>
            <translation>The life of the dead.</translation>
        </marginalia> 
        
        <marginalia anchor_text="inobscurabit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>inobscurare</marginalia_text>
                </position>
            </language>
            <translation>To reveal.</translation> 
        </marginalia> 
        
        <marginalia anchor_text="iuris co[n]sultus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Jurisco[n]sultus.</marginalia_text>
                </position>
            </language>
            <translation>A lawyer.</translation>
        </marginalia> 
        
        <marginalia anchor_text="vitae gloriosae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Vita gloriosa</marginalia_text>
                </position>
            </language>
            <translation>A glorious life.</translation>
        </marginalia> 
        
        <marginalia anchor_text="pietas filii" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Filij Pietas.</marginalia_text>
                </position>
            </language>
