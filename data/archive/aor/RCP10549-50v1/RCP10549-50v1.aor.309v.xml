        </marginalia> 
        
        <marginalia anchor_text="animati" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>animatus</marginalia_text>
                </position>
            </language>
            <translation>Animated.</translation>
        </marginalia> 
        
        <marginalia anchor_text="Ser. Sulpitius" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Ser. Sulpitius</marginalia_text> 
                    <person name="Servius Sulpicius Rufus"/>
                </position>
            </language>
            <translation>Servius Sulpicius Rufus.</translation>
        </marginalia> 
        
        <marginalia anchor_text="sepultura" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Sepultura</marginalia_text>
                </position>
            </language>
            <translation>Burial.</translation>
        </marginalia> 
        
        <marginalia anchor_text="mors Legati sine caede atque ferro nullu[m] honorem desyderat" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>Mors Legati sine ca[e]de atq[ue] ferro nullu[m] honore[m] desyderat</marginalia_text>
                </position>
            </language>
            <translation>'The death of an envoy without bloodshed and weapons calls for no honor' [Loeb 507, 87].</translation>
        </marginalia> 
        
        <underline language="LA" method="pen" text="Is enim profecto morte[m] attulit, qui causa mortis fuit" type="straight"/>
        <underline language="LA" method="pen" text="ad posteritatis etiam memoria[m] pertinere" type="straight"/>
        <underline language="LA" method="pen" text="graue dictu est, sed dicendu[m] tame[n]" type="straight"/>
        <underline language="LA" method="pen" text="Ser. Sulpitii ferre didicissent" type="straight"/>
        <underline language="LA" method="pen" text="vt eius oratio omen fati videretur" type="straight"/>
        <underline language="LA" method="pen" text="ei vita[m], cui ademistis" type="straight"/>
        <underline language="LA" method="pen" text="vita enim mortuoru[m] in memoria viuoru[m] est posita" type="straight"/> 
        <underline language="LA" method="pen" text="eius legationis posteritate[m] inobscurabit obliuio" type="straight"/>
        <underline language="LA" method="pen" text="ad omne[m] memoria[m co[m]medata[m]" type="straight"/>
        <underline language="LA" method="pen" text="omniu[m] mortaliu[m] fama celebrabit" type="straight"/>
        <underline language="LA" method="pen" text="qui ex omni aetate hac" type="straight"/>
        <underline language="LA" method="pen" text="enim ille magis iuris co[n]sultus quÃ m iustitiae" type="straight"/>
        <underline language="LA" method="pen" text="semper ad facilitate[m], aequitatemque referebat" type="straight"/>
        <underline language="LA" method="pen" text="tame[n] sic animati esse debetis" type="straight"/>
        <underline language="LA" method="pen" text="cuius luctus aut hoc honore vestro, aut nullo solatio leuari potest" type="straight"/>
        <underline language="LA" method="pen" text="Nam si mors Legati sine caede atque ferro nullu[m] honorem desyderat: cur decernit honore[m] sepulturae, qui maximus haberi potest mortuo? fin id" type="straight"/>
        
