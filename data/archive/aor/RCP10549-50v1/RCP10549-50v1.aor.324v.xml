    <annotation>
        
        <marginalia anchor_text="postrauit, fudit, occidit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>prosteruere fundere occidere.</marginalia_text>
                </position>
            </language>
            <translation>To knock over, to scatter, to kill.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="EL">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>πάθος</marginalia_text>
                </position>
            </language>
            <translation>Pathos.</translation>
        </marginalia>
        
        <marginalia anchor_text="mors" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>MORS</marginalia_text>
                </position>
            </language>
            <translation>Death.</translation>
        </marginalia>
        
        <marginalia anchor_text="in fuga, foeda mors: in victoria, gloriosa" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>In fuga fo[e]da mors in victoria gloriosa</marginalia_text>
                </position>
            </language>
            <translation>When fleeing, death is wretched; when victorious, death is glorious.</translation>
        </marginalia>
        
        <marginalia anchor_text="extremu[m] spiitu[m] in victoria effundistis" hand="Italian">
