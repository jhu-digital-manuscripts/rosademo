                </position>
            </language>
            <translation>To discharge the last breath.</translation>
        </marginalia>
        
        <marginalia anchor_text="vita" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>VITA</marginalia_text>
                </position>
            </language>
            <translation>Life.</translation>
        </marginalia>
        
        <marginalia anchor_text="reticentia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>retice[n]tia.</marginalia_text>
                </position>
            </language>
            <translation>To keep silent.</translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="prostrauit, fudit, occidit" type="straight"/>
