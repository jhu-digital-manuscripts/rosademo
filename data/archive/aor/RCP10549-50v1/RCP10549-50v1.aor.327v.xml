    <annotation>
        
        <marginalia anchor_text="par ac similis scelestoru[m] ciuiu[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>par. similis } scelestor[um] ciuiu[m]</marginalia_text>
                </position>
            </language>
            <translation>"The equal and counterpart of the criminals" [source: Loeb Classical Library 462: 378-379].</translation>
        </marginalia>
        
        <marginalia anchor_text="culpas; increpuisti" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Culpare. increpare</marginalia_text>
                </position>
            </language>
            <translation>To blame. To rebuke.</translation>
        </marginalia>
        
        <marginalia anchor_text="rudimenta &amp; incunabula virtutis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>rudime[n]ta cunabula } virtutis.</marginalia_text>
                </position>
            </language>
            <translation>The rudiments and cradles of virtue.</translation>
        </marginalia>
        
        <marginalia anchor_text="suu[m] annu[m] in Rep[ublica] perbacchatus est" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>suu[m] annu[m] p[er]bacchari</marginalia_text>
                </position>
            </language>
            <translation>To waste his year.</translation>
        </marginalia>
        
        <marginalia anchor_text="ancillatus sum, neque me addixi" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>ancillari addicere.</marginalia_text>
                </position>
            </language>
            <translation>To serve. To enslave.</translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="par ac similis scelestoru[m]" type="straight"/>
        <underline language="LA" method="pen" text="Nam quòd mea[m] aetate[m] increpuisti, tantu[m] me abesse puto ab impudicitia, quantu[m] tu à pudicitia. Sed" type="straight"/>
        <underline language="LA" method="pen" text="petula[n]ter in vxore[m] &amp; in filia[m] mea[m] inuasisti" type="straight"/>
        <underline language="LA" method="pen" text="vnus enim satis es materiae habens, nequi quicquam" type="straight"/>
        <underline language="LA" method="pen" text="testame[n]tis eoru[m] locupletior essem" type="straight"/>
        <underline language="LA" method="pen" text="quae si tus mihi vt vicia obiicis, temeritas tua reprehe[n]dentur, non mea vicia culpabuntur" type="straight"/>
        <underline language="LA" method="pen" text="facere non potuit, quàm quòd te tale[m] filium genuit" type="straight"/> 
        <underline language="LA" method="pen" text="immensae gulae impudicissimi corporis quaestus sufficere non potuit, &amp; aetas tua iam" type="straight"/>
        <underline language="LA" method="pen" text="vtru[m] inhonestioribus corporis partibus re[m] quaesierit, an amiserit. Domu[m]" type="straight"/>
        <underline language="LA" method="pen" text="patre turpissime venalem habuit, ve[n]didit" type="straight"/>
        
