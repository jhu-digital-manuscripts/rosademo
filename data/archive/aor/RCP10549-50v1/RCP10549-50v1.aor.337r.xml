        <underline language="LA" method="pen" text="animos à minore cura ad summum timorem traduceret. Sed tamen in eiusmodi perturbatione rerum quanquam omnia sunt metuenda,              nihil magis quàm perfidiam timemus: &amp;" type="straight"/>
        <underline language="LA" method="pen" text="tantum habeo polliceri" type="straight"/>
        <underline language="LA" method="pen" text="praesentibus cumulatè satisfacturum" type="straight"/>
        <underline language="LA" method="pen" text="quorum vtrum minus velim, non facile possum existimare" type="straight"/>
        <underline language="LA" method="pen" text="est quiddam tertiu[m]" type="straight"/> 
        <underline language="LA" method="pen" text=",&amp; dignitatem tuam in virtute, atque in rebus gestis tuis, atque in tua grauitate positam existimare. Siquid ex iis rebus,              quas tibi fortuna largita est, nonnullorum hominum perfidia detraxerit, id maiori illis fraudi, quàm tibi futurum" type="straight"/>
        <underline language="LA" method="pen" text="vtor ad omnia" type="straight"/>
        <underline language="LA" method="pen" text="posita sunt in coniectura" type="straight"/>
        <underline language="LA" method="pen" text="clamore, conuitióque iactatus est" type="straight"/>
        <underline language="LA" method="pen" text="aspere &amp; acerbe" type="straight"/>
        <underline language="LA" method="pen" text="magno silentio est" type="straight"/>
        <underline language="LA" method="pen" text="desertum se, atque abiectum fore" type="straight"/>
        <underline language="LA" method="pen" text="sine vlla dubitatione" type="straight"/>
        <underline language="LA" method="pen" text="Sed nosti hominis tarditatem &amp; taciturnitatem" type="straight"/>
        <underline language="LA" method="pen" text="Caeteris iniuriis, quae praepositae" type="straight"/>
        <underline language="LA" method="pen" text="ex consularibus" type="straight"/>
        <underline language="LA" method="pen" text="sunt partim obscurius iniqui, partim non dissumlanter irati." type="straight"/>
        <underline language="LA" method="pen" text="vt infringatur hominum improbitas &amp; co[n]siliis tuorum amicorum" type="straight"/>
        <underline language="LA" method="pen" text="debilitat cogitationes &amp; inimicorum &amp; proditorum" type="straight"/>
        <underline language="LA" method="pen" text="mihi à teneris (vt Graeci dicunt) vnguiculis" type="straight"/>
        <underline language="LA" method="pen" text="Illustrabit (mihi crede) tuam amplitudinem hominum iniuria" type="straight"/>
        <underline language="LA" method="pen" text="non fallam opinionem tuam." type="straight"/>
        <underline language="LA" method="pen" text="quonia[m] interuallo locorum, &amp; temporu[m] disiuncti sumus, per" type="straight"/>
        <underline language="LA" method="pen" text="id erit causae" type="straight"/>
        <underline language="LA" method="pen" text="mihi certoru[m] hominu[m] potestas" type="straight"/>
        
