        <marginalia anchor_text="cum pares esse non possent, pugnare desisterent" hand="Italian">
            <language ident="LA"> 
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Cu[m] par esse no[n] possis pugnare dessistas</marginalia_text> 
                </position>
            </language>
            <translation>When you are not able to be evenly matched, avoid battle.</translation> 
        </marginalia>
        
