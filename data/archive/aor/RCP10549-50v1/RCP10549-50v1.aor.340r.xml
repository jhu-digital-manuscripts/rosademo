        <underline language="LA" method="pen" text="non enim illae manubiae meae, sed" type="straight"/>
        <underline language="LA" method="pen" text="quemadmodum medici: sed etiam, vt aliptae, virium, &amp; coloris rationem habere voluissent" type="straight"/>
        <underline language="LA" method="pen" text="in capite meo solu[m] elaborarunt, reliquum" type="straight"/>
        <underline language="LA" method="pen" text="falsam opinionem acceperunt" type="straight"/>
        <underline language="LA" method="pen" text="dictitant fracto animo, &amp; demisso fuisse" type="straight"/>
        <underline language="LA" method="pen" text="abiectiore animo" type="straight"/>
        <underline language="LA" method="pen" text="promulgantibus omnibus" type="straight"/>
        <underline language="LA" method="pen" text="maleuolentissimum iure" type="straight"/>
        <underline language="LA" method="pen" text="tantum enitor, vt neque amicis, neque etiam alienoribus, opera, consilio, labore desim." type="straight"/>
        <underline language="LA" method="pen" text="offendit eos fortasse qui splendorem &amp; speciem huius vitae intuentur, solicitudinem autem &amp; labore[m] perspicere non possunt. illud verò non obscure queruntur" type="straight"/>
        <underline language="LA" method="pen" text="tum hoc non in postremis" type="straight"/>
        <underline language="LA" method="pen" text="non solum fronte, aqtue vultu, quibus simulatio" type="straight"/>
        <underline language="LA" method="pen" text="Itaque tota iam sapientium ciuium, qualem me &amp; esse &amp; numerari volo, &amp; sententia &amp; voluntas mutata esse debet. Id enim iubet" type="straight"/>
        <underline language="LA" method="pen" text="quantum probare tuis ciuibus possis: vim neque parenti, nec patriae afferre oportere" type="straight"/>
        <underline language="LA" method="pen" text="Atheniensem propè iam desipientem senectute, cúmque eum nec persuadendo, nec cogendo regi posse vidisset, cum persuaderi posse diffideret, cogi fas esse non arbitraretur" type="straight"/>
        <underline language="LA" method="pen" text="implicatus tenebar." type="straight"/>
        <underline language="LA" method="pen" text="Huc accessit" type="straight"/>
        <underline language="LA" method="pen" text="cum à vobis meae salutis authoribus discesserim, neminem esse, cuius officiis me tam esse deuinctum non solum confitear, sed etia[m] gaudeam" type="straight"/>
        <underline language="LA" method="pen" text="grauissimis in senatu sententiis oppugnassem," type="straight"/>
        <underline language="LA" method="pen" text="me néve in hoc reo, néve in aliis requiras" type="straight"/>
        
