        <marginalia anchor_text="dicitant fracto animo" hand="Italian">
            <language ident="LA"> 
                <position book_orientation="0" place="head">
                    <marginalia_text>a[n]i[m]o { fracto demisso abiecto</marginalia_text>                
                </position>
            </language>
            <translation>With a broken spirit. With a downcast spirit. With an abject spirit</translation>   
        </marginalia>
  
        <marginalia hand="Italian">
            <language ident="LA"> 
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Iudicat Domum sua[m] impe[n]diis Reip[ublicis] extructa[m].</marginalia_text>               
                </position>
            </language>
            <translation>He appraises his house built with public expense.</translation> <!-- NBW: Doesn't quite track  with the passage, which talks about him ordering a monument built...-->  
        </marginalia>
        
