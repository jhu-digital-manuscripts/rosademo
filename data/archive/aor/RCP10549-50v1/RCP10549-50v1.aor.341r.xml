        <underline language="LA" method="pen" text="perditoru[m] de me consilia frangere" type="straight"/>
        <underline language="LA" method="pen" text="hanc etiam potentium beneuolentiam" type="straight"/>
        <underline language="LA" method="pen" text="vt opinio mea fert" type="straight"/>
        <underline language="LA" method="pen" text="nulla in caeteros maleuolentia suffusum: contráque" type="straight"/>
        <underline language="LA" method="pen" text="magnum &amp; excelsum, tum etiam apertum &amp; simplicem. Vidi" type="straight"/>
        <underline language="LA" method="pen" text="mihi potestas praesentis tui fuerit" type="straight"/>
        <underline language="LA" method="pen" text="tibi erit eidem, cui salus mea fuit, etiam dignitas curae" type="straight"/>
        <underline language="LA" method="pen" text="neque ita multae" type="straight"/>
        <underline language="LA" method="pen" text="diiungo me fere" type="straight"/>
        <underline language="LA" method="pen" text="à prima adolescentia delectarunt" type="straight"/>
        <underline language="LA" method="pen" text="sed eos quos erat infinitum bene de me" type="straight"/>
        <underline language="LA" method="pen" text="veteribus nostris delectationibus" type="straight"/>
        <underline language="LA" method="pen" text="non transfieris" type="straight"/>
        <underline language="LA" method="pen" text="velim quàm familiarissime certiore[m], &amp; quàm saepissime" type="straight"/>
        <underline language="LA" method="pen" text="idque me, non modo vt tu sentias, sed omnes gentes, etiam vt posteritas omnis intelligat, esse" type="straight"/>
        <underline language="LA" method="pen" text="tam de iure certum est" type="straight"/>
        <underline language="LA" method="pen" text="sine vlla mora prouinciam successori concedere:" type="straight"/>
        <underline language="LA" method="pen" text="suspicione tuae cupiditatis non possis illius cupiditatem refutare. Ego vtru[m]que meum puto esse, &amp; quid sentiam oste[n]dere, &amp; quod feceris defendere." type="straight"/>
        <underline language="LA" method="pen" text="foelicitate quidem vellem, consequi" type="straight"/>
  
