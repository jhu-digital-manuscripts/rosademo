        <underline language="LA" method="pen" text="in quo omnia mea posita esse decreui" type="straight"/> <!-- NBW: Mark leading into this underlining -->
        <underline language="LA" method="pen" text="Dux nobis &amp; author opus" type="straight"/>
        <underline language="LA" method="pen" text="tuum studium dices" type="straight"/>
        <underline language="LA" method="pen" text="si complecti hominem volueris," type="straight"/>
        <underline language="LA" method="pen" text="quantum officii sustinerem" type="straight"/>
        <underline language="LA" method="pen" text="non enim mihi tam mea salus chara fuit, in qua praecipue sum" type="straight"/>
        <underline language="LA" method="pen" text="adiutus, quàm pietas erit in referenda gratia iucunda" type="straight"/>
        <underline language="LA" method="pen" text="SERA gratulatio reprehe[n]di non solet" type="straight"/>
        <underline language="LA" method="pen" text=", &amp; vt sempiternae laudi tibi sit iste Tribunatus, exopto" type="straight"/>
        <underline language="LA" method="pen" text="Nemo est qui tibi sapientius suadere possit te ipso: nunquam labêre, si te audies" type="straight"/>
        <underline language="LA" method="pen" text="si ea defendes quae ipse recta esse senties." type="straight"/>
        <underline language="LA" method="pen" text="tempus no[n] incideris sed veneris" type="straight"/>
        <underline language="LA" method="pen" text="temporum sit, qua[n]ta varietas rerum, quàm incerti exitus, quàm flexibiles hominu[m] voluntates," type="straight"/>
        <underline language="LA" method="pen" text="vanitatis in vita, non dubito quin cogites" type="straight"/>
        <underline language="LA" method="pen" text="loquere &amp; te adhibe in co[n]silium, te audi, tibi obte[m]pera." type="straight"/>
        <underline language="LA" method="pen" text="particeps, vel socius," type="straight"/>
        <underline language="LA" method="pen" text="vis amoris mei" type="straight"/>
        <underline language="LA" method="pen" text="consilio te vt possem iuuare" type="straight"/>
        <underline language="LA" method="pen" text="Scribam ad te plura alias" type="straight"/>
        <underline language="LA" method="pen" text="enim diebus era[m] missurus domesticos" type="straight"/>
        <underline language="LA" method="pen" text="vnis literis totius aestatis res gestas ad Senatu[m] perscriberem" type="straight"/>
        <underline language="LA" method="pen" text="Te mi Curio pro tua incredibili in me beneuolentia, meáque item in te singulari" type="straight"/>
        <underline language="LA" method="pen" text="Tribuno, non vt decernatur aliquid noui" type="straight"/>
        <underline language="LA" method="pen" text="solet esse difficilus: sed vt nequid noui decernatur:" type="straight"/>
        
