        <underline language="LA" method="pen" text="longe in posterum prospiciente" type="straight"/>
        <underline language="LA" method="pen" text="dies nullis in aliis, nisi de rep. sermonibus versatus sum: quae nec possunt scribi, nec scribe[n]da sunt. tantu[m]" type="straight"/> <!-- NBW: Check Abbreviation -->
