        <underline language="LA" method="pen" text="in eo mihi sunt omnia" type="straight"/>
        <underline language="LA" method="pen" text="egíque omneis illos adolescenteis, quos ille iactitat. difficile" type="straight"/>
        <underline language="LA" method="pen" text="verò incessi omnibus laetitiis" type="straight"/>
        <underline language="LA" method="pen" text="Ego voluptatem animi nimiam." type="straight"/>
        <underline language="LA" method="pen" text="dum illum rideo, penè sum factus ille." type="straight"/>
        <underline language="LA" method="pen" text="enim possum adduci vt" type="straight"/>
        <underline language="LA" method="pen" text="me laetitia extulerunt: ex" type="straight"/>
        <underline language="LA" method="pen" text="domum misi vnam epistolam, quin esset ad te altera: nec" type="straight"/>
        <underline language="LA" method="pen" text="tantum modo" type="straight"/>
        <underline language="LA" method="pen" text="quod esset ad laureolam" type="straight"/>
        <underline language="LA" method="pen" text="exercitum adduxi" type="straight"/>
        <underline language="LA" method="pen" text="probe ornatum auxiliis" type="straight"/>
        <underline language="LA" method="pen" text="enim in his locis, Hiccine est ille, qui vrbem? quem" type="straight"/>
