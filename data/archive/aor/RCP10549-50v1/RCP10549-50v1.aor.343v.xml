        <underline language="LA" method="pen" text="superatum: abduxi exercitum" type="straight"/>
        <underline language="LA" method="pen" text="Ibi quintum &amp; vigesimum iam diem aggeribus, vinies, turribus" type="straight"/>
        <underline language="LA" method="pen" text="oppugnabam oppidum munitissimum Pindenissum, tantis opibus, ta[n]tóque negotio" type="straight"/>
        <underline language="LA" method="pen" text="ad summam" type="straight"/>
        <underline language="LA" method="pen" text="te in praesenti scripsi" type="straight"/>
        <underline language="LA" method="pen" text="haec aestas habuit hunc exitum satis foelicem" type="straight"/>
        <underline language="LA" method="pen" text="spissus," type="straight"/>
        <underline language="LA" method="pen" text="vestra oratoria, sed haec etiam leuia nostratia? desunt" type="straight"/>
        <underline language="LA" method="pen" text="Mirum me desyderium tenet vrbis" type="straight"/>
        <underline language="LA" method="pen" text="incredibile meorum, atqui in primis tui, satietas autem provinciae: vel" type="straight"/>
        <underline language="LA" method="pen" text="tam accessio" type="straight"/>
        <underline language="LA" method="pen" text="vel quia belli magni timor impendet" type="straight"/>
        <underline language="LA" method="pen" text="eos qui venari" type="straight"/>
        <underline language="LA" method="pen" text="sed mira paucitas est" type="straight"/>
        <underline language="LA" method="pen" text="siquae ridenda essent" type="straight"/>
        <underline language="LA" method="pen" text="iam annum munus co[n]fecero" type="straight"/>
        <underline language="LA" method="pen" text="me erudiant de omni rep." type="straight"/> 
        <underline language="LA" method="pen" text="discessit, iter habebant" type="straight"/>
        <underline language="LA" method="pen" text="Omnis peregrinatio (quod ego ab adoclescentia iudicaui) obscura &amp; sordida est iis, quorum industria Romae potest illustris" type="straight"/>
        <underline language="LA" method="pen" text="hercule ambulatiuncula" type="straight"/>
        <underline language="LA" method="pen" text="prudentibus, fidelitérque suadentibus idem videri. Ego" type="straight"/>
        
