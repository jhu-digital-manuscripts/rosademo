    <page filename="RCP10549-50v2.140v.tif" pagination="20" reader="John Dee" signature="[b.ii.[v]]"/> 
    <annotation>
        
        <marginalia anchor_text="CONCLVSIONES; tripertitae sunt" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Conclusiones tripertita[e].</marginalia_text>
                </position>
            </language>
            <translation>Threefold conclusions.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="In quatuor locis vti possumus conclusionibus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>In quot &amp; quib[us] locis vti co[n]clusionibus possimus.</marginalia_text>
                </position>
            </language>
            <translation>In how many and in which places we can use conclusions.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="Enumeratio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Enumeratio.</marginalia_text>
                </position>
            </language>
            <translation>Enumeration.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="Item curandum est, ne aut ab exordio, aut à narratione repetatur orationis enumeratio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>vnde enumeratio exordietur.</marginalia_text>
                </position>
            </language>
            <translation>From where the enumeration will begin.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="Amplificatio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Amplificatio.</marginalia_text>
                </position>
            </language>
            <translation>Amplification.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="Loci co[m]munes ex dece[m] praeceptis co[m]modissime sumuntur adauge[n]di criminis causa" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Loci co[mmun]es adaugendi criminis caussa ex decem praeceptis.</marginalia_text>
                </position>
            </language>
            <translation>Commonplaces from ten formulae in the case of augmenting an accusation.</translation>  <!-- DO: Not sure about translation - how 'adaugendi' fits in - 'Commonplaces to be amplified with the cause...'? NBW: This isn't as literal a translation, but I think adaugendi criminis are attached to causa in the ablative "in the case of augmenting an accusation"  -->
        </marginalia>
        
        
        <marginalia anchor_text="singulare esse maleficium, spurcum, nefarium, inusitatum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>maleficiu[m] singulare spurcum nefarium invsitatum</marginalia_text>
                </position>
            </language>
            <translation>A singular, base, nefarious and unheard of crime.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="Misericordia co[m]mouebitur auditoris animus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Quo modo auditoris a[n]i[m]us misericordia mouebitur.</marginalia_text>
                </position>
            </language>
            <translation>How the spirit of the listener will be moved by pity.</translation>
        </marginalia>
        
        
        <underline language="LA" method="pen" text="Item curandum est, ne aut ab exordio, aut à narratione repetatur orationis" type="straight"/>
        <underline language="LA" method="pen" text="dicemus volu[n]tario facinori nulla[m] esse excusatione[m], sed imprude[n]tiae" type="straight"/>
        <underline language="LA" method="pen" text="stuprare ingenuam" type="straight"/>
        <underline language="LA" method="pen" text="Commiserationem breuem esse oportet. Nihil enim lachryma" type="straight"/>
        
        
