<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v2.146r.tif" pagination="31" reader="John Dee"/>   
    <annotation>
        
        <marginalia anchor_text="laudem modestiae venentur; ex aliorum laboribus libare laudem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>laude[m] venari libare.</marginalia_text>
                </position>
            </language>
            <translation>To pursue praise, to sip praise.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="mediocriter literatus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>mediocrit[er] literatus</marginalia_text>
                </position>
            </language>
            <translation>Moderately educated.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Iam Secundu[m] refellit.</marginalia_text>
                </position>
            </language>
            <translation>Now he refutes the second.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="exempla ponuntur hîc no[n] confirmandi, neque testificandi causa, sed demonstrandi" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Exemplum non esse testimonium.</marginalia_text>
                </position>
            </language>
            <translation>That an example is no evidence.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="Hoc igitur interest inter exemplu[m] &amp; testimoniu[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Quid interest inter exemplum &amp; Testimonium.</marginalia_text>
                </position>
            </language>
            <translation>What differs between an example and evidence.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Iam tertiu[m] tollit.</marginalia_text>
                </position>
            </language>
            <translation>Now he destroys the third.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="transcripsissetis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>transcribere.</marginalia_text>
                </position>
            </language>
            <translation>To transcribe.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="non tum cum parere ipsi, &amp; gignere, &amp; proferre debent" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>parere gignere p[ro]ferre</marginalia_text>
                </position>
            </language>
            <translation>To produce, to create, to bring forth.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="Si concederem aliena oportere assumere exempla, vincerem vnius oportere" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Quod si exempla aliena su[n]t sumenda vni[us] no[n] pluriu[m] que[m]admodu[m] greci faciu[n]t, sume[n]da</marginalia_text>
                </position>
            </language>
            <translation>That if examples of others are to be taken, only those of one person and not of many - as the Greeks do - are to be taken.</translation> 
        </marginalia>
        
        
        <marginalia anchor_text="vtrum vnu[m] omnia, an omnia nemine[m], sed aliud alium putet co[n]sequi posse" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>An vnus aut nemo o[mn]ia consequi potest</marginalia_text>
                </position>
            </language>
            <translation>Whether a single person or no one is able to achieve all [qualities].</translation>
        </marginalia>
        
        
        <underline language="LA" method="pen" text="ne qua in re laudem modestiae venentur, in ea ipsa re sint impudentes" type="straight"/>
        <underline language="LA" method="pen" text="quis est qui possit id quod de arte scripserit, comprobare" type="straight"/>
        <underline language="LA" method="pen" text="Si laboriosum, non statim praeclarum. Sunt" type="straight"/>
        <underline language="LA" method="pen" text="transcripsissetis" type="straight"/>
        <underline language="LA" method="pen" text="quia planè rudis id facere nemo poterit, cum feceris, te literatissimum putes, ineptus sis, propterea quòd id facile faciat quiuis mediocriter literatus" type="straight"/>
        <underline language="LA" method="pen" text="cum parere ipsi, &amp; gignere, &amp; proferre debent" type="straight"/>
        <underline language="LA" method="pen" text="hoc nulla staret eorum ratio" type="straight"/>
        <underline language="LA" method="pen" text="vtrum vnu[m] omnia, an omnia nemine[m], sed aliud alium" type="straight"/>
        <underline language="LA" method="pen" text="ab omnibus putare omnia" type="straight"/>
        
        <mark language="LA" method="pen" name="pin" place="head" text="mediocrit[er] literatus"/>
        
        <mark method="pen" name="pin" place="right_margin"/>
        
        <drawing anchor_text="ne qua in re laudem modestiae venentur, in ea ipsa re sint impudentes" book_orientation="0" method="pen" place="right_margin" type="florilegium"/>
        
        <drawing anchor_text="An vnus aut nemo o[mn]ia consequi potest" book_orientation="0" method="pen" place="right_margin" type="florilegium"/>
        
    </annotation>
