        <marginalia anchor_text="genus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Genus quid.</marginalia_text>
                </position>
            </language>
            <translation>What a genus is.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="Pars" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Pars quid.</marginalia_text>
                </position>
            </language>
            <translation>What a species is.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="homo animalis pars est, Thebani aut Troiani genus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>homo a[n]i[m]alis [gen[us] deleted] pars thebani genus.</marginalia_text>
                </position>
            </language>
            <translation>'man is a species of animal, but a genus of which Thebans or Trojans are species' [Loeb 386, 65].</translation> 
        </marginalia>
        
        
        <marginalia anchor_text="genus est omnium nimirum libidinum cupiditas" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Cupiditas omnium libidinu[m] genus.</marginalia_text>
                </position>
            </language>
            <translation>Desire is the genus of all lusts.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="vt nequid posterius praeter conclusionem inferatur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Nihil posterius p[rae]ter co[n]clusio[n]em</marginalia_text>
                </position>
            </language>
            <translation>Nothing following beyond the conclusion.</translation>
        </marginalia>
        
        
        <underline language="LA" method="pen" text="permiste cum" type="straight"/>
        <underline language="LA" method="pen" text="Na[m] homo animalis pars est, Thebani aut Troiani genus" type="straight"/>
        <underline language="LA" method="pen" text="est, ne cuius genus posueris, eius secum aliquam, sicuti diuersam &amp; dissimilem partem ponas" type="straight"/>
        <underline language="LA" method="pen" text="ex quibus haec ipsa transtulimus, quae conuenire videbantur" type="straight"/>
        <underline language="LA" method="pen" text="vt &amp; prima quaeque pars, vt exposita est in partitione, sic ordine transigatur: &amp; omnibus explicatis, peroratum sit, hoc modo, vt nequid posterius praeter conclusionem inferatur" type="straight"/>
        <underline language="LA" method="pen" text="sic nobis placet &amp; ad singulas partes accedere, &amp; omnibus absolutis, perorare. Nunc de confirmatione" type="straight"/>
        
