        <marginalia anchor_text="vt excellente[m] muliebris formae pulchritudine[m] muta in sese imago contineret, Helenae se pingere simulacru[m] velle dixit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        muta imago                  
                        mutu[m] simulachru[m]. ]
                    </marginalia_text>
                </position>
            </language>
            <translation>A mute image; a mute likeness. ]</translation>
        </marginalia>
        
        
        <marginalia anchor_text="excellente[m] muliebris formae pulchritudine[m]; magna praeditos dignitate; corporu[m] viribus &amp; dignitatibus antesteterunt; ex istis virginibus formosissimas" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        excellens muliebris forma[e] pulchritudo
                        virgo formosa.
                        preditus magna dignitate
                        corporis dignitatibus antestare.
                    </marginalia_text>
                </position>
            </language>
            <translation>The excellent beauty of the female form. A beautiful girl. Bestowed with great merit. To excel in merit of the body.</translation>
        </marginalia>
        
        <marginalia anchor_text="longe caeteris excellere; plurimu[m] aliis praestare; multum omnibus corporu[m] viribus &amp; dignitatibus antesteterunt" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        longe ca[e]teris excellere
                        plurimu[m] alijs pr[a]estare
                        multis o[mn]ib[us] antestare ]
                    </marginalia_text>
                </position>
            </language>
            <translation>To surpass the others by far. To stand ahead of most others. To excel before all.</translation>   
        </marginalia>
        
        
