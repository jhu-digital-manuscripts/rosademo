    <annotation>
        
        <marginalia anchor_text="Quoniam igitur in generali constitutione tandiu propter eius partium multitudinem commoramur, ne forte varietate, dissimilitudine rerum deductus alicuius animus in quendam errorem deferatur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        multitudo
                        varietas
                        dissimilitudo.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A great number; variety; difference.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                       nota.
                    </marginalia_text>
                </position>
            </language>
            <translation>Note.</translation>
        </marginalia>
        
        
        <marginalia anchor_text=".L. Licinius Crassus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        L Licinius Crassus.
                    </marginalia_text>
                    <person name="Lucius Licinius Crassus"/>
                </position>
            </language>
            <translation>Lucius Licinius Crassus.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="Ratio igitur praemii quatuor est in partes distributa" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Ratio premij 4x
                    </marginalia_text>
                </position>
            </language>
            <translation>'The matter of reward has four heads' [Loeb 386, 279].</translation>
        </marginalia>
        
        
        <marginalia anchor_text="Beneficia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Beneficia [secunda]
                    </marginalia_text>
                </position>
            </language>
            <translation>The second benefices.</translation>
        </marginalia>
     
        
        <marginalia anchor_text="qui contra aliquem praemium postula[n]tem dicet" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Contra pra[e]mia pete[n]te[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>Against seeking rewards.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="peruagetur; peruulgari" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        p[er]uulgari
                        p[er]uagari.
                    </marginalia_text>
                </position>
            </language>
            <translation>To be made common. To pervade.</translation>
        </marginalia>
        
       
        <marginalia anchor_text="praeiudicium" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        p[rae]iudiciu[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>A prejudgement.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="tali honore dignati sunt" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        tali honore dignati su[n]t.
                    </marginalia_text>
                </position>
            </language>
            <translation>They were esteemed with such honour.</translation>
        </marginalia>
        
        
