    <page filename="RCP10549-50v2.197r.tif" pagination="133" reader="John Dee" signature="i.iii."/>   
    <annotation>
        
        
        <marginalia anchor_text="oratoris" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Orator</marginalia_text>
                </position>
            </language>
            <translation>An orator.</translation>
        </marginalia>
        
        
        <underline language="LA" method="pen" text="cognosce[n]dis atque tracta[n]dis" type="straight"/>
        <underline language="LA" method="pen" text="plenum" type="straight"/>
        <underline language="LA" method="pen" text="varie copioséque" type="straight"/>
        <underline language="LA" method="pen" text="ex obscuriore aliqua scie[n]tia sit promendu[m] &amp; assumendu[m]" type="straight"/>
        <underline language="LA" method="pen" text="legibus iubendis" type="straight"/>
        <underline language="LA" method="pen" text="vetandis" type="straight"/>
        <underline language="LA" method="pen" text="admoueri possit oratio ad sensus animoru[m] atque motus vel infla[m]mandos, vel etia[m] extinguendos, quod vnu[m] in oratore dominatur, sine dilige[n]tissima peruestigatione" type="straight"/>
        <underline language="LA" method="pen" text="minus hoc vobis sim probaturus" type="straight"/>
        <underline language="LA" method="pen" text="qui illa profite[n]tur: illustrare" type="straight"/>
        <underline language="LA" method="pen" text="confugiendu[m] est facultatem. Neque" type="straight"/>
        <underline language="LA" method="pen" text="architectu[m]" type="straight"/>
        <underline language="LA" method="pen" text="armamentariu[m] fecit" type="straight"/>
        <underline language="LA" method="pen" text="architecti potius artificio disertum, quàm oratoris fuisse" type="straight"/>
        <underline language="LA" method="pen" text="de naualium opere" type="straight"/>
        <underline language="LA" method="pen" text="non" type="straight"/>
        <underline language="LA" method="pen" text="Neque vero" type="straight"/>
        <underline language="LA" method="pen" text="vincebat" type="straight"/>
        <underline language="LA" method="pen" text="in eo ipso quod" type="straight"/>
        <underline language="LA" method="pen" text="non" type="straight"/>
        <underline language="LA" method="pen" text="illud est probabilius, neque tame[n] verum" type="straight"/>
        <underline language="LA" method="pen" text="omnes in eo quod scirent, satis esse eloquentes" type="straight"/>
        <underline language="LA" method="pen" text="Illud verius, neque quenqua[m] in eo disertum esse posse quod nesciat, neque si id optime sciat, ignarúsque sit faciundae ac poliendae orationis, diserte id ipsum posse, de quo sciat, dicere" type="straight"/>
        <underline language="LA" method="pen" text="vniuersam &amp; propria[m] oratoris vim definire complectíque vult" type="straight"/>
        <underline language="LA" method="pen" text="hoc tam graui dignus nomine" type="straight"/>
        <underline language="LA" method="pen" text="dictione explicanda" type="straight"/>
        <underline language="LA" method="pen" text="memoriter" type="straight"/>
        <underline language="LA" method="pen" text="quadam etia[m] actionis dignitate" type="straight"/>
        <underline language="LA" method="pen" text="quod ita proposui" type="straight"/>
        <underline language="LA" method="pen" text="circu[m]cîdat" type="straight"/>
        <underline language="LA" method="pen" text="amputet" type="straight"/>
        <underline language="LA" method="pen" text="tenebo" type="straight"/>
        <underline language="LA" method="pen" text="sita sunt" type="straight"/>
        <underline language="LA" method="pen" text="in disceptationibus, aut" type="straight"/>
        <underline language="LA" method="pen" text="quae sint in quaque re" type="straight"/>
        <underline language="LA" method="pen" text="cum acceperit, ita pronu[n]tiabit" type="straight"/>
        <underline language="LA" method="pen" text="tecum co[m]municabit, téque" type="straight"/>
        <underline language="LA" method="pen" text="co[m]municabit" type="straight"/>
        <underline language="LA" method="pen" text="si me audierit" type="straight"/>
        <underline language="LA" method="pen" text="in naturae obscuritate[m], in dissere[n]di subtilitate[m], in vitam atque mores" type="straight"/>
        <underline language="LA" method="pen" text="ídque largiamur inertiae nostrae" type="straight"/>
        <underline language="LA" method="pen" text="in quo magnus esse possit" type="straight"/>
        <underline language="LA" method="pen" text="totus est oratori perdiscendus" type="straight"/>
        <underline language="LA" method="pen" text="delata &amp; tradita" type="straight"/>
        <underline language="LA" method="pen" text="ab agro remotissimu[m]" type="straight"/>
        <underline language="LA" method="pen" text="ad certam causam tempúsque cognorit? Est enim finitimus oratori poeta, numeris astrictior paulo, verboru[m] aute[m] licentia liberior" type="straight"/>
        
