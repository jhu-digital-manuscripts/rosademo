<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v2.197v.tif" pagination="134" reader="John Dee" signature="[i.iii.[v]]"/>
    <annotation>
        
        
        <underline language="LA" method="pen" text="multis vero ornandi generibus socius ac pene par: in hoc quide[m] certè propè idem, nullis vt terminis circu[m]scribat, aut definiat ius suu[m], quo minus ei liceat eadem illa facultate &amp; copia vagari, qua velit" type="straight"/>
        <underline language="LA" method="pen" text="negasti te fuisse laturu[m]" type="straight"/>
        <underline language="LA" method="pen" text="in omni parte humanitatis dixerim" type="straight"/>
        <underline language="LA" method="pen" text="Nunqua[m] mehercule hoc dicere[m], si eum, que[m] fingo, meipsum esse arbitrarer" type="straight"/>
        <underline language="LA" method="pen" text="homo tibi subiratus" type="straight"/>
        <underline language="LA" method="pen" text="minus quàm volebat, familiaris, sed tame[n] &amp; doctus &amp; perurbanus" type="straight"/>
        <underline language="LA" method="pen" text="numero habendu[m]" type="straight"/>
        <underline language="LA" method="pen" text="tame[n] apparet" type="straight"/>
        <underline language="LA" method="pen" text="extat" type="straight"/>
        <underline language="LA" method="pen" text="earu[m] reru[m] rudes" type="straight"/>
        <underline language="LA" method="pen" text="didicerimus. vt qui pila ludunt, no[n] vtuntur in ipsa lusione arificio proprio palaestrae, sed indicat ipse motus didiceríntne palaestra, an nescia[n]t" type="straight"/>
        <underline language="LA" method="pen" text="fingu[n]t" type="straight"/>
        <underline language="LA" method="pen" text="no[n] obscurum est" type="straight"/>
        <underline language="LA" method="pen" text="facile declaratur" type="straight"/>
        <underline language="LA" method="pen" text="in hoc declamatorio sit opere iactatus, an ad dicendu[m] omnibus ingenuis artibus instructus accesserit" type="straight"/>
        <underline language="LA" method="pen" text="No[n] luctabor" type="straight"/>
        <underline language="LA" method="pen" text="detorqueres, atq[ue] oratori propria traderes" type="straight"/>
        <underline language="LA" method="pen" text="accepera[m], co[n]tulissem" type="straight"/>
        <underline language="LA" method="pen" text="comites" type="straight"/>
        <underline language="LA" method="pen" text="ministras" type="straight"/>
        <underline language="LA" method="pen" text="complexus" type="straight"/>
        <underline language="LA" method="pen" text="ad eas facultates ista[m] ornatissimae orationis adiunxerit" type="straight"/>
        <underline language="LA" method="pen" text="pace horu[m] dixerim" type="straight"/>
        <underline language="LA" method="pen" text="deest" type="straight"/>
        <underline language="LA" method="pen" text="videamus ne plus ei tribuas, quàm res &amp; veritas ipsa co[n]cedat" type="straight"/>
        <underline language="LA" method="pen" text="ad agendu[m], quàm ad cognoscendu[m] venimus" type="straight"/>
        <underline language="LA" method="pen" text="res ipsa antè co[n]fecit" type="straight"/>
        <underline language="LA" method="pen" text="suspicari" type="straight"/>
        <underline language="LA" method="pen" text="tantu[m] in nobis videtur esse" type="straight"/>
        <underline language="LA" method="pen" text="vel maius" type="straight"/>
        <underline language="LA" method="pen" text="Probas" type="straight"/>
        <underline language="LA" method="pen" text="multo locupletior in" type="straight"/>
        <underline language="LA" method="pen" text="rationem naturámque co[m]prehenderit. Sed" type="straight"/>
        <underline language="LA" method="pen" text="id difficile factu est" type="straight"/>
        <underline language="LA" method="pen" text="occupationibus" type="straight"/>
        <underline language="LA" method="pen" text="abstrahamur" type="straight"/>
        <underline language="LA" method="pen" text="populari &amp; forensi" type="straight"/>
        <underline language="LA" method="pen" text="Nitidu[m]" type="straight"/>
        <underline language="LA" method="pen" text="&amp; laetum" type="straight"/>
        <underline language="LA" method="pen" text="palaestrae magis" type="straight"/>
        <underline language="LA" method="pen" text="olei" type="straight"/>
        <underline language="LA" method="pen" text="huius ciuilis turbae ac fori" type="straight"/>
        <underline language="LA" method="pen" text="sero ac leuiter Graecas literas attigissem" type="straight"/>
        <underline language="LA" method="pen" text="propter nauiga[n]di difficultate[m]" type="straight"/>
        <underline language="LA" method="pen" text="increbuisset" type="straight"/>
        <underline language="LA" method="pen" text="pro se quisque eoru[m] quantu[m] poterat" type="straight"/>
        <underline language="LA" method="pen" text="quosdam operarios lingua celeri &amp; exercitata: oratorem autem, nisi qui sapiens esset, esse neminem" type="straight"/>
        
