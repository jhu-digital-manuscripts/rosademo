    <page filename="RCP10549-50v2.198v.tif" pagination="136" reader="John Dee" signature="[i.iiii.[v]]"/> 
    <annotation>
        
        <underline language="LA" method="pen" text="tenerent, &amp; audirentur ab iis, quibus no[n] scientia esset trade[n]da" type="straight"/>
        <underline language="LA" method="pen" text="sed exigui temporis aut falsa aut certe obscura opinio" type="straight"/>
        <underline language="LA" method="pen" text="artificium vllum" type="straight"/>
        <underline language="LA" method="pen" text="me sibi perfacilem in audiendo, te perpugnace[m] in disputando esse visum" type="straight"/>
        <underline language="LA" method="pen" text="imprude[n]te" type="straight"/>
        <underline language="LA" method="pen" text="excidit" type="straight"/>
        <underline language="LA" method="pen" text="acute atque dilucide apud mediocres homines ex co[m]muni quadam hominu[m] opinione dicere" type="straight"/>
        <underline language="LA" method="pen" text="qui mirabilius &amp; magnificentius augere posset, atque ornare quae vellet, omnésque omniu[m] reru[m], quae ad dicendu[m] pertinere[n]t, fontes animo ac memoria co[n]tineret" type="straight"/>
        <underline language="LA" method="pen" text="ingressi simus, obruimur ambitione &amp; foro, sit tamen in re positu[m] atque natura" type="straight"/>
        <underline language="LA" method="pen" text="auguror co[n]iectura" type="straight"/>
        <underline language="LA" method="pen" text="in nostris hominibus" type="straight"/>
        <underline language="LA" method="pen" text="studio acriore" type="straight"/>
        <underline language="LA" method="pen" text="maiore ac maturiore" type="straight"/>
        <underline language="LA" method="pen" text="superiore" type="straight"/>
        <underline language="LA" method="pen" text="dediderit, existat" type="straight"/>
        <underline language="LA" method="pen" text="qualem quaerimus" type="straight"/>
        <underline language="LA" method="pen" text="lectitarit" type="straight"/>
        <underline language="LA" method="pen" text="paulu[m] huic aliquid poterit addere" type="straight"/>
        <underline language="LA" method="pen" text="Inspera[n]ti" type="straight"/>
        <underline language="LA" method="pen" text="sed valde optani vtrique nostru[m] cecidit" type="straight"/>
        <underline language="LA" method="pen" text="in istum sermone[m] Crasse delaberemini" type="straight"/>
        <underline language="LA" method="pen" text="memoria dignu[m] excipere possemus" type="straight"/>
        <underline language="LA" method="pen" text="vel studii, vel artificii, vel facultatis disputationem penè intima[m] perueniretis" type="straight"/>
        <underline language="LA" method="pen" text="ab ineunte aetate incensus essem studio vtriusque vestru[m], Crassi vero etia[m] amore" type="straight"/>
        <underline language="LA" method="pen" text="elicere" type="straight"/>
        <underline language="LA" method="pen" text="per meipsum egissem" type="straight"/>
        <underline language="LA" method="pen" text="per Drusum saepe tentassem" type="straight"/>
        <underline language="LA" method="pen" text="defuisti" type="straight"/>
        <underline language="LA" method="pen" text="obseruare" type="straight"/>
        <underline language="LA" method="pen" text="patefecit earum rerum ipsarum aditu[m]" type="straight"/>
        <underline language="LA" method="pen" text="princeps Crassus eius sermonis ordiendi fuit, date nobis hanc venia[m]" type="straight"/>
        <underline language="LA" method="pen" text="subtiliter prosequamini" type="straight"/>
        <underline language="LA" method="pen" text="huic palaestrae" type="straight"/>
        <underline language="LA" method="pen" text="tuu[m] hoc suburbanu[m] gymnasium antepona[m]" type="straight"/>
        <underline language="LA" method="pen" text="id quod requiris" type="straight"/>
        <underline language="LA" method="pen" text="refugisse" type="straight"/>
        <underline language="LA" method="pen" text="instanti" type="straight"/>
        <underline language="LA" method="pen" text="inhumanitate" type="straight"/>
        <underline language="LA" method="pen" text="quò tuo studio rectissimo atque optimo no[n] obsequi velle[m]" type="straight"/>
        <underline language="LA" method="pen" text="maxime natum aptúmque cognossem" type="straight"/>
        <underline language="LA" method="pen" text="disputationis insolentia, atque earu[m] rerum, quae quasi traduntur in arte, inscitia" type="straight"/>
        <underline language="LA" method="pen" text="omnino" type="straight"/>
        <underline language="LA" method="pen" text="de reliquo ia[m] nostra culpa fuerit" type="straight"/>
        <underline language="LA" method="pen" text="dimiserimus" type="straight"/>
        <underline language="LA" method="pen" text="vt in co[n]ditionibus scribi solt, de quibus scia[m] poteróque" type="straight"/>
        <underline language="LA" method="pen" text="tam impudens est" type="straight"/>
        <underline language="LA" method="pen" text="scire aut posse postulet" type="straight"/>
        <underline language="LA" method="pen" text="ista conditione, dum mihi liceat negare posse quod non potero, &amp; fateri nescire quod nescia[m], licet" type="straight"/>
        <underline language="LA" method="pen" text="vestro arbitratu perco[n]temini" type="straight"/>
        <underline language="LA" method="pen" text="exposuit" type="straight"/>
        <underline language="LA" method="pen" text="tanquam alicui Graeculo otioso &amp; loquaci, &amp; fortasse docto atque erudito quaestiunculam, de qua meo arbitratu loquar, ponitis" type="straight"/>
        
