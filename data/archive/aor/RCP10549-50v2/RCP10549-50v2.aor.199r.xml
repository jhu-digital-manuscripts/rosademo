<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v2.199r.tif" pagination="137" reader="John Dee"/>   
    <annotation>
        
        <underline language="LA" method="pen" text="in schola assedissent, ex magna hominu[m] frequentia dicere iuberent, siquis quid quaereret" type="straight"/>
        <underline language="LA" method="pen" text="permagnum quiddam suscipere ac profiteri" type="straight"/>
        <underline language="LA" method="pen" text="denuntiaret" type="straight"/>
        <underline language="LA" method="pen" text="improuisa" type="straight"/>
        <underline language="LA" method="pen" text="audire velle" type="straight"/>
        <underline language="LA" method="pen" text="difficile factu est" type="straight"/>
        <underline language="LA" method="pen" text="hominem" type="straight"/>
        <underline language="LA" method="pen" text="nostríque cupidissimum" type="straight"/>
        <underline language="LA" method="pen" text="in illo suo genere omniu[m] princeps" type="straight"/>
        <underline language="LA" method="pen" text="gerendus est tibi mos adolescentibus" type="straight"/>
        <underline language="LA" method="pen" text="quotidianam loquacitatem sine vsu, neque ex scholis cantilenam requiru[n]t" type="straight"/>
        <underline language="LA" method="pen" text="in hoc domicilio imperii &amp;" type="straight"/>
        <underline language="LA" method="pen" text="vestigia qui persequi" type="straight"/>
        <underline language="LA" method="pen" text="eius sententiam sciscita[n]tur" type="straight"/>
        <underline language="LA" method="pen" text="deum" type="straight"/>
        <underline language="LA" method="pen" text="defugere" type="straight"/>
        <underline language="LA" method="pen" text="ad quam te duo excellentis ingenii adolescentes cupiunt accedere" type="straight"/>
        <underline language="LA" method="pen" text="grauabor" type="straight"/>
        <underline language="LA" method="pen" text="mihi dicendi aut nullam artem, aut pertenuem videri" type="straight"/>
        <underline language="LA" method="pen" text="contentionem inter homines doctos in verbi controuersia positam" type="straight"/>
        <underline language="LA" method="pen" text="ab opinionis arbitrio seiunctis" type="straight"/>
        <underline language="LA" method="pen" text="scientiàque comprehe[n]sis" type="straight"/>
        <underline language="LA" method="pen" text="ad vulgarem popularémque sensum acco[m]modata omnia genera huius forensis nostrae dictionis" type="straight"/>
        <underline language="LA" method="pen" text="obseruata sunt" type="straight"/>
        <underline language="LA" method="pen" text="callidis ac peritis animaduersa ac nostra, verbis designata, generibus illustrata" type="straight"/>
        <underline language="LA" method="pen" text="partibus distributa sunt" type="straight"/>
        <underline language="LA" method="pen" text="non, si minus" type="straight"/>
        <underline language="LA" method="pen" text="at" type="straight"/>
        <underline language="LA" method="pen" text="similitudo" type="straight"/>
        <underline language="LA" method="pen" text="maiora" type="straight"/>
        <underline language="LA" method="pen" text="ponerent" type="straight"/>
        <underline language="LA" method="pen" text="gratum te his esse Crasse facturu[m], si" type="straight"/>
        <underline language="LA" method="pen" text="has meas ineptias efferatis" type="straight"/>
        <underline language="LA" method="pen" text="moderabor ipse, ne vt quidam magister atque artifex, sed quasi vnus è togatoru[m] numero atque è forensi vsu homo mediocris, neque omnino rudis, videar non ipse aliquid à me prompsisse, sed fortuito in sermonem vestru[m] incidisse" type="straight"/>
        <underline language="LA" method="pen" text="cum peterem magistratus, solebam in prehe[n]sando dimittere à me Scaeuolam, cum ei ita dicere[m], me velle esse ineptu[m]" type="straight"/>
        <underline language="LA" method="pen" text="Id erat petere blandius" type="straight"/>
        <underline language="LA" method="pen" text="Hunc autem esse vnum homine[m] ex omnibus, quo praesente ego ineptus esse minime vellem, que[m] quide[m] nunc mearu[m] ineptiaru[m] testem &amp; spectatore[m] fortuna constituit" type="straight"/>
        <underline language="LA" method="pen" text="sit no[n] ineptu[m]" type="straight"/>
        <underline language="LA" method="pen" text="culpa[m]" type="straight"/>
        <underline language="LA" method="pen" text="ego praestabo" type="straight"/>
        <underline language="LA" method="pen" text="vim affere maxima[m]" type="straight"/>
        <underline language="LA" method="pen" text="celeres quidam motus esse debe[n]t, qui ad excogitandum acuti, &amp; ad explicandum ornandúmque sint vberes, &amp; ad memoria[m] firmi atque diuturni" type="straight"/>
        <underline language="LA" method="pen" text="praeclare enim se res" type="straight"/>
        
        <drawing book_orientation="0" method="pen" place="right_margin" type="florilegium"/>
        
    </annotation>
</transcription>
