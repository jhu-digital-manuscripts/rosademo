    <annotation>
        
        <underline language="LA" method="pen" text="si haec accendi, aut co[m]moueri arte possint: inseri quidem &amp; donari ab arte no[n] possunt" type="straight"/>
        <underline language="LA" method="pen" text="cum ipso homine nascuntur? linguae solutio, vocis sonus, latera, vires, conformatio quaedam &amp; figura totius oris &amp; corporis" type="straight"/>
        <underline language="LA" method="pen" text="vt ars aliquid limare non possit" type="straight"/>
        <underline language="LA" method="pen" text="aut ita lingua haesita[n]tes, aut ita voce absoni, aut ita vultu motúque corporis vasti atque agrestes" type="straight"/>
        <underline language="LA" method="pen" text="venire" type="straight"/>
        <underline language="LA" method="pen" text="ita naturae muneribus in iisdem rebus habiles" type="straight"/>
        <underline language="LA" method="pen" text="vt non nati" type="straight"/>
        <underline language="LA" method="pen" text="sed ab aliquo deo ficti esse videantur" type="straight"/>
        <underline language="LA" method="pen" text="offenditur" type="straight"/>
