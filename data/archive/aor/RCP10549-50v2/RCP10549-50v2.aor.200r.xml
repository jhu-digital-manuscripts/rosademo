<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v2.200r.tif" pagination="139" reader="John Dee"/>
    <annotation>
        
        <underline language="LA" method="pen" text="aut crudior fuit" type="straight"/>
        <underline language="LA" method="pen" text="quòd ita maluerit" type="straight"/>
        <underline language="LA" method="pen" text="subimus" type="straight"/>
        <underline language="LA" method="pen" text="non co[n]tinuo existimatur nescire gestum" type="straight"/>
        <underline language="LA" method="pen" text="reprehensum est, aut aeterna in eo, aut certe diuturna valet opinio tarditatis" type="straight"/>
        <underline language="LA" method="pen" text="qui cum mercede doceret" type="straight"/>
        <underline language="LA" method="pen" text="operam apud sese perdere" type="straight"/>
        <underline language="LA" method="pen" text="impellere" type="straight"/>
        <underline language="LA" method="pen" text="similem esse hominis" type="straight"/>
        <underline language="LA" method="pen" text="tradatur" type="straight"/>
        <underline language="LA" method="pen" text="inculcetur" type="straight"/>
        <underline language="LA" method="pen" text="sit tardior, posse percipere animo, &amp; memoria custodire" type="straight"/>
        <underline language="LA" method="pen" text="mobilitas linguae" type="straight"/>
        <underline language="LA" method="pen" text="celeritas verboru[m]" type="straight"/>
        <underline language="LA" method="pen" text="nobis non possumus fingere" type="straight"/>
        <underline language="LA" method="pen" text="In oratore autem acumen dialecticoru[m]" type="straight"/>
        <underline language="LA" method="pen" text="sententiae philosophoru[m], verba propè poëtarum, memoria iurisconsultoru[m], vox tragoedorum" type="straight"/>
        <underline language="LA" method="pen" text="gestus penè summoru[m] actorum est requirendus" type="straight"/>
        <underline language="LA" method="pen" text="nihil in hominu[m] genere rarius perfecto oratore inueniri potest: quae enim singularu[m] rerum artifices singula si mediocriter adepti sunt, probantur, ea nisi omnia summa sunt in oratore, probari no[n] possunt" type="straight"/>
        <underline language="LA" method="pen" text="in artificio &amp; opera quàm tenui &amp; leui" type="straight"/>
        <underline language="LA" method="pen" text="soleo" type="straight"/>
        <underline language="LA" method="pen" text="probabiles" type="straight"/>
        <underline language="LA" method="pen" text="ad hanc similitudine[m] huius histrionis, oratoriam laudem dirigamus" type="straight"/>
        <underline language="LA" method="pen" text="quàm nihil" type="straight"/>
        <underline language="LA" method="pen" text="moueat" type="straight"/>
        <underline language="LA" method="pen" text="vt in quo quisque artificio excelleret, is in suo genere Roscius diceretur" type="straight"/>
        <underline language="LA" method="pen" text="absolutionem" type="straight"/>
        <underline language="LA" method="pen" text="desydera[n]s, à qua ipse longe absum, facio impudenter" type="straight"/>
        <underline language="LA" method="pen" text="mihi enim volo ignosci, caeteris ipse no[n] ignosco" type="straight"/>
        <underline language="LA" method="pen" text="qui non potest" type="straight"/>
        <underline language="LA" method="pen" text="qui vitiose facit" type="straight"/>
        <underline language="LA" method="pen" text="quem denique non decet" type="straight"/>
        <underline language="LA" method="pen" text="detrudendum puto" type="straight"/>
        <underline language="LA" method="pen" text="summa atque" type="straight"/>
        <underline language="LA" method="pen" text="indolem" type="straight"/>
        <underline language="LA" method="pen" text="nec magis ad eos deterre[n]dos qui no[n] possent, quàm ad vos qui possetis, exacuendos acco[m]modaui" type="straight"/>
        <underline language="LA" method="pen" text="haec quae sunt in specie posita" type="straight"/>
        <underline language="LA" method="pen" text="diuina sunt" type="straight"/>
        <underline language="LA" method="pen" text="ipso habitu" type="straight"/>
        <underline language="LA" method="pen" text="voce pleniore[m]" type="straight"/>
        <underline language="LA" method="pen" text="quae quibus à natura minora data sunt" type="straight"/>
        <underline language="LA" method="pen" text="modice" type="straight"/>
        <underline language="LA" method="pen" text="scienter" type="straight"/>
        <underline language="LA" method="pen" text="&amp; vt ne dedeceat" type="straight"/>
        <underline language="LA" method="pen" text="minime est facile praecipere" type="straight"/>
        <underline language="LA" method="pen" text="sicut vnus paterfamiliâs his" type="straight"/>
        <underline language="LA" method="pen" text="caput esse artis decere: quod tamen vnu[m] id esse, quod tradi arte non possit" type="straight"/>
        <underline language="LA" method="pen" text="sermone[m] aliò transferamus" type="straight"/>
        <underline language="LA" method="pen" text="retines nos in hoc studio, nec ad aliam dimittis artem" type="straight"/>
        <underline language="LA" method="pen" text="quicquid est" type="straight"/>
        <underline language="LA" method="pen" text="potes" type="straight"/>
        <underline language="LA" method="pen" text="neque enim sumus nimis auidi, ista tua mediocri eloquentia conte[n]ti sumus" type="straight"/>
        <underline language="LA" method="pen" text="quantulum" type="straight"/>
        <underline language="LA" method="pen" text="expetenda" type="straight"/>
        <underline language="LA" method="pen" text="assume[n]dum" type="straight"/>
        
        <drawing book_orientation="0" method="pen" place="right_margin" type="florilegium"/>
        <drawing book_orientation="0" method="pen" place="right_margin" type="florilegium"/>
        
    </annotation>
