       <underline language="LA" method="pen" text="probabiliorem" type="straight"/>
        <underline language="LA" method="pen" text="si alter contemnere, alter ne nosse quidem Graecos videretur" type="straight"/>
        <underline language="LA" method="pen" text="nihil sanè ad hoc tempus" type="straight"/>
        <underline language="LA" method="pen" text="institutae scriptionis ac temporis" type="straight"/>
        <underline language="LA" method="pen" text="florere" type="straight"/>
        <underline language="LA" method="pen" text="praestare" type="straight"/>
        <underline language="LA" method="pen" text="definitam aliqua[m] regionem, cuius terminis septa teneatur" type="straight"/>
        <underline language="LA" method="pen" text="in hominum disceptationemn cadere possunt" type="straight"/>
        <underline language="LA" method="pen" text="in tantam prudentia[m] pertinerent" type="straight"/>
        <underline language="LA" method="pen" text="confirmo" type="straight"/>
        <underline language="LA" method="pen" text="feci libentius, vt" type="straight"/>
        <underline language="LA" method="pen" text="ma[n]darem literis" type="straight"/>
        <underline language="LA" method="pen" text="vt" type="straight"/>
        <underline language="LA" method="pen" text="tolleretur" type="straight"/>
        <underline language="LA" method="pen" text="custodirem literis" type="straight"/>
        <underline language="LA" method="pen" text="prope senescentem" type="straight"/>
        <underline language="LA" method="pen" text="ab obliuione hominu[m] atque à silentio vindicarem" type="straight"/>
        <underline language="LA" method="pen" text="cognosci" type="straight"/>
        <underline language="LA" method="pen" text="deberi" type="straight"/>
        <underline language="LA" method="pen" text="viua[m] illorum memoria[m] teneremus, hanc immortalem redderem, si possem" type="straight"/>
        <underline language="LA" method="pen" text="spe aggredior maiore ad probandu[m]" type="straight"/>
        <underline language="LA" method="pen" text="nullius memoria iam refellente" type="straight"/>
        <underline language="LA" method="pen" text="edo" type="straight"/>
        <underline language="LA" method="pen" text="iis qui neutrum illorum viderint, eorum, quibus ambo illi oratores cogniti sint, viuorum &amp; praesentium, memoria teste, co[m]mendemus" type="straight"/>
        <underline language="LA" method="pen" text="te charissime atque" type="straight"/>
        <underline language="LA" method="pen" text="quos tu agrestes putas" type="straight"/>
        <underline language="LA" method="pen" text="insequor vt erudia[m]" type="straight"/>
        <underline language="LA" method="pen" text="pudore à dicendo &amp; timiditate ingenua quadam refugisti" type="straight"/>
        <underline language="LA" method="pen" text="vnum putasti satis esse non modo in vna familia rhetorem, sed penè in tota ciuitate" type="straight"/>
        <underline language="LA" method="pen" text="ieiunitatem" type="straight"/>
        <underline language="LA" method="pen" text="acerrimis studiis" type="straight"/>
        <underline language="LA" method="pen" text="prudentiam rationémque dicendi per teipsum, vsum autem per nos percipere voluisti" type="straight"/>
        <underline language="LA" method="pen" text="non mediocre munus" type="straight"/>
        <underline language="LA" method="pen" text="porticu" type="straight"/>
        <underline language="LA" method="pen" text="co[m]motus" type="straight"/>
        <underline language="LA" method="pen" text="vt ipsorum vsus ferebat, amicissime consalutassent" type="straight"/>
        <underline language="LA" method="pen" text="tu nos ineptos licet, inquit, vel molestos putes" type="straight"/>
        <underline language="LA" method="pen" text="de Tusculano suo" type="straight"/>
        <underline language="LA" method="pen" text="mihi à se" type="straight"/>
        <underline language="LA" method="pen" text="conuentum" type="straight"/>
        
