        <underline language="LA" method="pen" text="Res mihi videtur esse, inquit, facultate praeclara, arte mediocris" type="straight"/>
        <underline language="LA" method="pen" text="quae sciunturor atoris" type="straight"/>
        <underline language="LA" method="pen" text="actio" type="straight"/>
        <underline language="LA" method="pen" text="Nam &amp; apud eos dicimus, qui nesciunt, &amp; ea dicimus, quae nescimus ipsi: itaque" type="straight"/>
        <underline language="LA" method="pen" text="cum plus vno verum esse no[n] possit" type="straight"/>
        <underline language="LA" method="pen" text="mendacio nixa" type="straight"/>
