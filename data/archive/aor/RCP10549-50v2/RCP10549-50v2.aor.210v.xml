        <underline language="LA" method="pen" text="addidit historiae maiore[m] sonum vocis vir" type="straight"/>
        <underline language="LA" method="pen" text="no[n] exornatores rerum" type="straight"/>
        <underline language="LA" method="pen" text="tantu[m]modo narratores" type="straight"/>
