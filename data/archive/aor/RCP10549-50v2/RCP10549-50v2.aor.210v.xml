        <underline language="LA" method="pen" text="remoti à" type="straight"/>
        <underline language="LA" method="pen" text="princeps" type="straight"/>
        <underline language="LA" method="pen" text="post illum" type="straight"/>
        <underline language="LA" method="pen" text="qui ita creber est rerum frequentia, vt verborum propè numerum sententiarum numero consequatur: ita porro verbis aptus &amp; pressus, vt nescias vtrum res oratione, an verba sententiis illustrentur" type="straight"/>
        <underline language="LA" method="pen" text="ex numero accepimus eorum, qui causas dictitarunt" type="straight"/>
        <underline language="LA" method="pen" text="à repub. remotus" type="straight"/>
        <underline language="LA" method="pen" text="id quod optimo cuique Athenis accidere solitum est, in exilium pulsus esset" type="straight"/>
        <underline language="LA" method="pen" text="ex clarissima rhetoris officina duo" type="straight"/>
        <underline language="LA" method="pen" text="causas omnino nunquam attigerunt" type="straight"/>
        <underline language="LA" method="pen" text="à philosophia profectus princeps" type="straight"/>
        <underline language="LA" method="pen" text="leniore quoda[m] sono est vsus: &amp; qui illum impetu[m] oratoris non habeat, vehemens fortasse minus, sed aliquanto tamen est, vt mihi quidem videtur, dulcior" type="straight"/>
        <underline language="LA" method="pen" text="Minimus natu" type="straight"/>
        <underline language="LA" method="pen" text="rerum copia" type="straight"/>
        <underline language="LA" method="pen" text="sententiarum varietate abunda[n]tissimus" type="straight"/>
        <underline language="LA" method="pen" text="ipsa compositione verborum no[n] impolitus" type="straight"/>
        <underline language="LA" method="pen" text="magnam eloquentiam ad scribe[n]dum attulit, sed nullum vsum forensem" type="straight"/>
        <underline language="LA" method="pen" text="Graece negant scire" type="straight"/>
        <underline language="LA" method="pen" text="quàm scienter? quàm proprie de vnoquoque dixit" type="straight"/>
        <underline language="LA" method="pen" text="admirans illud iam mirari desino, quod" type="straight"/>
        <underline language="LA" method="pen" text="posse tantum" type="straight"/>
        <underline language="LA" method="pen" text="aucupans" type="straight"/>
        <underline language="LA" method="pen" text="fatebor aliquid tamen, vt cum in sole ambulem, etiam si aliam ob causam ambulem, fieri natura tame[n] vt colorer: sic cum istos libros ad Misenum (nam Romae vix licet) studiosius legerim, sentio orationem meam illorum cantu quasi colorari" type="straight"/>
        <underline language="LA" method="pen" text="latius hoc vobis patere" type="straight"/>
        <underline language="LA" method="pen" text="voluerunt à vulgo intelligi" type="straight"/>
        <underline language="LA" method="pen" text="deceptus indicibus" type="straight"/>
        <underline language="LA" method="pen" text="angustis" type="straight"/>
        <underline language="LA" method="pen" text="co[n]cisis disputationibus illigati" type="straight"/>
        <underline language="LA" method="pen" text="aliena quadam lingua locutos no[n]" type="straight"/>
        <underline language="LA" method="pen" text="vt videantur voluisse esse nobis, qui non sumus eruditissimi, familiares" type="straight"/>
        <underline language="LA" method="pen" text="Sed illuc redeo" type="straight"/>
        
