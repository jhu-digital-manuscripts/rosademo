        <underline language="LA" method="pen" text="cu[m] quo verbo quid appellandu[m] sit, co[n]tenditur" type="straight"/>
        <underline language="LA" method="pen" text="maiestatem imminutam" type="straight"/>
        <underline language="LA" method="pen" text="pendebat" type="straight"/>
        <underline language="LA" method="pen" text="quod causam facit" type="straight"/>
        <underline language="LA" method="pen" text="perquàm puerile" type="straight"/>
        <underline language="LA" method="pen" text="vt neque absit quicqua[m], neque supersit" type="straight"/>
        <underline language="LA" method="pen" text="dilatauit" type="straight"/>
