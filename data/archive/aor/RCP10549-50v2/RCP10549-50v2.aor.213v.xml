        <underline language="LA" method="pen" text="suggesta sunt" type="straight"/>
        <underline language="LA" method="pen" text="non nouu[m] genus nascitur, sed superioris generis causa duplicatur" type="straight"/>
        <underline language="LA" method="pen" text="referendis praeteritis verbis id scriptu[m], quodcu[m]que defendimus, suppleatur" type="straight"/>
        <underline language="LA" method="pen" text="Iterum aute[m] peccant" type="straight"/>
        <underline language="LA" method="pen" text="disceptatur" type="straight"/>
        <underline language="LA" method="pen" text="seiungu[n]t" type="straight"/>
