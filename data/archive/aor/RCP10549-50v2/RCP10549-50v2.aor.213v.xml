        <underline language="LA" method="pen" text="Iure aute[m] omnia defenduntur, quae sunt eius generis" type="straight"/>
        <underline language="LA" method="pen" text="aut imprude[n]tia" type="straight"/>
        <underline language="LA" method="pen" text="casu" type="straight"/>
