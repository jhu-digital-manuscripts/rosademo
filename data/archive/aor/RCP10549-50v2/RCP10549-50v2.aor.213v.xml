        <underline language="LA" method="pen" text="co[m]mendationem habet nostra[m]" type="straight"/>
        <underline language="LA" method="pen" text="Ita ratio omnis dice[n]di tribus ad persuadendu[m] rebus est nixa: vt probemus vera esse ea quae defendimus: vt co[n]ciliemus nobis eos qui audiu[n]t: vt animos eoru[m] ad quencunque causa postulabit motu[m], vocemus" type="straight"/>
        <underline language="LA" method="pen" text="oratori subiecta materies" type="straight"/>
        <underline language="LA" method="pen" text="vna rerum earum" type="straight"/>
        <underline language="LA" method="pen" text="quae no[n] excogitantur ab oratore, sed in re proposita ratione tractantur" type="straight"/>
        <underline language="LA" method="pen" text="pariuntur" type="straight"/>
        <underline language="LA" method="pen" text="deferentur" type="straight"/>
        <underline language="LA" method="pen" text="argumentatione" type="straight"/>
        <underline language="LA" method="pen" text="Ita in superiore genere de tractandis argumentis, in hoc autem etia[m] de inueniendis cogitandum est" type="straight"/>
        <underline language="LA" method="pen" text="causas in plura genera secueru[n]t" type="straight"/>
        <underline language="LA" method="pen" text="argumentorum copiam suggerunt" type="straight"/>
        <underline language="LA" method="pen" text="posita sit" type="straight"/>
        <underline language="LA" method="pen" text="habeant quo se referant, vnde statim expedita possint argumenta depromere" type="straight"/>
        <underline language="LA" method="pen" text="riuulos consectari" type="straight"/>
        <underline language="LA" method="pen" text="fontes rerum non videre" type="straight"/>
        <underline language="LA" method="pen" text="etiam aetatis est vsúsque nostri, à capite quod velimus arcessere, &amp;" type="straight"/>
        
