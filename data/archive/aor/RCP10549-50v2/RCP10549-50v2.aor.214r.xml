<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v2.214r.tif" pagination="167" reader="John Dee" signature="l.iiii."/>   
    <annotation>
        
        <underline language="LA" method="pen" text="vnde omnia manant, videre" type="straight"/>
        <underline language="LA" method="pen" text="deferuntur" type="straight"/>
        <underline language="LA" method="pen" text="meditatum nobis in perpetuum" type="straight"/>
        <underline language="LA" method="pen" text="debebit" type="straight"/>
        <underline language="LA" method="pen" text="separatim dicere solemus de genere vniuerso" type="straight"/>
        <underline language="LA" method="pen" text="multa co[m]mentatione atque meditatione paratos atque expeditos" type="straight"/>
        <underline language="LA" method="pen" text="Longu[m] est" type="straight"/>
        <underline language="LA" method="pen" text="Haec sunt omnia ingenii vel mediocris, exercitationis autem vel maximae: arte[m] quidem &amp; praecepta du[m]taxat hactenus requirunt, vt certis dicendi luminibus ornentur" type="straight"/>
        <underline language="LA" method="pen" text="pariuntur" type="straight"/>
        <underline language="LA" method="pen" text="excogitationem non habent difficilem" type="straight"/>
        <underline language="LA" method="pen" text="explicationem magis illustrem perpolitámque desyderant" type="straight"/>
        <underline language="LA" method="pen" text="arte tinctum" type="straight"/>
        <underline language="LA" method="pen" text="artem requirit" type="straight"/>
        <underline language="LA" method="pen" text="vis illa diuina virtúsque cernitur" type="straight"/>
        <underline language="LA" method="pen" text="non recusabo, quo minus perpoliam atque conficiam: quantum consequar" type="straight"/>
        <underline language="LA" method="pen" text="valent" type="straight"/>
        <underline language="LA" method="pen" text="praesto est" type="straight"/>
        <underline language="LA" method="pen" text="qui hoc primus in nostros mores induxit" type="straight"/>
        <underline language="LA" method="pen" text="qui maxime auxit, qui solus effecit" type="straight"/>
        <underline language="LA" method="pen" text="no[n] reuerens assentandi suspicionem" type="straight"/>
        <underline language="LA" method="pen" text="quem aetas nostra tulerit" type="straight"/>
        <underline language="LA" method="pen" text="Itaque siquid est in me, quod iam sperare videor" type="straight"/>
        <underline language="LA" method="pen" text="tantum operae mihi ad audiendum datis" type="straight"/>
        <underline language="LA" method="pen" text="quod non in memoria mea penitus insederit" type="straight"/>
        <underline language="LA" method="pen" text="is qui sum, qua[n]tuscunque sum, adiudicandus omnibus auditis oratoribus, sine vlla dubitatione sic statuo &amp; iudico" type="straight"/>
        <underline language="LA" method="pen" text="ornamenta dicendi" type="straight"/>
        <underline language="LA" method="pen" text="iniqua partitio, si cu[m] ego hunc oratore[m], quem nunc fingo, vt institui, crearo, aluero, confirmaro, trada[m] eum Crasso &amp; vestiendum &amp; orna[n]dum" type="straight"/>
        <underline language="LA" method="pen" text="Neque enim boni est, neque liberalis pare[n]tis, quem procrearit &amp; eduxerit, eum non &amp; vestire &amp; ornare, praesertim cum te locupletem esse negare non possis" type="straight"/>
        <underline language="LA" method="pen" text="in causa peroranda non dubitauit excitare reum Consulare[m], &amp; eius diloricare tunica[m], &amp; iudicibus cicatrices auersas senis Imperatoris ostendere" type="straight"/>
        <underline language="LA" method="pen" text="quos praestare nemo possit" type="straight"/>
        <underline language="LA" method="pen" text="essent exacti" type="straight"/>
        <underline language="LA" method="pen" text="Tribunitia potestas esset constituta" type="straight"/>
        <underline language="LA" method="pen" text="neque reprimi" type="straight"/>
        <underline language="LA" method="pen" text="iure esse conflatam" type="straight"/>
        <underline language="LA" method="pen" text="tam anceps" type="straight"/>
        <underline language="LA" method="pen" text="tam lubricus" type="straight"/>
        <underline language="LA" method="pen" text="co[m]miseratione" type="straight"/>
        <underline language="LA" method="pen" text="enituit, quod tibi omnes dant" type="straight"/>
        <underline language="LA" method="pen" text="quae nu[n]c ad me delegare vis" type="straight"/>
        <underline language="LA" method="pen" text="inter vos in dice[n]do dissimillimi sitis" type="straight"/>
        <underline language="LA" method="pen" text="vt ei nihil neque à natura denegatu[m], neque à doctrina no[n] delatu[m] esse videatur" type="straight"/>
        <underline language="LA" method="pen" text="tua suauitate nos priuabis" type="straight"/>
        <underline language="LA" method="pen" text="non potuisse potius, quàm à Crasso dici maluisse" type="straight"/>
        <underline language="LA" method="pen" text="Quin tu" type="straight"/>
        
        <mark method="pen" name="vertical_bar" place="right_margin"/>
        
        
    </annotation>
</transcription>
