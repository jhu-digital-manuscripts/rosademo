        <marginalia anchor_text="aquula" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>aquula.</marginalia_text>
                </position>
            </language>
            <translation>A rivulet.</translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="illam sapientam constituendae ciuitatis duobus propè seculis antè cognouit" type="straight"/>
        <underline language="LA" method="pen" text="tulit" type="straight"/>
        <underline language="LA" method="pen" text="gloria clariores" type="straight"/>
        <underline language="LA" method="pen" text="authoritate grauiores" type="straight"/>
        <underline language="LA" method="pen" text="humanitate politiores" type="straight"/>
        <underline language="LA" method="pen" text="propè bellum indixeris" type="straight"/>
        <underline language="LA" method="pen" text="ac sic decreui philosophari potius, vt Neoptolemus apud Ennium, paucis" type="straight"/>
        <underline language="LA" method="pen" text="opinionem istorum studiorum, &amp; suspicionem artificii apud eos qui res iudicent, oratori aduersariam esse arbitror" type="straight"/>
        <underline language="LA" method="pen" text="vt eò reuocetur vnde huc declinauit oratio" type="straight"/>
        <underline language="EL" method="pen" text="διαλεκτικὴν" type="straight"/>         
        <underline language="LA" method="pen" text="simpliciter" type="straight"/>
        <underline language="LA" method="pen" text="coniuncte sit elatum" type="straight"/>
        <underline language="LA" method="pen" text="se compu[n]gunt suis acuminibus" type="straight"/>
        <underline language="LA" method="pen" text="quibus ante exorsa" type="straight"/>
        <underline language="LA" method="pen" text="potius detexta propè retexantur" type="straight"/>
        <underline language="LA" method="pen" text="non liquidu[m], non fusum ac profluens" type="straight"/>
        <underline language="LA" method="pen" text="exile" type="straight"/>
        <underline language="LA" method="pen" text="aridum, concisum" type="straight"/>
        <underline language="LA" method="pen" text="minutum" type="straight"/>
        <underline language="LA" method="pen" text="ad oblectandos animos, ad impellendos" type="straight"/>
        <underline language="LA" method="pen" text="quae non aurificis statera, sed quada[m] populari trutina examina[n]tur" type="straight"/>
        <underline language="LA" method="pen" text="dimittamus" type="straight"/>
        <underline language="LA" method="pen" text="muta nimiu[m] est, in" type="straight"/>
        <underline language="LA" method="pen" text="nimium loquax" type="straight"/>
        <underline language="LA" method="pen" text="co[m]memoras" type="straight"/>
        <underline language="LA" method="pen" text="Erat enim ab isto Aristotele" type="straight"/>
        <underline language="LA" method="pen" text="omnium superioru[m]" type="straight"/>
        <underline language="LA" method="pen" text="hos germanos huius artis magistros" type="straight"/>
        <underline language="LA" method="pen" text="eadem acie me[n]tis" type="straight"/>
        <underline language="LA" method="pen" text="haec quoque aspexit" type="straight"/>
        <underline language="LA" method="pen" text="perquàm esset opta[n]da nobis, qui nullam vnquam in illis suis disputationibus rem defendit, quam non probarit: nullam oppugnauit, quam non euerterit" type="straight"/>
        <underline language="LA" method="pen" text="hoc maius est quiddam, quàm ab iis, qui haec tradunt &amp; docent, postulandum sit. Ego" type="straight"/>
        <underline language="LA" method="pen" text="rudem" type="straight"/>
        <underline language="LA" method="pen" text="vno opere eandem incudem diem noctémque tunde[n]tibus, qui omnes tenuissimas particulas, atque omnia minima mansa (vt aiunt nutrices infantibus pueris) in os inserant" type="straight"/>
        <underline language="LA" method="pen" text="illuc eum rapia[m], vbi non seclusa aliqua aquula teneatur, sed vnde vniuersum flumen erumpat, &amp; illi sedes, &amp; tanquam domicilia omnium argumentoru[m] commonstret" type="straight"/>
        <underline language="LA" method="pen" text="in quo haereat, qui" type="straight"/>
        
