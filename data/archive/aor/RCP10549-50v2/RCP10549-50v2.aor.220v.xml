<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v2.220v.tif" pagination="180" reader="John Dee" signature="[m.ii[v]]"/> 
    <annotation>
        
        <underline language="LA" method="pen" text="foenu[m] alios aiebat esse oportere" type="straight"/>
        <underline language="LA" method="pen" text="vt ambrosia alendus esse videatur" type="straight"/>
        <underline language="LA" method="pen" text="ad Antonii reliqua redeamus" type="straight"/>
        <underline language="LA" method="pen" text="defessus iam labore atque itinere disputationis meae" type="straight"/>
        <underline language="LA" method="pen" text="requiescam in Caesaris sermone, quasi in aliquo peropportuno diuersorio" type="straight"/>
        <underline language="LA" method="pen" text="non nimium liberale hospitium meum dices: nam te in viam, simul ac perpaululum gustaris, extrudam &amp; eiiciam" type="straight"/>
        <underline language="LA" method="pen" text="vnum" type="straight"/>
        <underline language="LA" method="pen" text="alterum" type="straight"/>
        <underline language="LA" method="pen" text="velle risum mouere" type="straight"/>
        <underline language="LA" method="pen" text="repente erumpat" type="straight"/>
        <underline language="LA" method="pen" text="viderit Democritus" type="straight"/>
        <underline language="LA" method="pen" text="quidem illi scirent, qui pollicerentur" type="straight"/>
        <underline language="LA" method="pen" text="Locus autem &amp; regio quasi ridiculi" type="straight"/>
        <underline language="LA" method="pen" text="turpitudine &amp; deformitate quadam co[n]tinetur" type="straight"/>
        <underline language="LA" method="pen" text="quae notant &amp; designa[n]t turpitudine[m] aliquam no[n] turpiter" type="straight"/>
        <underline language="LA" method="pen" text="acumen vno saepe in verbo positum, maxime respondentis, no[n]nunquam etiam lacessentis" type="straight"/>
        <underline language="LA" method="pen" text="frangit aduersarium" type="straight"/>
        <underline language="LA" method="pen" text="impedit" type="straight"/>
        <underline language="LA" method="pen" text="eleuat" type="straight"/>
        <underline language="LA" method="pen" text="deterret" type="straight"/>
        <underline language="LA" method="pen" text="refutat" type="straight"/>
        <underline language="LA" method="pen" text="seueritatem mitigat &amp; relaxat" type="straight"/>
        <underline language="LA" method="pen" text="dilui no[n] facile est, ioco risúque dissoluit" type="straight"/>
        <underline language="LA" method="pen" text="in quarto loco quaere[n]di posueramus" type="straight"/>
        <underline language="LA" method="pen" text="insignis improbitas, &amp; scelere iuncta, nec rursus miseria insignis agitata ridetur. facinorosos enim maiore quada[m] vi quàm ridiculi vulnerari volunt: miseros illudi nolunt, nisi si se forte iactant" type="straight"/>
        <underline language="LA" method="pen" text="Parcendum est aute[m] maxime charitati hominu[m], ne temere in eos dicas, qui diligu[n]tur" type="straight"/>
        <underline language="LA" method="pen" text="ad suppliciu[m] rapiendi videntur" type="straight"/>
        <underline language="LA" method="pen" text="eáque belle agitata ridentur" type="straight"/>
        <underline language="LA" method="pen" text="Est etiam deformitatis &amp; corporis vitiorum satis bella materies ad iocandum" type="straight"/>
        <underline language="LA" method="pen" text="insulse" type="straight"/>
        <underline language="LA" method="pen" text="perridicule possis" type="straight"/>
        <underline language="LA" method="pen" text="scurrilis" type="straight"/>
        <underline language="LA" method="pen" text="mimicus" type="straight"/>
        <underline language="LA" method="pen" text="fabella" type="straight"/>
        <underline language="LA" method="pen" text="de amicula rixatus: salsa, attame[n] à teipso ficta tota narratio" type="straight"/>
        <underline language="LA" method="pen" text="addidisti clausulam" type="straight"/>
        <underline language="LA" method="pen" text="tum omnibus in parietibus inscriptas fuisse literas, tria LLL, duo MM. Cum quaereres id quid esset, senem tibi que[m]dam oppidanum dixisse, Lacerat lacertum Largii mordax Memmius. Perspicitis" type="straight"/>
        <underline language="LA" method="pen" text="mendaciunculis aspergendum" type="straight"/>
        <underline language="LA" method="pen" text="Est aute[m] haec huius generis virtus" type="straight"/>
        <underline language="LA" method="pen" text="In re est item ridiculu[m], quod ex quada[m] deprauata imitatione sumi solet" type="straight"/>
        <underline language="LA" method="pen" text="extento brachio paululum etiam de gestu addidit" type="straight"/>
        <underline language="LA" method="pen" text="Mimoru[m] est enim ethologoru[m]" type="straight"/>
        
