        <underline language="LA" method="pen" text="num claudicat" type="straight"/>
        <underline language="LA" method="pen" text="at hic clodicat" type="straight"/>
        <underline language="LA" method="pen" text="At in male olentem, video me à te circunueniri" type="straight"/>
        <underline language="LA" method="pen" text="verbi ad literam immutati similitudo" type="straight"/>
        <underline language="LA" method="pen" text="Ex ambiguo dicta, vel argutissima putanter" type="straight"/>
        <underline language="LA" method="pen" text="coronam sibi in conuiuio ad caput acco[m]modanti, cum ea saepius rumperetur" type="straight"/>
