        <underline language="LA" method="pen" text="breuior etiam quàm ipse testis" type="straight"/>
        <underline language="LA" method="pen" text="quae cadere possunt in quos nolis, quanuis sint bella, sunt tamen ipso genere scurrilia, vt" type="straight"/>
        <underline language="LA" method="pen" text="delabitur" type="straight"/>
        <underline language="LA" method="pen" text="ex tempore" type="straight"/>
        <underline language="LA" method="pen" text="raritas dictorum distinguet oratorem à scurra" type="straight"/>
        <underline language="LA" method="pen" text="cum causa dicimus" type="straight"/>
        <underline language="LA" method="pen" text="candidatus A." type="straight"/>
        <underline language="LA" method="pen" text="qui est mea sententia vel tenuissimus ingenii fructus" type="straight"/>
        <underline language="LA" method="pen" text="domina natura est" type="straight"/>
        <underline language="LA" method="pen" text="summatim" type="straight"/>
        <underline language="LA" method="pen" text="id aliàs in re habere, aliàs in verbo facetias" type="straight"/>
        <underline language="LA" method="pen" text="seuere" type="straight"/>
