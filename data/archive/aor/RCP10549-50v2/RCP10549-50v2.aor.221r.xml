    <annotation>
        
        <underline language="LA" method="pen" text="obscoenitas" type="straight"/>
        <underline language="LA" method="pen" text="surripiat oportet imitatione[m]" type="straight"/>
        <underline language="LA" method="pen" text="eius ridiculi, quod in re positum est" type="straight"/>
        <underline language="LA" method="pen" text="iniecta in aliquo insigni ad irridendum vitio reperiantur" type="straight"/>
        <underline language="LA" method="pen" text="quodam acumine mouetur" type="straight"/>
        <underline language="LA" method="pen" text="est mimorum ethologorum similitudo" type="straight"/>
        <underline language="LA" method="pen" text="scurrilis oratori dicacitas magnopere fugienda est" type="straight"/>
        <underline language="LA" method="pen" text="potuerit dictum dici, necesse habeamus dicere" type="straight"/>
        <underline language="LA" method="pen" text="Pusillus testis processit" type="straight"/>
        <underline language="LA" method="pen" text="quaesitor" type="straight"/>
        <underline language="LA" method="pen" text="perpusillum rogabo" type="straight"/>
