        <underline language="LA" method="pen" text="quas personas agitare solemus non sustinere" type="straight"/>
        <underline language="LA" method="pen" text="furtim, siquando &amp; cursim" type="straight"/>
        <underline language="LA" method="pen" text="oris deprauatio" type="straight"/>
        <underline language="LA" method="pen" text="belle" type="straight"/>
        <underline language="LA" method="pen" text="literate" type="straight"/>
        <underline language="LA" method="pen" text="studiose pila luderet" type="straight"/>
        <underline language="LA" method="pen" text="signa sacra noctu frangere putaretur" type="straight"/>
        <underline language="LA" method="pen" text="confixum" type="straight"/>
        <underline language="LA" method="pen" text="sextantis" type="straight"/>
        <underline language="LA" method="pen" text="vim verbi in aliud, atque caeteri accipiant, posse ducere: sed" type="straight"/>
        <underline language="LA" method="pen" text="notissimu[m] ridiculi genus, cum aliud expectamus, aliud dicitur" type="straight"/>
        <underline language="LA" method="pen" text="fit salsius" type="straight"/>
        <underline language="LA" method="pen" text="Quanti addictus" type="straight"/>
        <underline language="LA" method="pen" text="venustissimu[m]" type="straight"/>
        <underline language="LA" method="pen" text="in altercatione arripitur ab aduersario verbum, &amp; ex eo, vt à Catulo in Philippum, in eum ipsum aliquid, qui lacessiuit, infligitur" type="straight"/>
        <underline language="LA" method="pen" text="attendere &amp; aucupari verba oportebit" type="straight"/>
        <underline language="LA" method="pen" text="frigidiora vitemus: etenim" type="straight"/>
        <underline language="LA" method="pen" text="accersitum dictum" type="straight"/>
        <underline language="LA" method="pen" text="paruam verbi immutatione[m], quod in litera positum Graeci voca[n]t , vt nobiliorem mobiliorem Cato" type="straight"/> 
        <underline language="EL" method="pen" text="παρονομασίαν" type="straight"/>
        <underline language="LA" method="pen" text="Eamus deambulatum: &amp; ille, Quid opus fuit de? imò vero, inquit, Quid opus fuit te" type="straight"/>
        <underline language="LA" method="pen" text="aduersus &amp; auersus impudicus es" type="straight"/>
        <underline language="LA" method="pen" text="interpretatio nominis habet acumen, cu[m] ad ridiculum co[n]uertas, quamobre[m] ita quis vocetur" type="straight"/>
        <underline language="LA" method="pen" text="diuisorem" type="straight"/>
        <underline language="LA" method="pen" text="in campo Martio nomen inuenisse" type="straight"/>
        <underline language="LA" method="pen" text="à se pecunia[m] profectam diceret testis, &amp; haberet filium delicatiorem, abeunte iam illo" type="straight"/>
        <underline language="LA" method="pen" text="tactum triginta" type="straight"/>
        <underline language="LA" method="pen" text="coniiciuntur" type="straight"/>
        <underline language="LA" method="pen" text="omneis prouincias stipendia merentem se peragrasse gloriaretur" type="straight"/>
        <underline language="LA" method="pen" text="abeo à mimis" type="straight"/>
        <underline language="LA" method="pen" text="bene ante lucem venisset" type="straight"/>
        <underline language="LA" method="pen" text="iubebis igitur te, inquit, suscitari" type="straight"/>
        <underline language="LA" method="pen" text="certe negaras te molestum futurum" type="straight"/>
        
