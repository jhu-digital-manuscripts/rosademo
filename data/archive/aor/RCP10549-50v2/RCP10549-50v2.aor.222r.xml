    <page filename="RCP10549-50v2.222r.tif" pagination="183" reader="John Dee" signature="m.iiii."/>   
    <annotation>
        
        <underline language="LA" method="pen" text="renuntiaret" type="straight"/>
        <underline language="LA" method="pen" text="natura enim nos (vt antè dixi) noster delectat error, ex quo cum quasi decepti sumus expectatione, ridemus" type="straight"/>
        <underline language="LA" method="pen" text="immutata oratione" type="straight"/>
        <underline language="LA" method="pen" text="vnius verbi translatione" type="straight"/>
        <underline language="LA" method="pen" text="ex inuersione verboru[m]" type="straight"/>
        <underline language="LA" method="pen" text="annale[m]" type="straight"/>
        <underline language="LA" method="pen" text="dissuasor" type="straight"/>
        <underline language="LA" method="pen" text="Vt sementem feceris, ita metes" type="straight"/>
        <underline language="LA" method="pen" text="qui cum interpellaret odiose, Audiamus, inquit, pulchellum pueru[m] Crassus" type="straight"/>
        <underline language="LA" method="pen" text="Cum esset arrisum" type="straight"/>
        <underline language="LA" method="pen" text="Audiamus, inquit, disertum" type="straight"/>
        <underline language="LA" method="pen" text="verba relata contrarie" type="straight"/>
        <underline language="LA" method="pen" text="cortem" type="straight"/>
        <underline language="LA" method="pen" text="subturpia" type="straight"/>
        <underline language="LA" method="pen" text="narrationes apologorum" type="straight"/>
        <underline language="LA" method="pen" text="cum Sex. Titius se Cassandra[m] esse diceret, Multos, inquit Antonius, possum tuos Aiaces Oileos nominare" type="straight"/>
        <underline language="LA" method="pen" text="collationem habet, aut tanquam imaginem" type="straight"/>
        <underline language="LA" method="pen" text="innumerabilem" type="straight"/>
        <underline language="LA" method="pen" text="pecuniam" type="straight"/>
        <underline language="LA" method="pen" text="tenuitate Magii redargueret" type="straight"/>
        <underline language="LA" method="pen" text="non conseruasse dico, sed tanquam nudus nuces legeret in ventrem abstulisse" type="straight"/>
        <underline language="LA" method="pen" text="nostros homines similes esse Syrorum vaenalium, vt quisque optime Graece sciret, ita esse nequissimum. Valde" type="straight"/>
        <underline language="LA" method="pen" text="demonstraui digito pictum Gallum in Mariano scuto Cimbrico sub nodis distortum, eiecta lingua, buccis fluentibus" type="straight"/>
        <underline language="LA" method="pen" text="mentum in dicendo intorquenti" type="straight"/>
        <underline language="LA" method="pen" text="ad incredibilem admirationem efferuntur" type="straight"/>
        <underline language="LA" method="pen" text="ipsum magnum videri Memmium, vt in forum descendens caput ad fornicem Fabii demitteret" type="straight"/>
        <underline language="LA" method="pen" text="Si quintum pareret mater eius, asinum fuisse parituram" type="straight"/>
        <underline language="LA" method="pen" text="parua re, &amp; saepe verbo res obscura &amp; latens illustratur" type="straight"/>
        <underline language="LA" method="pen" text="furax" type="straight"/>
        <underline language="LA" method="pen" text="nihil est quo mihi gratias agas, inquit, si malui co[m]pilari, quàm vaenire" type="straight"/>
        <underline language="LA" method="pen" text="lustrum" type="straight"/>
        
    </annotation>
