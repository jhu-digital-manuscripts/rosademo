<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v2.222v.tif" pagination="184" reader="John Dee" signature="[m.iiii.[v]]"/>   
    <annotation>
        
        <underline language="LA" method="pen" text="ex aerariis exemit, lustrum condidit, &amp; tauru[m] immolauit" type="straight"/>
        <underline language="LA" method="pen" text="religione ciuitatem obstrinxisse" type="straight"/>
        <underline language="LA" method="pen" text="ignominia leuarit" type="straight"/>
        <underline language="LA" method="pen" text="aurum repensum" type="straight"/>
        <underline language="LA" method="pen" text="ta[n]ta malorum est multitudo ciuium, vt tibi ego hoc confirmem, si Romae manseris, te paucis annis ad maximas pecunias esse venturum" type="straight"/>
        <underline language="EL" method="pen" text="εἴρωνα" type="straight"/>
        <underline language="LA" method="pen" text="hac ironia dissimulantiáque longe lepôre &amp; humanitate omnibus praestitisse" type="straight"/>
        <underline language="LA" method="pen" text="no[n] maiora forensium actionum, quàm omniu[m] sermonu[m] condimenta sunt" type="straight"/>
        <underline language="LA" method="pen" text="per mihi scitum videtur" type="straight"/>
        <underline language="LA" method="pen" text="P. Mummium cuiuis tempori hominem esse" type="straight"/>
        <underline language="LA" method="pen" text="honesto verbo vitiosa res appellatur: vt" type="straight"/>
        <underline language="LA" method="pen" text="ab eo notaretur" type="straight"/>
        <underline language="LA" method="pen" text="Non amo, inquit, nimium diligentes" type="straight"/>
        <underline language="LA" method="pen" text="aliud excipias atque ille vult" type="straight"/>
        <underline language="LA" method="pen" text="multáque ex ea praelia praeclara fecisset, cu[m]" type="straight"/>
        <underline language="LA" method="pen" text="id oppidum recepisset" type="straight"/>
        <underline language="LA" method="pen" text="Quid ni, inquit, meminerim? nunquam ego recepissem, nisi tu perdidisses" type="straight"/>
        <underline language="LA" method="pen" text="subabsurda" type="straight"/>
        <underline language="LA" method="pen" text="mimis perapposita" type="straight"/>
        <underline language="LA" method="pen" text="quodammodo nobis" type="straight"/>
        <underline language="LA" method="pen" text="rem habere coepit" type="straight"/>
        <underline language="LA" method="pen" text="de ambitu postulatum" type="straight"/>
        <underline language="LA" method="pen" text="tibi tuum negotium agere licebit" type="straight"/>
        <underline language="LA" method="pen" text="tardum" type="straight"/>
        <underline language="LA" method="pen" text="Tu igitur nihil vides? Ego vero inquam à porta Exquilina video villam tuam" type="straight"/>
        <underline language="LA" method="pen" text="ab ostio quaerenti Ennium" type="straight"/>
        <underline language="LA" method="pen" text="à ianua quaereret" type="straight"/>
        <underline language="LA" method="pen" text="ego cum te quaererem, ancillae tuae credidi te domi non esse, tu mihi non credis ipsi" type="straight"/>
        <underline language="LA" method="pen" text="male audisset, festiuo homini" type="straight"/>
        <underline language="LA" method="pen" text="cum tua colu &amp; lana" type="straight"/>
        <underline language="LA" method="pen" text="ad famosas vetuit" type="straight"/>
        <underline language="LA" method="pen" text="suspicionem ridiculi absconditam" type="straight"/>
        <underline language="LA" method="pen" text="suspe[n]disse se de ficu" type="straight"/>
        <underline language="LA" method="pen" text="Amabo te, inquit, da mihi ex ista arbore quos" type="straight"/>
        
    </annotation>
</transcription>
