<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v2.223v.tif" pagination="186" reader="John Dee"/>   
    <annotation>
        
        <underline language="LA" method="pen" text="laudari magis, quàm rideri solent" type="straight"/>
        <underline language="LA" method="pen" text="partibus sunt innumerabilia" type="straight"/>
        <underline language="LA" method="pen" text="generibus pauca" type="straight"/>
        <underline language="LA" method="pen" text="Expectationibus enim decipiendis, &amp; naturis alioru[m] irridendis, ipsorum ridicule indicandis, &amp; similitudine turpioris, &amp; dissimulatione, &amp; subabsurda dicendo, &amp; dicendo, &amp; stulta reprehe[n]dendo, risus mouentur" type="straight"/>
        <underline language="LA" method="pen" text="imbuendus est" type="straight"/>
        <underline language="LA" method="pen" text="iocose volet dicere" type="straight"/>
        <underline language="LA" method="pen" text="tanquam in Pontinum diuerteris, neque amoenum, neque salubrem locu[m], censeo vt satis diu te putes requieuisse, &amp; iter reliquum co[n]ficere pergas" type="straight"/>
        <underline language="LA" method="pen" text="hilare quide[m] à te acceptus" type="straight"/>
        <underline language="LA" method="pen" text="reliqua nascu[n]tur omnia" type="straight"/>
        <underline language="LA" method="pen" text="co[n]stituo" type="straight"/>
        <underline language="LA" method="pen" text="in dicendi disceptationem aut controuersiam vocari" type="straight"/>
        <underline language="LA" method="pen" text="vt boni quod habeat, id amplectar, exornem, exaggerem, ibi co[m]morer, ibi habitem, ibi haeream: à malo autem vitióque causae ita recedam, non vt id me defugere appareat, sed vt totum bono illo orna[n]do &amp; augendo dissimulatu[m] obruatur" type="straight"/>
        <underline language="LA" method="pen" text="causa est in argumentis" type="straight"/>
        <underline language="LA" method="pen" text="aduersario firmior" type="straight"/>
        <underline language="LA" method="pen" text="omnia in illum confera[m] tela" type="straight"/>
        <underline language="LA" method="pen" text="facilius probari, quàm illa redargui" type="straight"/>
        <underline language="LA" method="pen" text="abducere animos à" type="straight"/>
        <underline language="LA" method="pen" text="ad nostram conor traducere" type="straight"/>
        <underline language="LA" method="pen" text="mihi pro meo iure sumo" type="straight"/>
        <underline language="LA" method="pen" text="siquae premat res vehementius, ita co[n]cedere solere, vt non modo non abiecto, sed ne reiecto quide[m] scuto fugere videar, sed adhibere qua[n]dam in dicendo speciem atque pompam, &amp; pugnae similem fugam: co[n]sistere vero in meo praesidio, sic vt non fugiendi hostis, sed capiendi loci causa cessisse videar" type="straight"/>
        <underline language="LA" method="pen" text="solicitare summe solet" type="straight"/>
        <underline language="LA" method="pen" text="vt nequid obsim" type="straight"/>
        <underline language="LA" method="pen" text="no[n] quin enitendum sit in vtroque: sed" type="straight"/>
        <underline language="LA" method="pen" text="vos inter vos" type="straight"/>
        <underline language="LA" method="pen" text="semper is fui, qui de te oratore sic praedicarem, vnum te in dicendo mihi videri tectissimum" type="straight"/>
        <underline language="LA" method="pen" text="sermo" type="straight"/>
        <underline language="LA" method="pen" text="de te institutus" type="straight"/>
        <underline language="LA" method="pen" text="quòd non solum quod opus esset, diceres, sed etiam quod non opus esset, no[n] diceres" type="straight"/>
        
        <drawing book_orientation="0" method="pen" place="left_margin" type="florilegium"/>
        
    </annotation>
</transcription>
