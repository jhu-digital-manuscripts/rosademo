<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v2.224v.tif" pagination="188" reader="John Dee"/>   
    <annotation>
        
        <marginalia anchor_text="Philippum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Philippus</marginalia_text>
                    <person name="Lucius Marcius Philippus, consul 91 BC"/>   
                </position>
            </language>
            <translation>Philippus.</translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="illuc redeo" type="straight"/>
        <underline language="LA" method="pen" text="neque tanti sit illud quod prodesse videatur, vt cum aliquo malo coniu[n]gatur" type="straight"/>
        <underline language="LA" method="pen" text="expendere" type="straight"/>
        <underline language="LA" method="pen" text="prae nobis est fere[n]da" type="straight"/>
        <underline language="LA" method="pen" text="reliquae duae sicuti sanguis in corporibus, sic illae in perpetuis orationibus fusae esse debebu[n]t" type="straight"/>
        <underline language="LA" method="pen" text="vt ad eoru[m] mentes, apud quos agitur, mouendas permanare possint" type="straight"/>
        <underline language="LA" method="pen" text="digredi tamen ab eo quod proposueris atque agas" type="straight"/>
        <underline language="LA" method="pen" text="plenissimae, quae plurimos exitus dant ad eiusmodi digressionem" type="straight"/>
        <underline language="LA" method="pen" text="impellantur" type="straight"/>
        <underline language="LA" method="pen" text="reflectantur" type="straight"/>
        <underline language="LA" method="pen" text="ea prima colloca[n]t" type="straight"/>
        <underline language="LA" method="pen" text="plureis adhibe[n]t patronos" type="straight"/>
        <underline language="LA" method="pen" text="vti quenque eorum minimum putant esse, ita eum primum volunt dicere" type="straight"/>
        <underline language="LA" method="pen" text="eorum expectationi, qui audiu[n]t, quamcelerrime succurratur" type="straight"/>
        <underline language="LA" method="pen" text="Male enim se res habet, quae non statim, vt dici coepta est, melior fieri videtur" type="straight"/>
        <underline language="LA" method="pen" text="in mediam turbam atque in gregem coniiciantur" type="straight"/>
        <underline language="LA" method="pen" text="exile" type="straight"/>
        <underline language="LA" method="pen" text="nugatorium" type="straight"/>
        <underline language="LA" method="pen" text="vulgare" type="straight"/>
        <underline language="LA" method="pen" text="co[m]mune" type="straight"/>
        <underline language="LA" method="pen" text="accurata" type="straight"/>
        <underline language="LA" method="pen" text="acuta" type="straight"/>
        <underline language="LA" method="pen" text="instructa sente[n]tiis" type="straight"/>
        <underline language="LA" method="pen" text="apta verbis" type="straight"/>
        <underline language="LA" method="pen" text="causaru[m] propria esse debe[n]t" type="straight"/>
        <underline language="LA" method="pen" text="cognitio" type="straight"/>
        <underline language="LA" method="pen" text="co[m]mendatio orationis" type="straight"/>
        <underline language="LA" method="pen" text="permulcere atque allicere debet" type="straight"/>
        <underline language="LA" method="pen" text="in primis disertum" type="straight"/>
        <underline language="LA" method="pen" text="ad dicendu[m] surgere, vt quod primum verbu[m] habiturus sit, nesciat" type="straight"/>
        <underline language="LA" method="pen" text="&amp; ait idem, cum brachium concalefecerit, tum se solere pugnare: neque attendit eos ipsos, vnde hoc simile ducat, illas primas hastas ita iactare leniter, vt &amp; venustati vel maxime seruiant, &amp; reliquis viribus suis consulant" type="straight"/>
        <underline language="LA" method="pen" text="pugnax non" type="straight"/>
        <underline language="LA" method="pen" text="sed si in ipso illo gladiatorio vitae certamine" type="straight"/>
        <underline language="LA" method="pen" text="quod ferro decernitur, tamen ante co[n]gressum multa fiunt, quae non ad vulnus, sed ad spe[ciem]" type="straight"/>
        
        <mark method="pen" name="vertical_bar" place="left_margin"/>
        
        <drawing book_orientation="0" method="pen" place="left_margin" type="florilegium"/>
        
    </annotation>
