    <page filename="RCP10549-50v2.225r.tif" pagination="189" reader="John Dee"/>   
    <annotation>
        
        <underline language="LA" method="pen" text="[spe]ciem valere videantur: quanto hoc magis in oratione expectandum est, in qua non vis potius, sed delectatio postulatur? Nihil est denique in natura rerum omniu[m], quod se vniuersum prefundat, &amp; quod totum repente euolet: sic omnia quae fiunt, quaéque aguntur acerrime, lenioribus principiis natura ipsa praetexuit" type="straight"/>
        <underline language="LA" method="pen" text="ex ipsis visceribus causae sume[n]da sunt" type="straight"/>
        <underline language="LA" method="pen" text="pertentata" type="straight"/>
        <underline language="LA" method="pen" text="perspecta" type="straight"/>
        <underline language="LA" method="pen" text="inuentis" type="straight"/>
        <underline language="LA" method="pen" text="instructis" type="straight"/>
        <underline language="LA" method="pen" text="momenti aliquid afferent" type="straight"/>
        <underline language="LA" method="pen" text="intima defensione depromptae" type="straight"/>
        <underline language="LA" method="pen" text="penitus ex ea causa, quae tum agatur, effloruisse" type="straight"/>
        <underline language="LA" method="pen" text="vt aedibus ac templis vestibula &amp; aditus, sic causis principia pro portione rerum praeponere" type="straight"/>
        <underline language="LA" method="pen" text="reos appello, quoru[m] res est" type="straight"/>
        <underline language="LA" method="pen" text="infanda" type="straight"/>
        <underline language="LA" method="pen" text="restitui sanaríque non possit" type="straight"/>
        <underline language="LA" method="pen" text="agendo efficitur melius quàm rogando" type="straight"/>
        <underline language="LA" method="pen" text="confundendum" type="straight"/>
        <underline language="LA" method="pen" text="gignuntur" type="straight"/>
        <underline language="LA" method="pen" text="in mediis causis dicuntur" type="straight"/>
        <underline language="LA" method="pen" text="alliciendu[m]" type="straight"/>
        <underline language="LA" method="pen" text="incitandum" type="straight"/>
        <underline language="LA" method="pen" text="impelli primo iudicem leuiter" type="straight"/>
        <underline language="LA" method="pen" text="iam inclinato reliqua incumbat oratio" type="straight"/>
        <underline language="LA" method="pen" text="Connexum autem ita sit principum co[n]sequenti orationi, vt non ta[n]quam citharoedi prooemium affictum aliquod, sed cohaerens cum omni corpore membrum esse videatur" type="straight"/>
        <underline language="LA" method="pen" text="meditati ediderunt" type="straight"/>
        <underline language="LA" method="pen" text="audientiam" type="straight"/>
        <underline language="LA" method="pen" text="Atque eiusmodi illa prolusio debet esse, non vt Samnitum, qui vibrant hastas ante pugnam, quibus in pugnando nihil vtuntur: sed vt ipsis sententiis, quibus proluserunt, vel pugnare possint" type="straight"/>
        <underline language="LA" method="pen" text="verbum nullum redu[n]det" type="straight"/>
        <underline language="LA" method="pen" text="obscuritatem affert" type="straight"/>
        <underline language="LA" method="pen" text="quae narrationis est maxima" type="straight"/>
        <underline language="LA" method="pen" text="seruilis percontatio" type="straight"/>
        <underline language="LA" method="pen" text="peruarie iucundéque" type="straight"/>
        <underline language="LA" method="pen" text="totum conficere" type="straight"/>
        <underline language="LA" method="pen" text="concisum est, ita vt non breuitati seruitum sit, sed magis venustati" type="straight"/>
        <underline language="LA" method="pen" text="festiuitatem habet narratio distincta personis &amp; interpuncta sermonibus" type="straight"/>
        <underline language="LA" method="pen" text="multo apertius ad intelligendu[m] est, si sic consistitur aliquando, ac non ista breuitate percurritur" type="straight"/>
        
