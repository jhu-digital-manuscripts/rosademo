    <page filename="RCP10549-50v2.227r.tif" pagination="193" reader="John Dee" signature="n.i."/> 
    <annotation>
        
        <underline language="LA" method="pen" text="educet atque confirmet" type="straight"/>
        <underline language="LA" method="pen" text="tam acri memoria ferè" type="straight"/>
        <underline language="LA" method="pen" text="dispositis notatísque rebus, ordinem verborum aut sententiarum co[m]plectatur" type="straight"/>
        <underline language="LA" method="pen" text="tam hebeti" type="straight"/>
        <underline language="LA" method="pen" text="animis affigi nostris, quae essent à sensu tradita atque impressa" type="straight"/>
        <underline language="LA" method="pen" text="oculorum commendatione animis traderentur, vt res caecas, &amp; ab aspectus iudicio remotas conformatio quaeda[m] &amp; imago &amp; figura ita notaret, vt ea, quae cogita[n]do complecti vix possemus, intuendo quasi teneremus" type="straight"/>
        <underline language="LA" method="pen" text="formis" type="straight"/>
        <underline language="LA" method="pen" text="corporibus" type="straight"/>
        <underline language="LA" method="pen" text="sub aspectum veniunt" type="straight"/>
        <underline language="LA" method="pen" text="sedes opus est, etenim corpus intelligi sine loco non potest" type="straight"/>
        <underline language="LA" method="pen" text="peruulgata multus &amp; insole[n]s sim" type="straight"/>
        <underline language="LA" method="pen" text="illustribus" type="straight"/>
        <underline language="LA" method="pen" text="explicatis, modicis interuallis" type="straight"/>
        <underline language="LA" method="pen" text="age[n]tibus, acribus, insignitis" type="straight"/>
        <underline language="LA" method="pen" text="quae occurrere celeritérque percutere animum possint" type="straight"/>
        <underline language="LA" method="pen" text="co[n]suetudo gignitur" type="straight"/>
        <underline language="LA" method="pen" text="co[n]seruata" type="straight"/>
        <underline language="LA" method="pen" text="immutata" type="straight"/>
        <underline language="LA" method="pen" text="traducta ex parte ad genus notatio" type="straight"/>
        <underline language="LA" method="pen" text="vnius verbi imagine totius sententiae informatio" type="straight"/>
        <underline language="LA" method="pen" text="formarum varietate locos distinguentis" type="straight"/>
        <underline language="LA" method="pen" text="quasi articuli connectunt membra orationis" type="straight"/>
        <underline language="LA" method="pen" text="propria est oratoris" type="straight"/>
        <underline language="LA" method="pen" text="ab inertibus dicitur" type="straight"/>
        <underline language="LA" method="pen" text="opprimi memoria[m] imaginu[m] pondere, &amp; obscurari etia[m] id, quod per se natura tenere potuisset" type="straight"/>
        <underline language="LA" method="pen" text="ta[n]quam literis in cera" type="straight"/>
        <underline language="LA" method="pen" text="non eruenda memoria est, si est nulla naturalis: sed certe, si latet, euocanda est" type="straight"/>
        <underline language="LA" method="pen" text="bene longum hominis vtinam non impudentis, illud quidem certe, non nimis verecundi: qui" type="straight"/>
        <underline language="LA" method="pen" text="insolita[m] mihi loquacitate[m] impulerit" type="straight"/>
        <underline language="LA" method="pen" text="acceperitis" type="straight"/>
        <underline language="LA" method="pen" text="fratre respondeo" type="straight"/>
        <underline language="LA" method="pen" text="facilitatem" type="straight"/>
        <underline language="LA" method="pen" text="quòd magno sum leuatus errore" type="straight"/>
        <underline language="LA" method="pen" text="illa tanta tua in causis diuinitas" type="straight"/>
        <underline language="LA" method="pen" text="ista attigisse" type="straight"/>
        <underline language="LA" method="pen" text="vndique collegisse" type="straight"/>
        <underline language="LA" method="pen" text="partim correxisse video, partim comprobasse" type="straight"/>
        <underline language="LA" method="pen" text="id quod dixisti, fore vt tibi" type="straight"/>
        <underline language="LA" method="pen" text="qui te attentissime audieru[n]t" type="straight"/>
        <underline language="LA" method="pen" text="Adimere" type="straight"/>
        <underline language="LA" method="pen" text="omnem recusationem Crasso volui" type="straight"/>
        <underline language="LA" method="pen" text="quem ego paulo scieba[m] vel pude[n]tius vel inuitius" type="straight"/>
        <underline language="LA" method="pen" text="nolo enim dicere de tam suaui homine fastidiosius) ad hoc genus sermonis accedere" type="straight"/>
        <underline language="LA" method="pen" text="An aetate[m] afferet? quadriennio minor est" type="straight"/>
        <underline language="LA" method="pen" text="quae succisiuis operis" type="straight"/>
        <underline language="LA" method="pen" text="tam sui despiciens fuit, qui desperaret aut melius, aut eodem modo se posse dicere" type="straight"/>
        <underline language="LA" method="pen" text="tam arrogans" type="straight"/>
        
