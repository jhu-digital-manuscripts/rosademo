<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v2.228r.tif" pagination="195" reader="John Dee" signature="n.ii."/> 
    <annotation>
        
        <underline language="LA" method="pen" text="haec tibi est excidenda lingua, qua vel euulsa spiritu ipso libidinem tuam libertas mea refutabit" type="straight"/>
        <underline language="LA" method="pen" text="vehementissima contentione animi" type="straight"/>
        <underline language="LA" method="pen" text="quam senatus frequens secutus est" type="straight"/>
        <underline language="LA" method="pen" text="cygnea fuit diuini hominis vox &amp; oratio, qua[m] quasi expecta[n]tes post eius interitum veniebamus in curiam, vt vestigium illud ipsum, in quo ille postremum institisset, contueremur" type="straight"/>
        <underline language="LA" method="pen" text="tum latus ei dicenti condoluisse, sudorémque multum co[n]secutum esse audiebamus: ex quo cum cohorruisset, cum febri domu[m] rediit, diéque septimo est lateris dolore consumptus" type="straight"/>
        <underline language="LA" method="pen" text="quae in medio spatio saepe franguntur &amp; corruunt, &amp; antè in ipso cursu obruuntur, quàm portum conspicere potuerunt" type="straight"/>
        <underline language="LA" method="pen" text="ambitionis labore vita districta" type="straight"/>
        <underline language="LA" method="pen" text="floruit" type="straight"/>
        <underline language="LA" method="pen" text="fructu amplitudinis aut reip. dignitate" type="straight"/>
        <underline language="LA" method="pen" text="annus primus ab honorum perfunctione aditum omnium co[n]cessu ad summam authoritatem dabat" type="straight"/>
        <underline language="LA" method="pen" text="is eius omnem spem, atque omnia vitae consilia morte peruertit" type="straight"/>
        <underline language="LA" method="pen" text="luctuosum" type="straight"/>
        <underline language="LA" method="pen" text="non erepta" type="straight"/>
        <underline language="LA" method="pen" text="donata mors esse videatur" type="straight"/>
        <underline language="LA" method="pen" text="deformatam" type="straight"/>
        <underline language="LA" method="pen" text="non vagabitur oratio mea longius" type="straight"/>
        <underline language="LA" method="pen" text="definietur" type="straight"/>
        <underline language="LA" method="pen" text="defleta" type="straight"/>
        <underline language="LA" method="pen" text="recordatus" type="straight"/>
        <underline language="LA" method="pen" text="incolumen fortuna[m]" type="straight"/>
        <underline language="LA" method="pen" text="imperatoriis manubiis ornarat" type="straight"/>
        <underline language="LA" method="pen" text="scelere proditu[m]" type="straight"/>
        <underline language="LA" method="pen" text="vt ille qui haec non vidit, &amp; vixisse cum repub. pariter, &amp; cum illa simul extrinctus esse videatur" type="straight"/>
        <underline language="LA" method="pen" text="Crasso se dicarant, horribiles" type="straight"/>
        <underline language="LA" method="pen" text="florentem" type="straight"/>
        <underline language="LA" method="pen" text="depulsis per inuidiam tribunatu, non multis ab eo te[m]pore me[n]sibus eiectus est è ciuitate" type="straight"/>
        <underline language="LA" method="pen" text="qui in eade[m] inuidiae flamma fuisset" type="straight"/>
        <underline language="LA" method="pen" text="florescenti, ferro erepta" type="straight"/>
        <underline language="LA" method="pen" text="cum vitae flore, tum mortis opportunitate, diuino consilio &amp; ortum &amp; extinctum esse arbitror" type="straight"/>
        <underline language="LA" method="pen" text="subeunda" type="straight"/>
        <underline language="LA" method="pen" text="eadem esse te funerum patriae spectatorem coegisset" type="straight"/>
        <underline language="LA" method="pen" text="admistam" type="straight"/>
        
        <drawing book_orientation="0" method="pen" place="right_margin" type="florilegium"/> 
        
    </annotation>
</transcription>
