        <underline language="LA" method="pen" text="interquieuissem" type="straight"/>
        <underline language="LA" method="pen" text="Alios no[n] laborare vt meliores fiant: nulla enim res tantum ad dice[n]dum proficit quantum scriptio: memoria[m]" type="straight"/>
