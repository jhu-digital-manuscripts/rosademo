    <annotation>
        
        <marginalia anchor_text="verecunde &amp; dubita[n]ter recepisse" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>verecunde et dubitanter recipere.</marginalia_text>
                </position>
            </language>
            <translation>To accept modestly and hesitantly.</translation>
        </marginalia>
        
        <marginalia anchor_text="cognitionis dies" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Dies Cognitionis</marginalia_text>
                </position>
            </language>
            <translation>The day of the hearing.</translation>
        </marginalia>
        
        <marginalia anchor_text="Galbam" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Galba[e] or[ati]o.</marginalia_text>
                    <person name="Servius Sulpicius Galba"/>
                </position>
            </language>
            <translation>The speech of Galba.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="duae summae sint in oratore laudes" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Du[a]e summ[a]e oratore laudes</marginalia_text>
                </position>
            </language>
            <translation>The two greatest glories in oratory.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="Libonem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Libo.</marginalia_text>
                    <person name="Lucius Scribonius Libo"/>
                </position>
            </language>
            <translation>Lucius Scribonius Libo.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="Nec enim est eadem, inqua[m], Brute, causa no[n] scribendi, &amp; non tam bene scribe[n]di quàm dixerint" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Cur quida[m] Oratores [...] mandaru[n]t, et quida[m] qu[a]e scribunt, no[n] ta[m] bene scribunt q[uam] dixerint</marginalia_text>  <!-- DO: Not sure --> <!--JG: Can't figure out the fourth word either-->
                </position>
            </language>
            <translation>Why some orators ordered [...], and some who write, do not write as well as they have spoken.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="oratore[m], qua ille vtens eode[m] modo possit &amp; dicere &amp; scribere" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Orator eode[m] modo &amp; dicere &amp; scribere possit.</marginalia_text>
                </position>
            </language>
            <translation>An orator may be able to speak and write in the same way.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="Laelii me[n]s spirare etia[m] in scriptis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>La[e]lij mens spirat in scriptis</marginalia_text>
                    <person name="Gaius Laelius Sapiens"/>
                </position>
            </language>
            <translation>The mind of Laelius bursts forth from his writings.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="L. &amp; Sp. Mu[m]mii" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>L. &amp; Sp. Mummij</marginalia_text>
                    <person name="Lucius Mummius Achaicus"/>
                    <person name="Spurius Mummius"/>
                </position>
            </language>
            <translation>Lucius and Spurius Mummius.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="P. etia[m] Popilius" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>P. Popilius</marginalia_text>
                    <person name="Publius Popillius Laenas"/>
                </position>
            </language>
            <translation>Publius Popillius.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="Caiúsque Tuditanus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>C. Tuditanus.</marginalia_text>
                    <person name="Gaius Sempronius Tuditanus"/>
                </position>
            </language>
            <translation>Gaius Tuditanus.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="cuius orbitas &amp; fletus mire miserabilis fuit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>Cui[us] orbitas et fletus mire miserabilis fuit.</marginalia_text>
                </position>
            </language>
            <translation>Whose bereavement and weeping were remarkably pitiable.</translation>  
        </marginalia>
        
        
        <marginalia anchor_text="interquieuissem" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>interquiescere</marginalia_text>
                </position>
            </language>
            <translation>To rest for a while.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="Quid igitur, inquit, est causae" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>quid est caus[a]e, cur.</marginalia_text>
                </position>
            </language>
            <translation>What is of the cause; why.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="peringeniosis hominibus neque satis doctis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>p[er]ingeniosus neq[ue] satis doctus</marginalia_text>
                </position>
            </language>
            <translation>Very intelligent and not educated enough.</translation>
        </marginalia>
        
        
