        <underline language="LA" method="pen" text="Nonnunqua[m] quidem eandem Fortuna[m], quòd efficiat multa improuisa haec nec opinata nobis, propter obscuritatem ignorationémque causaru[m]" type="straight"/>
        <underline language="LA" method="pen" text="Quanqua[m] oriretur à sensibus, tamen no[n]" type="straight"/>
        <underline language="LA" method="pen" text="Mentem voleba[n]t rerum esse iudicem: solam" type="straight"/>
        <underline language="LA" method="pen" text="quia sola cerneret id quod semper esset simplex, &amp; vnius modi, &amp; tale quale esset" type="straight"/>
        <underline language="LA" method="pen" text="hebetes &amp; tardos" type="straight"/>
        <underline language="LA" method="pen" text="mobiles &amp; concitatae" type="straight"/>
        <underline language="LA" method="pen" text="quia continenter laberentur &amp; fluerent" type="straight"/>
        <underline language="LA" method="pen" text="nisi in animi motionibus atque rationibus" type="straight"/>
        <underline language="LA" method="pen" text="definitiones rerum" type="straight"/>
        <underline language="LA" method="pen" text="Verboru[m] etiam explicatio probabatur" type="straight"/>
        <underline language="LA" method="pen" text="id est orationis ratione conclusae" type="straight"/>
        <underline language="LA" method="pen" text="ex altera parte" type="straight"/>
        <underline language="LA" method="pen" text="oratoria vis dicendi adhibebatur, explicatrix orationis perpetuae, ad persuadendum accommodatae" type="straight"/>
        <underline language="LA" method="pen" text="Plato erat amplexatus" type="straight"/>
        <underline language="LA" method="pen" text="vir &amp; oratione suauis, &amp; ita moratus, vt prae se probitatem quandam &amp; ingenuitatem ferat, vehementius" type="straight"/>
        <underline language="LA" method="pen" text="Spoliauit enim virtutem suo decore" type="straight"/>
        <underline language="LA" method="pen" text="imbecillámque reddidit" type="straight"/>
        <underline language="LA" method="pen" text="fuit acri ingenio" type="straight"/>
        <underline language="LA" method="pen" text="omnino semouendus" type="straight"/>
        <underline language="LA" method="pen" text="posita est in virtute &amp; in moribus" type="straight"/>
        <underline language="LA" method="pen" text="ad inuestigationem naturae" type="straight"/>
        <underline language="LA" method="pen" text="anteiret aetate, valdéque" type="straight"/>
        <underline language="LA" method="pen" text="&amp; peracute moueretur" type="straight"/>
