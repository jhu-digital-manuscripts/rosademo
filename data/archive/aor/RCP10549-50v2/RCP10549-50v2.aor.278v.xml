    <annotation>
        
        <marginalia anchor_text="nihil omnino esse mome[n]ti" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>nihil o[mn]i[n]o mome[n]ti</marginalia_text>
                </position>
            </language>
            <translation>Nothing of movement whatsoever.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="interiecta &amp; media; in mediis relinquebat; in bonis actionibus pone[n]s; statuebat" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        ponere
                        collocare
                        numerare } in bonis [etc]
                        statuere } hoc esse [?]. [ interiecta media in medijs relinquere.        <!-- DO: Not sure of word after 'esse' --> <!--JG: Me neither-->
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To put.
                To situate.
                To count in good things, etc.
                To establish: this is [?] placed in between, in the middle, left in the middle sections.
            </translation>
        </marginalia>
        
        
        <marginalia anchor_text="in bonis actionibus; virtutis vsum; habitum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        virtutis [ vsus
                        actio
                        Habitus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Of virtue the use; the act, the habit.
            </translation>
        </marginalia>
        
        
        <marginalia anchor_text="ex iis alia pluris esse aestima[n]da, alia minoris" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        a[e]stimia[n]da pluris minoris.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Thought to be more; less.
            </translation>
        </marginalia>
        
        
        <marginalia anchor_text="in mediis relinquebat; media locabat; media putabat; inter scientiam &amp; inscientiam, comprehensionem illam quam dixi, collocabat" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        in medijs reli[n]quere
                        hèc media locare
                        media putare
                        collocare inter [etc].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To leave in the middle sections.
                To situate this as intermediate.
                To consider as intermediate.
                To distinguish between, etc.
            </translation>
        </marginalia>
        
        
        <marginalia anchor_text="contraherent, in angustúmque deducerent" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        co[n]trahere
                        in angustu[m] deducere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To gather. To assemble in a small group.
            </translation>
        </marginalia>
        
        
        <marginalia anchor_text="accepta sensibus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        res subiecta[e]
                        Accepta sensibus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Things subject to; Accepted by the senses.
            </translation>
        </marginalia>
        
        
        <marginalia anchor_text="tame[n] alia secundum natura[m] dicebat, alia naturae esse contraria" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Alia sunt secundum na[t]u[r]a
                                  co[n]traria na[tura]e.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Some are in accordance with nature; contrary to nature.
            </translation>
        </marginalia>
        
        
        <marginalia anchor_text="dignanda" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Digna[n]da.</marginalia_text>
                </position>
            </language>
            <translation>To be esteemed.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="recte facta sola in bonis actionibus pone[n]s: praue" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Recte ac prave factu[m].</marginalia_text>
                </position>
            </language>
            <translation>Done rightly and wrongly.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="peccata" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>peccata.</marginalia_text>
                </position>
            </language>
            <translation>Sins.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="non omnem virtutem in ratione esse dicerent, sed quasdam virtutes natura aut more perfectas" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        virtus alia in r[ati]o[n]e natura et more p[er]fecta.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Some virtues in reason; perfected in nature and custom.
            </translation>
        </marginalia>
        
        
        <marginalia anchor_text="nec tamen virtutem cuiquam adesse quin ea semper vteretur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>virtus sine p[er]petuo vsu nulli adest.</marginalia_text>
                </position>
            </language>
            <translation>Virtue without continuation is of no use.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="carere sapie[n]tem. Quúmque eas perturbationes antiqui naturales esse dicerent" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>sapiens caret perturbatio[n]ibus</marginalia_text>
                </position>
            </language>
            <translation>A wise man is without perturbations.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="perturbationes voluntarias" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Perturbationes volu[n]taria[e].</marginalia_text>
                </position>
            </language>
            <translation>Voluntary perturbations.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="perturbationum arbitrabatur matrem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Perturbationu[m] mater</marginalia_text>
                </position>
            </language>
            <translation>The mother of perurbations.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="quinta[m] hanc naturam" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Quinta Natura.</marginalia_text>
                </position>
            </language>
            <translation>The fifth nature.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="Xenocrates" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>XENOCRATES</marginalia_text>
                    <person name="Xenocrates of Chalcedon"/>
                </position>
            </language>
            <translation>Xenocrates.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="sensibus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>SENSVS</marginalia_text>
                </position>
            </language>
            <translation>The senses.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="impulsione" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Impulsio.</marginalia_text>
                </position>
            </language>
            <translation>An impulse.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        [Ap[er]ta se[n]te[n]tia p[er] disceptatio[n]e - deleted]    
                    </marginalia_text>
                </position>
            </language>
            <translation/>
        </marginalia>
        
        <marginalia anchor_text="assensionem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Assensio</marginalia_text>
                </position>
            </language>
            <translation>Assent.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="Visis non omnibus adiungebat fidem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Quibus visis fides</marginalia_text>
                </position>
            </language>
            <translation>Not all appearances are trustworthy.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="comprehensibile" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Comprehensibile.</marginalia_text>
                </position>
            </language>
            <translation>Comprehensible.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="comprehensionem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Comprehensio</marginalia_text>
                </position>
            </language>
            <translation>Comprehension.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="nomen hoc duxerat" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Ducere no[m]en</marginalia_text>
                </position>
            </language>
            <translation>To assign a name.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="Quod autem erat sensu comprehensum, idipsum sensum appellabat" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Sensu[m] vnde.</marginalia_text>
                </position>
            </language>
            <translation>From where the term sense arose.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="si ita erat co[m]prehensum, vt conuelli ratione non posset, scientiam" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Scientia vnde</marginalia_text>
                </position>
            </language>
            <translation>From where the term knowledge arose.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="inscientiam nominabat, ex qua existeret etiam opinio quae esset imbecilla" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Inscie[n]tia. Opinio.</marginalia_text>
                </position>
            </language>
            <translation>Ignorance. Opinion.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="Sed inter scientiam &amp; inscientiam, comprehensionem illam quam dixi, collocabat" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Inter scie[n]tia[m] et inscie[n]tia[m] interiecta est co[m]prehensio.</marginalia_text>
                </position>
            </language>
            <translation>Comprehension is placed between knowledge and ignorance.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="comprehensio facta sensibus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Co[m]p[re]hensio facta se[n]sibus</marginalia_text>
                </position>
            </language>
            <translation>Comprehension is made by the senses.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="Errorem autem, &amp; temeritatem, &amp; ignora[n]tiam, &amp; opinationem, &amp; suspicione[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Error: temeritas: ignoratia.
                        Opinatio: suspicio.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Error. Rashness. Ignorance. Opinion. Suspicion.
            </translation>
        </marginalia>
        
        
        <marginalia anchor_text="nullo modo arbitrabatur quicquam effici posse ab ea quae expers esset corporis; Nec vero aut quòd efficeret aliquid, aut quòd efficeretur, posse esse non corpus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        Nihil effici p[otes]t ab ea re qua expers est Corporis
                        Quicquid vel efficit vel efficitur est Corpus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Nothing can be acted on by a thing which lacks a body.
                Anything which either acts or is acted on is a body.
            </translation>
        </marginalia>
        
        
        <marginalia anchor_text="acceptum iam &amp; approbatum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        acceptu[m]
                        approbatu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Accepted.
                Approved.
            </translation>
        </marginalia>
        
        
        <marginalia anchor_text="Phantasiam, nos Visum appellemus licet" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        phantasia
                        visum
                    </marginalia_text>
                </position>
            </language>
            <translation>
                An apparition.
                A vision.
            </translation>
        </marginalia>
        
        
        <marginalia anchor_text="firmae &amp; consta[n]tis assensionis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        assensio firma
                                 constans
                    </marginalia_text>
                </position>
            </language>
            <translation>Firm and constant assent.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="commutauerat; disserebat" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        co[m]mutatio
                        Disse[n]sio. } à.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Change [and] conflict from.
            </translation>
        </marginalia>
        
        
        <marginalia anchor_text="adiungebat fidem; fidem tribuebat" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        adiungere
                        attribuero } fide[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>To join to, to add to trust.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="comprehensio facta sensibus, &amp; vera esse &amp; fidelis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        co[m]prehensio vera
                                       fidelis.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                True, trustworthy comprehension.
            </translation>
        </marginalia>
        
        
