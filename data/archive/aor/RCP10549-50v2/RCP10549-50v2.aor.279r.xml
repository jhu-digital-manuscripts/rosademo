        <underline language="LA" method="pen" text="perducta" type="straight"/>
        <underline language="LA" method="pen" text="philosophiae partis ignarus" type="straight"/>
        <underline language="LA" method="pen" text="vnum tamen praeter caeteros mirabatur) incredibili quadam fuit facultate" type="straight"/>
