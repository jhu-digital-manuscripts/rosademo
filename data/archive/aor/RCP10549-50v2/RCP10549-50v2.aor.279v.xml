        <marginalia anchor_text="plus in negotiis gerendis res, quàm verba prosunt" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>Plus in negotijs gerendis res qua[m] verba prosunt.</marginalia_text>
                </position>
