        <marginalia anchor_text="Repetunt; co[m]memorant reliquos; tum ad hos; etia[m] referre in eundem numeru[m] solent; Addunt etia[m]" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        repetunt.
                        tum ad hos.                
                        co[m]memorant reliquos.
                        etia[m] refere in eunde[m] numeru[m] sole[n]t.
                        addunt etia[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                They recount.
                Then to those.
                They recall the others.
                They even tend to refer in the same number.
                They even add.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="quin contra suum doctorem librum" type="straight"/>
        <underline language="LA" method="pen" text="Heraclitu[m] studiose audirem" type="straight"/>
        <underline language="LA" method="pen" text="dedi Antiocho operam" type="straight"/>
        <underline language="LA" method="pen" text="secundum fratrem plurimu[m]" type="straight"/>
        <underline language="LA" method="pen" text="multu[m] temporis in ista vna disputatione" type="straight"/>
        <underline language="LA" method="pen" text="ista quae sunt heri defensa" type="straight"/>
        <underline language="LA" method="pen" text="mentitur, tamen est aduersarius lenior" type="straight"/>
        <underline language="LA" method="pen" text="aliquos ex antiquis claros viros proferunt, quos dicant fuisse populares" type="straight"/>
        <underline language="LA" method="pen" text="similes esse videantur" type="straight"/>
        <underline language="LA" method="pen" text="vero sapientissimos" type="straight"/>
        <underline language="LA" method="pen" text="philosophia[m] bene iam constituta[m]" type="straight"/>
        <underline language="LA" method="pen" text="Arcesilae calumnia conferenda" type="straight"/>
        <underline language="LA" method="pen" text="tamen isti physici raro admodum, quum haerent, aliquo loco exclamant, quasi mente incitati: Empedocles quide[m], vt interdum mihi furere videatur,              abstrusa esse" type="straight"/>
        <underline language="LA" method="pen" text="sentire, nihil" type="straight"/>
        <underline language="LA" method="pen" text="Quòd si illi tum in nouis rebus, quasi modo nascentes haesitauerunt, nihílne" type="straight"/>
        <underline language="LA" method="pen" text="seculis, summis ingeniis, maximis studiis explicatum putamus? Nónne" type="straight"/>
        <underline language="LA" method="pen" text="qui co[n]stitutam philosophia[m]" type="straight"/>
        <underline language="LA" method="pen" text="eorum authoritate delitesceret, qui negauissent quicquam sciri, aut percipi posse" type="straight"/>
        <underline language="LA" method="pen" text="Peripateticos &amp; Academicos nominibus differentes, re congruentes" type="straight"/>
        <underline language="LA" method="pen" text="verbis magis, quàm sententiis dissenserunt" type="straight"/>
        <underline language="LA" method="pen" text="ita quum aliud diceret atque sentiret" type="straight"/>
        <underline language="LA" method="pen" text="libenter vti solitus est ea dissimulatione, quam Graeci voca[n]t" type="straight"/>     <!-- Greek needed -->
        <underline language="LA" method="pen" text="Africano fuisse Fannius: ídque propterea vitiosum in illo non" type="straight"/>
        <underline language="LA" method="pen" text="fuerit in" type="straight"/>
        <underline language="LA" method="pen" text="Sed fuerint illa vetera, si vultis, incognita" type="straight"/>
        <underline language="LA" method="pen" text="clarissimis rebus tenebras obducere" type="straight"/>
        <underline language="LA" method="pen" text="quanquam floruit cum acumine ingenii, tum admirabili quodam lepore" type="straight"/>
        <underline language="LA" method="pen" text="admodu[m] florueru[n]t" type="straight"/>
        <underline language="LA" method="pen" text="multitudo libroru[m] ingenii no[n] minus in hoc, quàm in Carneade eloque[n]tiae" type="straight"/>
        <underline language="LA" method="pen" text="Rhodio suauitatis" type="straight"/>
        
