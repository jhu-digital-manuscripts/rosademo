        <marginalia anchor_text="At nemo inuenit falsa: nec ea quae incerta permanent, iuenta esse possunt" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        falsa no[n] possu[n]t esse inventa nec incerta
                    </marginalia_text>
                </position>
            </language>
            <translation>False things cannot be discovered, nor be uncertain.</translation>
        </marginalia>
        
        <marginalia anchor_text="Sed quum ea quae quasi inuoluta fuerunt, aperta sunt, tum inuenta dicuntur" hand="Italian">
            <language ident="LA">
