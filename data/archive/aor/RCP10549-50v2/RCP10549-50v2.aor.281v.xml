        <marginalia anchor_text="à falsis discerni; internosci  à falsis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        Discerni
                        internosci } à
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be separated; to be distinguished from.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="consentaneum; acco[m]modatum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                       co[n]se[n]taneu[m]
                       accomodatu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
