        <marginalia anchor_text="in ipsis nobis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>in ip[s]is nobis</marginalia_text>
                </position>
            </language>
            <translation>In ourselves.</translation>
        </marginalia>
        
        <marginalia anchor_text="Qui enim quid percipit, assentitur statim" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>Qui enim quid percipit asse[n]t[it]ur statim.</marginalia_text>
                </position>
            </language>
            <translation>For who perceives it agrees at once.</translation>
        </marginalia>
        
        <marginalia anchor_text="circunspectione aliqua, &amp; accurata consyderatione" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        circu[n]spectio
                        accurata co[n]sideratio.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Close attention.
                Accurate consideration.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="internosci" type="straight"/>
        <underline language="LA" method="pen" text="faciunt pueriliter" type="straight"/>
        <underline language="LA" method="pen" text="iudicantur sublato, reliqua se nega[n]t tollere: vt" type="straight"/>
        <underline language="LA" method="pen" text="improbabilem" type="straight"/>
        <underline language="LA" method="pen" text="tibi erit reuertendum" type="straight"/>
        <underline language="LA" method="pen" text="quia proprium in communi signo notari no[n]" type="straight"/>
        <underline language="LA" method="pen" text="Simili in errore versantur, quum conuicti" type="straight"/>
        <underline language="LA" method="pen" text="coacti, perspicua à perceptis volunt distinguere, &amp; conantur ostendere" type="straight"/>
        <underline language="LA" method="pen" text="perspicue dixeris" type="straight"/>
        <underline language="LA" method="pen" text="vere inanitérve" type="straight"/>
        <underline language="LA" method="pen" text="Et hoc illud his vsu venire solet, vt quicquid dixerint, à quibusdam interrogentur" type="straight"/>
        <underline language="LA" method="pen" text="nec asseuerare sine" type="straight"/>
        <underline language="LA" method="pen" text="quod cuique occurrit, &amp; primo quasi aspectu probabile videtur, id co[n]firmatur: quid eo leuius" type="straight"/>
        <underline language="LA" method="pen" text="circunspectione aliqua, &amp; accurata consyderatione" type="straight"/>
        <underline language="LA" method="pen" text="omnibus abrogatur fides" type="straight"/>
        <underline language="LA" method="pen" text="quamproxime" type="straight"/>
        <underline language="LA" method="pen" text="veri: quo obscuro &amp; oppresso, quod tandem verum sibi videbuntur attingere? Quid autem tam absurde dici potest, quàm" type="straight"/>
        <underline language="LA" method="pen" text="rei signum aut argumentum, &amp; ea re id sequor" type="straight"/>
        <underline language="LA" method="pen" text="sunt labefactare volet, facile etia[m] absentibus nobis veritas se ipsa defendet" type="straight"/>
        <underline language="LA" method="pen" text="quo non latus locus sit, sed paulo" type="straight"/>
        <underline language="LA" method="pen" text="inter inanimu[m] &amp; animal hoc maxime" type="straight"/>
        <underline language="LA" method="pen" text="ponderibus impositis deprimi, sic animu[m] perspicuis cedere. Nam" type="straight"/>
        <underline language="LA" method="pen" text="assentitur" type="straight"/>
        
