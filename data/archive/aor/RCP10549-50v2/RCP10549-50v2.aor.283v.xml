    <annotation>
        
        <marginalia anchor_text="firmius &amp; constantius" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        firmius
                        constantius
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Firmer.
                More constant.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="vt nihil praeteritis relinquamus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        vt nihil in p[rae]teritis reli[n]quam[us].
                    </marginalia_text>
                </position>
            </language>
            <translation>As we leave nothing passed over [Loeb 268, 525].</translation>
        </marginalia>
        
        <marginalia anchor_text="captionibus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Captiones.
                    </marginalia_text>
                </position>
            </language>
            <translation>Deceptions.</translation>
        </marginalia>
        
        <marginalia anchor_text="defigunt animos, &amp; intendunt in ea quae perspicua sunt" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        defigere
                        intendere ] a[n]i[m]os
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To fix, to focus minds.
            </translation>
        </marginalia>

        <marginalia anchor_text="Aduersantur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Dua[e] res, rebus p[er]spicuis aduersantur.
                    </marginalia_text>
                </position>
            </language>
            <translation>Two things are against transparent things.</translation> 
        </marginalia>
        
        <marginalia anchor_text="dissoluere; discutere" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Dissolvere
                        Discutere. }  
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To dissolve. To refute.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="facere constitui" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        facere co[n]stitui.
                    </marginalia_text>
                </position>
            </language>
            <translation>I have decided to do.</translation>
        </marginalia>
        
        <marginalia anchor_text="generatim" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        generatim.
                    </marginalia_text>
                </position>
            </language>
            <translation>Generally.</translation>
        </marginalia>
        
        <marginalia anchor_text="confuse loqui" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        co[n]fuse loq[ui].
                    </marginalia_text>
                </position>
            </language>
            <translation>To speak confusedly.</translation>
        </marginalia>
        
        <marginalia anchor_text="animi inaniter moueantur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Animoru[m] inanis motus.
                    </marginalia_text>
                </position>
            </language>
            <translation>The unreal movement of minds.</translation>
        </marginalia>
        
        <marginalia anchor_text="quos contra disputant" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        quos co[n]tra disputa[n]t.
                    </marginalia_text>
                </position>
            </language>
            <translation>Against whom they argue.</translation>
        </marginalia>
        
        <marginalia anchor_text="perdifficiliter" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        perdifficiliter
                    </marginalia_text>
                </position>
            </language>
            <translation>With great difficulty.</translation>
        </marginalia>
        
        <marginalia anchor_text="cogitatione depingimus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Cogitatio[n]e depingere.
                    </marginalia_text>
                </position>
            </language>
            <translation>To portray knowledge.</translation>
        </marginalia>
        
        <marginalia anchor_text="mens moueatur ipsa per sese" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        A se mens mouetur
                    </marginalia_text>
                </position>
            </language>
            <translation>The mind is moved by itself.</translation>
        </marginalia>
        
        <marginalia anchor_text="exalbescerent" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        exalbesco
                    </marginalia_text>
                </position>
            </language>
            <translation>I become pale.</translation>
        </marginalia>
        
        <marginalia anchor_text="intestinum &amp; oblatum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        intestinu[m]
                        oblatum.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Internal.
                External.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="sapiente[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Sapiens
                    </marginalia_text>
                </position>
            </language>
            <translation>Wise.</translation>
        </marginalia>
        
        <marginalia anchor_text="sapiente[m] in furore sustinere se sb omni assensu" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                       Sapiens in furore sustinet ab omni assensu. 
                    </marginalia_text>
                </position>
            </language>
            <translation>A wise person puts off all agreement in a frenzy.</translation>
        </marginalia>
        
        <marginalia anchor_text="captiosissimo genere interrogationis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Captiosissimu[m] interrogatio[n]is genus.
                    </marginalia_text>
                </position>
            </language>
            <translation>The kind of deceptive argument.</translation>
        </marginalia>
        
        <marginalia anchor_text="Quod genus minime in philosophia probari solet" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Sorita[e]s no[n] debe[n]t p[ro]bari in phi.
                    </marginalia_text>
                </position>
            </language>
            <translation>Sophisms should not be proved in philosophy.</translation>
        </marginalia>
        
        <marginalia anchor_text="Soritas hos voca[n]t qui aceruu[m] efficiu[n]t vno addito grano" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Sorites quid.
                    </marginalia_text>
                </position>
            </language>
            <translation>What sophisms are.</translation>
        </marginalia>
        
        <marginalia anchor_text="deu[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Deus.
                    </marginalia_text>
                </position>
            </language>
            <translation>A deity.</translation>
        </marginalia>
        
        <marginalia anchor_text="ad extremu[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        ad extremu[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>To the end.</translation>
        </marginalia>
        
        <marginalia anchor_text="simile" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        simile
                    </marginalia_text>
                </position>
            </language>
            <translation>Similar.</translation>
        </marginalia>
        
        <marginalia anchor_text="Et quidem honestis similia sunt quaedam non honesta, &amp; bonis no[n] bona, &amp; artificiosis minime artificiosa" hand="Italian">
            <language ident="EN">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        al [that] glytterthe is not golde.
                    </marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <marginalia anchor_text="differentiu[m] generu[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        differe[n]tiu[m] generu[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>Of different kinds.</translation>
        </marginalia>
        
        <marginalia anchor_text="Quod fieri quî potest" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        quod fieri, quì potest.
                    </marginalia_text>
                </position>
            </language>
            <translation>Can this be?</translation>
        </marginalia>
        
        <marginalia anchor_text="inaniu[m] visoru[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        visa inania
                    </marginalia_text>
                </position>
            </language>
            <translation>Unreal visions.</translation>
        </marginalia>
        
        <marginalia anchor_text="mordicus tenere" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        mordicus tenere
                    </marginalia_text>
                </position>
            </language>
            <translation>To hold with the teeth.</translation>
        </marginalia>
        
        <marginalia anchor_text="Nihil est enim quod de suo genere in aliud genus transferri possit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        Nihil est q[uod] de suo genere in aliud tra[n]sferri p[otes]t.
                    </marginalia_text>
                </position>
            </language>
            <translation>There is nothing which can be transferred from its own kind into another.</translation>
        </marginalia>
        
        <marginalia anchor_text="vitiosum sanè &amp; captiosum genus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        disputationis genus. { vitiosu[m] captiosu[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A kind of argument: flawed; deceptive.
            </translation>
        </marginalia>
        
