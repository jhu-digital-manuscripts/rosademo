    <annotation>
        
        <marginalia anchor_text="Quid enim est tam futile, quàm quicqua[m] approbare non cognitum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>Quid est tam futile, quàm quicqu[am] approbare no[n] cognitu[m].</marginalia_text>
                </position>
            </language>
            <translation>What is as futile as approving something unknown.</translation>
        </marginalia>
        
        <marginalia anchor_text="nec vnqua[m] vlli viso assensuru[m], nisi quod tale fuerit, quale falsum esse non possit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>sapiens veris semper asse[n]titur visis</marginalia_text>
                </position>
            </language>
            <translation>A wise person always approves a true vision.</translation>
        </marginalia>
        
        <marginalia anchor_text="Vt mater geminos internoscit co[n]suetudine oculoru[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Prouerb[ium].</marginalia_text>
                </position>
            </language>
            <translation>A proverb.</translation>
        </marginalia>
        
        <marginalia anchor_text="gallinas; hi quum ouum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Gallina[e]. ova.</marginalia_text>
                </position>
            </language>
            <translation>Hens. Eggs.</translation>
        </marginalia>
        
        <marginalia anchor_text="Habeo enim regula[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>habeo regula[m]</marginalia_text>
                </position>
            </language>
            <translation>I have a rule.</translation>
        </marginalia>
        
        <marginalia anchor_text="ab hac mihi no[n] licet tra[n]suersum (vt aiu[n]t) digitu[m] discedere" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Prouerb[ium].</marginalia_text>
                </position>
            </language>
            <translation>A proverb.</translation>
        </marginalia>
        
        <marginalia anchor_text="Illud vero perabsurdum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Illud vero perabsurdu[m].</marginalia_text>
                </position>
            </language>
            <translation>That truly very absurd thing.</translation>
        </marginalia>
        
        <marginalia anchor_text="" hand="Italian">
            <language ident="EL">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>ἐποχὴ</marginalia_text>              <!-- JG: for ἐποχή -->
                </position>
            </language>
            <translation>The suspension of judgement.</translation>
        </marginalia>
        
        <marginalia anchor_text="opinaturu[m], id est peccaturu[m] esse sapientem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Sapiens opinatur</marginalia_text>
                </position>
            </language>
            <translation>A wise person supposes.</translation>
        </marginalia>
        
        <marginalia anchor_text="opinari, id est nunqua[m] assentiri rei vel falsae, vel incognitae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Opinari quid</marginalia_text>
                </position>
            </language>
            <translation>What supposing is.</translation>
        </marginalia>
        
        <marginalia anchor_text="mysteria" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Mysteria.</marginalia_text>
                </position>
            </language>
            <translation>Mysteries.</translation>
        </marginalia>
        
        <marginalia anchor_text="multis annis post, multo etiam asseuerantius; amicissimum (me autem appellabat) &amp; aliquot annis minorem natu" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        [multis annis post. assevera[n]tius amicissimus aliquot annis minor natu. - deleted]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                [After many years. More emphatically. The friendliest. Some years younger - deleted].
            </translation>
        </marginalia>
        
        <marginalia anchor_text="spoliat nos iudicio, priuat approbatione omni, orbat sensibus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        spolio
                        priuo
                        orbo } 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                I strip.
                I deprive.
                I bereave.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Cimmeriis quidem, quibus aspectum solis siue deus aliquis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Cymerijs solis no[n] fit aspectus</marginalia_text>
                </position>
            </language>
            <translation>The sight of the sun was not made for the Cimmerian people.</translation>
        </marginalia>
        
        <marginalia anchor_text="Cimmeriis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>CYMERII.</marginalia_text> 
                </position>
            </language>
            <translation>The Cimmerian people.</translation>
        </marginalia>
        
        <marginalia anchor_text="iuratúsque dixeris" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>iuratus Dicere.</marginalia_text>
                </position>
            </language>
            <translation>To speak under oath.</translation>
        </marginalia>
        
        <marginalia anchor_text="Horte[n]sius" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Hortensius</marginalia_text>
                    <person name="Quintus Hortensius"/>
                </position>
            </language>
            <translation>Quintus Hortensius.</translation>
        </marginalia>
        
        <marginalia anchor_text="nec mirum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>nec miru[m]</marginalia_text>
                </position>
            </language>
            <translation>Not wonderful.</translation>
        </marginalia>
        
        <marginalia anchor_text="" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>asseuera[n]tius</marginalia_text>
                </position>
            </language>
            <translation>More emphatically.</translation>
        </marginalia>
        
