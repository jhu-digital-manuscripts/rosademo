        <underline language="LA" method="pen" text="qui fulcire putatur porticu[m] Stoicorum" type="straight"/>
        <underline language="LA" method="pen" text="in co[n]suetudine proba[n]tur" type="straight"/>
        <underline language="LA" method="pen" text="certe tam multa no[n] collegisset, quae nos fallere[n]t probabilitate magna" type="straight"/>
        <underline language="LA" method="pen" text="nisi videret his" type="straight"/>
        <underline language="LA" method="pen" text="nónne putare[m] post illos veteres tot seculis inueniri verum potuisse, tot ingeniis, tantísque studiis quaere[n]tibus" type="straight"/>
        <underline language="LA" method="pen" text="nec solum posse, sed ita necesse esse sapienti, visa est Arcesilae cum" type="straight"/>
        <underline language="LA" method="pen" text="ex eo quod esset sicut esset impressum &amp; signatum &amp; effectum" type="straight"/>
        <underline language="LA" method="pen" text="falsum. Incubuit aute[m] in eas disputationes" type="straight"/>
        <underline language="LA" method="pen" text="no[n] eiusde[m]modi" type="straight"/>
        <underline language="LA" method="pen" text="hoc magis ab eo disputatu[m], quàm probatum puto" type="straight"/>
        <underline language="LA" method="pen" text="idcirco heri no[n] necessario loco co[n]tra sensus tam multa" type="straight"/>
        <underline language="LA" method="pen" text="infracto remo neque columbae collo co[m]moueri" type="straight"/>
        <underline language="LA" method="pen" text="lacerat ista causa" type="straight"/>
        <underline language="LA" method="pen" text="confidere suis testibus, &amp; importune insistere" type="straight"/>
        <underline language="LA" method="pen" text="quum oculum torsisset" type="straight"/>
        <underline language="LA" method="pen" text="opinionis enim esse mendaciu[m], no[n]" type="straight"/>
        <underline language="LA" method="pen" text="Desine quaeso co[m]munibus locis, domi nobis ista nascuntur" type="straight"/>
        <underline language="LA" method="pen" text="Vt enim vera videamus" type="straight"/>
        <underline language="LA" method="pen" text="quàm longe videbimus" type="straight"/>
        <underline language="LA" method="pen" text="sed intendi acies longius non potest" type="straight"/>
        <underline language="LA" method="pen" text="O praeclarum prospectum" type="straight"/>
        <underline language="LA" method="pen" text="audacter isti vestro deo, me" type="straight"/>
        <underline language="LA" method="pen" text="sub oculis sunt" type="straight"/>
        <underline language="LA" method="pen" text="neque ipsi nos suspicere possunt" type="straight"/>
        <underline language="LA" method="pen" text="nobis aer crassus offunditur" type="straight"/>
        
