<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v2.286v.tif" pagination="24" reader="John Dee" signature="[Bb.iiii.[v]]"/>   
    <annotation>
        
        <marginalia anchor_text="Maiora fortasse quaeris. Quid potest esse sole maius, que[m] mathematici amplius duodeuiginti partibus confirmant maiorem esse quàm terra[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Magnitudo.</marginalia_text>
                </position>
            </language>
            <translation>Magnitude.</translation>
        </marginalia>
        
        <marginalia anchor_text="credulo" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Credulus.</marginalia_text>
                </position>
            </language>
            <translation>Credulous.</translation>
        </marginalia>
        
        <marginalia anchor_text="Quatuor sunt capita, quae concludunt nihil esse quod nosci, percipi, comprehe[n]di possit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Quatuor capita qua[e] concludu[n]t o[m]n[i]u[m] reru[m] ignorationem.</marginalia_text>
                </position>
            </language>
            <translation>Four chapters which conclude that people are ignorant of all things.</translation>
        </marginalia>
        
        <marginalia anchor_text="Qui igitur P. Seruilium Geminu[m] videbatm si Quintum se videre putabat" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        P Seruilius gemin[us]
                        Quintus.
                    </marginalia_text>
                    <person name="Publius Servilius Geminus"/>
                    <person name="Quintus Servilius Geminus"/>
                </position>
            </language>
            <translation>
                Publius Servilius Geminus. Quintus Servilius Geminus.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="similitudinem in rerum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Similitudo reru[m].</marginalia_text>
                </position>
            </language>
            <translation>A similarity in things.</translation>
        </marginalia>
        
        <marginalia anchor_text="Nullum esse pilum omnibus rebus talem qualis sit pilus alius, nullum granum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Pilus
                        granu[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A hair. A grain.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="An tibi erit quaerendus annularius aliqui, quoniam gallinarium inuenisti Deliacum illum; Pictor" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Anularius
                        gallinarius.
                        Pictor.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A ring-maker. A poultry-farmer. A painter.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="qua[n]do? cur? quomodo?" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Qua[n]do, cur, q[uo]modo.</marginalia_text>
                </position>
            </language>
            <translation>When, why, how.</translation>
        </marginalia>
        
        <marginalia anchor_text="Chrysippo" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Chrysippus.</marginalia_text>
                    <person name="Chrysippus"/>
                </position>
            </language>
            <translation>Chrysippus.</translation>
        </marginalia>
        
        <marginalia anchor_text="nota; notione" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        nota
                        notio ]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A mark.
                A mark of acquaintance.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="annularius aliqui" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>aliqui anularius</marginalia_text>
                </position>
            </language>
            <translation>A certain ring-maker.</translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="mathematici amplius duodeuiginti partibus confirmant maiorem esse quàm terra[m]" type="straight"/>
        <underline language="LA" method="pen" text="pedalis" type="straight"/>
        <underline language="LA" method="pen" text="quum ille sol, qui tanta incitatione fertur, vt celeritas eius quanta sit ne cogitari" type="straight"/>
        <underline language="LA" method="pen" text="possit, tamen nobis stare videatur" type="straight"/>
        <underline language="LA" method="pen" text="vt minuam controuersiam" type="straight"/>
        <underline language="LA" method="pen" text="paruulis" type="straight"/>
        <underline language="LA" method="pen" text="vos quibuscu[m] res est, id quoque conceditis. omnis pugna de quarto" type="straight"/>
        <underline language="LA" method="pen" text="distinctione sublata" type="straight"/>
        <underline language="LA" method="pen" text="pugnas omnino, sed cu[m] aduersario" type="straight"/>
        <underline language="LA" method="pen" text="quid habes explorati" type="straight"/>
        <underline language="LA" method="pen" text="sui generis esse, nihil esse idem quod sit aliud. Stoicu[m] est quide[m], nec admodum" type="straight"/>
        <underline language="LA" method="pen" text="Haec refelli possunt, sed pugnare nolo" type="straight"/>
        <underline language="LA" method="pen" text="notione" type="straight"/>
        <underline language="LA" method="pen" text="annularius aliqui" type="straight"/>
        <underline language="LA" method="pen" text="Sed adhibes artem aduocatam etiam sensibus" type="straight"/>
        <underline language="LA" method="pen" text="videtur contra te valere" type="straight"/>
        <underline language="LA" method="pen" text="quanto artificio esset sensus nostros mentémque &amp;" type="straight"/>
        <underline language="LA" method="pen" text="constructionem hominis fabricata natura, cur non extimescam opinandi temeritate[m]" type="straight"/>
        <underline language="LA" method="pen" text="ista fabrica est" type="straight"/>
        <underline language="LA" method="pen" text="Tractantur ista ingeniose, disputa[n]tur etia[m] eleganter" type="straight"/>
        <underline language="LA" method="pen" text="res iam vniuersas profunda[m], de quibus volumina impleta sunt no[n] à nostris solum, sed" type="straight"/>
        <underline language="LA" method="pen" text="à Chrysippo: de quo queri solent Stoici" type="straight"/>
        <underline language="LA" method="pen" text="quae à te dilige[n]tissime" type="straight"/>
        
