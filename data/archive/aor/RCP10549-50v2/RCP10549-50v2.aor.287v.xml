        <underline language="LA" method="pen" text="minutatim interroga[n]ti" type="straight"/>
        <underline language="LA" method="pen" text="gradatim" type="straight"/>
        <underline language="LA" method="pen" text="dicitur" type="straight"/>   <!-- Greek needed -->
        <underline language="LA" method="pen" text="Cuius generis error" type="straight"/>
        <underline language="LA" method="pen" text="manat, vt non videam quò non possit accedere" type="straight"/>
        <underline language="LA" method="pen" text="veniam equos sustinebo" type="straight"/>
        <underline language="LA" method="pen" text="ferentur equi praeceps" type="straight"/>
        <underline language="LA" method="pen" text="nec diutius captiose interroganti" type="straight"/>
        <underline language="LA" method="pen" text="Si habes quod liqueat, neque respondes: superbis. si non habes, ne tu quidem percipis, si quia obscura, concedo.              sed negas te vsque ad obscura progredi. illustribus" type="straight"/>
        <underline language="LA" method="pen" text="rebus insistis" type="straight"/>
        <underline language="LA" method="pen" text="eadem illa ars quasi Penelope, telam retexens tollit" type="straight"/>
        <underline language="LA" method="pen" text="Nempe fundamentum Dialecticae est" type="straight"/>
        <underline language="LA" method="pen" text="Si te mentiri dicis, ídque veru[m] dicis: me[n]tiri veru[m] dicis" type="straight"/>
        <underline language="LA" method="pen" text="scilicet inexplicabilia esse" type="straight"/>
        <underline language="LA" method="pen" text="quod est odiosius" type="straight"/>
        <underline language="LA" method="pen" text="Effatum esse id quod aut verum aut falsum sit? Rebus sumptis" type="straight"/>
        <underline language="LA" method="pen" text="mentiris: dicis autem te mentiri, verúmqu edicis, mentiris igitur" type="straight"/>
        <underline language="LA" method="pen" text="connexi, quum concesseris superius, cogit inferius concedere" type="straight"/>
        <underline language="LA" method="pen" text="nec approbare nec improbare: quî" type="straight"/>
        <underline language="LA" method="pen" text="ars, si ratio, si via, si vis denique conclusionis valet, eadem est in vtroque" type="straight"/>
        <underline language="LA" method="pen" text="postulant vt excipiantur haec inexplicabilia" type="straight"/>
        <underline language="LA" method="pen" text="à me istam exceptionem nunquam impetrabunt" type="straight"/>
        <underline language="LA" method="pen" text="omne quod ita disiunctum sit, quasi aut etiam, aut non, non" type="straight"/>
        <underline language="LA" method="pen" text="autem est in natura rerum talis necessitas" type="straight"/>
        <underline language="LA" method="pen" text="autem ea dico quum alterum ait, alterum neget) si talis disiunctio falsa potest" type="straight"/>
        
