    <annotation>
        
        <marginalia anchor_text="merces erat Dialecticorum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>Merces Dialecticoru[m]</marginalia_text>
                </position>
            </language>
            <translation>The payment of dialecticians.</translation>
        </marginalia>
        
        <marginalia anchor_text="ab eo enim Stoico Dialecticorum didicerat" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Diogenes Carneadis magister.</marginalia_text>
                    <person name="Diogenes of Sinope"/>
                    <person name="Carneades"/>
                </position>
            </language>
            <translation>Diogenes of Sinope, the teacher of Carneades.</translation>
        </marginalia>
        
        <marginalia anchor_text="Sequor igitur eas vias quas didici ab Antiocho" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Antiochus cicer. magister</marginalia_text>
                    <person name="Antiochus of Ascalon"/>
                    <person name="Cicero"/>
                </position>
            </language>
            <translation>Antiochus of Ascalon, the teacher of Cicero.</translation>
        </marginalia>
        
        <marginalia anchor_text="ostendamúsque qui simus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Ostendet qui sit.</marginalia_text>
                </position>
            </language>
            <translation>He will show who he is.</translation>
        </marginalia>
        
        <marginalia anchor_text="quatuor eius libri sunt de sustine[n]dis assensionibus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Quatuor Clitomachi Libri de sustine[n]dis assensionibus.</marginalia_text>
                    <book title="On the Withholding of Assent"/>
                </position>
            </language>
            <translation>The four books of Clitomachus about withholding agreement.</translation>
        </marginalia>
        
        <marginalia anchor_text="probabile" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Probabile.</marginalia_text>
                </position>
            </language>
            <translation>Probable.</translation>
        </marginalia>
        
        <marginalia anchor_text="similia veri" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Probabilia veri similia sunt.</marginalia_text>
                </position>
            </language>
            <translation>Probable things are similar to true things.</translation>
        </marginalia>
        
        <marginalia anchor_text="Quid enim, conscendens nauem sapiens, num co[m]prehensum animo habet atque perceptum. se ex sente[n]tia nauigaturum?" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>futuroru[m] cognitio</marginalia_text>
                </position>
            </language>
            <translation>Knowledge of things to be.</translation>
        </marginalia>
        
        <marginalia anchor_text="faciliórque erit vt albam esse niuem probet, quàm erat Anaxagoras, qui id non modo ita esse negabat" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Anaxagora[e] nix nigra</marginalia_text>
                    <person name="Anaxagoras"/>
                </position>
            </language>
            <translation>The black snow of Anaxagoras.</translation>
        </marginalia>
        
        <marginalia anchor_text="Neque nos co[n]tra sensus aliter dicimus ac Stoici, qui multa falsa esse dicunt, longéque aliter se habere ac sensibus videantur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Stoicoru[m] de sensibus opinio.</marginalia_text>
                </position>
            </language>
            <translation>The opinions of the Stoics on the senses.</translation>
        </marginalia>
        
        <marginalia anchor_text="Clitomacho" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Clitomachus</marginalia_text>
                    <person name="Clitomachus"/>
                </position>
            </language>
            <translation>Clitomachus.</translation>
        </marginalia>
        
        <marginalia anchor_text="C. Lucilium scripsit poeta[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Lucilius poëta.</marginalia_text>
                    <person name="Gaius Lucilius"/>
                </position>
            </language>
            <translation>Gaius Lucilius the poet.</translation>
        </marginalia>
        
        <marginalia anchor_text="Neque tamen habere insignem illa[m] &amp; propriam percipiendi nota[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        insignis
                        propria ] nota.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A distinct [and] particular mark.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="Mecu[m] vero quid habent litium, qui ipsorum disciplinam sequor" type="straight"/>
        <underline language="LA" method="pen" text="minam" type="double"/>
        <underline language="LA" method="pen" text="Diogenes reddet" type="straight"/>
        <underline language="LA" method="pen" text="totu[m] tortuosum genus disputandi relinquamus, ostendamúsque qui simus, iam explicata tota Carneadis sententia,              Antiochi ista corruent vniuersa" type="straight"/>
        <underline language="LA" method="pen" text="qui vsque ad senectutem" type="straight"/>
        <underline language="LA" method="pen" text="etenim contra naturam" type="straight"/>
        <underline language="LA" method="pen" text="omnis vitae ea, quam tu Luculle co[m]memorabas, euersio" type="straight"/>
        <underline language="LA" method="pen" text="sit probabilitati illi contrarium: vtetur" type="straight"/>
        <underline language="LA" method="pen" text="se ex sente[n]tia nauigaturum? quî potest" type="straight"/>
        <underline language="LA" method="pen" text="faciliórque erit vt albam" type="straight"/>
        <underline language="LA" method="pen" text="est è saxo sculptus, aut è robore dolatus: habet corpus, habet animu[m], mouetur me[n]te, mouetur sensibus, vt ei vera multa videantur" type="straight"/>
        <underline language="LA" method="pen" text="Vt taceam: co[n]clusio ipsa loquitur nihil" type="straight"/>
        <underline language="LA" method="pen" text="quiuis arbitratu suo reprehe[n]dat, quod" type="straight"/>
        <underline language="LA" method="pen" text="Sunt enim mihi nota propterea, quòd earum ipsaru[m] rerum" type="straight"/>
        
