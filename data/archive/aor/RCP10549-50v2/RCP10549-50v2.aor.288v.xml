    <annotation>
        
        <marginalia anchor_text="expedito, soluto, libero, nulla re implicato" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        expeditus
                        solutus
                        liber
                        nulla re implicatus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Dispatched.
                Released.
                Free.
                Entangled in nothing.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="multa falsa probabilia sint" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Multa falsa probabilia</marginalia_text>
                </position>
            </language>
            <translation>Many false things [are] probable.</translation>
        </marginalia>
        
        <marginalia anchor_text="Itaque ait vehementer errare eos qui dicant ab Academia sensus eripi, à quibus no[n]nunquam dictum sit, aut colorem, aut saporem, aut sonum nullum esse" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Academica[e] opinio</marginalia_text>
                </position>
            </language>
            <translation>The opinion of the Academy.</translation>
        </marginalia>
        
        <marginalia anchor_text="adiungit dupliciter dici assensus sustinere sapiente[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Dupl[icite]r cohibetur asse[n]sus</marginalia_text>
                </position>
            </language>
            <translation>Agreement is withheld in two ways.</translation>
        </marginalia>
        
        <marginalia anchor_text="visa quibus ad actionem excitemur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>visa quibus ad actione[m] excitamur.</marginalia_text>
                </position>
            </language>
            <translation>The visions by which we are called to action</translation>
        </marginalia>
        
        <marginalia anchor_text="sed ea quae vos percipi comprehe[n]díque" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>p[er]cipi. .videri.</marginalia_text>
                </position>
            </language>
            <translation>To be perceived. To be seen.</translation>
        </marginalia>
        
        <marginalia anchor_text="Mare illus quod nunc Fauonio nascente purpureum videtur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Mare fauonio nasce[n]te purpureu[m] videtur.</marginalia_text>
                </position>
            </language>
            <translation>The sea seems purple with the emerging west wind.</translation>
        </marginalia>

        <marginalia anchor_text="quia nobismetipsis modo caeruleu[m] videbatur, mane flauum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Maris varij colores.</marginalia_text>
                </position>
            </language>
            <translation>The various colours of the sea.</translation>
        </marginalia>
        
        <marginalia anchor_text="memoria" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Memoria</marginalia_text>
                </position>
            </language>
            <translation>Memory.</translation>
        </marginalia>
        
        <marginalia anchor_text="Polyaenus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Polya[e]nus.</marginalia_text>
                    <person name="Polyaenus of Lampsacus"/>
                </position>
            </language>
            <translation>Polyaenus of Lampsacus.</translation>
        </marginalia>
        
        <marginalia anchor_text="falsum quod est, id percipi no[n] potest" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>falsum percipi no[n] p[otes]t.</marginalia_text>
                </position>
            </language>
            <translation>A false thing cannot be perceived.</translation>
        </marginalia>
        
        <marginalia anchor_text="memoria" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Memoria</marginalia_text>
                </position>
            </language>
            <translation>Memory.</translation>
        </marginalia>
        
        <marginalia anchor_text="Scyron" hand="Italian">
