        <underline language="LA" method="pen" text="ne subarroga[n]ter" type="straight"/>
        <underline language="LA" method="pen" text="restitero Peripateticis" type="straight"/>
        <underline language="LA" method="pen" text="tam inter se amantes" type="straight"/>
        <underline language="LA" method="pen" text="plura enim vera discrepantia esse non possunt" type="straight"/>
        <underline language="LA" method="pen" text="Vtrum igitur nos impude[n]tes qui labi nolumus, an illi arrogantes" type="straight"/>
        <underline language="LA" method="pen" text="sibi persuaserint scire se solos omnia" type="straight"/>
        <underline language="LA" method="pen" text="Sed discedamus à nobismetipsis" type="straight"/>
        <underline language="LA" method="pen" text="quaero rationes eas quae ex coniectura pende[n]t, quae disputationibus huc &amp; illuc trahu[n]tur" type="straight"/>
        <underline language="LA" method="pen" text="nullam adhibent persuadendi necessitatem" type="straight"/>
        <underline language="LA" method="pen" text="qui omnia vobis quae describunt probant" type="straight"/>
        <underline language="LA" method="pen" text="quibus non concessis digitu[m] progredi no[n] possunt" type="straight"/>
        <underline language="LA" method="pen" text="quod magnitudine[m] nullam" type="straight"/>
        <underline language="LA" method="pen" text="extremitatem &amp; quasi librame[n]tum" type="straight"/>
        <underline language="LA" method="pen" text="liniamento sine vlla latitudine" type="straight"/>
        <underline language="LA" method="pen" text="adiiciam iusiurandu[m]" type="straight"/>
        <underline language="LA" method="pen" text="efficitur multis partibus solem maiore[m]" type="straight"/>
        <underline language="LA" method="pen" text="quae vim afferunt in" type="straight"/>
        <underline language="LA" method="pen" text="Sed sit ingenio diuino" type="straight"/>
        <underline language="LA" method="pen" text="quaestiones infinitas: tantu[m] de principiis rerum, è quibus omnia constant" type="straight"/>
        <underline language="LA" method="pen" text="populari &amp; sodali suo non" type="straight"/>
        <underline language="LA" method="pen" text="infinitatem naturae" type="straight"/>
        <underline language="LA" method="pen" text="sed ex ea particulas similes inter se minutas, eas primum" type="straight"/>
        <underline language="LA" method="pen" text="neque id esse mutabile, &amp; id esse deum: neque natum vsquam quicquam &amp; sempiternum conglobata figura" type="straight"/>
        <underline language="LA" method="pen" text="plenu[m] &amp; inane" type="straight"/>
        <underline language="LA" method="pen" text="vberior in" type="straight"/>
        <underline language="LA" method="pen" text="haec peruulgata, &amp; nota quatuor" type="straight"/>
        <underline language="LA" method="pen" text="Plato ex materia in se omnia recipiente mundum esse factum censet à Deo sempiternum" type="straight"/>
        <underline language="LA" method="pen" text="caeteri tot viri &amp; tanti repudiati ab eo condemnatíque" type="straight"/>
        <underline language="LA" method="pen" text="habere me[n]tem, quae &amp; se &amp; ipsum fabricata sit, &amp; omnia moderetur, moueat, regat" type="straight"/>
        <underline language="LA" method="pen" text="persuasum etiam solem, lunam, stellas omnes, terram, mare, deos esse" type="straight"/>
        <underline language="LA" method="pen" text="animalis intellige[n]tia per omnia ea permeet &amp; transeat: fore tame[n] aliqua[n]do vt omnis hic mundus ardore deflagret. Sint" type="straight"/>
        
