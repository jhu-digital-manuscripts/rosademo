    <annotation>
        
        <marginalia anchor_text="incoeptio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>incoeptio.</marginalia_text>
                </position>
            </language>
            <translation>A beginning.</translation>
        </marginalia>
        
        <marginalia anchor_text="syllabatim" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>syllabatim</marginalia_text>
                </position>
            </language>
            <translation>Syllabically.</translation>
        </marginalia>
        
        <marginalia anchor_text="crassis occultata &amp; circunfusa tenebris" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        crassis occultata et circumfusa tenebris
                    </marginalia_text>
                </position>
            </language>
            <translation>Hidden in fogs and enclosed in darkness.</translation>
        </marginalia>
        
        <marginalia anchor_text="Aristoteles" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Aristoteles.</marginalia_text>
                    <person name="Aristotle"/>
                </position>
            </language>
            <translation>Aristotle.</translation>
        </marginalia>
        
        <marginalia anchor_text="Neque enim ortum esse vnquam mundu[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Mundus infinitus.</marginalia_text>
                </position>
            </language>
            <translation>An infinite world.</translation>
        </marginalia>
        
        <marginalia anchor_text="Deus omnia nostri causa quum faceret" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Deus o[mn]ia n[ost]ra causa fecit</marginalia_text>
                </position>
            </language>
            <translation>The deity made everything for our sake.</translation>
        </marginalia>
        
        <marginalia anchor_text="maiestate[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Dei maiestas.</marginalia_text>
                </position>
            </language>
            <translation>The majesty of the deity.</translation>
        </marginalia>
        
        <marginalia anchor_text="Myrmecides" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Myrmecides</marginalia_text>
                    <person name="Myrmecides"/>
                </position>
            </language>
            <translation>Myrmecides.</translation>
        </marginalia>
        
        <marginalia anchor_text="Negas sine deo posse quicquam" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Sine deo nihil</marginalia_text>
                </position>
            </language>
            <translation>[There is] nothing without a deity.</translation>
        </marginalia>
        
        <marginalia anchor_text="Lampsacenus Strato" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Lampsacenus strato.</marginalia_text>
                    <person name="Strato of Lampsacus"/>
                </position>
            </language>
            <translation>Strato of Lampsacus.</translation>
        </marginalia>
        
        <marginalia anchor_text="somnia censet haec esse Democriti" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Democriti Somnia</marginalia_text>
                    <person name="Democritus"/>
                </position>
            </language>
            <translation>The dreams of Democritus.</translation>
        </marginalia>
        
        <marginalia anchor_text="quicquid aut sit, aut fiat, naturalibus fieri aut factum esse docet ponderibus &amp; motibus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>omnia naturalibus facta ponderibus et motibus</marginalia_text>
                </position>
            </language>
            <translation>Everything made by natural weights and motions.</translation>
        </marginalia>
        
        <marginalia anchor_text="sic ille &amp; deum opere magno liberat, &amp; me timore" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Dei timor. opera</marginalia_text>
                </position>
            </language>
            <translation>Fear of the deity. Works.</translation>
        </marginalia>
        
        <marginalia anchor_text="situs partium" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>partiu[m] situs</marginalia_text>
                </position>
            </language>
            <translation>The positions of the parts.</translation>
        </marginalia>
        
        <marginalia anchor_text="Corpora nostra non nouimus, qui sint situs partium, quam vim quaeque pars habeat ignoramus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Anatomia[e].</marginalia_text>
                </position>
            </language>
            <translation>Anatomies.</translation>
        </marginalia>
        
        <marginalia anchor_text="Empiricis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Empirici.</marginalia_text>
                </position>
            </language>
            <translation>The Empirics.</translation>
        </marginalia>
        
        <marginalia anchor_text="vt videamus terra penitúsne defixa sit, &amp; quasi radicibus suis haereat, an media pendeat" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Terra an media pe[n]deat</marginalia_text>
                </position>
            </language>
            <translation>Whether the Earth hangs in the middle.</translation>
        </marginalia>
        
        <marginalia anchor_text="Habitari ait Xenophanes in luna, eámque esse terram multaru[m] vrbium &amp; mo[n]tium" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Xenophanes Habitari in ☾ ait, eamq[ue] eße terra[m] multaru[m] vrbiu[m] et mo[n]tiu[m].</marginalia_text>
                    <symbol_in_text name="Moon"/>
                </position>
            </language>
            <translation>'Xenophanes says that the moon is inhabited and that it is a land of many cities and mountains' [Loeb 268, 625].</translation>
        </marginalia>
        
        <marginalia anchor_text="Antipodas" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Antipodes.</marginalia_text>
                </position>
            </language>
            <translation>Antipodes.</translation>
        </marginalia>
       
        <marginalia anchor_text="Nicetas Syracusius, vt ait Theophrastus, caelu[m], solem, lunam, stellas, supera denique omnia stare censet, neque praeter terram rem vllam in mundo moueri" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Nicetas syracusius terra[m] stare arbitratur.</marginalia_text>
                    <person name="Hicetas"/>
                </position>
            </language>
            <translation>Hicetas the Syracusian judges that the Earth stays still.</translation>  <!-- DO: Not sure; I think the text is saying the opposite... Maybe 'judges the idea that the Earth stays still'? -->
        </marginalia>
        
        <marginalia anchor_text="hoc etiam Platonem in Timaeo dicere quidam arbitra[n]tur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Plato terra[m] stare ait</marginalia_text>
                    <person name="Plato"/>
                </position>
            </language>
            <translation>Plato says that the Earth stays still.</translation>
        </marginalia>
        
        <marginalia anchor_text="Socrates, liber Aristo Chius" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Socrates. Aristo.</marginalia_text>
                    <person name="Socrates"/>
                    <person name="Aristo of Chios"/>
                </position>
            </language>
            <translation>Socrates. Aristo.</translation>
        </marginalia>
        
        <marginalia anchor_text="animus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Animus</marginalia_text>
                </position>
            </language>
            <translation>The mind.</translation>
        </marginalia>
        
        <marginalia anchor_text="Dicaearcho" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Dicearchius.</marginalia_text>
                    <person name="Dicaearchus"/>
                </position>
            </language>
            <translation>Dicaearchus.</translation>
        </marginalia>
        
        <marginalia anchor_text="animus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>De a[n]i[m]o varia[e] qu[a]estio[n]es.</marginalia_text>
                </position>
            </language>
            <translation>Various questions about the mind.</translation>
        </marginalia>
        
        <marginalia anchor_text="eluditis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>eludere alique[m].</marginalia_text>
                </position>
            </language>
            <translation>To make fun of someone.</translation>
        </marginalia>
        
        <marginalia anchor_text="" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>hinc Copernicus sua[e] theorica[e] occasione[m] [arripuit].</marginalia_text>   
                   <person name="Nicolaus Copernicus"/>
                </position>
            </language>
            <translation>Here Corpernius seized the occasion for his own theory.</translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="sapiens syllabatim" type="straight"/>
        <underline language="LA" method="pen" text="veniet illud flumen orationis aureum fundens Aristoteles, qui illum" type="straight"/>
        <underline language="LA" method="pen" text="incoeptio" type="straight"/>
        <underline language="LA" method="pen" text="dilapsus óccidat" type="straight"/>
        <underline language="LA" method="pen" text="sicut caput &amp; fama[m] tuam" type="straight"/>
        <underline language="LA" method="pen" text="omnia nostri causa quum faceret (sic enim vultis) tantam vim natricu[m], viperarúmque fecerit,              cur mortifera ta[m] multa perniciosa terra maríque disperserit? Negatis" type="straight"/>
        <underline language="LA" method="pen" text="cuius quide[m] vos maiestate[m] deducitis vsque ad apium formicarúmque perfectione[m]:              vt etiam inter deos Myrmecides aliquis minutoru[m] opusculorum fabricator fuisse videatur. Negas" type="straight"/>
        <underline language="LA" method="pen" text="Sed quum sacerdotes deoru[m], vacatione[m] habeant: quanto est aequius" type="straight"/>
        <underline language="LA" method="pen" text="Negat opera deorum se vti ad fabricandum mundu[m]" type="straight"/>
        <underline language="LA" method="pen" text="docet omnia effecta esse natura, nec vt ille qui asperis &amp; leuibus,              &amp; hamatis vncinatísque corporibus concreta haec esse dicat interiecto inani, somnia censet haec esse Democriti non docentis, sed optantis. Ipse" type="straight"/>
        <underline language="LA" method="pen" text="naturalibus fieri aut factum esse docet ponderibus &amp; motibus" type="straight"/>
        <underline language="LA" method="pen" text="Quis enim potest (quum existimet curari se à deo) no[n] &amp; dies &amp; noctes diuinu[m] nume[n] horrere?              &amp; siquid aduersi acciderit (quod cui no[n] accidit) extimescere, ne id iure euenerit? Nec" type="straight"/>
        <underline language="LA" method="pen" text="omnia Luculle crassis occultata &amp; circunfusa tenebris" type="straight"/>
        <underline language="LA" method="pen" text="vt nulla acies humani ingenii tanta sit quae penetrare" type="straight"/>
        <underline language="LA" method="pen" text="Corpora nostra non nouimus, qui sint situs partium, quam vim quaeque pars habeat ignoramus" type="straight"/>
        <underline language="LA" method="pen" text="vt patefacta &amp; detecta mutentur" type="straight"/>
        <underline language="LA" method="pen" text="persecare, aperire, diuidere possumus" type="straight"/>
        <underline language="LA" method="pen" text="videamus terra penitúsne defixa sit" type="straight"/>
        <underline language="LA" method="pen" text="&amp; quasi radicibus suis haereat, an media pendeat" type="straight"/>
        <underline language="LA" method="pen" text="ait Xenophanes in luna, eámque esse terram multaru[m] vrbium &amp; mo[n]tium. Portenta vide[n]tur:              sed tame[n] nec ille qui dixit iurare posset ita se rem" type="straight"/>
        <underline language="LA" method="pen" text="quos Antipodas vocatis" type="straight"/>
        <underline language="LA" method="pen" text="quae quum circum axem se summa celeritate conuertat &amp; torqueat, eadem effici omnia, quasi stante terra caelum moueretur. Atque" type="straight"/>
        <underline language="LA" method="pen" text="sed paulo obscurius" type="straight"/>
        <underline language="LA" method="pen" text="vicissim eluditis. Liber igitur à tali irrisione Socrates, liber Aristo Chius, qui nihil istoru[m]" type="straight"/>
        <underline language="LA" method="pen" text="placuit, rationis, irae, cupiditatis" type="straight"/>
        <underline language="LA" method="pen" text="ita sunt in plerisque contrariaru[m] rationu[m] paria momenta" type="straight"/>
        
