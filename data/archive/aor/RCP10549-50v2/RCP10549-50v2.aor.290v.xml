<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v2.290v.tif" pagination="32" reader="John Dee"/>
    <annotation>
        
        <marginalia anchor_text="magnitudinem quasi decempeda" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>dece[m]peda magnitudo</marginalia_text>
                </position>
            </language>
            <translation>A size of ten feet.</translation>
        </marginalia>
        
        <marginalia anchor_text="Vrgebor iam omnium vestru[m] co[n]uitio" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>o[mn]i[u]m co[n]vitio vrgeri</marginalia_text>
                </position>
            </language>
            <translation>To be oppressed by the scorn of all.</translation>
        </marginalia>
        
        <marginalia anchor_text="rapior in inuidiam" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>in invidia[m] rapi</marginalia_text>
                </position>
            </language>
            <translation>To be dragged into disrepute.</translation>
        </marginalia>
        
        <marginalia anchor_text="Solis autem magnitudo (ipse enim hic radiatus me intueri videtur)" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>sol radiatus</marginalia_text>
                </position>
            </language>
            <translation>The radiating sun.</translation>
        </marginalia>
        
        <marginalia anchor_text="inane" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Inane</marginalia_text>
                </position>
            </language>
            <translation>Inane.</translation>
        </marginalia>
        
        <marginalia anchor_text="atomos" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Atomi</marginalia_text>
                </position>
            </language>
            <translation>Atoms.</translation>
        </marginalia>
        
        <marginalia anchor_text="Et quum in vno mundo ornatus hic tam sit mirabilis, innumerabilis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Mundi innumerabiles</marginalia_text>
                </position>
            </language>
            <translation>Innumerable worlds.</translation>
        </marginalia>
        
        <marginalia anchor_text="imagines" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Imagines.</marginalia_text>
                </position>
            </language>
            <translation>Images.</translation>
        </marginalia>
        
        <marginalia anchor_text="Nihil sentire est melius, quàm tam praua sentire" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Nihil sentire melius quàm tam praua sentire.</marginalia_text>
                </position>
            </language>
            <translation>It is better to think nothing than to think such perverse things.</translation>
        </marginalia>
        
        <marginalia anchor_text="Nec enim diuinationem, quam probatis, vllam esse arbitror" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Diuinatio nulla</marginalia_text>
                </position>
            </language>
            <translation>No divination.</translation>
        </marginalia>
        
        <marginalia anchor_text="fatúmque illud etiam, quo omnia contineri dicitis, conte[m]no" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>fatum nullum.</marginalia_text>
                </position>
            </language>
            <translation>No fate.</translation>
        </marginalia>
        
        <marginalia anchor_text="aether videtur summus Deus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Æther summus DEVS</marginalia_text>
                </position>
            </language>
            <translation>The aether, the highest deity.</translation>
        </marginalia>
        
        <marginalia anchor_text="Solem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Sol.</marginalia_text>
                </position>
            </language>
            <translation>The sun.</translation>
        </marginalia>
        
        <marginalia anchor_text="cogimur dissensione sapientum dominum nostrum ignorare" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Dissentione sapie[n]tum dominu[m] n[ost]r[u]m ignorare cogimur</marginalia_text>
                </position>
            </language>
            <translation>We are compelled by the disagreement of the wise to be ignorant of our lord.</translation>
        </marginalia>
        
        <marginalia anchor_text="nec tamen istas quaestiones Physicorum exterminandas puto" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Ista[e] physicoru[m] qua[e]stiones non exterminanda[e].</marginalia_text>
                </position>
            </language>
            <translation>Those questions of the natural philosophers are not to be expelled.</translation>
        </marginalia>
        
        <marginalia anchor_text="pabulum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Animoru[m] pabulu[m].</marginalia_text>
                </position>
            </language>
            <translation>The nourishment of minds.</translation>
        </marginalia>
        
        <marginalia anchor_text="Quaeret igitur haec &amp; vester sapie[n]s, &amp; hic noster" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Sapiens                  
                        Noster &amp; vester
                        dissentiu[n]t
                    </marginalia_text>
                </position>
            </language>
            <translation>Our wise person and your wise person disagree.</translation>
        </marginalia>
        
        <marginalia anchor_text="quum cornix cecinerit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Quum cornix cecinerit.</marginalia_text>
                </position>
            </language>
            <translation>When a crow caws.</translation>
        </marginalia>
        
        <marginalia anchor_text="quàm solem, quem metiri non possunt plus quàm  duodeuiginti partibus maiore[m] esse quàm terra[m]" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>An ta[n]tum ☉ 18 terras co[n]tinuet</marginalia_text>
                    <symbol_in_text name="Sun"/>
                </position>
            </language>
            <translation>Whether the sun covers as much space as 18 Earths.</translation>
        </marginalia>
        
        <marginalia anchor_text="vna definitio co[m]prehendendi" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>vna est definitio co[m]prehendendi.</marginalia_text>
                </position>
            </language>
            <translation>The is one definition to be comprehended.</translation>
        </marginalia>
        
        <marginalia anchor_text="Herillum qui in cognitione &amp; scientia summu[m] bonu[m] ponit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Herillus summu[m] bonu[m] in cognitio[n]e et scie[n]tia posuit.</marginalia_text>
                    <person name="Herillus"/>
                </position>
            </language>
            <translation>Herillus put the greatest good into understanding and knowledge.</translation>
        </marginalia>
        
        <marginalia anchor_text="Xenophanes; Parmenides &amp; Zeno; Euclides" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Xenophanes
                        Parmenides
                        Zeno.
                        Euclides.
                    </marginalia_text>
                    <person name="Xenophanes"/>
                    <person name="Parmenides"/>
                    <person name="Zeno of Citium"/>
                    <person name="Euclid"/>
                </position>
            </language>
            <translation>
                Xenophanes.
                Parmenides.
                Zeno of Citium.
                Euclid.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="paululum antè dicendum est" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        paululu[m] antè dice[n]du[m] est
                        [superius - deleted] repete[n]du[m] altius.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                'A few words must be said first' [Loeb 268, 633].
                [A few words] must be repeated higher up.            
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="Vrgebor iam omnium vestru[m]" type="straight"/>
        <underline language="LA" method="pen" text="praeclara[m]? Et quum in vno mundo ornatus hic tam sit mirabilis, innumerabilis" type="straight"/>
        <underline language="LA" method="pen" text="nominibus, honoribus" type="straight"/>
        <underline language="LA" method="pen" text="rebus gestis, ingeniis" type="straight"/>
        <underline language="LA" method="pen" text="si etiam dormientes aliquid animo videre videamur, imagines" type="straight"/>
        <underline language="LA" method="pen" text="animos nostros per corpus irrumpere" type="straight"/>
        <underline language="LA" method="pen" text="Nihil sentire est melius, quàm tam praua sentire" type="straight"/>
        <underline language="LA" method="pen" text="diuinationem, quam probatis, vllam esse arbitror: fatúmque illud etiam, quo" type="straight"/>
        <underline language="LA" method="pen" text="contineri dicitis, conte[m]no: ne exaedificatu[m] quidem hunc mundu diuino co[n]silio existimo" type="straight"/>
        <underline language="LA" method="pen" text="haud scio an ita sit" type="straight"/>
        <underline language="LA" method="pen" text="Sed quum rapior in inuidiam" type="straight"/>
        <underline language="LA" method="pen" text="videtur summus Deus, mente praeditus, qua omnia regantur" type="straight"/>
        <underline language="LA" method="pen" text="cogimur dissensione sapientum dominum nostrum ignorare" type="straight"/>
        <underline language="LA" method="pen" text="quippe qui nesciamus Soli, an aetheri seruiamus" type="straight"/>
        <underline language="LA" method="pen" text="ipse enim hic radiatus me intueri videtur" type="straight"/>
        <underline language="LA" method="pen" text="admonet, vt crebro faciam mentionem sui" type="straight"/>
        <underline language="LA" method="pen" text="decempeda" type="straight"/>
        <underline language="LA" method="pen" text="exterminandas puto. Est enim animoru[m] ingeniorúmque naturale quoddam quasi pabulum consyderatio contemplatióque naturae.              Erigimur, latiores fieri videmur, humana despicimus: cogitantésque supera atque caelestia, haec nostra vt exigua &amp; minima contemnimus. Indagatio ipsa reru[m] cum maximaru[m],              tum etiam occultissimarum habet oblectatione[m]. Si vero aliquid occurret quod verisimile videatur, humanissima co[m]pletur animus" type="straight"/>
        <underline language="LA" method="pen" text="assentiatur, credat, affirmet: noster, vt vereatur" type="straight"/>
        <underline language="LA" method="pen" text="eiusmodi rebus verisimile quod sit inuenerit" type="straight"/>
        <underline language="LA" method="pen" text="&amp; paululum antè dicendum" type="straight"/>
        <underline language="LA" method="pen" text="quum cornix cecinerit, tum" type="straight"/>
        <underline language="LA" method="pen" text="co[n]stituendi sunt ad quos &amp;" type="straight"/>
        <underline language="LA" method="pen" text="cognitione &amp; scientia summu[m] bonu[m] ponit" type="straight"/>
        
