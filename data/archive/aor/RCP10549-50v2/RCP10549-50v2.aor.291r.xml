    <page filename="RCP10549-50v2.291r.tif" pagination="33" reader="John Dee" signature="Cc.i."/>
    <annotation>
        
        <marginalia anchor_text="bonum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Bonu[m].</marginalia_text>
                </position>
            </language>
            <translation>Good.</translation>
        </marginalia>
        
        <marginalia anchor_text="Nihil esse bonum, nisi virtutem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Nihil Bonu[m] nisi virtus.</marginalia_text>
                </position>
            </language>
            <translation>Nothing is good except virtue.</translation>
        </marginalia>
        
        <marginalia anchor_text="In mediis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>In medijs.</marginalia_text>      
                </position>
            </language>
            <translation>In intermediate things.</translation>
        </marginalia>
        
        <marginalia anchor_text="" hand="Italian">
            <language ident="EL">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>ἀδιαφορία </marginalia_text>               
                </position>
            </language>
            <translation>Indifference.</translation>
        </marginalia>
        
        <marginalia anchor_text="" hand="Italian">
            <language ident="EL">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>ἀπάθεια </marginalia_text>              
                </position>
            </language>
            <translation>Impassibility.</translation>
        </marginalia>
        
        <marginalia anchor_text="voluptate[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>voluptas.</marginalia_text>
                </position>
            </language>
            <translation>Pleasure.</translation>
        </marginalia>
        
        <marginalia anchor_text="Voluptatem autem &amp; honestatem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Voluptas cu[m] Honestate</marginalia_text>
                </position>
            </language>
            <translation>Pleasure with honour.</translation>
        </marginalia>
        
        <marginalia anchor_text="Hieronymus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Hieronymus.</marginalia_text>
                    <person name="Hieronymus of Rhodes"/>
                </position>
            </language>
            <translation>Hieronymus.</translation>
        </marginalia>
        
        <marginalia anchor_text="Diodorus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Diodorus.</marginalia_text>
                    <person name="Diodorus of Tyre"/>
                </position>
            </language>
            <translation>Diodorus.</translation>
        </marginalia>
        
        <marginalia anchor_text="Aristoteles" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Aristoteles.</marginalia_text>
                    <person name="Aristotle"/>
                </position>
            </language>
            <translation>Aristotle.</translation>
        </marginalia>
        
        <marginalia anchor_text="summum bonum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Summu[m] Bonu[m] carneadis</marginalia_text>
                    <person name="Carneades"/>
                </position>
            </language>
            <translation>The greatest good of Carneades.</translation>
        </marginalia>
        
        <marginalia anchor_text="Zeno statuit finem esse bonorum, qui inue[n]tor &amp; princeps Stoicorum fuit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Zeno stoicoru[m] princeps.</marginalia_text>
                    <person name="Zeno of Citium"/>
                </position>
            </language>
            <translation>Zeno of Citium, the founder of the Stoics.</translation>
        </marginalia>
        
        <marginalia anchor_text="Aristotelem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Aristoteles.</marginalia_text>
                    <person name="Aristotle"/>
                </position>
            </language>
            <translation>Aristotle.</translation>
        </marginalia>
        
        <marginalia anchor_text="Antiochum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Antiochus</marginalia_text>
                    <person name="Antiochus of Ascalon"/>
                </position>
            </language>
            <translation>Antiochus of Ascalon.</translation>
        </marginalia>
        
        <marginalia anchor_text="omnis ratio vitae definitione summi boni continetur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>O[mn]is vita[e] r[ati]o summi Boni definitione co[n]tinetur</marginalia_text>
                </position>
            </language>
            <translation>The entire reasoning of life is bound up with the definition of the greatest good.</translation>
        </marginalia>
        
        <marginalia anchor_text="Quid, quum ipse Antiochus dissentit quibusdam in rebus ab his quos amat Stoicis, nónne indicat no[n] posse illa proba[n]da esse sapienti" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Antiochus cur disse[n]tiat.</marginalia_text>
                    <person name="Antiochus of Ascalon"/>
                </position>
            </language>
            <translation>Why Antiochus disagrees.</translation>
        </marginalia>
        
        <marginalia anchor_text="Placet Stoicis omnia peccata esse paria" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>peccata o[mn]ia paria secu[n]dum stoicos</marginalia_text>
                </position>
            </language>
            <translation>All sins are equal according to the Stoics.</translation>
        </marginalia>
        
        <marginalia anchor_text="Scelus enim dicebas esse Luculle dogma prodere" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Dogma prodere, scelus e[sse].</marginalia_text>
                </position>
            </language>
            <translation>To abandon a dogma is a crime.</translation>
        </marginalia>
        
        <marginalia anchor_text="Etiam, inquit, beatam, sed non beatissimam" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>vita Beata, beatissima</marginalia_text>
                </position>
            </language>
            <translation>A charmed life; the most charmed life.</translation>
        </marginalia>
        
        <marginalia anchor_text="Theophrasto" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Theophrastus.</marginalia_text>
                    <person name="Theophrastus"/>
                </position>
            </language>
            <translation>Theophrastus.</translation>
        </marginalia>
        
        <marginalia anchor_text="Distrahor" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Distrahor.</marginalia_text>
                </position>
            </language>
            <translation>I am dragged.</translation>
        </marginalia>
        
        <marginalia anchor_text="Zenoni" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Zeno.</marginalia_text>
                    <person name="Zeno of Citium"/>
                </position>
            </language>
            <translation>Zeno of Citium.</translation>
        </marginalia>
        
        <marginalia anchor_text="sed Zenoni necessarium, cui praeter honestu[m] nihil est in bonis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Honestum solu[m], Bonu[m].</marginalia_text>
                </position>
            </language>
            <translation>Only honourable, good.</translation>
        </marginalia>
        
        <marginalia anchor_text="Tibi vero Antioche minime, cui praeter honestatem multa bona, praeter turpitudinem multa mala videntur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Antiochus aliter</marginalia_text>
                </position>
            </language>
            <translation>Antiochus [thinks] otherwise.</translation>
        </marginalia>
        
        <marginalia anchor_text="homuncio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>Homuncio.</marginalia_text>
                </position>
            </language>
            <translation>A little man.</translation>
        </marginalia>
        
        <marginalia anchor_text="Praecide, inquit: statue" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        pra[e]cide
                        statue.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Cut it short.
                Make up your mind.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="dicebant, quòd esset" type="straight"/>
        <underline language="LA" method="pen" text="quorum omne bonum in mente positum, &amp;" type="straight"/>
        <underline language="LA" method="pen" text="acie, qua verum" type="straight"/>
        <underline language="LA" method="pen" text="illis similia, sed opinor explicata vberius &amp;" type="straight"/>
        <underline language="LA" method="pen" text="Nihil esse bonum, nisi virtutem" type="straight"/>
        <underline language="LA" method="pen" text="nec malu[m], nisi quod virtuti esset contrarium. In" type="straight"/>
        <underline language="LA" method="pen" text="haec videamus quae nunc diu multúmque defensa sunt" type="straight"/>
        <underline language="LA" method="pen" text="molestia Hieronymus" type="straight"/>
        <underline language="LA" method="pen" text="Honeste autem viuere fruentem rebus his, quas primas homini natura conciliet, &amp; vetus Academia censuit,              vt indicant scripta Polemonis, quae Antiochus probat maxime &amp; Aristoteles, eiúsque amici nu[n]c proxime videntur accedere" type="straight"/>
        <underline language="LA" method="pen" text="vt opponeret" type="straight"/>
        <underline language="LA" method="pen" text="bonum esse frui his rebus, quas primas natura conciliauisset" type="straight"/>
        <underline language="LA" method="pen" text="à conciliatione naturae" type="straight"/>
        <underline language="LA" method="pen" text="bonoru[m], quos" type="straight"/>
        <underline language="LA" method="pen" text="Ad vos nunc refero" type="straight"/>
        <underline language="LA" method="pen" text="tam ineruditum absurdúmque respondeat" type="straight"/>
        <underline language="LA" method="pen" text="dici inconsyderatius" type="straight"/>
        <underline language="LA" method="pen" text="per ipsum Aristotelem" type="straight"/>
        <underline language="LA" method="pen" text="germanissimus Stoicis" type="straight"/>
        <underline language="LA" method="pen" text="inter eos non de terminis, sed de tota possessione contentio" type="straight"/>
        <underline language="LA" method="pen" text="veheme[n]tissime displicet" type="straight"/>
        <underline language="LA" method="pen" text="nónne caueam ne scelus faciam" type="straight"/>
        <underline language="LA" method="pen" text="dogma prodere" type="straight"/>
        <underline language="LA" method="pen" text="Etiam, inquit, beatam, sed non beatissimam" type="straight"/>
        <underline language="LA" method="pen" text="homuncio hic" type="straight"/>
        <underline language="LA" method="pen" text="praesertim Theophrasto multa diserte copioséque dicente" type="straight"/>
        <underline language="LA" method="pen" text="Distrahor, tu[m] hoc mihi probabilius, tum illud videtur: &amp; tamen nisi alterutru[m] sit, virtute[m] iacere plane puto" type="straight"/>
        
