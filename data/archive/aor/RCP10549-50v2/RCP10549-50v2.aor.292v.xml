    <annotation>
        
        <marginalia anchor_text="in varias reprehensiones incurreret" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>in reprehensiore[m] incurrere</marginalia_text>
                </position>
            </language>
            <translation>To incur criticism.</translation>
        </marginalia>
        
        <marginalia anchor_text="quas plagas ipsi co[n]tra se Stoici texuerunt" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>plagas texere co[n]tra.</marginalia_text>
                </position>
            </language>
            <translation>To plant misfortunes against.</translation>
        </marginalia>
        
        <marginalia anchor_text="si remissius agatur; moderatius tamen id volunt fieri" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        Si remissius agatur
                        moderatius id volu[n]t fieri.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                If it is done in a more relaxed way.
                They want it to be done more moderate.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="vt percipi nihil putem posse" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Catulus nihil p[er]cipi putat.</marginalia_text>
                    <person name="Quintus Lutatius Catulus"/>
                </position>
            </language>
            <translation>Quintus Lutatius Catulus thinks nothing can be perceived.</translation>
        </marginalia>
        
        <marginalia anchor_text="ista Academiae est propria sententia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Sente[n]tia academia[e] p[ro]p[ri]a</marginalia_text>
                </position>
            </language>
            <translation>The distinct opinion of the Academy.</translation>
        </marginalia>
        
        <marginalia anchor_text="Graeco" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Gra[e]ci.</marginalia_text>
                </position>
