            <translation>The Greeks.</translation>
        </marginalia>
        
        <marginalia anchor_text="philosophari" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Philosophari.</marginalia_text>
                </position>
            </language>
            <translation>To philosophise.</translation>
        </marginalia>
        
        <marginalia anchor_text="studium" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Philosophia[e] studiu[m]</marginalia_text>
                </position>
            </language>
            <translation>The study of philosophy.</translation>
        </marginalia>
        
        <marginalia anchor_text="Latinas" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Latina[e] litera[e]</marginalia_text>
                </position>
            </language>
            <translation>Latin literature.</translation>
        </marginalia>
        
        <marginalia anchor_text="philosophiae quidem vituperatoribus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>philosophia[e] vituperatores</marginalia_text>
                </position>
            </language>
            <translation>Critics of philosophy.</translation>
        </marginalia>
        
        <marginalia anchor_text="Hortensio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Hortensius.</marginalia_text>
                    <person name="Quintus Hortensius"/>
                </position>
            </language>
            <translation>Quintus Hortensius.</translation>
        </marginalia>
        
        <marginalia anchor_text="in réque eo meliore quo maior sit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Philiosophia quo maior eo melior erit.</marginalia_text>
                </position>
            </language>
            <translation>Where philosophy is bigger it will be better.</translation>
        </marginalia>
        
        <marginalia anchor_text="modus est vllus inuestigandi veri, nisi inueneris" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Modus nullus inuestigandi veri, nisi inueneris</marginalia_text>
                </position>
            </language>
            <translation>There is no method of investigating the truth unless you discover it.</translation>
        </marginalia>
        
        <marginalia anchor_text="illiberali labore" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>labor illiberalis.</marginalia_text>
                </position>
            </language>
            <translation>An ignoble labour.</translation>
        </marginalia>
        
        <marginalia anchor_text="curiosi" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Curiosi</marginalia_text>
                </position>
            </language>
            <translation>Interfering people.</translation>
        </marginalia>
        
        <marginalia anchor_text="Latina scripta dicu[n]t co[n]temnere" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Latina scripta co[n]temnentes taxantur.</marginalia_text>
                </position>
            </language>
            <translation>Those who scorn Latin writings are valued.</translation>  
        </marginalia>
        
        <marginalia anchor_text="fabellas Latinas" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>fabella[e] latina[e].</marginalia_text>
                </position>
            </language>
            <translation>Latin plays.</translation>
        </marginalia>
        
        <marginalia anchor_text="Sophocles" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Sophocles.</marginalia_text>
                    <person name="Sophocles"/>
                </position>
            </language>
            <translation>Sophocles.</translation>
        </marginalia>
        
        <marginalia anchor_text="Attilii" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Attilius.</marginalia_text> <!--JG: Unclear who this is-->
                </position>
            </language>
            <translation>Attilius.</translation>
        </marginalia>
        
        <marginalia anchor_text="coerceri reprimíque" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        Coerceri         
                        reprimi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be forced.
                To be restrained.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="quaere[n]di defatigatio turpis est, quum id quod quaeritur sit pulcherrimu[m]" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        Qu[a]erendi defatigatio turpis est quu[m] id quod qu[a]eritur sit pulcherimu[m] 
                    </marginalia_text>
                </position>
            </language>
            <translation>'It were base to flag in the pursuit, when the object pursued is so supremely lovely' [Loeb 40, 5].</translation>
        </marginalia>
        
