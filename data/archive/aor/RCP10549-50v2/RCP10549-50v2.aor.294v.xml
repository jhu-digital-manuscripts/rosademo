        <marginalia anchor_text="honesta" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Honesta.</marginalia_text>
                </position>
            </language>
            <translation>Moral integrity.</translation>
        </marginalia>
        
        <marginalia anchor_text="per se esset &amp; virtus &amp; cognitio reru[m]" hand="Italian">
