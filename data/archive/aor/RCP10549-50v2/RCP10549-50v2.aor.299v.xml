    <page filename="RCP10549-50v2.299v.tif" pagination="50" reader="John Dee" signature="[Dd.i.[v]]"/> 
    <annotation>
        
       <marginalia anchor_text="dialecticas captiones" hand="English_secretary">
           <language ident="LA">
               <position book_orientation="0" place="head">
                   <marginalia_text>dialectic[a]e captio[n]es</marginalia_text>
               </position>
           </language>
           <translation>Quibbling dialectics.</translation>
       </marginalia> 
        
        <marginalia anchor_text="misceat mulsum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>miscere mulsu[m]</marginalia_text>
                </position>
            </language>
            <translation>To mix honeyed wine.</translation>
        </marginalia> 
        
        <marginalia anchor_text="dulciter ac iucunde" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        Iucundè
                        dulciter ]
                        [deleted]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Sweetly; pleasantly.
            </translation>
        </marginalia> 
        
        <marginalia anchor_text="ruit in dicendo" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>ruit in dice[n]do</marginalia_text>
                </position>
            </language>
            <translation>He fails in speaking.</translation>
        </marginalia> 
        
        <marginalia anchor_text="nec laetantiu[m] nec dolentiu[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Ho[m]i[n]es nec la[e]tantes nec Dolentes.</marginalia_text>
                </position>
            </language>
            <translation>Men neither rejoicing nor grieving.</translation>
        </marginalia> 
        
        <marginalia anchor_text="Rhetorice igitur, inquam, nos mauis quàm dialectice disputare" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Rhetoricè &amp; dialecticè disputare</marginalia_text>
                </position>
            </language>
            <translation>To argue rhetorically and dialectically.</translation>
        </marginalia>
        
        <marginalia anchor_text="quòd latius loquerentur Rhetores, Dialectici autem compressius" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Latius loquu[n]tur Rhetores 
                        Compressius Dialectici
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Rhetoricians speak more widely.
                Dialecticians [speak] in a more compressed way.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="quum populariter loquatur, esse interdum paulo hebetiorem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Rhetorica hebetior quu[m] populariter loquatur</marginalia_text>
                </position>
            </language>
            <translation>Rhetoric [is] blunter when spoken popularly.</translation>
        </marginalia>
        
        <marginalia anchor_text="" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Contra Epicuru[m] Dialectica[m] sperne[n]tem</marginalia_text>
                    <person name="Epicurus"/>
                </position>
            </language>
            <translation>Against Epicurus rejecting dialectics.</translation>
        </marginalia>
        
        <marginalia anchor_text="voluptatem tueretur, quam Aristippus, id est qua sensus dulciter ac iucunde mouetur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Aristippi voluptas</marginalia_text>
                    <person name="Aristippus"/>
                </position>
            </language>
            <translation>Pleasure according to Aristippus.</translation>
        </marginalia>
        
        <marginalia anchor_text="Aristoteles, qui virtutis vsum cum vitae perfectae prosperitate coniunxit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Aristotelis foelicitas</marginalia_text>
                    <person name="Aristotle"/>
                </position>
            </language>
            <translation>Happiness according to Aristotle.</translation>
        </marginalia>
        
        <marginalia anchor_text="Callipho" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Callipho.</marginalia_text>
                    <person name="Calliphon"/>
                </position>
            </language>
            <translation>Calliphon.</translation>
        </marginalia>
        
        <marginalia anchor_text="Diodotus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Diodorus.</marginalia_text>
                    <person name="Diodorus of Tyre"/>
                </position>
            </language>
            <translation>Diodorus of Tyre.</translation>
        </marginalia>
        
        <marginalia anchor_text="Duae sunt enim res quoque, ne tu verba solum putes" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Res: verba.</marginalia_text>
                </position>
            </language>
            <translation>Things. Words.</translation>
        </marginalia>
        
        <marginalia anchor_text="ab illo Aristippeo genere voluptatis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Aristippeu[m] genus volupt.</marginalia_text>
                    <person name="Aristippus"/>
                </position>
            </language>
            <translation>The Aristippean kind of pleasure.</translation>
        </marginalia>
        
        <marginalia anchor_text="" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Epicuri</marginalia_text>            
                    <person name="Epicurus"/>
                </position>
            </language>
            <language ident="EL">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>κυριαὶ δόξαι</marginalia_text> <!-- for the Κύριαι Δόξαι, the lordly doctrines, the Epicurean Principal Doctrines -->
                </position>
            </language>
            <translation>The lordly doctrines.</translation>
        </marginalia>
        
        <marginalia anchor_text="alia sentit, inquam, alia loquitur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Alia se[n]tit, alia loquitur</marginalia_text>
                </position>
            </language>
            <translation>He feels one way, he speaks another.</translation>
        </marginalia>
        
        <marginalia anchor_text="finibus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>finis definitio</marginalia_text>
                </position>
            </language>
            <translation>End. Definition.</translation>
        </marginalia>
        
        <marginalia anchor_text="Graece egregie loquatur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>gra[e]cè egregie loqui.</marginalia_text>
                </position>
            </language>
            <translation>To speak excellently in Greek.</translation>
        </marginalia>
        
