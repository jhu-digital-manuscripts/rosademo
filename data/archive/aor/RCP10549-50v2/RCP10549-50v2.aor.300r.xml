                    <marginalia_text>Cur ad coenam ëu[n]dum</marginalia_text>
                </position>
            </language>
            <translation>Why to go to dinner.</translation>
        </marginalia>
        
        <marginalia anchor_text="tria genera cupiditatum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Tria Cupiditatu[m] genera.</marginalia_text>
                </position>
            </language>
