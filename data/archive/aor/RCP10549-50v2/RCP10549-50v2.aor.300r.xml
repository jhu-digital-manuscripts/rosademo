        <marginalia anchor_text="si grauis, breuis: si longus, leuis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Si grauis breuis, si lo[n]g[us] leuis</marginalia_text>
                </position>
            </language>
            <translation>Brief if strong, light if long.</translation>
        </marginalia>
        
