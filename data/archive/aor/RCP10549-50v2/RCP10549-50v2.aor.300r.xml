        </marginalia>
        
        <marginalia anchor_text="hoc est non diuidere, sed fra[n]gere rem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Hoc no[n] est diuidere sed frangere rem.</marginalia_text>
                </position>
            </language>
            <translation>This is not dividing a thing but smashing it.</translation>
        </marginalia>
        
        <marginalia anchor_text="bene, id recte, frugaliter, honeste: ille porrò male, praue, nequiter, turpiter" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        bene
                        recte
                        frugaliter
                        honeste X
                        malè
                        praue
                        nequiter
                        turpiter
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Well.
                Rightly.
                Frugally.
                Honestly. 
                Badly.
                Wrongly.
                Vilely.
                Basely.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ineleganter" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>inelega[n]ter.</marginalia_text>
                </position>
            </language>
            <translation>Inelegantly.</translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="attinet luxuriosis vllam exceptionem dari, aut" type="straight"/>
        <underline language="LA" method="pen" text="asotos, primùm" type="straight"/>
        <underline language="LA" method="pen" text="edant de patella" type="straight"/>
        <underline language="LA" method="pen" text="narthecio" type="straight"/>
        <underline language="LA" method="pen" text="qui in mensam vomant" type="straight"/>
        <underline language="LA" method="pen" text="se rursus ingurgitent, qui solem, vt aiunt, neque occidentem vnquam viderint" type="straight"/>
        <underline language="LA" method="pen" text="nec orientem, qui consumptis patrimoniis egeant" type="straight"/>
        <underline language="LA" method="pen" text="Nemo nostru[m] istius generis asotos iucunde putat viuere, mundos, elegantes, optimis cocis, pistoribus, piscatu, aucupio, venatione,              his omnibus exquisitis vita[n]tis cruditate[m], quibus vinu[m] diffusum è pleno sit hirsiphon, vt ait Lucilius, cui nihil dum sit, ius &amp; sarculos abstulerit, adhibe[n]tis ludos, &amp; quae sequuntur illa,              quibus detractis clamat Epicurus se nescire quid sit bonu[m]. adsint etia[m] formosi pueri" type="straight"/>
        <underline language="LA" method="pen" text="qui ministrent, resplendeat iis vestis, argentu[m], odrilitiu[m] locus ipse, aedificiu[m]" type="straight"/>
        <underline language="LA" method="pen" text="vt cui cor sapiat, ei non sapiat palatus, sed" type="straight"/>
        <underline language="LA" method="pen" text="gumas" type="straight"/>
        <underline language="LA" method="pen" text="Coenasti in vita nunquam bene, quum omnia in isto Consumis, squilla, atque acipensere cum decumano" type="straight"/>
        <underline language="LA" method="pen" text="ita grauiter &amp; seuere" type="straight"/>
        <underline language="LA" method="pen" text="horto condito" type="straight"/>
        <underline language="LA" method="pen" text="Veniebat enim ad coenam, vt satiaret animo quieto desyderia" type="straight"/>
        <underline language="LA" method="pen" text="quod bene, id recte, frugaliter, honeste: ille porrò male, praue, nequiter, turpiter coenabat" type="straight"/>
        <underline language="LA" method="pen" text="igitur nec Lapathi suauitate[m] acipenseri Gallonii Laelius anteponebat" type="straight"/>
        <underline language="LA" method="pen" text="ineleganter" type="straight"/>
        <underline language="LA" method="pen" text="hoc est non diuidere, sed" type="straight"/>
        <underline language="LA" method="pen" text="Vitiosum est enim in diuide[n]do" type="straight"/>
        
