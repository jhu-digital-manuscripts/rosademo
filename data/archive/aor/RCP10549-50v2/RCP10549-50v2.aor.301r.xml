        <marginalia anchor_text="praecipua quaeda[m]" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>pr[a]ecipua qu[a]eda[m]</marginalia_text>
                </position>
            </language>
            <translation>Certain distinctions.</translation>
        </marginalia>
        
        <marginalia anchor_text="feraru[m] natura no[n] est illa quidem deprauata mala disciplina" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Natura. Disciplina</marginalia_text>
                </position>
            </language>
            <translation>Nature. Discipline.</translation>
        </marginalia>
        
        <marginalia anchor_text="infantem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Infans.</marginalia_text>
                </position>
            </language>
            <translation>An infant.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Animaliu[m] sagacitas.</marginalia_text>
                </position>
            </language>
            <translation>The shrewdness of living things.</translation>
        </marginalia>
        
        <marginalia anchor_text="primis naturalibus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Prima in animali</marginalia_text>
                </position>
            </language>
            <translation>Primary in a living thing.</translation>
        </marginalia>
        
        <marginalia anchor_text="ab ipso capite fluere necesse est omne[m] ratione[m] bonoru[m] &amp; maloru[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Omnis r[ati]o bonoru[m] et maloru[m] ab ip[s]o capite fluit.</marginalia_text>
                </position>
            </language>
            <translation>The whole reasoning of goods and evils flows from the head itself.</translation>
        </marginalia>
        
        <marginalia anchor_text="nata est sententia veterum Academicorum &amp; Peripateticorum, vt finem bonorum dicerent secundum naturam viuere, id est virtute adhibita frui primis à natura datis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Veteru[m] academicoru[m] &amp; peripateticorum Beata vita</marginalia_text>
                </position>
            </language>
            <translation>The happy life of the old Academics and Peripatetics.</translation>
        </marginalia>
        
        <marginalia anchor_text="honeste viuere" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Honeste viuere</marginalia_text>
                </position>
            </language>
            <translation>To live honourably.</translation>
        </marginalia>
        
        <marginalia anchor_text="tres sunt fines expertes honestatis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Tres fines expertes Honestat[is]</marginalia_text>
                </position>
            </language>
            <translation>Three ends without honour.</translation>
        </marginalia>
        
        <marginalia anchor_text="Pyrrho, Aristo, Herillus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Pyrrho. Aristo. Herillus</marginalia_text>
                    <person name="Pyrrho"/>
                    <person name="Aristo of Chios"/>
                    <person name="Herillus"/>
                </position>
            </language>
            <translation>Pyrrho. Aristo of Chios. Herillus.</translation>
        </marginalia>
        
        <marginalia anchor_text="sensibus ipsis iudicari" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Sensus iudices</marginalia_text>
                </position>
            </language>
            <translation>The senses, judges.</translation>
        </marginalia>
        
        <marginalia anchor_text="Priuataru[m] litium iudices sumus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Priuataru[m] litiu[m] Iudices sumus</marginalia_text>
                </position>
            </language>
            <translation>We are judges of private cases.</translation>
        </marginalia>
        
        <marginalia anchor_text="iudicatum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Iudicatum.</marginalia_text>
                </position>
            </language>
            <translation>Judgement.</translation>
        </marginalia>
        
        <marginalia anchor_text="sapientia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Sapientia</marginalia_text>
                </position>
            </language>
            <translation>Wisdom.</translation>
        </marginalia>
        
        <marginalia anchor_text="virtutibus, quas ratio rerum omnium dominas" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>virtutes reru[m] o[mn]i[u]m do[m]i[n]a[e].</marginalia_text>
                </position>
            </language>
            <translation>Virtues, the mistresses of all things.</translation>
        </marginalia>
        
        <marginalia anchor_text="etiam atque etia[m] consyderet" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Etia[m] atq[ue] etia[m] co[n]syderet.</marginalia_text>
                </position>
            </language>
            <translation>It considers now and yet again.</translation> 
        </marginalia>
        
        <marginalia anchor_text="Quantu[m] enim potero minuam conte[n]tiones" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Qua[n]tum potero minua[m] co[n]te[n]tiones</marginalia_text>
                </position>
            </language>
            <translation>'I will narrow the issue as far as possible' [Loeb 40, 127].</translation>
        </marginalia>
        
        <marginalia anchor_text="Aristippi Cyrenaicorúmque" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Aristippus
                        Cyrenaici.
                    </marginalia_text>
                    <person name="Aristippus"/>
                </position>
            </language>
            <translation>
                Aristippus.
                The Cyrenaics.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="intellige[n]dum &amp; age[n]dum esse natum, quasi mortalem deum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Homo, mortalis Deus ad intelligendu[m] et age[n]dum natus.</marginalia_text>
                </position>
            </language>
            <translation>Man, a mortal god born for understanding and action.</translation>
        </marginalia>
        
        <marginalia anchor_text="liquidius" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>liquidius</marginalia_text>
                </position>
            </language>
            <translation>Clearer.</translation>
        </marginalia>
        
        <marginalia anchor_text="in summi boni sede" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        loco
                        sede ] summi boni.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                In the place and in the seat of the greatest good.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="est veritum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>veritu[m] est.</marginalia_text>
                </position>
            </language>
            <translation>It is true.</translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="de industria, aliud" type="straight"/>
        <underline language="LA" method="pen" text="sed ta[n]tum" type="straight"/>
        <underline language="LA" method="pen" text="Omne enim animal simul vt ortu[m] est, &amp; seipsum, &amp; omnes partes suas diligit: duásque, quae maximae sunt,              in primis amplectitur, animum scilicet &amp; corpus, deinde vtriusque partes" type="straight"/>
        <underline language="LA" method="pen" text="praecipua quae" type="straight"/>
        <underline language="LA" method="pen" text="integritatem corporis, non valetudine[m], summae" type="straight"/>
        <underline language="LA" method="pen" text="fluere necesse est omne[m] ratione[m] bonoru[m] &amp; maloru[m]" type="straight"/>
        <underline language="LA" method="pen" text="dicerent secundum naturam viuere, id est virtute adhibita frui primis à natura datis" type="straight"/>
        <underline language="LA" method="pen" text="cum intelligentia earum" type="straight"/>
        <underline language="LA" method="pen" text="reiicientéque" type="straight"/>
        <underline language="LA" method="pen" text="vnus Aristippi" type="straight"/>
        <underline language="LA" method="pen" text="Epicuri" type="straight"/>
        <underline language="LA" method="pen" text="alter Hieronymi, Carneadis" type="straight"/>
        <underline language="LA" method="pen" text="diu abiecti" type="straight"/>
        <underline language="LA" method="pen" text="vt extrema cum initiis conuenirent" type="straight"/>
        <underline language="LA" method="pen" text="quòd ait sensibus ipsis iudicari voluptatem bonum esse, dolorem" type="straight"/>
        <underline language="LA" method="pen" text="quàm nobis leges permittunt" type="straight"/>
        <underline language="LA" method="pen" text="Priuataru[m] litium" type="straight"/>
        <underline language="LA" method="pen" text="Siquid mei iudicii" type="straight"/>
        <underline language="LA" method="pen" text="asperum, prope longe, stare mouere, quadratu[m] rotu[n]dum" type="straight"/>
        <underline language="LA" method="pen" text="adhibita primùm diuinarum humanarúmque rerum scientia, quae potest appellari" type="straight"/>
        <underline language="LA" method="pen" text="Deinde adiunctis virtutibus, quas ratio rerum omnium dominas, tu voluptatum satellites &amp; ministras esse voluisti" type="straight"/>
        <underline language="LA" method="pen" text="in summi boni sede, qua[m]" type="straight"/>
        <underline language="LA" method="pen" text="qua[m] quum honestate ornatissima[m], tu[m] etia[m] ipsis initiis naturae, &amp; totius perfectione vitae locupletata[m] videbit" type="straight"/>
        <underline language="LA" method="pen" text="liquidius" type="straight"/>
        <underline language="LA" method="pen" text="Quantu[m] enim potero minuam conte[n]tiones, omnésque" type="straight"/>
        <underline language="LA" method="pen" text="nulla inest virtutis adiunctio, omnino à philosophia semouendas putabo" type="straight"/>
        <underline language="LA" method="pen" text="quos non est veritum" type="straight"/>
        <underline language="LA" method="pen" text="maxima dulcedine sensum moueret" type="straight"/>
        <underline language="LA" method="pen" text="Hi no[n] viderunt vt ad cursum equum, ad ara[n]dum bouem, ad indaga[n]dum canem: sic homine[m] ad duas res,              vt ait Aristoteles, intellige[n]dum &amp; age[n]dum esse natum, quasi mortalem deum: contráque vt tardam aliquam &amp; languidam pecudem" type="straight"/>
                
