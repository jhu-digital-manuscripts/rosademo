        <marginalia anchor_text="bellum gerebat" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        belle[m] gerere
                        co[n]tendere
                        disserere.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To wage war. To contend. To argue.
            </translation>
        </marginalia>
        
