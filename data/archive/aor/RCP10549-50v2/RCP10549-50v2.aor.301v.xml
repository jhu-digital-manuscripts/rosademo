        <underline language="LA" method="pen" text="expleturum cumulate" type="straight"/>
        <underline language="LA" method="pen" text="tam parce támque restricte faciant" type="straight"/>
        <underline language="LA" method="pen" text="emendum eis sit quod addant ad virtutem" type="straight"/>
        <underline language="LA" method="pen" text="omnino visa sunt pro nihilo" type="straight"/>
        <underline language="LA" method="pen" text="desitum est disputari" type="straight"/>
        <underline language="LA" method="pen" text="selectione" type="straight"/>
        <underline language="LA" method="pen" text="post enim Chrysippum non sanè est disputatum" type="straight"/>
        <underline language="LA" method="pen" text="Nam cum Academicis incerta luctatio est, qui nihil affirmant" type="straight"/>
        <underline language="LA" method="pen" text="quasi desperata cognitione certi, id sequi volunt quodcunque verisimile videatur" type="straight"/>
        <underline language="LA" method="pen" text="genere voluptatis coniunctus" type="straight"/>
        <underline language="LA" method="pen" text="postea defensores eius sententiae" type="straight"/>
        <underline language="LA" method="pen" text="is qui authoritatem minimam habet, maximam vim cum illis" type="straight"/>
        <underline language="LA" method="pen" text="nisi arguimus, omnis virtus, omne decus, omnis vera laus deserenda" type="straight"/>
        <underline language="LA" method="pen" text="homo &amp; acutus &amp; diligens" type="straight"/>
        <underline language="LA" method="pen" text="iacere vestra" type="straight"/>
        <underline language="LA" method="pen" text="forte defecerit, tuum est vt suggeras" type="straight"/>
        <underline language="LA" method="pen" text="studiis atque factis, qui permulta ob eam vna[m] causam faciunt quia decet" type="straight"/>
        <underline language="LA" method="pen" text="honestum est, etsi nullum consecuturu[m] emolumentum vident" type="straight"/>
        <underline language="LA" method="pen" text="mentémque &amp; acrem &amp; vigentem, celerriméque multa simul agitantem" type="straight"/>
        <underline language="LA" method="pen" text="sagacem, quae &amp; causas reru[m] &amp; consecutiones videat, &amp; similitudines transferat" type="straight"/>
        <underline language="LA" method="pen" text="disiuncta co[n]iungat, &amp; cum praesentibus futura copulet, omnémque co[m]plectatur" type="straight"/>
        <underline language="LA" method="pen" text="statu[m]. Eadémque ratio facit homine[m] hominu[m] appetente[m], cúmque his natura &amp; sermone" type="straight"/>
        <underline language="LA" method="pen" text="vsu congruente[m], vt profectus à charitate domesticoru[m] ac suoru[m] currat longius, &amp;" type="straight"/>
        <underline language="LA" method="pen" text="implicet primum ciuium, deinde omniu[m] mortalium societate: atque vt" type="straight"/>
        
