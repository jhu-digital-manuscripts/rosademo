    <annotation>
        
        <marginalia anchor_text="alligatus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>alligatus</marginalia_text>
                </position>
            </language>
            <translation>Bound.</translation>
        </marginalia>
        
        <marginalia anchor_text="Quod non sub dubia vtilitatis ratione effici solet" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Amicitia no[n] vtilitatis regula est metienda.</marginalia_text>
                </position>
            </language>
            <translation>Friendship is not to be measured by a rule of expediency.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Amicitia perfecta.</marginalia_text>
                </position>
            </language>
            <translation>Perfect friendship.</translation>
        </marginalia>
        
        <marginalia anchor_text="metu" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Metus.</marginalia_text>
                </position>
            </language>
            <translation>Fear.</translation>
        </marginalia>
        
        <marginalia anchor_text="Epicurus, vt facit, in caelum efferat laudibus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Epicurus amicitia[m] in coelu[m] extulit laudibus.</marginalia_text>
                    <person name="Epicurus"/>
                </position>
            </language>
            <translation>Epicurus extolled friendship to the sky with praises.</translation>
        </marginalia>
        
        <marginalia anchor_text="Sit ista in Graecorum leuitate peruersitas, qui maledictis insecta[n]tur eos à quibus de veritate dissentiunt" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Contra gra[e]cos</marginalia_text>
                </position>
            </language>
            <translation>Against the Greeks.</translation>
        </marginalia>
        
        <marginalia anchor_text="no[n] grauissimum est testimonium multitudinis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>No[n] grauissimu[m] est testimoniu[m] Multitudinis</marginalia_text>
                </position>
            </language>
            <translation>The testimony of the crowd is not very weighty.</translation>
        </marginalia>
        
        <marginalia anchor_text="optimum quidque rarissimum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Optimu[m] quidq[ue] rarissimum est.</marginalia_text>
                </position>
            </language>
            <translation>'Supreme excellence is extremely rare' [Loeb 40, 173.</translation>  
        </marginalia>
        
        <marginalia anchor_text="vt eorum vita refellatur oratio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Quorunda[m] Ho[m]i[nu]m vita, refellitur Oratio.</marginalia_text>
                </position>
            </language>
            <translation>The lives of certain men disprove their speech.</translation>
        </marginalia>
        
        <marginalia anchor_text="melius facere quàm dicere" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Dicere. facere.</marginalia_text>
                </position>
            </language>
            <translation>To say. To do.</translation>
        </marginalia>
        
        <marginalia anchor_text="Foedus quoddam facere inter se sapie[n]tes" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Foedus sapientum</marginalia_text>
                </position>
            </language>
            <translation>The agreement of the wise.</translation>
        </marginalia>
        
        <marginalia anchor_text="charitas" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Charitas.</marginalia_text>
                </position>
            </language>
            <translation>Charity.</translation>
        </marginalia>
        
        <marginalia anchor_text="satis est tibi in te" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Satis est tibi in te.</marginalia_text>
                </position>
            </language>
            <translation>There is enough for you in yourself.</translation>
        </marginalia>
        
        <marginalia anchor_text="Pyladea amicitia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Pyladea Amicitia</marginalia_text>
                </position>
            </language>
            <translation>Romantic friendship.</translation>
        </marginalia>
                
        <marginalia anchor_text="fructibus &amp; emolume[n]tis &amp; vtilitatibus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        fructus
                        emolume[n]tum
                        vtilitas.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Fruit. Advantage. Use.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ab ipso illo" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>ab ip[s]o illo.</marginalia_text>
                </position>
            </language>
            <translation>From that man himself.</translation>
        </marginalia>
        
        <marginalia anchor_text="ioca &amp; seria, vt dicitur, quicum arcana" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        ioca
                        seria
                        arcana
                        occulta.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Jokes. Serious thoughts. Secret. Hidden.
            </translation>
        </marginalia>
        
