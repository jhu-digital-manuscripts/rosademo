        <marginalia anchor_text="Neque vt ea cognoscas, quae tibi notissima sunt, ad te mitto, sed quia facillime in nomine tuo acquiesco,              &amp; quia te aequissimum habeo eoru[m] studioru[m] quae mihi communia tecum sunt, aestimatorem &amp; iudicem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Cur Bruto scribit ha[e]c.</marginalia_text>
                    <person name="Marcus Junius Brutus the Younger"/>
                </position>
            </language>
            <translation>Why he writes these things to Brutus.</translation>
        </marginalia>
        
        <marginalia anchor_text="bibliotheca" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
