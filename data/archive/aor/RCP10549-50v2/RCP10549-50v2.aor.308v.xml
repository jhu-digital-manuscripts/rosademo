        </marginalia>
        
        <marginalia anchor_text="fortuna fortis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        fortuna fortis.
                    </marginalia_text>
                </position>
            </language>
            <translation>Fortune [favours] the brave.</translation> 
        </marginalia>
        
        <marginalia anchor_text="animal" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
