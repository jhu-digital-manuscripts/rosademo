        <marginalia anchor_text="lumen extinxeris; euerteris; deprauare" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        lume[n] extinguere
                        euertere
                        deprauare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To extinguish a light.
                To overturn.
                To distort.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="verborum gloriam" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        verboru[m] gloria.
                    </marginalia_text>
                </position>
            </language>
            <translation>The glory of words.</translation>
        </marginalia>
        
        <marginalia anchor_text="sine vlla doctrina naturam" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Natura sine doctrina.
                    </marginalia_text>
                </position>
            </language>
            <translation>Nature without learning.</translation>
        </marginalia>
        
        <marginalia anchor_text="id solum bonum esse quod honestum sit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Ïd solum bonu[m] q[uod] honestu[m]. 
                    </marginalia_text>
                </position>
            </language>
            <translation>That which is honourable is the sole good.</translation>
        </marginalia>
        
        <marginalia anchor_text="Pyrrhonem aut Aristonem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Pyrrho. Aristo.
                    </marginalia_text>
                    <person name="Pyrrho"/>
                    <person name="Aristo of Chios"/>
                </position>
            </language>
            <translation>Pyrrho. Aristo of Chios.</translation>
        </marginalia>
        
        <marginalia anchor_text="Omnes semper beatos esse sapientes" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Omnes semper beati su[n]t sapientes.
                    </marginalia_text>
                </position>
            </language>
            <translation>All the wise are always happy.</translation>
        </marginalia>
        
        <marginalia anchor_text="virtutis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        virtutis munus.
                    </marginalia_text>
                </position>
            </language>
            <translation>The duty of virtue.</translation>
        </marginalia>
        
        <marginalia anchor_text="erit enim notius quale sit, pluribus notatum vocabulis idem declarantibus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Notius quid erit, pluribus verbis idem declara[n]tibus notatum.
                    </marginalia_text>
                </position>
            </language>
            <translation>That will be better known which is known by many synonyms [Loeb 40, 231].</translation>
        </marginalia>
        
        <marginalia anchor_text="Stoicorúmque sente[n]tiam" hand="Italian">
