        <underline language="LA" method="pen" text="posito, &amp; omnium assensu approbato" type="straight"/>
        <underline language="LA" method="pen" text="altus &amp; excelle[n]s" type="straight"/>
        <underline language="LA" method="pen" text="eoru[m] dico" type="straight"/>
        <underline language="LA" method="pen" text="qui mancam fore putauerunt sine aliqua accessione virtutem" type="straight"/>
        <underline language="LA" method="pen" text="obsistere" type="straight"/>
        <underline language="LA" method="pen" text="assensúsque" type="straight"/>
        <underline language="LA" method="pen" text="Circunscriptis igitur his sententiis" type="straight"/>
        <underline language="LA" method="pen" text="bonum sit viuere: scientiam adhibentem earum rerum quae natura eueniant, seligentem quae secundum naturam,              &amp; siquae contra naturam sunt, reiicientem, id est conuenienter congruentérque naturae viuere" type="straight"/>
        <underline language="LA" method="pen" text="id apprime rectissime dicitur" type="straight"/>
        <underline language="LA" method="pen" text="expletum esse omnibus suis" type="straight"/>
        <underline language="LA" method="pen" text="est patriam prodere, parentes violare, fana depeculari" type="straight"/>
        <underline language="LA" method="pen" text="paulum oppidò inter" type="straight"/>
        <underline language="LA" method="pen" text="id quod esset natura absolutu[m]" type="straight"/>
        <underline language="LA" method="pen" text="Quúmque rerum notiones in animis fiant, si aut vsu aliquid cognitum sit, aut coniunctione, aut similitudine, aut collatione rationis, hoc quarto" type="straight"/>
        <underline language="LA" method="pen" text="Quum enim ab his rebus, quae sunt secundum naturam, ascendit animus collatione rationis, tum ad notionem boni peruenit" type="straight"/>
        <underline language="LA" method="pen" text="non comparatione cum aliis dulce" type="straight"/>
        <underline language="LA" method="pen" text="sed ea aestimatio genere valet, non magnitudine" type="straight"/>
        <underline language="LA" method="pen" text="vitam insipientium miseram acerbámque reddunt" type="straight"/>
        
