    <annotation>
        
        <marginalia anchor_text="frequentia; multitudo" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        bonor[um] [ multitudo
                                    freque[n]tia
                                    copia
                                    abunda[n]tia
                                    affluentia.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A multitude; a crowd; a supply; an abundance; a plethora of goods.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="vt si quum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>vt si quum.</marginalia_text>
                </position>
            </language>
            <translation>As when.</translation>   
            <!-- DO: Not sure how to translate --> 
            <!--JG: According to Loeb it's 'vt si cum': https://www.loebclassics.com/view/marcus_tullius_cicero-de_finibus_bonorum_et_malorum/1914/pb_LCL040.263.xml -->
        </marginalia>
        
        <marginalia anchor_text="rerum omnino discrepantiam, quàm verboru[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Reru[m] discrepa[n]tia et verboru[m]</marginalia_text> 
                </position>
            </language>
            <translation>A discrepancy of facts and words.</translation>
        </marginalia>
        
        <marginalia anchor_text="omnia quae ipsi bona appellant, pertinere dicant ad beate viuendum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Omnia bona co[n]curru[n]t ad beatitatis absolutionem.</marginalia_text>
                </position>
            </language>
            <translation>All good things contribute to absolute happiness.</translation>
        </marginalia>
        
        <marginalia anchor_text="eculeo" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Eculeus.</marginalia_text>
                </position>
            </language>
            <translation>The rack.</translation>
        </marginalia>
        
        <marginalia anchor_text="opinio facit non natura" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Opinio no[n] natura.</marginalia_text>
                </position>
            </language>
            <translation>Not a natural opinion.</translation>
        </marginalia>
        
        <marginalia anchor_text="tria genera bonoru[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Tria genera bonoru[m].</marginalia_text>
                </position>
            </language>
            <translation>Three kinds of goods.</translation>
        </marginalia>
        
        <marginalia anchor_text="corporis co[m]modorum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Corporis commoda.</marginalia_text>
                </position>
            </language>
            <translation>Bodily advantages.</translation>
        </marginalia>
        
        <marginalia anchor_text="valetudinem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>valetudo.</marginalia_text>
                </position>
            </language>
            <translation>Health.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>virtus dignitas, splendor.</marginalia_text>
                </position>
            </language>
            <translation>Virtue, esteem, splendour.</translation>
        </marginalia>
        
        <marginalia anchor_text="" hand="Italian">
            <language ident="EL">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>εὐκαρία</marginalia_text>        <!-- for εὐκαιρία -->
                </position>
            </language>
            <translation>Opportunity.</translation>
        </marginalia>
        
        <marginalia anchor_text="" hand="Italian">
            <language ident="EL">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>κατορθωσίς</marginalia_text>        <!-- for κατόρθωσις -->
                </position>
            </language>
            <translation>Right action.</translation>
        </marginalia>
        
        <marginalia anchor_text="" hand="Italian">
            <language ident="EL">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>κατόρθωμα </marginalia_text>        
                </position>
            </language>
            <translation>Virtuous action.</translation>
        </marginalia>
        
        <marginalia anchor_text="cothurni laus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Cothurni Laus.</marginalia_text>
                </position>
            </language>
            <translation>Praise of a boot.</translation>
        </marginalia>
        
        <marginalia anchor_text="bonam morte[m] &amp; bonum partum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Mors. Partus.</marginalia_text>
                </position>
            </language>
            <translation>Death. Birth.</translation>
        </marginalia>
        
        <marginalia anchor_text="quod appellamus extremu[m], quod vltimum, pute[n]t crescere posse" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Summu[m] Bonu[m] crescet.</marginalia_text>
                </position>
            </language>
            <translation>The greatest good will extend.</translation>
        </marginalia>
        
        <marginalia anchor_text="crescere bonorum finem non putamus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Bonoru[m] finis no[n] crescit.</marginalia_text>
                </position>
            </language>
            <translation>The extreme of goods will not extend.</translation>
        </marginalia>
        
        <marginalia anchor_text="Vt enim qui demersi sunt in aqua, nihilo magis respirare possunt, si non longe absunt à summo vt iam iámque possint emergere,              quàm si etiam tum essent in profundo: nec catulus ille qui iam appropinquat vt videat, plus cernit quàm is qui modo est natus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Similitudo</marginalia_text>
                </position>
            </language>
            <translation>Comparison.</translation>
        </marginalia>
        
        <marginalia anchor_text="offunditur; obscuretur &amp; obruatur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        offundi
                        obscurari
                        obrui
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be covered.
                To be hidden.
                To be covered up.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="productione temporis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        productio te[m]poris
                    </marginalia_text>
                </position>
            </language>
            <translation>The prolonging of time.</translation>
        </marginalia>
        
