    <page filename="RCP10549-50v2.312v.tif" pagination="76" reader="John Dee"/>  
    <annotation>
        
        <marginalia anchor_text="vt liberi à parentibus ame[n]tur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Pare[n]tum amor in lib.</marginalia_text>
                </position>
            </language>
            <translation>The love of parents for their children.</translation>
        </marginalia>
        
        <marginalia anchor_text="procreandi à natura habitam esse ratione[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Procrea[n]di, à natura est habita ratio</marginalia_text>
                </position>
            </language>
            <translation>The reasoning of procreation was maintained by nature.</translation>
        </marginalia>
        
        <marginalia anchor_text="in bestiis vis naturae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>vis natura[e] in bestijs.</marginalia_text>
                </position>
            </language>
            <translation>The force of nature in beasts.</translation>
        </marginalia>
        
        <marginalia anchor_text="Vt enim in membris alia sunt tanquam sibi nata, vt oculi, vt aures: aliqua etiam caeterorum membroru[m] vsum adiuuant,              vt crura, vt manus: sic immanes quaedam bestiae sibi solum natae sunt" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Membroru[m] partitio</marginalia_text>
                </position>
            </language>
            <translation>The separation of the parts of the body.</translation>
        </marginalia>
        
        <marginalia anchor_text="Pinna; Pinnoteres" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Pinna.
                        Pinnoteres.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A sea pen. Pea crabs.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="formicae, apes" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Formica[e].
                        Apes.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Ants. Bees.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Mundum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>.Mundus.</marginalia_text>
                </position>
            </language>
            <translation>The universe.</translation>
        </marginalia>
        
        <marginalia anchor_text="vt co[m]munem vtilitatem nostrae anteponamus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Co[mmun]is vtilitas n[ost]ra[e] a[n]tepone[n]da</marginalia_text>
                </position>
            </language>
            <translation>The common use is preferred to our own use.</translation>
        </marginalia>
        
        <marginalia anchor_text="proditor patriae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Proditor patria[e].</marginalia_text>
                </position>
            </language>
            <translation>A traitor to the homeland.</translation>
        </marginalia>
        
        <marginalia anchor_text="doceat chariorem nobis esse patriam, quàm nosmetipsos" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Charior nobis Patria, q[uam] nos[met]ip[s]i</marginalia_text>
                </position>
            </language>
            <translation>The homeland is dearer to us than ourselves.</translation>
        </marginalia>
        
        <marginalia anchor_text="ipsis mortuis terrarum omnium deflagratio consequatur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Me mortuo terra misceatur incendio.</marginalia_text>
                </position>
            </language>
            <translation>With my death the land is consumed by an inferno.</translation>
        </marginalia>
        
        <marginalia anchor_text="in summa solitudine vitam" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Vita in solitudine.</marginalia_text>
                </position>
            </language>
            <translation>A life in solitude.</translation>
        </marginalia>
        
        <marginalia anchor_text="rationibúsque prudentiae tradendis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Quod scis, alteri trade</marginalia_text>
                </position>
            </language>
            <translation>Impart to another what you know.</translation>
        </marginalia>
        
        <marginalia anchor_text="vt tauris natura datum est, vt pro vitulis contra leones summa vi impetúque conte[n]dant" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Tauroru[m] pugna cu[m] leonibus</marginalia_text>
                </position>
            </language>
            <translation>A fight between bulls and lions.</translation>
        </marginalia>
        
        <marginalia anchor_text="de Hercule, &amp; de Libero" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Hercules. Liber.</marginalia_text>
                    <person name="Hercules"/>
                    <person name="Liber"/>
                </position>
            </language>
            <translation>Hercules. Liber.</translation>
        </marginalia>
        
        <marginalia anchor_text="Iouem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Iupiter.</marginalia_text>
                    <person name="Jupiter"/>
                </position>
            </language>
            <translation>Jupiter.</translation>
        </marginalia>
        
        <marginalia anchor_text="iuris esse vincula" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Iuris vincula.</marginalia_text>
                </position>
            </language>
            <translation>The bonds of the law.</translation>
        </marginalia>
        
        <marginalia anchor_text="Chrysippus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Chrysippus.</marginalia_text>
                    <person name="Chrysippus"/>
                </position>
            </language>
            <translation>Chrysippus.</translation>
        </marginalia>
        
        <marginalia anchor_text="theatrum, quum co[m]mune" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Theatru[m] comune.</marginalia_text>
                </position>
            </language>
            <translation>A communal theatre.</translation>
        </marginalia>
       
        <marginalia anchor_text="coniuncti &amp; consociati" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        co[n]iuncti
                        co[n]sociati.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Joined together. Connected.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="ame[n]tur, à quo initio" type="straight"/>
        <underline language="LA" method="pen" text="procreandi à natura habitam esse ratione[m]" type="straight"/>
        <underline language="LA" method="pen" text="natura &amp; procreari vellet, &amp; diligi procreatos non curaret" type="straight"/>
        <underline language="LA" method="pen" text="inspici potest, quarum in foetu &amp; in educatione laborem" type="straight"/>
        <underline language="LA" method="pen" text="apparet, à natura ipsa, vt eos quos genuerimus, amemus, impelli" type="straight"/>
        <underline language="LA" method="pen" text="co[m]munis hominu[m] inter homines naturalis" type="straight"/>
        <underline language="LA" method="pen" text="id ipsum quòd" type="straight"/>
        <underline language="LA" method="pen" text="sic immanes quaedam bestiae sibi solum natae sunt" type="straight"/>
        <underline language="LA" method="pen" text="Itaque natura sumus apti ad coetus, concilia, ciuitates. Mundum" type="straight"/>
        <underline language="LA" method="pen" text="numine deorum, eúmque esse quasi co[m]munem vrbem &amp; ciuitatem hominum" type="straight"/>
        <underline language="LA" method="pen" text="vnumque[m]que nostrum eius mundi esse parte[m], ex quo illud natura consequi" type="straight"/>
        <underline language="LA" method="pen" text="patriae, quàm co[m]munis vtilitatis, aut salutis desertor propter" type="straight"/>
        <underline language="LA" method="pen" text="nobis esse patriam, quàm" type="straight"/>
        <underline language="LA" method="pen" text="Quoniámque illa vox inhumana &amp; scelerata ducitur eorum,              qui nega[n]t se recusare quo minus ipsis mortuis terrarum omnium deflagratio consequatur: quod vulgari quodam versu Graeco pronu[n]tiari solet" type="straight"/>
        <underline language="LA" method="pen" text="ad coniunctione[m] congregationémque hominu[m]" type="straight"/>
        <underline language="LA" method="pen" text="Impellimur aute[m] natura, vt prodesse velimus quàm plurimis, in primísque docendo,              rationibúsque prudentiae tradendis. Itaque non facile est inuenire" type="straight"/>
        <underline language="LA" method="pen" text="ipse non tradat alteri. Ita non solum ad discendum propensi sumus, veru[m]etiam" type="straight"/>
        <underline language="LA" method="pen" text="optimum &amp; maximu[m] dicimus, quúmque eundem salutarem, hospitalem, statorem,              hoc intelligi volumus, salutem hominu[m] in eius esse tutela. Minime" type="straight"/>
        <underline language="LA" method="pen" text="nos abiecti neglectíque simus, postulare vt diis immortalibus chari simus, &amp; ab" type="straight"/>
        <underline language="LA" method="pen" text="diligamur" type="straight"/>
        <underline language="LA" method="pen" text="coniuncti &amp;" type="straight"/>
        <underline language="LA" method="pen" text="Chrysippus, caetera nata esse causa hominu[m] &amp; deorum" type="straight"/>
        <underline language="LA" method="pen" text="co[m]munitatis &amp; societatis suae, vt bestiis homines vti ad vtilitate[m] suam possint sine" type="straight"/>
        <underline language="LA" method="pen" text="quoniámque ea natura esset hominis, vt cum genere humano quasi ciuile" type="straight"/>
        <underline language="LA" method="pen" text="qui id conseruaret, eum iustum: qui migraret" type="straight"/>
        <underline language="LA" method="pen" text="quo minus suum quidque cuiusque" type="straight"/>
        <underline language="LA" method="pen" text="Quum autem ad tuendos, conserua[n]dósque homines homine[m] natum esse videamus" type="straight"/>
        
