        <underline language="LA" method="pen" text="Inest in eadem explicatione naturae, insatiabilis quaedam è cognoscendis rebus voluptas, in qua vna confectis rebus necessariis,              vacui negotiis honeste ac liberaliter possumus" type="triple"/>
        <underline language="LA" method="pen" text="ignem, nonnulla deinde" type="triple"/>
        <underline language="LA" method="pen" text="eos copiam exilem, apud illos vberrimam" type="triple"/>
        <underline language="LA" method="pen" text="multa ab his conquisita &amp; collecta sunt, de omnium animantium genere, ortu, membris, aetatibus" type="triple"/>
        <underline language="LA" method="pen" text="omni copia, plurima &amp; certissima argumenta sumuntur ad cuiusque" type="triple"/>
        <underline language="LA" method="pen" text="Quod item quum" type="triple"/>
        <underline language="LA" method="pen" text="magnam tribuunt inue[n]toribus gratiam" type="triple"/>
        <underline language="LA" method="pen" text="viuere, summu[m] bonum esse dixisset, his verbis tria significari Stoici dicunt" type="triple"/>
        <underline language="LA" method="pen" text="eiusmodi, viuere adhibente[m] scientiam earum rerum, quae natura euenirent. Hunc" type="triple"/>
        <underline language="LA" method="pen" text="completur enim" type="triple"/>
        <underline language="LA" method="pen" text="eáque vita quae ex summo bono degitur" type="triple"/>
        <underline language="LA" method="pen" text="Omnis natura vult" type="triple"/>
        <underline language="LA" method="pen" text="quae est viue[n]di" type="triple"/>
        <underline language="LA" method="pen" text="hominis in animu[m]" type="triple"/>
        <underline language="LA" method="pen" text="animu[m] quadam infinita laude anteponerent corpori, virtutes quoque animi bonis" type="triple"/>
        <underline language="LA" method="pen" text="totius hominis custodem &amp; procuratricem esse vellent, quae esset naturae comes" type="triple"/>
        <underline language="LA" method="pen" text="adiutrix, hoc sapientiae munus esse dicebant" type="triple"/>
        <underline language="LA" method="pen" text="accuratius" type="triple"/>
        <underline language="LA" method="pen" text="primíque ex omnibus philosophis natura tributu[m] esse docuerunt, vt hi qui procreati essent, à procreatoribus amare[n]tur: &amp;" type="triple"/>
        <underline language="LA" method="pen" text="quod temporum ordine antiquius" type="triple"/>
        
