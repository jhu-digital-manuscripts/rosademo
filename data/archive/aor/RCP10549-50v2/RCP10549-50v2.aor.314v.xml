        <marginalia anchor_text="Omnis natura vult esse conseruatrix sui, vt &amp; salua sit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Omnis natura est co[n]seruatrix sui vt salua</marginalia_text>
                </position>
            </language>
            <translation>Every natural thing is its own conservator so that it is safe.</translation>
        </marginalia>
        
