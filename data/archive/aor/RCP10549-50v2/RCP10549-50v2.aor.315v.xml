        <marginalia anchor_text="Panaetius, quum ad Q. Tuberonem de dolore patiendo" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Pana[e]tius de dolore patiendo ad Q' Tuberone[m].</marginalia_text>
                    <person name="Panaetius"/>
                    <person name="Quintus Aelius Tubero"/>
                </position>
            </language>
            <translation>Panaetius on enduring pain, [in a letter] to Quintus Tubero.</translation>
        </marginalia>
        
        <marginalia anchor_text="vt nos quales esse oportet seruemus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Quales sumus.</marginalia_text>
                </position>
            </language>
            <translation>Such as we are.</translation>
        </marginalia>
        
        <marginalia anchor_text="secu[n]dum natura[m] viuere" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Secundum natura[m] viuere.</marginalia_text>
                </position>
            </language>
            <translation>To live according to nature.</translation>
        </marginalia>
        
        <marginalia anchor_text="Quòd si no[n] hominis summu[m] bonu[m] quaereremus, sed cuiusdam anima[n]tis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Anima[n]tis sumu[m] bonu[m].</marginalia_text>
                </position>
            </language>
            <translation>The greatest good of a living thing.</translation>
        </marginalia>
        
        <marginalia anchor_text="liceat enim fingere aliquid eiusmodi, quo veru[m] facilius reperiamus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Lic[et] fingere docendi causa</marginalia_text>
                </position>
            </language>
            <translation>Pretending is allowed, in order to demonstrate.</translation>
        </marginalia>
        
        <marginalia anchor_text="Chrysippus" hand="Italian">
            <language ident="LA">
