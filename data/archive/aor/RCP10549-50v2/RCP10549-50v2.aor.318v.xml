        <marginalia anchor_text="rerumpublicaru[m] rectiones, genera, status, mutationes, leges etiam &amp; instituta" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        reru[m]pub. rectiones
                                    genera
                                    status
                                    mutationes
                                    leges.
                                    instituta.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Government; kinds; status; changes; laws; principles of republics.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="orationem dictauisses" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        dictare or[ati]o[n]em.
                    </marginalia_text>
                </position>
            </language>
            <translation>To compose a speech.</translation>
        </marginalia>
        
        <marginalia anchor_text="Platoni" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Plato.
                    </marginalia_text>
                    <person name="Plato"/>
                </position>
            </language>
            <translation>Plato.</translation>
        </marginalia>
        
        <marginalia anchor_text="Eloquentiae vero quae &amp; principibus maxime ornamento est" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Eloque[n]tia principibus est maxime ornamento.
                    </marginalia_text>
                </position>
            </language>
            <translation>Eloquence is the greatest adornment for statesmen.</translation>
        </marginalia>
        
        <marginalia anchor_text="barbati" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Barbati.
                    </marginalia_text>
                </position>
            </language>
            <translation>Bearded.</translation>
        </marginalia>
        
        <marginalia anchor_text="Stoici" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Stoicoru[m] opiniones.
                    </marginalia_text>
                </position>
            </language>
            <translation>The opinions of the Stoics.</translation>
        </marginalia>
        
        <marginalia anchor_text="Atque hoc loco similitudines eas, quibus illi vti sole[n]t, dissimillimas proferebas" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
