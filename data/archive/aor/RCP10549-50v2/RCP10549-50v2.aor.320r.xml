        <underline language="LA" method="pen" text="noster cognatione patruelis, amore germanus" type="straight"/>
        <underline language="LA" method="pen" text="ambulationem postméridiana[m] conficeremus" type="straight"/>
        <underline language="LA" method="pen" text="vario sermone sex illa à Dipylo stadia confecimus" type="straight"/>
        <underline language="LA" method="pen" text="nobilitata spatia, solitudo erat ea quam volueramus" type="straight"/>
        <underline language="LA" method="pen" text="audiamus, aut scriptum aliquod legamus" type="straight"/>
        <underline language="LA" method="pen" text="enim mihi Platonis in mentem, quem" type="straight"/>
        <underline language="LA" method="pen" text="conspectu meo" type="straight"/>
        <underline language="LA" method="pen" text="Hîc Speusippus, hîc Xenocrates, hîc eius auditor Polemo" type="straight"/>
        <underline language="LA" method="pen" text="sessio fuit" type="straight"/>
        <underline language="LA" method="pen" text="primis auum cogitare" type="straight"/>
        <underline language="LA" method="pen" text="conuertebat ad sese Coloneus" type="straight"/>
        <underline language="LA" method="pen" text="oculos versabatur, quem scis quàm admirere" type="straight"/>
        <underline language="LA" method="pen" text="Me quide[m] ad altiore[m] memoriam OEdipodis huc venientis, &amp; illo mollissimo carmine, quaenam essent ipsa haec loca requirentis,              species quaedam co[m]mouit, inanis scilicet, sed commouit tame[n]" type="straight"/>
        <underline language="LA" method="pen" text="sed veteris prouerbii admonitu, viuoru[m] memini" type="straight"/>
        <underline language="LA" method="pen" text="Epicuri licet obliuisci, si cupiam, cuius imaginem no[n] modo in tabulis nostri familiares, sed etiam in poculis &amp; in anulis habent" type="straight"/>
        <underline language="LA" method="pen" text="vt acrius aliquanto &amp; attentius de claris viris locoru[m] admonitu cogitemus" type="straight"/>
        <underline language="LA" method="pen" text="vita[m] ediderat" type="straight"/>
        <underline language="LA" method="pen" text="inter se decertate soliti" type="straight"/>
        <underline language="LA" method="pen" text="vt fremitu[m] assuesceret voce vincere" type="straight"/>
        <underline language="LA" method="pen" text="paulum ad dextram de via declinaui" type="straight"/>
        <underline language="LA" method="pen" text="quide[m] infinitum est" type="straight"/>
        <underline language="LA" method="pen" text="Atqui Cicero, inquit, ista studia si ad imitandos summos viros spectant, ingeniosorum sunt: sin tantummodo ad indicia veteris memoriae cognoscenda,              curiosoru[m]. Tu aute[m] hortamur omnes, currente[m] quide[m] vt spero, vt eos quos nouisse vis, etiam imitari velis" type="straight"/>
        <underline language="LA" method="pen" text="primísque vt aliquid suoru[m] studioru[m] philosophiae quoque" type="straight"/>
        <underline language="LA" method="pen" text="vel vt te imitetur quem amat, vel vt illud ipsum quod studet, facere pos[sit]" type="straight"/>
        
