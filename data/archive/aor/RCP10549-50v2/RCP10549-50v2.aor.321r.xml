        <underline language="LA" method="pen" text="an possit aut labefactari, aut eripi rebus" type="straight"/>
        <underline language="LA" method="pen" text="nonnunquam variari" type="straight"/>
        <underline language="LA" method="pen" text="Haec mihi videtur delicatior, vt ita dicam" type="straight"/>
        <underline language="LA" method="pen" text="Nicomachu[m], cuius accurate scripti de moribus libri, dicuntur illi quide[m] esse Aristotelis, sed non video cur non potuerit patri similis esse filius" type="straight"/>
        <underline language="LA" method="pen" text="est magnus" type="straight"/>
        <underline language="LA" method="pen" text="Lysias &amp; oratione locuples, rebus" type="straight"/>
        <underline language="LA" method="pen" text="Co[n]cinnus deinde &amp; elegans huius Aristo" type="straight"/>
        <underline language="LA" method="pen" text="Scripta sanè &amp; multa &amp; polita, sed nescio quo pacto, authoritatem oratio non habet. Praetereo multos,              in his doctum homine[m] &amp; suauem Hieronymum" type="straight"/>
        <underline language="LA" method="pen" text="quidem est grauitate proximus, &amp; redundat" type="straight"/>
        <underline language="LA" method="pen" text="patriis institutis manet" type="straight"/>
        <underline language="LA" method="pen" text="quoque suus est" type="straight"/>
        <underline language="LA" method="pen" text="Nam caeteris in rebus siue praetermissum, siue ignoratum est quippiam,              non plus incommodi est quàm quanti quaeque earum rerum est, in quibus neglectum est aliquid. Summum aute[m] bonum si ignoretur, viuendi rationem ignorari necesse est:              ex quo tantus error consequitur, vt quem in portum se recipiant scire non possint" type="straight"/>
        <underline language="LA" method="pen" text="Negabat igitur vllam esse artem, quae ipsa à se proficisceretur" type="straight"/>
        <underline language="LA" method="pen" text="per se inuitaret &amp; alliceret appetitum" type="straight"/>
        <underline language="LA" method="pen" text="vltimum, fons reperiendus est in" type="straight"/>
        <underline language="LA" method="pen" text="prima inuitame[n]ta naturae" type="straight"/>
        <underline language="LA" method="pen" text="primum declinatu[m] dolore[m]" type="straight"/>
        
