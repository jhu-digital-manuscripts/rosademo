    <annotation>
        
        <marginalia anchor_text="in eo nonnunquam variari inter eos &amp; dubitari videtur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>in eo nunq[uam] int[er] eos et dubitari v[idetu]r</marginalia_text>  
                    <!-- DO: I don't know if it's meant to be 'nonnunquam' as it is in the text (and thus the opposite meaning)? --> <!--JG: Could very well be, but let's stick to nunquam-->
                </position>
            </language>
            <translation>In this matter there never seems to be any variation and doubt.</translation>  
        </marginalia>
        
        <marginalia anchor_text="an possit aut labefactari, aut eripi rebus aduersis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>An vita beata possit eripi aut labefactari rebus adu[er]sis</marginalia_text>
                </position>
            </language>
            <translation>Whether a happy life can be snatched away or destroyed by adverse things.</translation>
        </marginalia>
        
