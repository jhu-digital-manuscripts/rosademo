<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v2.322r.tif" pagination="95" reader="John Dee"/> 
    <annotation>
        
        <marginalia anchor_text="attingat, ad séque pertineat" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        attingere
                        p[er]tinere.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To affect. To touch.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="coeptátque" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>coeptare</marginalia_text> 
                </position>
            </language>
            <translation>To venture.</translation>
        </marginalia>
        
        <marginalia anchor_text="primus ad omnem vitam tuendam appetitus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Primus a[n]i[m]alium appetitus.</marginalia_text>
                </position>
            </language>
            <translation>The first impulse of living things.</translation>
        </marginalia>
        
        <marginalia anchor_text="omnium est natura co[m]munis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Omniu[m] est natura co[mmun]is.</marginalia_text>
                </position>
            </language>
            <translation>Nature is common to all.</translation>
        </marginalia>
        
        <marginalia anchor_text="non ita accipiendu[m] est, quasi dicamus vnu[m] esse omnium extremu[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>No[n] est vnu[m] o[mn]i[u]m a[n]i[m]aliu[m] extremu[m]</marginalia_text>
                </position>
            </language>
            <translation>There is not one end for all living things.</translation>
        </marginalia>
        
        <marginalia anchor_text="in omnibus summa co[m]munis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Summa o[mn]i[u]m Comunis.</marginalia_text>
                </position>
            </language>
            <translation>The ultimate end is common to all.</translation>
        </marginalia>
        
        <marginalia anchor_text="omnem naturam esse seruatricem sui" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Omnis natura sui seruatrix.</marginalia_text>
                </position>
            </language>
            <translation>All nature is its own preserver.</translation>
        </marginalia>
        
        <marginalia anchor_text="Ex quo intelligi debet homini id esse in bonis vltimu[m], secu[n]dum naturam viuere: quod ita interpretemur, viuere ex hominis natura vndique perfecta, &amp; nihil require[n]te" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Quid in ho[m]i[n]e sit secundum naturam viuere.</marginalia_text>
                </position>
            </language>
            <translation>What in a man may be living according to nature.</translation>
        </marginalia>
        
        <marginalia anchor_text="omne animal seipsum diligere" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Omne a[n]i[m]al seip[su]m diligit.</marginalia_text>
                </position>
            </language>
            <translation>Every living thing loves itself.</translation>
        </marginalia>
        
        <marginalia anchor_text="Necesse est quidem siquis sibipsi inimicus est, eum quae bona sunt, mala putare: bona contrà, quae mala:              &amp; quae appete[n]da fugere, &amp; quae fugienda appetere: quae sine dubio vitae sunt euerio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Vita[e] euersio est cu[m] quis sibi inimicus esse coeperit.</marginalia_text>
                </position>
            </language>
            <translation>Life is destroyed when one has begun to be an enemy of oneself.</translation>
        </marginalia>
        
        <marginalia anchor_text="apud Terentium" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Heautontimoroumenos.</marginalia_text>
                    <book title="Heauton Timorumenos"/>
                </position>
            </language>
            <translation>Heauton Timorumenos.</translation>
        </marginalia>
        
        <marginalia anchor_text="in mala scientes irruunt" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>In mala scientes irruunt</marginalia_text>
                </position>
            </language>
            <translation>They knowingly rush into trouble.</translation>
        </marginalia>
        
        <marginalia anchor_text="reapse" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>reapse.</marginalia_text>
                </position>
            </language>
            <translation>Really.</translation>
        </marginalia>
        
        <marginalia anchor_text="Haec igitur nobis explicanda sunt, sed si enodatius, vos ignoscetis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>enodatius explicare.</marginalia_text>
                </position>
            </language>
            <translation>To explain more clearly.</translation>
        </marginalia>
        
        <marginalia anchor_text="terminatione" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>terminatio</marginalia_text>
                </position>
            </language>
            <translation>Determination.</translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="initio constitutionem confusam habet &amp; incertam" type="straight"/>
        <underline language="LA" method="pen" text="quicquid se attingat, ad séque pertineat" type="straight"/>
        <underline language="LA" method="pen" text="coeptátque" type="straight"/>
        <underline language="LA" method="pen" text="communia, quoniam omnium est natura co[m]munis. Sed" type="straight"/>
        <underline language="LA" method="pen" text="genera, distincta &amp; dispertita sunt" type="straight"/>
        <underline language="LA" method="pen" text="sed vt omnium artium recte dici potest co[m]mune esse, vt in aliqua scientia versetur" type="straight"/>
        <underline language="LA" method="pen" text="scientiam aute[m] suam cuiusque artis esse" type="straight"/>
        <underline language="LA" method="pen" text="tamen in omnibus summa co[m]munis. Et" type="straight"/>
        <underline language="LA" method="pen" text="omnem naturam esse seruatricem sui, ídque" type="straight"/>
        <underline language="LA" method="pen" text="quae natura vigeant, similem esse finem, non" type="straight"/>
        <underline language="LA" method="pen" text="viuere ex hominis natura vndique perfecta, &amp; nihil require[n]te" type="straight"/>
        <underline language="LA" method="pen" text="sed si enodatius" type="straight"/>
        <underline language="LA" method="pen" text="terminatione rerum" type="straight"/>
        <underline language="LA" method="pen" text="idem reapse primum" type="straight"/>
        <underline language="LA" method="pen" text="est enim infixu[m] in ipsa natura, comprehenditur suis cuiusque sensibus, sic vt contrà siquis dicere velit, non audiatur" type="straight"/>
        <underline language="LA" method="pen" text="concurre[n]t co[n]trariae" type="straight"/>
        <underline language="LA" method="pen" text="vitae sunt euersio" type="straight"/>
        <underline language="LA" method="pen" text="qui aut laqueos, aut alia exitia" type="straight"/>
        <underline language="LA" method="pen" text="quum in mala scientes irruunt" type="straight"/>
        <underline language="LA" method="pen" text="bellum indixissent, cruciari dies, noctes torqueri vellent: nec" type="straight"/>
        
