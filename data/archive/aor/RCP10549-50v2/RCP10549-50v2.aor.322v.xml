    <annotation>
        
        <marginalia anchor_text="sibi quenque esse charum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Sibi quisq[ue] Charus.</marginalia_text>
                </position>
            </language>
            <translation>Each person is dear to themself.</translation>
        </marginalia>
        
        <marginalia anchor_text="Quis est enim aut quotus quisque, cui mors quum appropinquet, Non refugiat timido sanguen, atque exalbescat metu?" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Mortem o[mn]es timemus.</marginalia_text>
                </position>
            </language>
            <translation>We all fear death.</translation>
        </marginalia>
        
        <marginalia anchor_text="in vitio, dissolutionem naturae tam valde perhorrescere" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>In vitio est morte[m] p[er]horrescere.</marginalia_text>
                </position>
            </language>
            <translation>It is a fault to shudder at death.</translation>
        </marginalia>
        
        <marginalia anchor_text="ab interitu naturam abhorrere" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Ab interitu natura abhorret.</marginalia_text>
                </position>
            </language>
            <translation>Nature recoils from extinction.</translation>
        </marginalia>
        
        <marginalia anchor_text="metum mortis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Mortis metus</marginalia_text>
                </position>
            </language>
            <translation>Fear of death.</translation>
        </marginalia>
        
        <marginalia anchor_text="Pacuuius" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Pacuuius.</marginalia_text>
                    <person name="Marcus Pacuvius"/>
                </position>
            </language>
            <translation>Marcus Pacuvius.</translation>
        </marginalia>
        
        <marginalia anchor_text="mendicitatem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Mendicitas.</marginalia_text>
                </position>
            </language>
            <translation>Begging.</translation>
        </marginalia>
        
        <marginalia anchor_text="Accium" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Accius.</marginalia_text>
                    <person name="Lucius Accius"/>
                </position>
            </language>
            <translation>Lucius Accius.</translation>
        </marginalia>
        
        <marginalia anchor_text="quum arboru[m] &amp; stirpium eadem penè natura sit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Arboru[m] &amp; stirpiu[m] natura.</marginalia_text>
                </position>
            </language>
            <translation>The nature of trees and plants.</translation>
        </marginalia>
        
        <marginalia anchor_text="sibi quenque natura esse charu[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Sibi quisq[ue] natura Charus.</marginalia_text>
                </position>
            </language>
            <translation>Each man is dear to himself by nature.</translation>
        </marginalia>
        
        <marginalia anchor_text="animúmque ita constitutum, vt &amp; sensibus instructus sit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Animus sensibus instruct[us]</marginalia_text>
                </position>
            </language>
            <translation>The mind equipped with the senses.</translation>
        </marginalia>
       
        <marginalia anchor_text="mors; dissolutionem naturae; interitu; discessu à suis; reliquenda luce" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        Mors
                        dissolutio natur[a]e.
                        interitus
                        discessus a suis.
                        relinquere luce.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Death. The dissolution of nature. Extinction. Departed from one's people. To leave the light.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="appropinquatione mortis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>appropinquatio mortis.</marginalia_text>
                </position>
            </language>
            <translation>The approach of death.</translation>
        </marginalia>
        
        <marginalia anchor_text="compactione membrorum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>co[m]pactio me[m]bror[um].</marginalia_text>
                </position>
            </language>
            <translation>The framework of the limbs.</translation>
        </marginalia>
        
        <marginalia anchor_text="ita figuratum corpus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>ita figuratu[m] corpus.</marginalia_text>
                </position>
            </language>
            <translation>The body is fashioned in this way.</translation>
        </marginalia>
        
