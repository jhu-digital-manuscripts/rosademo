                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Ingenium.</marginalia_text>
                </position>
            </language>
            <translation>Natural capacity.</translation>
        </marginalia>
        
        <marginalia anchor_text="magnarum verarúmque virtutum, quas appellamus volu[n]tarias" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>virtutes voluntaria[e].</marginalia_text>
                </position>
            </language>
            <translation>Voluntary virtues.</translation>
        </marginalia>
        
        <marginalia anchor_text="Ea enim vita expetitur quae sit animi corporísque expleta virtutibus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Vita a[nim]i corporisq[ue] virtutibus expleta</marginalia_text>
                </position>
            </language>
            <translation>A life filled with virtues of the mind and body.</translation>
        </marginalia>
        
        <marginalia anchor_text="animíque virtutes non voluntarias vincant virtutes voluntariae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Virtutes no[n] voluntarias vincunt voluntari[a]e, hijsq[ue] anteponu[n]tur.</marginalia_text>
                </position>
            </language>
            <translation>Volitional virtues outlast the non-volitional ones, and are preferred to these [Loeb 40, 439].</translation>
        </marginalia>
        
        <marginalia anchor_text="ratione" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Ratio.</marginalia_text>
