<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v2.323v.tif" pagination="98" reader="John Dee" signature="[Gg.i.[v]]"/>  
    <annotation>
        
        <marginalia anchor_text="vites" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Vites.</marginalia_text>
                </position>
            </language>
            <translation>Vines.</translation>
        </marginalia>
        
        <marginalia anchor_text="vinitorem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Vinitor.</marginalia_text>
                </position>
            </language>
            <translation>The vine dresser.</translation>
        </marginalia>
        
        <marginalia anchor_text="expletione naturae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Expletio natura[e].</marginalia_text>
                </position>
            </language>
            <translation>The fulfilment of nature.</translation>
        </marginalia>
        
        <marginalia anchor_text="vt ad summum perueniret, quod cumulatur ex integritate corporis, &amp; ex mentis ratione perfecta" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Summu[m] cumulatur ex integritate corporis et ex me[n]tis r[ati]o[n]e perfecta.</marginalia_text>
                </position>
            </language>
            <translation>The highest point is reached from integrity of the body and complete reasoning of the mind.</translation>
        </marginalia>
        
        <marginalia anchor_text="occulta natura est" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Natura occulta est.</marginalia_text>
                </position>
            </language>
            <translation>Nature is hidden.</translation>
        </marginalia>
        
        <marginalia anchor_text="prima co[m]mendatio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Prima co[m]mendatio.</marginalia_text>
                </position>
            </language>
            <translation>The earliest committal.</translation>
        </marginalia>
        
        <marginalia anchor_text="Quam similitudinem videmus in bestiis, quae primo in quo loco natae aunt, ex eo se non commouent: deinde suo quaeque appetitu mouetur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Bestiaru[m] appetitus.</marginalia_text>
                </position>
            </language>
            <translation>The inclination of beasts.</translation>
        </marginalia>
        
        <marginalia anchor_text="naturam esse ad viuendum duce[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Natura ad viue[n]dum dux.</marginalia_text>
                </position>
            </language>
            <translation>Nature, a guide for living.</translation>
        </marginalia>
        
        <marginalia anchor_text="Parui" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Parui.</marginalia_text>
                </position>
            </language>
            <translation>Children.</translation>
        </marginalia>
        
        <marginalia anchor_text="Est enim natura sic generata vis hominis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Vis ho[min]is natura q[uo]m[odo] est generata.</marginalia_text>
                </position>
            </language>
            <translation>How the power of man is generated by nature.</translation>
        </marginalia>
        
        <marginalia anchor_text="virtutis quasi carmen" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Virtutis Carmen.</marginalia_text>
                </position>
            </language>
            <translation>The song of virtue.</translation>
        </marginalia>
        
        <marginalia anchor_text="virtutum quasi scintillulas" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Virtutu[m] scintillula[e].</marginalia_text>
                </position>
            </language>
            <translation>Flickers of virtues.</translation>
        </marginalia>
        
        <marginalia anchor_text="vis naturae per caliginem cernitur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Vis natura[e] per caligine[m] cernit[ur]</marginalia_text>
                </position>
            </language>
            <translation>The power of nature is seen through the fog.</translation>
        </marginalia>
        
        <marginalia anchor_text="nosmetipsos nosse" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Nosce teip[su]m.</marginalia_text>
                </position>
            </language>
            <translation>Know yourself.</translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="habere possit si nulla cultura" type="straight"/>
        <underline language="LA" method="pen" text="ea quae per vinitorem" type="straight"/>
        <underline language="LA" method="pen" text="in eáque expletione" type="straight"/>
        <underline language="LA" method="pen" text="quod cumulatur ex integritate" type="straight"/>
        <underline language="LA" method="pen" text="occulta natura" type="straight"/>
        <underline language="LA" method="pen" text="nobis incerta &amp; obscura est: primúsque" type="straight"/>
        <underline language="LA" method="pen" text="serpere anguiculos" type="straight"/>
        <underline language="LA" method="pen" text="nare anaticulas" type="straight"/>
        <underline language="LA" method="pen" text="euolare merulas" type="straight"/>
        <underline language="LA" method="pen" text="cornibus vti videmus boues, nepas aculeis, suam denique cuique naturam" type="straight"/>
        <underline language="LA" method="pen" text="primo ortu sic iacent, ta[n]quam omnino sine animo" type="straight"/>
        <underline language="LA" method="pen" text="accesserit, &amp; animo vtuntur &amp; sensibus, connituntúrque vt sese erigant" type="straight"/>
        <underline language="LA" method="pen" text="&amp; eos agnoscunt à quibus educantur: deinde aequalibus" type="straight"/>
        <underline language="LA" method="pen" text="congregant" type="straight"/>
        <underline language="LA" method="pen" text="dántque se ad ludendum, fabellarúmque auditione ducuntur" type="straight"/>
        <underline language="LA" method="pen" text="ipsis superat, aliis gratificari volunt, animaduertúntque ea quae domi fiunt curiosius" type="straight"/>
        <underline language="LA" method="pen" text="incipiúntque commentari aliquid &amp; discere: &amp; eorum quos vident, volunt non ignorare nomina:              quibúsque rebus cum aequalibus decertant, si vincunt, efferunt" type="straight"/>
        <underline language="LA" method="pen" text="debilitantur, animósque demittunt. Quorum sine causa fieri nihil putandum" type="straight"/>
        <underline language="LA" method="pen" text="eámque causam parui virtutum simulacris, quarum in se habent semina" type="straight"/>
        <underline language="LA" method="pen" text="mouentur. Sunt enim prima elementa naturae, quibus auctis, virtutis quasi carmen efficitur" type="straight"/>
        <underline language="LA" method="pen" text="Nam quum ita nati factíque simus, vt &amp; agendi aliquid, &amp; diligendi" type="straight"/>
        <underline language="LA" method="pen" text="quasi scintillulas" type="straight"/>
        <underline language="LA" method="pen" text="imbecilláque mente vis naturae per caliginem cernitur" type="straight"/>
        <underline language="LA" method="pen" text="Intra[n]dum est igitur in rerum naturam, &amp; penitus quid ea postulet, peruidendum" type="straight"/>
        
        <symbol name="Jupiter" place="left_margin"/>
        
        <mark method="pen" name="bracket" place="left_margin"/>
        
        <drawing anchor_text="Vinitor" book_orientation="0" method="pen" place="left_margin" type="florilegium"/>
        <drawing book_orientation="0" method="pen" place="left_margin" type="manicule"/>
        
    </annotation>
