        <marginalia anchor_text="adumbrantur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>adumbra[n]tur.</marginalia_text>
                </position>
            </language>
           <translation>They are outlined.</translation>
        </marginalia>
        
        <marginalia anchor_text="pueri, in quibus vt in speculis natura cernitur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>In pueris tanq[uam] in speculis natura cernitur</marginalia_text>
                </position>
            </language>
            <translation>Nature is seen in boys just as in a mirror.</translation>
        </marginalia>
        
        <marginalia anchor_text="qui non moueatur &amp; offensione turpitudinis, &amp; comprobatione honestatis?" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Quis no[n] mouetur offensione turpitudinis, co[m]p[ro]batione Honestatis?</marginalia_text>
                </position>
            </language>
            <translation>Who is not moved to offense by baseness [and] to approval by honour?</translation>
        </marginalia>
        
        <marginalia anchor_text="Pullum Numitorem, Fregellanum proditorem; Codrum; Erichthei filias; Tubuli; Aristidem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Pullus Numitor
                        Fregellanus.
                        Codrus.
                        Ericthei filia[e]
                        Tubulus.
                        Aristides. }
                    </marginalia_text>
                    <person name="Pullus Numitorius"/>
                    <person name="Codrus"/>
                    <person name="Erechtheus II of Athens"/>
                    <person name="Lucius Hostilius Tubulus"/>
                    <person name="Aristides"/>
                </position>
            </language>
            <translation>
                Pullus Numitorius of Fregellae. Codrus. The daughters of Erechtheus. Tubulus. Aristides.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="clamores vulgi" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Clamores vulgi.</marginalia_text>
                </position>
            </language>
            <translation>The shouts of the common people.</translation>
        </marginalia>
        
        <marginalia anchor_text="contra vtilitatem etiam conseruatur fides" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>fides. vtilitas.</marginalia_text>   
                </position>
            </language>
            <translation>Trust. Advantage.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Romanoru[m] res gesta[e].</marginalia_text>
                </position>
            </language>
            <translation>The deeds of the Romans.</translation>
        </marginalia>
        
        <marginalia anchor_text="stuprum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Stuprum.</marginalia_text>
                </position>
            </language>
            <translation>Dishonour.</translation>
        </marginalia>
        
        <marginalia anchor_text="charitas" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Charitas.</marginalia_text>
                </position>
            </language>
            <translation>Affection.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Iusticia[e] ancilla[e] et fideles pedissequa[e].</marginalia_text>
                </position>
            </language>
            <translation>Righteous maids and faithful waiting women.</translation> 
        </marginalia>
        
        <marginalia anchor_text="co[n]spiratio, consensúsque virtutu[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Conspiratio, et co[n]se[n]sus virtutum.</marginalia_text>
                </position>
            </language>
            <translation>The union and agreement of virtues.</translation>
        </marginalia>
        
        <marginalia anchor_text="à primo satu" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>à primo satu.</marginalia_text>
                </position>
            </language>
            <translation>Straight from birth.</translation>
        </marginalia>
        
        <marginalia anchor_text="co[n]spiratio, consensúsque virtutu[m]; coniunctio confusióque virtutum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        co[n]iunctio
                        co[n]spiratio
                        co[n]se[n]sus
                        co[n]fusio ] virtutu[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A combination; a union; an agreement; a mixture of virtues.
            </translation>
        </marginalia>
        
