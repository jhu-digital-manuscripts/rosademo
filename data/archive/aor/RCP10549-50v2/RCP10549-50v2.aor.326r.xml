        <underline language="LA" method="pen" text="Quem enim deditum voluptatibus, cupiditatum incendiis inflammatum, in his potiendis quae" type="straight"/>
        <underline language="LA" method="pen" text="tanta laetitia perfundi" type="straight"/>
