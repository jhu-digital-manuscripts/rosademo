    <annotation>
        
        <marginalia anchor_text="omnes omnium participes" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Omnes virtutes o[mn]i[u]m participes
                    </marginalia_text>
                </position>
            </language>
            <translation>
                All virtues are participating in each other [Loeb 40, 469].
            </translation>
        </marginalia>
        
        <marginalia anchor_text="fortitudo in laboribus periculísque cernatur, temperantia in praetermittendis voluptatibus, prudentia in delectu bonorum &amp; malorum, iustita in suo cuique tribuendo" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        fortitudo. Temperantia. Prudentia. Iustitia.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Strength, temperance, prudence, justice.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="cura" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Cura
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A concern.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="duo genera propter se expetendorum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Duo expetendoru[m] genera.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Two kinds of things to be aspired to.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="omnia illa quae sunt extra, quanquam expetenda, summo bono continerentur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Omnia externa bona no[n] co[n]tine[n]tur in summo bono quare.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                For that reason not all good external things are sustained in the highest good.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="spe gloriae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Gloria[e] Spes
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The hope of glory.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="honestatem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Honestas.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Honesty.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="adubrata opinione" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Honestatis adumbrata opinio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The shadowy belief in honesty.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Africanum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Africanus
                    </marginalia_text>
                    <person name="Scipio Africanus"/>
                </position>
            </language>
            <translation>
                Scipio Africanus.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="L. Paulum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        L. Paulus
                    </marginalia_text>
                    <person name="Lucius Aemilius Paulus Macedonicus"/> 
                </position>
            </language>
            <translation>
                Lucius Aemilius Paulus Macedonicus.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="fortunae, mutationésque rerum &amp; temporum, leues" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        fortuna[e] mutatio leuis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Fortunes change quickly.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="bona corporis numerata, complent ea quidem beatissimam vitam" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Corporis bona qua[m]uis vita[m] co[m]plent beatissima[m], tame[n] ab eadem abesse possunt.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Although the goods of the body make up a the happiest of lives, they can still be absent from such. 
            </translation>
        </marginalia>
        
        <marginalia anchor_text="philosophi non tam gloriosa quàm vera quaerentis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Philisophus no[n] tam gloriosa q[uam] vera qua[e]rere debet.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A philosoper must seek not so much glorious as true things.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="laudata per se ipsa rerum scientia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Reru[m] scie[n]tia laudata.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Praised knowledge of matters.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="de contemnendis ac despiciendis rebus humanis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Res humanas co[n]temne.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Think little of human affairs.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Callipho" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Callipho
                    </marginalia_text>
                    <person name="Calliphon"/>
                </position>
            </language>
            <translation>
                Calliphon.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Diodorus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Diodorus
                    </marginalia_text>
                    <person name="Diodorus Siculus"/>
                </position>
            </language>
            <translation>
                Diodorus Siculus.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="suis laudibus amplificantis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        suis laudib[us] amplificantis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                With his exalted praises.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="gaudio complerentur ... laetitia perdundi" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        gaudio co[m]plexi l[a]etitia p[er]fundi. gaudio affici.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be embraced by joy, to be overwhelmed with happiness, to be moved by joy.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        mirabilit[er]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Amazingly.
            </translation>
        </marginalia>
        
