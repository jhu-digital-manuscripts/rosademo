        <underline language="LA" method="pen" text="extrue animo altitudinem excellentiámque virtutum" type="straight"/>
        <underline language="LA" method="pen" text="virtutis certamen venerint" type="straight"/>
        <underline language="LA" method="pen" text="beata vita" type="straight"/>
        <underline language="LA" method="pen" text="sunt istae accessiones bonorum" type="straight"/>
        <underline language="LA" method="pen" text="quemadmodum stellae in radio solis, sic istae in virtutu[m] splendore ne cerna[n]tur quidem. Atque" type="straight"/>
        <underline language="LA" method="pen" text="non tam gloriosa quàm vera quaerentis, nec pro nihilo putare ea quae secundum" type="straight"/>
