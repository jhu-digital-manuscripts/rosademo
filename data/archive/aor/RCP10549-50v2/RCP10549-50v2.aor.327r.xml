                    <person name="Marcus Atilius Regulus"/> 
                </position>
            </language>
            <translation>
                Marcus Atilius Regulus.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Quo no[n] sit beato beatior.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Why one is not happier than happy.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="co[n]textus rerum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Contextus reru[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A micture of things.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="In Geometria" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Geometraru[m] postulata.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Postulated by the geometricians.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="oratio Socratica" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Or[ati]o Socratica
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Socratic speech.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Paupertas si malum est, mendicus beatus esse non potest. At Zeno eum non beatu[m] modo, sed etiam diuitem dicere ausus est" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Paup[er] diues est et beatus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The poor are rich and happy.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Externa
                    </marginalia_text>
                </position>
            </language>
            <translation>
                External things.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Beatum, inquit, etiam beatissimu[m]: quippe, inquiet, quum tam docuerim gradus istam rem no[n] habere" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Beatitas no[n] habet gradus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Happiness has no degrees.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in Phalaridis tauro" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
