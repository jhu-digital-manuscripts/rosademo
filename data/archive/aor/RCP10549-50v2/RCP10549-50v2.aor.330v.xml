        <marginalia anchor_text="famae celebritate" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        fama[e] celebritas.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A celebration of fame.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="perlapsus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        perlapsus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Skimmed.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="manere animos post morte[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Animi post morte[m] manent
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Minds after death remain.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Authoribus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Authoritas.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Authority.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ab ortu" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Ortus mundi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The garden of the world.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Cascos" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Casci
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The ancient.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="esse in morte sensum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        In morte est sensus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                In death there is feeling.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="cerimoniis sepulchroru[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Cerimonia[e] Sepulchrorum
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Ceremonies of funerals.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="mortem non interitum esse omnia tollentem atque delentem, sed quandam quasi migratione[m] co[m]mutationémque vitae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Mors no[n] est interitus o[mn]ia tollens sed migratio et vita[e] co[m]mutatio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Death is not the ruin destroying everything, but a migration and a alteration of life.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="dux in caelum soleret esse" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Mors bonis, in co[e]lu[m] dux est
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To the good, death is the leader into the heavens.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="famae assentiens dixit Ennius" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        fama[e] assentiens Ennius
                    </marginalia_text>
                    <person name="Ennius"/>
                </position>
            </language>
            <translation>
                Ennius assenting to fame.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Hercules tantus &amp; tam praesens habetur deus. Hinc Liber deus Semele natus, eadémque famae celebritate Tyndaridae fratres, qui non adiutores in praeliis victoriae.              Po. Ro. sed etiam nuntii fuisse perhibe[n[tur. Quid Ino" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Hercules. Liber Tyndarida[e] f[rat]res Ino } 
                    </marginalia_text>
                    <person name="Hercules"/>
                    <person name="Liber"/>
                    <person name="Tyndareus"/>
                    <person name="Ino"/>
                </position>
            </language>
            <translation>
                Hercules, Liber, the brothers of Tyndareus, Ino.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="totum propè caelum, ne plures persequar, nónne humano genere completum est?" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Totum co[e]lu[m] humano genere co[m]pletum est.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The entire heavens are filled with the human kind.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Physica" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Physica.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Physics.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Visis quibusdam saepe mouebantur, hísque maxime nocturnis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        visa nocturna.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Seen at night.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="nemo omniu[m] tam sit immanis, cuius mentem no[n] imbuerit deorum opinio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        O[mn]i[u]m ho[m]i[nu]m me[n]tes aliqua deor[um] opinione imbuta[e]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The minds of all people are initiated by some reasoning of the gods.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Multi de diis praua sentiunt" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Multi de dijs praua se[n]tiu[n]t.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                'Many men have wrong notions about the gods' [Loeb 141, 37].
            </translation>
        </marginalia>
        
        <marginalia anchor_text="lex natura[e]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Lex natura[e]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The law of nature.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="suorum mortem primùm non eò lugeat" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Cur morte[m] luge[n]t ho[m]i[n]es suoru[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Why do people mourn the death of their relatives?
            </translation>
        </marginalia>
        
        <marginalia anchor_text="de immortalitate animoru[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Immortalitas animor[um].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The immortality of the mind.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Statius" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        statius
                    </marginalia_text>
                    <person name="Statius Caecilius"/>
                </position>
            </language>
            <translation>
                Statius Caecilius.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="quòd omnibus curae sunt, &amp; maxime quidem quae post mortem futura sunt" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Qua[n]ta[e] nobis cura[e] sint ea qua[e] post mortem futura sint?
                    </marginalia_text>
                </position>
            </language>
            <translation>
                How anxious are we about that which will be after our death?
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Tolle hanc opinionem, luctum sustuleris. Nemo enim moeret suo inco[m]modo, dolent fortasse &amp; anguntur. Sed illa lugubris lamentatio, fletúsque more[n]s" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        lugere luctus. mo[e]rere dolere angi lugibus lame[n]tatio fletus mo[e]rens
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To mourn our grief, to grieve, to hurt, to distress, the lament of our sorrows, mourning in tears.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="vitae commodis priuatum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        urbatus priuatus } vit[a]e co[m]modis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Being provided with [and] robbed of the comforts of life.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="leges, instituta, rempub. conseret" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        co[n]serere { leges instituta remp.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To compose the law, institutions and state.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="tum si minus id obtinebis" type="straight"/>
        <underline language="LA" method="pen" text="sententiam quam vis obtinere, vti optimis possumus, quod in omnibus causis" type="straight"/>
        <underline language="LA" method="pen" text="solet valere plurimu[m], &amp; primu[m] quidem omni antiquitate, quae quò propius aberat" type="straight"/>
        <underline language="LA" method="pen" text="diuina progenie, hoc melius ea fortasse quae era[n]t  vera cernebat" type="straight"/>
        <underline language="LA" method="pen" text="esse in morte sensum, neque excessu vitae" type="straight"/>
        <underline language="LA" method="pen" text="deleri homine[m] vt funditus interiret: ídque quum multis aliis rebus, tum è po[n]tificio" type="straight"/>
        <underline language="LA" method="pen" text="cerimoniis sepulchroru[m] intelligi licet" type="straight"/>
        <underline language="LA" method="pen" text="coluissent, nec violatas tam inexpiabili religione sanxissent" type="straight"/>
        <underline language="LA" method="pen" text="mortem non interitum esse omnia tollentem atque delentem, sed quandam" type="straight"/>
        <underline language="LA" method="pen" text="co[m]mutationémque vitae: quae in claris viris &amp; foeminis dux in caelum" type="straight"/>
        <underline language="LA" method="pen" text="caeteris humi retineretur, &amp; permaneret tamen" type="straight"/>
        <underline language="LA" method="pen" text="vt famae assentiens" type="straight"/>
        <underline language="LA" method="pen" text="indéque perlapsus ad nos, &amp;" type="straight"/>
        <underline language="LA" method="pen" text="eadémque famae celebritate" type="straight"/>
        <underline language="LA" method="pen" text="nónne humano genere completum est? Si vero scrutari vetera" type="straight"/>
        <underline language="LA" method="pen" text="tantu[m] sibi persuaserant quantum natura admonente cognouerant, rationes" type="straight"/>
        <underline language="LA" method="pen" text="causas rerum non tenebant. Visis quibusdam saepe mouebantur, hísque maxime nocturnis" type="straight"/>
        <underline language="LA" method="pen" text="videre[n]tur hi qui vita excesserant, viuere. Vt porrò firmissimu[m] hoc afferri videtur" type="straight"/>
        <underline language="LA" method="pen" text="esse creadamus, quòd nulla gens tam " type="straight"/>
        <underline language="LA" method="pen" text="nemo omniu[m] tam sit immanis, cuius mentem no[n] imbuerit deorum opinio. Multi de diis praua sentiunt: id enim vitioso more" type="straight"/>
        <underline language="LA" method="pen" text="collocutio" type="straight"/>
        <underline language="LA" method="pen" text="aut consensus efficit: non institutis opinio est confirmata, non" type="straight"/>
        <underline language="LA" method="pen" text="consensio omnium gentium lex naturae putanda est. Quis" type="straight"/>
        <underline language="LA" method="pen" text="Nemo enim moeret suo inco[m]modo, dolent fortasse &amp; anguntur" type="straight"/>
        <underline language="LA" method="pen" text="illa lugubris lamentatio, fletúsque moere[n]s ex eo est, quòd eum quem dileximus" type="straight"/>
        <underline language="LA" method="pen" text="commodis priuatum arbitramur, ídque sentire" type="straight"/>
        <underline language="LA" method="pen" text="Serit arbores quae alteri seculo prosint" type="straight"/>
        <underline language="LA" method="pen" text="magnus leges, instituta, rempub[licam] conseret" type="straight"/>
        <underline language="LA" method="pen" text="specimen naturae capi" type="straight"/>
        
