        <underline language="LA" method="pen" text="venisse, &amp; in ea cum alios multos, tum Archytam, Timaeúmque cognouisse, &amp; didicisse Pythagorea omnia:              primúmque de animorum aeternitate non solum sensisse idem quod Pythagoras, sed rationem etiam attulisse, quam (nisi quid dicis) praetermittamus, &amp; hanc totam spem immortalitatis relinquamus" type="straight"/>
        <underline language="LA" method="pen" text="quem ex tuo ore admiror: quàm cum istis vera" type="straight"/>
        <underline language="LA" method="pen" text="M. Macte virtute" type="straight"/>
        <underline language="LA" method="pen" text="mundi sitam ad vniuersi caeli complexum, quasi puncti instar obtinere, quod –––GREEK NEEDED––– illi vocant" type="straight"/> <!-- FSF: Greek needed --> 
        <underline language="LA" method="pen" text="inter se &amp; diuisa momenta, vt terrena, &amp; humida suopte" type="straight"/>
        <underline language="LA" method="pen" text="siue ipsa natura superiora" type="straight"/>
        <underline language="LA" method="pen" text="grauioribus" type="straight"/>
        <underline language="LA" method="pen" text="illi sint animales, id" type="straight"/>
        <underline language="LA" method="pen" text="quod subtiliter magis quàm dilucide dicitur, aut quinta illa non nominata magis quàm non intellecta natura,              multò etiam integriora ac puriora sunt vt à terra longissime se" type="straight"/>
        <underline language="LA" method="pen" text="nec tam vegata mens, aut in corde cerebróve, aut in Empedocleo sanguine demersa iaceat" type="straight"/>
        <underline language="LA" method="pen" text="aequali &amp; condiscipulo" type="straight"/>
        <underline language="LA" method="pen" text="ne condoluisse quidem vnquam videtur, qui animum se" type="straight"/>
        <underline language="LA" method="pen" text="non sentiat: alter ita delectatur suis cantibus, vt eos etiam ad haec transferre conetur" type="straight"/>
        <underline language="LA" method="pen" text="eruditus sit, sicut est, haec magistro concedat Aristoteli: canere ipse doceat" type="straight"/>
        <underline language="LA" method="pen" text="prouerbio praecipitur" type="straight"/>
        <underline language="LA" method="pen" text="Illam vero funditus eiiciamus indiuidoru[m] corporum leuium" type="straight"/>
        <underline language="LA" method="pen" text="concursionem fortuita[m], quam" type="straight"/>
        <underline language="LA" method="pen" text="concalefacta[m] &amp; spirabilem" type="straight"/>
        <underline language="LA" method="pen" text="Nihil enim habent haec duo genera proni" type="straight"/>
        <underline language="LA" method="pen" text="his perrumpatur &amp; diuidatur crassus" type="straight"/>
        <underline language="LA" method="pen" text="concretus aer qui est terrae proximus" type="straight"/>
        <underline language="LA" method="pen" text="nostra terreno principiorum genere confecta, ardore animi concalescunt" type="straight"/>
        <underline language="LA" method="pen" text="diuidat omne caelum hoc, in quo nubes, imbres, ventíque coguntur" type="straight"/>
        <underline language="LA" method="pen" text="caliginosum est propter exhalationes terrae" type="straight"/>
        <underline language="LA" method="pen" text="ignibus insistit" type="straight"/>
        <underline language="LA" method="pen" text="tanquam paribus examinatus ponderibus" type="straight"/>
