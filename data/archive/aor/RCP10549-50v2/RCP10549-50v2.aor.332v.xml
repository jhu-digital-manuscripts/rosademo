<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v2.332v.tif" pagination="116" reader="John Dee" signature="[Hh.ii.[v]]"/>   
    <annotation>
        
        <marginalia anchor_text="animus, an tanta sit eius tenuitas vt fugiat aciem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Animus aciem fugit
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The mind flees sight.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in liberu[m] coelum quasi in domu[m] suam" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Co[e]lum a[n]i[m]oru[m] n[ost]ror[um] domus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The heavens are the house of our minds.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="deum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Deus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                God.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="maximu[m], animo ipso animu[m] videre" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Animu[m] a[n]i[m]o videre, maximu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                'The mind sees the mind alone' [Loeb 141, 61].
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="EL">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Γνῶθι σεαυτὸν
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Know thyself. <!-- (Know thyself, the Delphic maxim mentioned in Plato’s Alcibiades)-->
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        ex platonis alcib
                    </marginalia_text>
                    <person name="Plato"/>
                    <book title="First Alcibiades"/>
                </position>
            </language>
            <translation>
                From the 'First Alcibiades' by Plato.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="corpus quide[m] quasi vas est" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Corpus quasi vas est.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The body is like a vessel.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="à Socrate est in Phaedro" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Socrates in Pha[e]dro.
                    </marginalia_text>
                    <person name="Socrates"/>
                    <book title="Phaedrus"/>
                </position>
            </language>
            <translation>
                Socrates in Phaedrus.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Principii autem nulla est origo" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Principij nulla est origo
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A beginning has no origin.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Quod no[n] ortu[m] habet, nec interitum id habebit.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                That which has no beginning has no end either.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="aeternum id esse quod seipsum moueat" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Æternu[m] est q[uod] seip[su]m mouet
                    </marginalia_text>
                </position>
            </language>
            <translation>
                What moves by itself is eternal.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Inanimu[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Inanimus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Inanimate.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="plebeii omnes philosophi" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Plebei philosophi.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Plebian philosophers.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Æternitas.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Eternity.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Corporis co[n]formatio.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The conformity of the body.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Animus quid secu[m] affert?
                    </marginalia_text>
                </position>
            </language>
            <translation>
                What does the mind carry with itself?
            </translation>
        </marginalia>
        
        <marginalia anchor_text="memoria[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Memoria.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Memory.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Plato recordatione[m] esse vult vitae superioris. Nam in illo qui inscribitur Memnon" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Plato in Memnone
                    </marginalia_text>
                    <person name="Plato"/>
                    <book title="Meno"/>
                </position>
            </language>
            <translation>
                Plato in Meno.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        In pha[e]done
                    </marginalia_text>
                    <book title="Phaedo"/>
                </position>
            </language>
            <translation>
                In Phaedo.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="possent in homine vno cerni omnia, quae nunc tecta sunt, casurúsne in conspectu[m] videatur animus, an tanta sit eius tenuitas vt fugiat" type="straight"/>
        <underline language="LA" method="pen" text="intuenti multò difficilior occurrit cogitatio, multóque obscurior qualis" type="straight"/>
        <underline language="LA" method="pen" text="corpore sit, tanqua[m] alienae domui" type="straight"/>
        <underline language="LA" method="pen" text="&amp; in liberu[m] coelum" type="straight"/>
        <underline language="LA" method="pen" text="Si enim quod nunqua[m] vidimus, id quale sit intelligere no[n] possumus, certe &amp; deum ipsum &amp; diuinu[m] animu[m] corpore liberatu[m],              cogitatione co[m]plecti no[n] possumus" type="straight"/>
        <underline language="LA" method="pen" text="neque ego haec dice[n]s corpori tuo dico" type="straight"/>
        <underline language="LA" method="pen" text="Principii autem nulla est origo" type="straight"/>
        <underline language="LA" method="pen" text="Nam principiu[m] extinctum, nec ipsum ab alio renascetur" type="straight"/>
        <underline language="LA" method="pen" text="Quum pateat igitur aeternum id esse quod" type="straight"/>
        <underline language="LA" method="pen" text="hominis vitam sustentari" type="straight"/>
        <underline language="LA" method="pen" text="quidem Plato recordatione[m] esse vult vitae superioris" type="straight"/>
        <underline language="LA" method="pen" text="de dime[n]sione quadrati" type="straight"/>
        
