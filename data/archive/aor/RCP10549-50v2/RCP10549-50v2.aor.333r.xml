        <marginalia anchor_text="nihil aliud est discere nisi recordari" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Discere nihil aliud q[uam] recorda
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Learning is nothing other than remembering.
            </translation>
        </marginalia>
        
