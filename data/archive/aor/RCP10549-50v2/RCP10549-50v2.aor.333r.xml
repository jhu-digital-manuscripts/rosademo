        <marginalia anchor_text="discere, sed reminiscendo" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Discere est reminisci
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To learn is to recollect.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
