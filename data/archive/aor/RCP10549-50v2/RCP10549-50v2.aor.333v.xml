        <underline language="LA" method="pen" text="pocula ministrante" type="straight"/>
        <underline language="LA" method="pen" text="Fingebat haec Homerus, &amp; humana ad deos transferebat" type="straight"/>
        <underline language="LA" method="pen" text="vigere, sapere, inuenire, meminisse" type="straight"/>
        <underline language="LA" method="pen" text="&amp; terra vacat &amp; humore" type="straight"/>
        <underline language="LA" method="pen" text="aut flabile, aut igneum" type="straight"/>
        <underline language="LA" method="pen" text="inuenietur vnquam vnde ad hominem venire possint nisi à deo" type="straight"/>
        <underline language="LA" method="pen" text="sentit, quod sapit, quod vult, quod viget, caeleste &amp; diuinum est" type="straight"/>
        <underline language="LA" method="pen" text="mens soluta quaedam &amp; libera segregata ab omni concretione mortali omnia sentiens" type="straight"/>
        <underline language="LA" method="pen" text="mouens, ipsáque praedita motu sempiterno" type="straight"/>
        <underline language="LA" method="pen" text="at vt oculus, sic animus sese non videns alia cernit" type="straight"/>
        <underline language="LA" method="pen" text="Qua facie quidem" type="straight"/>
        <underline language="LA" method="pen" text="habitet, ne quaere[n]dum quidem est: vt quum videmus speciem primùm candorémque caeli" type="straight"/>
        <underline language="LA" method="pen" text="co[m]mutationésque temporum quadripartitas ad" type="straight"/>
        <underline language="LA" method="pen" text="maturitatem frugum &amp; temperatione[m] corporum aptas, eorúmque omniu[m]" type="straight"/>
        <underline language="LA" method="pen" text="solem, lunámque accretione &amp; diminutione luminis quasi fastorum" type="straight"/>
        <underline language="LA" method="pen" text="nocturnámque caeli formam" type="straight"/>
        <underline language="LA" method="pen" text="Tum globum terrae" type="straight"/>
        <underline language="LA" method="pen" text="frigore rigeant, aut vrantur calore" type="straight"/>
        <underline language="LA" method="pen" text="Ramos baccarum vbertate incuruescere, Segetes largiri fruges, florere omnia, Fontes scatere, herbis prata conuestirier" type="straight"/>
        <underline language="LA" method="pen" text="caeli ac deorum, ipsorúmque cultorem, atque hominis vtilitati agros omnes &amp; maria parentia. Haec" type="straight"/>
        <underline language="LA" method="pen" text="praesit aliquis conditor vel effector, si haec nata sunt, vt Platoni videtur: vel si semper" type="straight"/>
        
