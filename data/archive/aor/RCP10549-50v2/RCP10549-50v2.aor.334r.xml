    <page filename="RCP10549-50v2.334r.tif" pagination="119" reader="John Dee" signature="Hh.iiii."/>   
    <annotation>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        digressus direptus secretio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Digression, pillage, separation.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="nec secerni, nec diuidi, nec discerpi, nec distrahi" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        secerni diuidi discerpi distrahi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be separated, to be divided, to be torn in pieces, to be pulled apart.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="vbivbi sit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        vbi vbi sit
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Wherever it was.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="deum agnoscis ex operibus eius" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Deu[m] nosce ex operibus eius
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Examine god through his works.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in capite" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Animus in Capite
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The mind in the head.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Animus purus et liber ab o[mn]i co[n]cretione mortali
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The mind, pure and free of any mortal materiality.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="interitus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Interitus.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Ruin.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Socrates" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Socrates in apol.
                    </marginalia_text>
                    <person name="Socrates"/>
                    <book title="The Apology"/>
                </position>
            </language>
            <translation>
                Socrates in The Apology of Socrates.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        In pha[e]done.
                    </marginalia_text>
                    <book title="Phaedo"/>
                </position>
            </language>
            <translation>
                In Phaedo.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="educi" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        In critone hoc agitur
                    </marginalia_text>
                    <book title="Crito"/>
                </position>
            </language>
            <translation>
                In Crito, this is delivered.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        In pha[e]done. in finem penè.
                    </marginalia_text>
                    <book title="Phaedo"/>
                </position>
            </language>
            <translation>
                In Phaedo, almost at the end.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Duplex a[n]i[m]oru[m] Cursus.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The twofold course of the minds.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Cygnus ante obitu[m] canit
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A swan sings before his death.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="cum cantu &amp; voluptate moriantur: sic omnibus &amp; &amp; doctus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        O[mn]ibus bonis et doctis cu[m] ca[n]tu et voluptate moriendum.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                All good and learned people ought to be dying with chant and pleasure.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Cato autem sic abiit è vita, vt causam moriendi nactum se esse gauderet" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Cato moriendi causa[m] se nactu[m] gaudebat.
                    </marginalia_text>
                    <!--JG: Which Cato?-->
                </position>
            </language>
            <translation>
                Cato rejoiced that he had found the reason for his death. 
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Vetat enim dominans ille in nobis deus iniussu hinc nos suo demigrare" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Vetat Deus nos iniussu suo hinc migrare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                God forbad that we migrate there without his order.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="philosophorum vita, vt ait idem, commentatio mortis est" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Philosophoru[m] vita mortis Commentatio est
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The life of philosophers is a preparation for death.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Secernere autem à corpore animum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Secernere a[n]i[mu]m à corpore.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To separate the mind from the body.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in compedibus corporis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        In Compedibus corporis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                In the chains of the body.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="haec quidem vita mors est" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        H[a]ec vita mors est.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                This life is death.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="volat enim aetas" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Volat a[e]tas
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A lifetime flies.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="tanquam ratis in mari immenso nostra vehitur oratio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        tanq[uam] ratis in mari imme[n]so n[ost]ra vehitur or[ati]o
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Our speech passes like a ship through a vast sea.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="à negotio omni seuocamus animum? quid, inquam, tum agimus, nisi animum ad sepisum aduocamus, secum esse cogimus, maximéque à corpore abducimus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        a[n]i[mu]m ad seip[su]m aduocam[us]. a[n]i[mu]m secu[m] esse cogimus. a[n]i[mu]m à corpore abducumus à negotio o[mn]i a[n]i[mu]m sevocam[us]
                        à corpore a[n]i[mu]m secernimus. nos disiungere a corporibus.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                We invoke the mind to itself, we think that the mind is independent, we withdraw the mind from the body, we separate the mind in every business, we separate the mind from the body, we disunite the mind from the bodies.
            </translation>
        </marginalia>
        
