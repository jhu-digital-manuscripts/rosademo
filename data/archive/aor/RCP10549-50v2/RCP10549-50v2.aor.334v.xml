        <underline language="LA" method="pen" text="&amp; disclusas putat" type="straight"/>
        <underline language="LA" method="pen" text="Multa enim è corpore existunt" type="straight"/>
        <underline language="LA" method="pen" text="acuant mentem, multa quae obtundant" type="straight"/>
