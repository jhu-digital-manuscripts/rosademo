        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        secundo id populo facere p[otes]t 
                    </marginalia_text> 
                </position>
            </language>
            <translation>
                That can be done to the following people.
            </translation>
        </marginalia>
        
