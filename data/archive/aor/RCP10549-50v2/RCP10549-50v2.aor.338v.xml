        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        scribere { distincte distribute eleganter ornate.
                    </marginalia_text>
                </position>
            </language>
            <translation>
