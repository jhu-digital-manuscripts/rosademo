    <annotation>
        
        <marginalia anchor_text="obiiciebatur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Motus mortis.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The commotion of death.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="nam efficit hoc philosophia: medetur animis, inanes solicitudines detrahit, cupiditatibus liberat, pellit timores" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Philosophia[e] co[m]moditates.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The utility of philosophy.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="FORTES enim non modo fortuna adiuuat" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Fortes fortuna adiuuat
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Fortune aids the powerful.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ratio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Ratio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Reason.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Philosophi inani ostentatione laborant pleriq[ue] omnes
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Foolish philosophers and many more work by showing off.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="vt cum eorum vita mirabiliter pugnet oratio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Ne cu[m] vita pugnet Or[ati]o.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Lest the speech fights with life.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="philosophus in ratione vitae peccans" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Philosophus magister vita[e]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A philosopher is a teacher of life.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Nam vt agri no[n] omnes frugiferi sunt qui colu[n]tur ... animi non omnes culti fructu[m] feru[n]t" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Animi no[n] omnes culti fructum ferent, vt agri no[n] omnes su[n]t frugiferi qui coluntur.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Not all minds bear fruit of the farmed lands, so that not all tilled fields are fruit-bearing.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Cultura aute[m] animi philosophia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Animi Cultura Philosophia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Training of the mind through philosophy.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Propositum de quo est dissere[n]du[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A practive of which one should learn.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Ratione[m] quo ea me cunq[ue] ducet seq[ua]r
                    </marginalia_text>
                </position>
            </language>
            <translation>
                I shall follow the reasoning through which she will lead me wherever.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Aristippus non dubitauit dolorem summum malum dicere.              Dehinc ad hanc eneruatam muliebrémque sententiam satis docilem se Epicurus praebuit. Hunc post, Rhodius Hieronymus dolore vacare summum bonum dixit,              tantum in dolore duxit mali. Caeteri praeter Zenonem, Aristonem, Pyrrhonem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Aristippus. Epicurus, Rhodius, Hieronimus. Zeno Aristo. Pyrho.
                    </marginalia_text>
                    <person name="Aristippus"/>
                    <person name="Epicurus"/>
                    <person name="Hieronymus of Rhodes"/>
                    <person name="Zeno of Citium"/>
                    <person name="Aristo of Chios"/>
                    <person name="Pyrrho"/>
                </position>
            </language>
            <translation>
                Aristippus, Epicurus, Hieronymus of Rhodes, Zeno of Citium, Aristo of Chios, Pyrrho.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="magistra vitae philosophia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Philosophia vita[e] magistra
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Philosophy is the teacher of life.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="eneruatam muliebrémque sententiam" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        eneruata et muliebris se[n]tentia.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                An opinion weakened also by women.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="de sententia esse deiectu[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        de sente[n]tia deijci Verbo de se[n]te[n]tia desistere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To retreat from an opinion, To desist from the word of an opinion.
            </translation>
        </marginalia>
        
