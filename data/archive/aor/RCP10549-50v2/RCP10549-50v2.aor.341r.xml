    <annotation>
        
        <marginalia anchor_text="contemnendus omnis dolor ... sepeliendus dolor" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        co[n]te[m]nendus dolor o[mn]is sepeliendus dolor
                    </marginalia_text>
                </position>
            </language>
            <translation>
                One should neglect all pain. To be buried pain.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="enuntiante commissa, prodente co[n]scios" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        enu[n]tiare co[m]missa. prodere co[n]scios
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To announce an undertaking. To create accomplices.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Prudentiámne vis esse, sine qua ne intelligi quidem vlla virtus potest?" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Nulla virtus sine prude[n]tia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                No virtue without prudence.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Corinthiis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Corinthia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Corinthian.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="amitti non potest virtus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        virtus amitti no[n] p[otes]t.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A virtue cannot be dismissed.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in lecto humido" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        in lecto Humido
                    </marginalia_text>
                </position>
            </language>
            <translation>
                In a damp place.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="eum opprimi dico patientia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Dolor opprime[n]dus patie[n]tia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Pain has to be oppressed with endurance.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Cetum quidem leges" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Cretum leges
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The laws of the Cretans.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Lycurgi" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Lycurgus
                    </marginalia_text>
                    <person name="Lycurgus of Sparta"/>
                </position>
            </language>
            <translation>
                Lycurgus of Sparta.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Spartae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Spartae
                    </marginalia_text>
                    <location name="Sparta"/>
                </position>
            </language>
            <translation>
                In Sparta.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="mos valet, rationon valebit? Interest aliquid inter laborem &amp; dolorem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Mos. Ratio. Labor. Dolor.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Custom, reason, labour, pain.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="industrios ... laboriosos" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Industrij. Laboriosi.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The industrious and laborious.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="O verborum inops interdu[m], quibus abundare te semper putas, Graecia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Gra[e]cia interdum verboru[m] inops.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Greek is sometimes destitute of words.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="C. Mario" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        C. Marius.
                    </marginalia_text>
                    <person name="Gaius Marius"/>
                </position>
            </language>
            <translation>
                Gaius Marius.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="corpora iuuenum firmari labore" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Corpora Labore firmari
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Bodies are hardened by labour.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        fo[e]minaru[m] Labores et exercitationes
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Tasks and exercises of women.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="labor quasi callum quoddam obducit dolori" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Labor callum obducit dolori.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Laour brings callousness to pain.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Militiam vero nostram dico non Spartiatarum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Spartiataru[m] Militia.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The military service of the Spartans. 
            </translation>
        </marginalia>
        
        <marginalia anchor_text="exercitus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Exercitus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Army.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Arma enim membra militis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Arma militis membra
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The arms of the soldiers.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="exercitatio legionum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Legionu[m] exercitatio.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Exercise of the troops.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="varices" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        Varices
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Varicose veins.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        pungit dolor. fodit
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Paint stings [and] stabs.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="callum quoddam obducit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        callum obducere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To lead to callousness.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="dolor intercurrit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        hij intercurrit dolor
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Pain moves them.
            </translation>
        </marginalia>
        
