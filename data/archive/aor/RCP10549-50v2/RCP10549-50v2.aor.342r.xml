    <annotation>
        
        <marginalia anchor_text="forticulum se in torminibus, &amp; in stranguria sua praebeat" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        forticulu[m] se prebere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To show oneself brave.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Itera[n]dum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        Itera[n]du[m] est
                    </marginalia_text>
                </position>
            </language>
            <translation>
                It is to be repeated.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="terminat" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        terminat
                    </marginalia_text>
                </position>
            </language>
            <translation>
                He marks the boundaries.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="appellata est enim ex viro virtus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Virtus à viro appellata.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A virtue called upon by man.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="fortitudo, cuius munera duo maxima sunt" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        fortis duo munera.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Two duties of strength.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="medicina[m] philosophia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Philosophia medicina.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Philosophy is medicine.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="summum dicas esse, quid breue" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Summu[m]. Breue.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The greatest. Brief.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Philoctetae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Philoctetes
                    </marginalia_text>
                    <person name="Philoctetes"/>
                </position>
            </language>
            <translation>
                Philoctetes.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="bonos viros nominare qui complures annos doloribus podagrae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Viri boni podagra[e] dolore labora[n]tes
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Good men suffering from the pain of gout.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in stranguria" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        stranguria.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Strangury.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="pueri" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Pueri
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Boys.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="optimum esse" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Quid optimum sit.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                What is best.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="turpi" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Turpitudo.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Shame.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="quasi duo simus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Quasi duo sumus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                As if we were two.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Est enim animus in partis tributus duas" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Anima[e] dua[e] partes
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The two parts of the soul.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="est domina omnium &amp; regina ratio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Ratio o[mn]i[u]m do[m]i[n]a et Regina
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Reason is the master and queen of everything.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="pudore, qui ratione nulla vincerentur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Pudor plus qua[m] r[ati]o valet interdum.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Decency prevails more often than reason.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="silentio ... constanter &amp; sedate" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        co[n]sta[n]ter sedate cu[m] sile[n]tio tolera[n]tes patie[n]tes ] dolore[m] ferre placide viriliter fortiter humane tranquille moderate no[n] turbute[n]tè
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To bear the pain firmly, calmly, in silence, in a tolerating and enduring manner, manly, strongly, humanely, calmly, moderately, without disturbance.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="dolor aliquis peruellerit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        dolor p[er]uellit
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Pain pinches.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="molle quiddam, demissum, humile, eneruatum, quodammodo &amp; languideum, senile" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        molle quidda[m] demissum humile eneruatu[m] languedu[m] senile
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Something tender, downhearted, low, weakened, listless, senile.
            </translation>
        </marginalia>
        
