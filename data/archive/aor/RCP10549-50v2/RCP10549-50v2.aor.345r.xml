            </translation>
        </marginalia>
        
        <marginalia anchor_text="Stoicoru[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        stoicoru[m] breuiaria
                    </marginalia_text>
                </position>
