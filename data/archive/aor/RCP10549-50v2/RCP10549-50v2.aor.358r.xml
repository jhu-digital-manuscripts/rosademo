    <annotation>
        
        <marginalia anchor_text="mollitia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        mollitia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Weakness.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="virtutem ad beate viuendum, seipsa esse contentam" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Virtus ad beate viue[n]du[m] seip[s]a co[n]tenta
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The courage to live happily is self-contained.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="famula fortunae est" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Virtus no[n] est fortuna[e] famula
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Virtue is not a handmaid of luck.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="humani generis imbecillitatem fragilitatémque" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Humani generis fragilitas
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The frailty of humankind.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="omnia quae cadere in homine[m] possunt, subter se habet" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Homo o[mn]ia qua[e] in se cadere possunt subter se habeat.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Man has under themselves all that can fall to him.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Philosophia[e] eximia &amp; excellens Laus.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                An extraordinary and excellent praise of philosophy,
            </translation>
        </marginalia>
        
        <marginalia anchor_text="sapientiam" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Sapientia[e] Definitio.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Definition of wisdom.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="culpae, &amp; caeterorum vitiorum peccatorúmque" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        Culpa vitiu[m] peccatu[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Guilt, vice, sin.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        subiecta sub
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Subjected to.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        paricidiose.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                By parricide.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        pra[e]fidia fiducia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Trust, faith.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        dolor intolerabilis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Unbearable pain.
            </translation>
        </marginalia>
        
