                    <marginalia_text>
                        Suauissimus pastus a[n]i[m]orum
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Pleasant pasture of the minds.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="gaudioru[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Vita beata ex perpetuis gaudijs
                    </marginalia_text>
                </position>
            </language>
            <translation>
