                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Anaxagoras
                    </marginalia_text>
                    <person name="Anaxagoras"/>
                </position>
            </language>
            <translation>
                Anaxagoras.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Nullus p[otes]t esse sensus in infinito
                    </marginalia_text>
                </position>
            </language>
            <translation>
                No sense can be unlimited.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Alcmaeo" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Alcma[e]o
                    </marginalia_text>
                    <person name="Alcmaeon pf Croton"/>
                </position>
            </language>
            <translation>
                Alcmaeon.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        innumerabilia secula ab infinito te[m]pore imme[n]so spatio ab [a]eterno p[rae]tenti te[m]poris spatio à principio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Innumerable centuries, from limitless time, immense space, from eternal space of time, from the beginning. 
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        q[uod] ne in cogitatione[m] quide[m] cadit fugere intellige[n]ti[a]e n[ost]ra[e] vim et r[ati]o[n]em <!-- FSF: not sure about the last abbreviation -->
                    </marginalia_text>
                </position>
            </language>
            <translation>
                 
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        co[m]modore[m] co[n]pensatione[m] leuire
                    </marginalia_text>
                    <emphasis method="pen" type="straight"/>
                </position>
            </language>
            <translation>
                To mitigate compensation.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        ea relecitate co[n]torqueri mundi co[n]mersio <!-- FSF: not sure about the second word -->
                    </marginalia_text>
                </position>
            </language>
            <translation>
                
