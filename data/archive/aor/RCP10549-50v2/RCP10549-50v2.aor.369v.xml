        <marginalia anchor_text="poetarum vocibus fusa, ipsa suauitate nocuerunt" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Poetaru[m] vocibus fusa ipsa suauitate nocuerunt
                    </marginalia_text>
                </position>
            </language>
            <translation>
                They harm with their sweetness, which is perfused by the voices of the poets.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Magorum, AEgyptiorúmque" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Magi Ægiptij
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The wise from Egypt.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="esse deos, quòd in omniu[m] animis eorum notionem impressisset ipsa natura" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Deos esse in o[mn]i[u]m a[nim]is impressis natura eoru[m] notione
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Gods are in the minds of all, impressed by their own natural examination. <!-- FSF: not sure --> 
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="EL">
                <position book_orientation="0" place="left_margin">
