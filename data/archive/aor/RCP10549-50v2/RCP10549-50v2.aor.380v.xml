    <page filename="RCP10549-50v2.380v.tif" pagination="212" reader="John Dee" signature="[Oo.ii.[v]]"/>   
    <annotation>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        Prouide[n]tia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Providence.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Democritus simulacra, &amp; Epicurus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Democrtius. Epicurus.
                    </marginalia_text>
                    <person name="Democritus"/>
                    <person name="Epicurus"/>
                </position>
            </language>
            <translation>
                Democritus, Epicurus.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="natura deorum praepote[n]s neque excellens" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Natura Deoru[m] p[rae]potens, excelle[n]s
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The nature of gods is powerful and escellent.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Nihil est autem praestantius" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Nihil est deo pra[e]stantius
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Nothing is more excellent than god.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="omnem ergo regit ipse naturam" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        O[mn]em Deus regit natura[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                God rules the entire nature.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Ignoratio reru[m] aliena est naturae deorum
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The ignorance of things is foreign to the nature of the gods.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="maiestatem deorum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Deorum maiestas
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The majesty of the gods.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="eadem veritas vtribique sit, eadémque lex" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Eade[m] veritas, Lex eadem
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The same truth, the same law.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="mens, fides, virtus, concordia, consecratae &amp; publice dedicatae sunt" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Mens, fides, virtus, co[n]cordia cur consecrata[e], et publice sint dedicat[a]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Why the mind, faith, virtue and concord are publicly consecrated and dedicated.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        simulachroru[m] Cultus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The worship of images.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Dei Consilium et prudentia ex suis percipi potest creaturis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The advice and prudence of God can be learned from his creatures.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="omnia subiecta esse naturae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Omnia subiecta e[ss]e Natura[e]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Everything is a subject of nature.
            </translation>
        </marginalia>

        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Natura[e] varìa[e] diffinitio[n]es
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Different definitions of nature.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Seminis enim vim" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Seminis vis mira
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The wonderful power of seed.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Epicurus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Epicurus
                    </marginalia_text>
                    <person name="Epicurus"/>
                </position>
            </language>
            <translation>
                Epicurus,
            </translation>
        </marginalia>
        
        <marginalia anchor_text="apparet, &amp; artis quaeda[m] similitudo" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        In natura apparet artis qua[e]dam similitudo.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                In nature a certain semblance of the art is apparent.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="terra" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Terra.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Earth.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="quae grauidata seminibus omnia pariat &amp; fundat ex sese" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        terra gravidata seminibus o[mn]ia parit &amp; fundit ex sese
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The earth impregnated with seeds brings forth and pours out from herself all things.
            </translation>
        </marginalia>
        
