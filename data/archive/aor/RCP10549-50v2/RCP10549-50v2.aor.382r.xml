        <underline language="LA" method="pen" text="is diem efficeret, toto" type="straight"/>
        <underline language="LA" method="pen" text="quum aute[m] terras nox opacasset, tum caelum" type="straight"/>
        <underline language="LA" method="pen" text="cernerent astris distinctu[m] &amp; ornatum, lunáeque" type="straight"/>
        <underline language="LA" method="pen" text="regiones obscurauisse" type="straight"/>
        <underline language="LA" method="pen" text="Sed assiduitate cotidiana, &amp; consuetudinte oculoru[m] assuescunt animi, neque admirantur,              neque reuirunt rationes earu[m] rerum quas semper vident, proinde quasi nouitas nos magis quàm magnitudo rerum debeat" type="straight"/>
        <underline language="LA" method="pen" text="ad exquirendas causas excitare" type="double"/>
        <underline language="LA" method="pen" text="tam ratos astrorum ordines, támque inter se omnia connexa &amp; apta viderit, neget in his vllam inesse" type="straight"/>
        <underline language="LA" method="pen" text="cum machinatione quada[m] moueri" type="straight"/>
        <underline language="LA" method="pen" text="impetum" type="straight"/>
        <underline language="LA" method="pen" text="vicissitudines anniuersarias" type="straight"/>
        <underline language="LA" method="pen" text="remota subtilitate" type="straight"/>
        <underline language="LA" method="pen" text="mundi, solida &amp; globosa, &amp; vndique ipsa in sese nutibus suis conglobata, vestita floribus, herbis, arboribus, frugibus" type="straight"/>
        <underline language="LA" method="pen" text="multitudo insatiabili varietate herbis, arboribus, frugibus" type="straight"/>
        <underline language="LA" method="pen" text="vel circuru[m]" type="straight"/>
        <underline language="LA" method="pen" text="nec immanitate beluarum efferari, nec stirpium asperitate vastari, quorúmque" type="straight"/>
        <underline language="LA" method="pen" text="insulae, littoráque collucent distincta tectis &amp;" type="straight"/>
        <underline language="LA" method="pen" text="genera quámque disparia partim submersarum, partim fluitantium &amp; innantium beluarum, partim ad saxa natiuis testis inhaere[n]tium?" type="straight"/>
        <underline language="LA" method="pen" text="cludit" type="straight"/>
        <underline language="LA" method="pen" text="Isque tum fusus &amp; extenuatus in fertus, tum autem concretus in nubes cogitur, humorémque colligens terram auget imbribus,              tum effluens huc &amp; illuc, ve[n]tos efficit" type="straight"/>
        <underline language="LA" method="pen" text="spiritus ductus alit" type="straight"/>
        <underline language="LA" method="pen" text="&amp; coercens caeli" type="straight"/>
        <underline language="LA" method="pen" text="extrema ora &amp; determinatio mundi" type="straight"/>
        <underline language="LA" method="pen" text="reuersiones" type="straight"/>
        <underline language="LA" method="pen" text="quadam contrahit terram, tum vicissim laetificat, vt cum caelo hilarata videatur. Luna" type="straight"/>
        <underline language="LA" method="pen" text="congrediens" type="straight"/>
        <underline language="LA" method="pen" text="tum digrediens" type="straight"/>
        <underline language="LA" method="pen" text="dubiecta atque opposita soli" type="straight"/>
        <underline language="LA" method="pen" text="radios eius &amp; lumen obscurat" type="straight"/>
        
