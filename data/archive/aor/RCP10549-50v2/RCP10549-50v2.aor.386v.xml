    <page filename="RCP10549-50v2.386v.tif" pagination="224" reader="John Dee" signature="[Oo.viii.[v]]"/>   
    <annotation>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        dimetati
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Measured.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Terrenorum item commodorum omnis est in homine dominatus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        Terrenoru[m] Commodoru[m] o[mn]is est in ho[m]i[n]e do[m]i[n]atus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Likewise the entire command of the commodities produced on land is vested in mankind.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        largitus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Lavish.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="moderationem nos soli habemus maris atque ventorum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Moderatione[m] maris &amp; ventorum nos habemus soli
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Only we have the government over the sea and the winds.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="astrorum ortus, obitus, cursúsque cognouimus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Astroru[m] Cognitio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The knowledge of the stars.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Lunae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Luna[e] [octes] qua[e], qua[n]ta[e], qua[n]do <!-- FSF: not sure about the 8; there is an es written over it; the word octes does however not exist -->
                    </marginalia_text>
                </position>
            </language>
            <translation>
                
            </translation>
        </marginalia>
        
        <marginalia anchor_text="omnia quae sint in hoc mundo, quibus vtantur hominus, hominum causa facta esse" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        O[mn]ia ho[m]i[nu]m causa facta
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Everything has been created for a use by men.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="mundus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Mundus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                World.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="circuitus Solis &amp; Lunae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Circuitus ☉ ☾
                    </marginalia_text>
                    <symbol_in_text name="Sun"/>
                    <symbol_in_text name="Moon"/>
                </position>
            </language>
            <translation>
                The orbit ofof Sun and Moon.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Terra" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Terra
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Earth.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Comparatio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Comparison.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="vbertas, &amp; varietas pomorum, eorúmque iucundus non gustatus solum, sed odoratus etiam &amp; aspectus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Pomorum iucundus gustat[us] oderatus, aspectus.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Fruits are pleasent in taste, smell and sight.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="oues" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        oues.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Sheep.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Canum vero tam fida custodia, támque amans dominorum adulatio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Canum fida custodia, et amans do[m]i[n]or[um] adulatio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The trusty watchfulness of dogs and its fawning affection for its master.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ferrea" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        ferrea a[e]tas
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The Iron Age.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="fissione glebarum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        fissio glebar[um]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Divison of land.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        coha[e]reuna
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Tied together.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="figurata" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        figurari
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be formed.
            </translation>
        </marginalia>
        
