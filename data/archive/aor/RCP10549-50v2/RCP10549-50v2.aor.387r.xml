        <marginalia anchor_text="co[n]docefactis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        condocefactus domitus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Disciplined, subdued.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="putesceret" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        putesceret
                    </marginalia_text>
                </position>
            </language>
            <translation>
                It begins to rot.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="visceribus vesci scelus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        bou[m] visceribus vesci scelus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Crime that feeds on internal organs of the oxen. <!-- FSF: not sure about this -->
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        sus
