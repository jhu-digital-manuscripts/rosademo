    <annotation>
        
        <marginalia anchor_text="Socrate" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Socrates
                    </marginalia_text>
                    <person name="Socrates"/>
                </position>
            </language>
            <translation>
                Socrates.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="co[n]tra deos disputandi" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Ne co[n]tra deos disputes.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                You shall not discuss with gods.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="suo cuique iudicio sit vtendum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Suo cuiq[ue] iudicio vtendum est
                    </marginalia_text>
                </position>
            </language>
            <translation>
                'Each man must use his own judgement' [LCL 268:286-87].
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Epicurus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Contra Epicurum
                    </marginalia_text>
                    <person name="Epicurus"/>
                </position>
            </language>
            <translation>
                Against Epicurus.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Cottam esse &amp; Pontifice[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Cotta es &amp; Pontifex
                    </marginalia_text>
                    <person name="Gaius Aurelius Cotta"/>
                </position>
            </language>
            <translation>
                Gaius Aurelius Cotta was a priest.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="populi R[omani] religio in sacta &amp; in auspicia diuisa sit, tertiu[m] adiunctu[m] sit,              siquid praedictionis causa ex porte[n]tis &amp; monstris Sibyllae interpretes, aruspicésve mouerunt" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Romanorum religio in quas secetur partes Sibylla[e]
                    </marginalia_text>
                    <person name="Sibyl"/>
                </position>
            </language>
            <translation>
                The devotion of Romans in parts of the Sibyl.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="philopho rationem accipere debeo" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Philosophus vacare r[ati]o[n]e no[n] debet
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A philosopher must not be free from reason.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Primum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        p[ri]mu[m] deinde tum postremo
                    </marginalia_text>
                </position>
            </language>
            <translation>
                First, next, then, finally.
            </translation>
        </marginalia>
        
