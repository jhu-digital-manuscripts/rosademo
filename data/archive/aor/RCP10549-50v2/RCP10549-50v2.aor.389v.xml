<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v2.389v.tif" pagination="230" reader="John Dee" signature="[Pp.iii.[v]]"/>   
    <annotation>
        
        <marginalia anchor_text="dirimi distrahíve ... secari ac diuidi" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        secari diuidi dirimi distrahi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be cut, to be divided, to be pulled apart, to be separated.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        partibilis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Divisible.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="omne animal patibilem naturam habeat" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        omne a[n]i[m]al partibilem natura[m] habet
                    </marginalia_text>
                </position>
            </language>
            <translation>
                'Every living thing is by its nature capable of feeling' [LCL 268:312-13].
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ne indiuiduum quidem, nec quod dirimi distrahíve non possit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Indiuiduum, quod nec dirimi nec distrahi p[otes]t.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Every being that cannot be pulled or torn apart.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="omne animal mortale est" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        omne a[n]i[m]al mortale est.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Every animal is mortal.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="omne animal &amp; dissolubile" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        o[mn]e animal est dissolubile
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Every animal is destructible.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Mutabilia autem sunt illa è quibus omnia constant" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Mutabilia su[n]t è quibus cuncta constant.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                'All things are composed are liable to change' [LCL 268;314-15].
            </translation>
        </marginalia>
        
        <marginalia anchor_text="omne corpus aut aqua, aut aer, aut ignis, aut terra est, aut id quod est concretu[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Omne corpus aut eleme[n]tum aut ex elementis co[n]cretum.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Every body consist either of elements or of combination of elements.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Mutua generatio eleme[n]toru[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A reciprocal generation of elements.
            </translation>
        </marginalia>

        <marginalia anchor_text="Omne enim animal sensus habet" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Omne a[n]i[m]al sensus habet.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                All animals have sensations.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Contraria versantur circa idem:
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Opposite things move about. 
            </translation>
        </marginalia>
        
        <marginalia anchor_text="nec voluptatem sentiat, nec dolore[m], id animal" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Omne a[n]i[m]al voluptatis &amp; doloris sensum capit.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Every animal holds the sensation of desire and pain.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="appetitio sit &amp; declinatio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Appetitio. Declinatio.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Appetite, divergence.
            </translation>
        </marginalia>

        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        obiectum quid veheme[n]tius, interemit
                    </marginalia_text>
                </position>
            </language>
            <translation>
                What kills that which is thrown before you more vehemently.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="simplex est natura animantis ... aut condretum ex pluribus naturis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Anima[n]tis natura no[n] simplex sed ex plurimis naturis co[n]creta.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The nature of the animals is not simple, but composed of different natures.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Locus rerum naturalium.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The place for all natural things.
            </translation>
        </marginalia>

        <marginalia anchor_text="Heraclitum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Heraclitus
                    </marginalia_text>
                    <person name="Heraclitus of Ephesus"/>
                </position>
            </language>
            <translation>
                Heraclitus.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        sic aristot[eles] in lib[ro] de lo[n]g[itudine]
                    </marginalia_text>
                    <person name="Aristotle"/>
                    <book title="On Length and Shortness of Life"/> 
                </position>
            </language>
            <translation>
                So Aristotle in his book On Length and Shortness of Life. 
            </translation>
        </marginalia>
        
        <marginalia anchor_text="animus, ex quo anima" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Animus. anima
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Mind. Intellect.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>
                        ex quib[us] { efficitur cogitur
                    </marginalia_text>
                </position>
            </language>
            <translation>
                From these it is brought about, gathered.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="ne indiuiduum quidem, nec quod dirimi distrahíve non possit" type="straight"/>
        <underline language="LA" method="pen" text="patibilem" type="straight"/>
        <underline language="LA" method="pen" text="extrinsecus, id quasi ferendi &amp; patiendi necessitatem. Et si omne" type="straight"/>
        <underline language="LA" method="pen" text="secari ac diuidi" type="straight"/>
        <underline language="LA" method="pen" text="mortale igitur omne animal &amp; dissolubile &amp; diuiduum sit necesse" type="straight"/>
        <underline language="LA" method="pen" text="igitur si omnia quae sunt è quibus cuncta constant, mutabilia sunt, nullum corpus esse potest non mutabile" type="straight"/>
        <underline language="LA" method="pen" text="sit. Etenim omne corpus aut aqua, aut aer, aut ignis, aut terra est, aut id quod est concretu[m] ex his, aut ex aliqua parte eoru[m]:              horum aute[m] nihil est quin intereat. Na[m] &amp; terrenum omne diuiditur: &amp; humor ita mollis est, vt facile premi collidíque possit: ignis vero &amp; aer omni impulsu facillime pellitur" type="straight"/>
        <underline language="LA" method="pen" text="quúmque eadem vicissim retro commeant" type="straight"/>
        <underline language="LA" method="pen" text="accipere, &amp; non accipere" type="straight"/>
        <underline language="LA" method="pen" text="appetitio sit &amp; declinatio naturalis" type="straight"/>
        <underline language="LA" method="pen" text="sunt ex quibus effici cogíque possit" type="straight"/>
        <underline language="LA" method="pen" text="vt cetera, quu[m] amplificata" type="straight"/>
        <underline language="LA" method="pen" text="quò naturae vi efferatur" type="straight"/>
        <underline language="LA" method="pen" text="Haec ad quodda[m] tempis cohaerere possunt" type="straight"/>
        <underline language="LA" method="pen" text="Necesse est enim in suu[m] quaeque locum natura rapiatur" type="straight"/>
        <underline language="LA" method="pen" text="omnes interpretantur vno modo" type="straight"/>
        <underline language="LA" method="pen" text="omnem vim" type="straight"/>
        <underline language="LA" method="pen" text="defecerit tu[m] interire" type="straight"/>
        <underline language="LA" method="pen" text="in omni natura rerum id viuere, id vigere quod caleat. Ego" type="straight"/>
        
        <symbol name="Jupiter" place="left_margin"/>
        <symbol name="Jupiter" place="left_margin"/>
        
