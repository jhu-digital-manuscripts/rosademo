        <marginalia anchor_text="Veram enim fuisse obnunciationem, vt ait idem Augur &amp; Censor, exitus approbauit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        vera[m] esse obnu[n]ciatione[m] exitus comprobat
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The death proves that the omen was true.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="nunciant euentura nisi prouideris" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Nuncia[n]t futura nisi p[ro]uideris
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The shall announce future events unless you foresee them.
            </translation>
        </marginalia>
        
