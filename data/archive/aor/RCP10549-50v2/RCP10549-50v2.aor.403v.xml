    <annotation>
        
        <marginalia anchor_text="" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Tonitru à sinistris</marginalia_text>
                </position>
            </language>
            <translation>By thunder from the left.</translation>
        </marginalia>
        
        <marginalia anchor_text="auguratus pastoralis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Auguratus pastoralis</marginalia_text>
                </position>
            </language>
            <translation>The augural art was pastoral.</translation>
        </marginalia>
        
        <marginalia anchor_text="dant operam simul auspicio augurióque. Hic Remus auspicio se deuouet, atque secundam              Solus auem seraut: at Romulus pulcher in alto" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Romulus &amp; Remus auspicio dant op[er]am.</marginalia_text>
                    <person name="Romulus"/>
                    <person name="Remus"/>
                </position>
            </language>
            <translation>Romulus and Remus gave attention to the auspicies.</translation>
        </marginalia>
        
        <marginalia anchor_text="artificiosae praesensionis facilis, diuinae autem paulò" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Artificiosa Diuinatio.</marginalia_text>
                </position>
            </language>
            <translation>Artificial divination.</translation>
        </marginalia>
        
        <marginalia anchor_text="obseruatione diuturna" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>obseruatio diuturna</marginalia_text>
                </position>
            </language>
            <translation>A long-lasting observation.</translation>
        </marginalia>
        
        <marginalia anchor_text="vetustas" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Vetustas</marginalia_text>
                </position>
            </language>
            <translation>A long duration.</translation>
        </marginalia>
        
        <marginalia anchor_text="haustos animos &amp; libatos habemus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Animi hausti et libati</marginalia_text>
                </position>
            </language>
            <translation>Souls drawn and poured forth.</translation>
        </marginalia>
        
        <marginalia anchor_text="natura futura praesentiunt" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Natura futura p[rae]se[n]tiu[n]t</marginalia_text>
                </position>
            </language>
            <translation>They predict the future [events] from nature.</translation>
        </marginalia>
        
        <marginalia anchor_text="prudentes possumus dicere, id est prouidentes" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Prude[n]tes, id est, p[rou]inde[n]tes</marginalia_text>
                </position>
            </language>
            <translation>'"Foresighted"—that is, "able to foresee the future"' [Loeb 154, 343].</translation>
        </marginalia>
        
        <marginalia anchor_text="Milesiu[m] Thalem" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Thales</marginalia_text>
                    <person name="Thales of Miletus"/>
                </position>
            </language>
            <translation>Thales of Miletus.</translation>
        </marginalia>
        
        <marginalia anchor_text="defectione[m] Solis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>☉ defictus</marginalia_text>
                    <symbol_in_text name="Sun"/>
                </position>
            </language>
            <translation>Solar eclipse.</translation>
        </marginalia>
        
