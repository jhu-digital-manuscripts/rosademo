        <underline language="LA" method="pen" text="Fieri igitur omnia fato ratio cogit fateri. Fatu[m] autem id appello, quod Graeci" type="straight"/>
        <underline language="EL" method="pen" text="" type="straight"/>
        <underline language="LA" method="pen" text="id est, ordine[m] seriémque causarum, quu[m] causae causa nexa re[m] ex se gignat. Ea est ex omni aeternitate fluens ceritas sempiterna" type="straight"/>
        <underline language="LA" method="pen" text="supersitiose" type="straight"/>
        <underline language="LA" method="pen" text="colligatione[m]" type="straight"/>
        <underline language="LA" method="pen" text="est quasi rudentis explicatio, sic traductio temporis" type="straight"/>
        <underline language="LA" method="pen" text="noui efficientis, &amp; primu[m] quique replicantis. Quod" type="straight"/>
        <underline language="LA" method="pen" text="progignuntur" type="straight"/>
        <underline language="LA" method="pen" text="diuturnitate pertractata notaureunt, aut semper, aut" type="straight"/>
        <underline language="LA" method="pen" text="euisdemmodi" type="straight"/>
        <underline language="LA" method="pen" text="Vt enim deoru[m] animi sine oculis, sine auribus" type="straight"/>
        <underline language="LA" method="pen" text="Na[m] si obscurior quasi caliginosa stella extiterit, pingue &amp; concretu[m] esse coelum, vt eius aspiratio grauis &amp; pestilens futura sit:              sin illustris &amp; perlucida stella apparuerit, significari coelum esse tenue purúmque, &amp; propterea salubre. Democritus" type="straight"/>
        <underline language="LA" method="pen" text="Plúsque ex alieno iecore sapiunt quàm ex suo" type="straight"/>
        <underline language="LA" method="pen" text="Quidquid est hoc, omnia animat, format, alit, auget, creat" type="straight"/>
        
