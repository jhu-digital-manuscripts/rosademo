            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Philosophi.</marginalia_text>
                </position>
            </language>
            <translation>Philosophers.</translation>
        </marginalia>
        
        <marginalia anchor_text="sapientes" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Sapientes</marginalia_text>
                </position>
            </language>
            <translation>Sages.</translation>
        </marginalia>
        
        <marginalia anchor_text="Dialecticis aut physicis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Dialectici. Physici</marginalia_text>
                </position>
            </language>
            <translation>Logicians and natural philosophers.</translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="non Latinis literis illustratus" type="straight"/>
        <underline language="LA" method="pen" text="quibus ita prolapsa est, vt omniu[m] opibus refrenanda ac coerce[n]da sit. Nec vero" type="straight"/>
        <underline language="LA" method="pen" text="posse co[n]fido, quod ne postulandu[m] quidem est, vt omnes adolescentes se ad haec studia co[n]uertant: pauci vtina[m], quorum tame[n]              in Remp. late patere poterit industria" type="straight"/>
        <underline language="LA" method="pen" text="Magnificu[m] illud etiam" type="straight"/>
        <underline language="LA" method="pen" text="hominibus gloriosum" type="straight"/>
        <underline language="LA" method="pen" text="Dabunt igitur mihi veniam mei ciues, vel gratiam potius habebu[n]t, quòd quu[m] esset in vnius potestate Respub[lica] neque ego me abdidi, neque              deserui, neque afflixi, neque ita gessi, quasi homini aut te[m]poribus iratus: neque porrò ita aut adulatus, aut admiratus fortuna[m] sum alterius, vt me meae poeniteret. Id enim" type="straight"/>
        <underline language="LA" method="pen" text="Reru[m]p[ublicarum] vt eae tum à principibus tenerentur, tu[m] à populis, aliquando à singulis. Quod" type="straight"/>
        <underline language="LA" method="pen" text="In libris enim sententia[m] dicebamus, concionabamur, philosophiam nobis pro Reipub[licae] procuratione substituta[m] putabamus. Nunc" type="straight"/>
        <underline language="LA" method="pen" text="delectat, plurimis nostris exemplis vsus es, &amp;" type="straight"/>
        <underline language="LA" method="pen" text="Dicendum est mihi igitur ad ea quae sunt à te dicta, sed ita, nihil vt affirmem: quaeram omnia, dubitans plerunque,             &amp; mihi ipse diffidens. si enim aliquid certi haberem quod dicerem, ego ipse diuinare[m], qui esse diuinatione[m] nego. Etenim" type="straight"/>
        <underline language="LA" method="pen" text="prouisione aut premotione me[n]tis magis" type="straight"/>
        <underline language="LA" method="pen" text="Ad nullam igitur" type="straight"/>
        <underline language="LA" method="pen" text="Ad sapientes haec, non ad diuinos referri solent" type="straight"/>
        
