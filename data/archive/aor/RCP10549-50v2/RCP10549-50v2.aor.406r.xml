        </marginalia>
        
        <marginalia anchor_text="earum reru[m] quae sensu accipiuntur, diuinatio abhidetur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Diuinatio no[n] est earum rerum qua[e] artibus co[n]tine[n]t</marginalia_text>
                </position>
            </language>
            <translation>Diviniation is not of those things which are sustained by knowledge.</translation>
        </marginalia>
        
        <marginalia anchor_text="diuinare dicuntur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Diuini qua[e] dicu[n]t</marginalia_text>
                </position>
            </language>
            <translation>The things the prophets say.</translation>
        </marginalia>
        
