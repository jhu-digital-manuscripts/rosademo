    <annotation>
        
        <marginalia anchor_text="munus Reipub. afferre maius meliúsve possumus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Maximu[m] munis quod reip[ublicae]. adferre possumus</marginalia_text>
                </position>
            </language>
            <translation>What [is] greatest service to the commonwealth we can render.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Inue[n]tus</marginalia_text>
                </position>
            </language>
            <translation>Discovered.</translation>
        </marginalia>
        
        <marginalia anchor_text="Mangificu[m] illud etiam, Romanísque hominibus gloriosum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Magnificu[m] et gloriosu[m]</marginalia_text>
                </position>
            </language>
            <translation>Splendid and glorious.</translation>
        </marginalia>
        
        <marginalia anchor_text="casus grauis ciuitatis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Casus grauis Ciuitatis</marginalia_text>
                </position>
            </language>
            <translation>The grave condition of the state.</translation>
        </marginalia>
        
        <marginalia anchor_text="Dabunt igitur mihi veniam mei ciues, vel gratiam potius habebu[n]t, quòd quu[m] esset in vnius potestate Respub. neque ego me abdidi, neque              deserui, neque afflixi, neque ita gessi, quasi homini aut te[m]poribus iratus: neque porrò ita aut adulatus, aut admiratus fortuna[m] sum alterius, vt me meae poeniteret" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Cic[ero] Constantia</marginalia_text>
                    <person name="Cicero"/>
                </position>
            </language>
            <translation>Cicero's constancy.</translation>
        </marginalia>
        
        <marginalia anchor_text="Platone" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Plato.</marginalia_text>
                    <person name="Plato"/>
                </position>
            </language>
            <translation>Plato.</translation>
        </marginalia>
        
        <marginalia anchor_text="naturales esse quasfam conuersiones Reru[m]p[ublicum]" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Naturales co[n]uersiones rerump[ublicum].</marginalia_text>
                </position>
            </language>
            <translation>Revolutions in government are to be expected [Loeb 154, 377].</translation>
        </marginalia>
        
        <marginalia anchor_text="In libris enim sententia[m] dicebamus, concionabamur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>In libris se[n]tentia[m] dicam[us] concionamur [etc].</marginalia_text> 
                </position>
            </language>
            <translation>'It was in my books that I made my senatorial speeches and my forensic harangues', et cetera [Loeb 154, 377].</translation>
        </marginalia>
        
        <marginalia anchor_text="Quintus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Quintus Cicero.</marginalia_text>
                    <person name="Quintus Tullius Cicero"/>
                </position>
            </language>
            <translation>Quintus Cicero.</translation>
        </marginalia>
        
        <marginalia anchor_text="Stoicorum sententiam" hand="English_secretary">
