                    <marginalia_text>ψευδόμενος</marginalia_text> 
                </position>
            </language>
            <translation>Liar.</translation>
        </marginalia>
        
        <marginalia anchor_text="verbo liceat" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Verborum vsus.</marginalia_text>
                </position>
            </language>
            <translation>The use of words.</translation>
        </marginalia>
        
        <marginalia anchor_text="earu[m] rerum quae subiectae sensibus sunt, vlla diuinatio" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Quaru[m] reru[m] no[n] sit Diuninationis</marginalia_text>
                </position>
            </language>
            <translation>Those things which are not part of divination.</translation>
        </marginalia>
        
        <marginalia anchor_text="omnium diuinatio est, vt ratio docuit: nec locus nec materia" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Diuninatio[n]is nulla materia</marginalia_text>
                </position>
            </language>
            <translation>No matter of divination.</translation>
        </marginalia>
        
