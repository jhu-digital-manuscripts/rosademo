                <position book_orientation="0" place="left_margin">
                    <marginalia_text>ventoru[m] et imbriu[m] p[rae]se[n]tio fortuita</marginalia_text>
                </position>
            </language>
            <translation>Fortuitous premonition of winds and rain.</translation>
        </marginalia>
        
        <marginalia anchor_text="diuinis tribuenda putas, sed peritis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Diuini. Periti</marginalia_text>
                </position>
            </language>
            <translation>Prophets. Skilled people.</translation>
        </marginalia>
        
        <marginalia anchor_text="M. Marcellum illu[m] qui ter Consul fuit, multis annis antè dixisset naufragio esse perituru[m]" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>M. Marcellus naufragio perijt.</marginalia_text>
                    <person name="Marcus Claudius Marcellus"/>
                </position>
            </language>
            <translation>Marcus Marcellus perished in a shipwreck.</translation>
        </marginalia>
        
        <marginalia anchor_text="rerum quae in fortuna positae sunt praesensio, diuinatio est." hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Quod no[n] fortuitaru[m] rerum sit vlla Diuinatio</marginalia_text>
                </position>
            </language>
            <translation>That divination is not about accidental matters.</translation>
        </marginalia>
        
        <marginalia anchor_text="fortuna" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>fortuna</marginalia_text>
                </position>
            </language>
            <translation>Fortune.</translation>
        </marginalia>
        
        <marginalia anchor_text="ipsi saepe falluntur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>falluntur periti</marginalia_text>
                </position>
            </language>
            <translation>The skillful are deceived.</translation>
        </marginalia>
        
        <marginalia anchor_text="stellae traiectio" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Stella[e] traiectio.</marginalia_text>
                </position>
            </language>
            <translation>The motion of a star.</translation>
        </marginalia>
        
        <marginalia anchor_text="Solis defectiones, itémque lunae praedicu[n]tur in multos annos" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Solis defectio[n]es et lun[a]e in multos predicu[n]t[u]r annos</marginalia_text>
                </position>
            </language>
            <translation>Eclipses of the sun and the moon are predicted for many years.</translation>
        </marginalia>
        
        <marginalia anchor_text="naturae necessitas" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Natura[e] necessitas</marginalia_text>
                </position>
            </language>
            <translation>The necessity of nature.</translation>
        </marginalia>
        
        <marginalia anchor_text="temere sit, caeco casu &amp; volubilitate fortunae" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>temere ca[e]co casu volubilitate fortun[a]e.</marginalia_text>
                </position>
            </language>
            <translation>Rashly; by blind chanc; by the twist of fortune.</translation>
        </marginalia>
        
