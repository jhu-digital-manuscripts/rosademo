        </marginalia>
        
        <marginalia anchor_text="Imperator" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Imperator.</marginalia_text>
                </position>
            </language>
            <translation>General.</translation>
        </marginalia>
        
        <marginalia anchor_text="Diuinatione[m]" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Diuinatio quid.</marginalia_text>
                </position>
            </language>
            <translation>What [is] divination?</translation>
        </marginalia>
        
        <marginalia anchor_text="ventos aut imbres impende[n]tes quibusdam praesentire signis, in quo nostra quaedam Aratea memoriter à te              pronu[n]ciata suntL etsi haec ipsa fortuita" hand="English_secretary">
