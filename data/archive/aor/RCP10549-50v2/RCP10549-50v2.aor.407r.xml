<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v2.407r.tif" pagination="265" reader="John Dee"/> 
    <annotation>
                
        <marginalia anchor_text="contrarium rationi &amp; constantiae quàm fortuna" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>fortuna. R[ati]o. consta[n]tia</marginalia_text>
                </position>
            </language>
            <translation>Fortune, reason, constancy.</translation>
        </marginalia>
        
        <marginalia anchor_text="" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Dei pra[e]scie[n]tia</marginalia_text>
                </position>
            </language>
            <translation>The prescience of god.</translation>
        </marginalia>
        
        <marginalia anchor_text="omnia que fiu[n]t, quá[e]que futura sunt, ex omni aeternitate definita dicis esse fataliter" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>An o[mn]ia qua[e] futura sunt, ex o[mn]i a[e]ternitate definita sunt fataliter</marginalia_text>
                </position>
            </language>
            <translation>Whether all things which are in the future, have been inevitably determined from all eternity.</translation>
        </marginalia>
        
        <marginalia anchor_text="Anile sanè plenu[m] supersitionis fati nomen" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Anile nome[n] fatum.</marginalia_text>
                </position>
            </language>
            <translation>The name fate is like an old woman.</translation>
        </marginalia>
        
        <marginalia anchor_text="Deiotarum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Deiotarus</marginalia_text>
                    <person name="Deiotarus"/>
                </position>
            </language>
            <translation>Deiotarus.</translation>
        </marginalia>
        
        <marginalia anchor_text="classes Populi R. bello Punico" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Classes Pop[uli] Ro[mani]</marginalia_text>
                </position>
            </language>
            <translation>Fleets of the Romans.</translation>
        </marginalia>
        
        <marginalia anchor_text="lacum Trasime" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>lacum Trasimenum.</marginalia_text>
                    <location name="Lake Trasimeno"/>
                </position>
            </language>
            <translation>Lake Trasimeno.</translation>
        </marginalia>
        
        <marginalia anchor_text="mutari enim fata non possunt" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Mutari fata no[n] possu[n]t</marginalia_text>
                </position>
            </language>
            <translation>Destinies cannot be altered.</translation>
        </marginalia>
        
        <marginalia anchor_text="nihil nos admonere potest vt cautiores simus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Nihil nos admonere p[otes]t [fatu[m] - deleted] vt cautiores simus Divin[ationis]</marginalia_text>  
                </position>
            </language>
            <translation>Nothing can admonish us to be more cautious of divination.</translation> 
        </marginalia>
        
        <marginalia anchor_text="vtilem quidem arbitror esse nobis futuraru[m]" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Non esse vtile[m] Diuinatione[m]</marginalia_text>
                </position>
            </language>
            <translation>Divination is not useful.</translation>
        </marginalia>
        
        <marginalia anchor_text="Priamo" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Priamus.</marginalia_text>
                </position>
            </language>
            <translation>Priam.</translation>
        </marginalia>
        
        <marginalia anchor_text="M. Crassus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>M[arcus]. Crassus.</marginalia_text>
                    <person name="Marcus Licinius Crassus"/>
                </position>
            </language>
            <translation>Marcus Crassus.</translation>
        </marginalia>
        
        <marginalia anchor_text="Cn. Pompeium" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Cn. Pompeius</marginalia_text>
                    <person name="Gnaeus Pompeius"/>
                </position>
            </language>
            <translation>Gnaeus Pomeius.</translation>
        </marginalia>
        
        <marginalia anchor_text="Caesre[m]" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Ca[e]sar.</marginalia_text>
                    <person name="Julius Caesar"/>
                </position>
            </language>
            <translation>Caesar.</translation>
        </marginalia>
        
        <marginalia anchor_text="ignoratio futurorum malorum vtilior est quàm scientia" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Ïgnoratio futuroru[m] maloru[m] vtilior q[uam] scientia.</marginalia_text>
                </position>
            </language>
            <translation>Ignorance of future ills is more profitable than knowledge of them.</translation>
        </marginalia>
        
        <marginalia anchor_text="omnia fato: nihil ergo illis profuisset diuinare" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>si fato o[m]nia euenia[n]t nihil prodest diuinare.</marginalia_text>
                </position>
            </language>
            <translation/>
        </marginalia>
        
        <marginalia anchor_text="fortuna" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>fortuna</marginalia_text>
                </position>
            </language>
            <translation>Fortune.</translation>
        </marginalia>
        
        <marginalia anchor_text="à fabulis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="intext">
                    <marginalia_text>historia troiana fabula est</marginalia_text> <!-- M.B. Not sure of the first word, but I think Dee is referencing 'Historia Troiana' by  Guido delle Colonne. See: https://en.wikipedia.org/wiki/Historia_destructionis_Troiae  -->
                    <book title="Historia Troiana"/>
                </position>
            </language>
            <translation>The 'Historia Troiana' is a fable.</translation>
        </marginalia>
        
        <marginalia anchor_text="cooptasset" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>coopto.</marginalia_text>
                </position>
            </language>
            <translation>I co-opt.</translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="ex omni aeternitate definita dicis esse fataliter" type="straight"/>
        <underline language="LA" method="pen" text="omni aeternitate certum fuerit esse" type="straight"/>
        <underline language="LA" method="pen" text="Anile sanè &amp; plenu[m] superstitionis fati nomen ipsum" type="straight"/>
        <underline language="LA" method="pen" text="sed tamen" type="double"/>
        <underline language="LA" method="pen" text="apud" type="straight"/>
        <underline language="LA" method="pen" text="at id neque si fatum fuerat effugisset: nec fi no[n] fuerat, in eum casum incidisset. Quid" type="double"/>
        <underline language="LA" method="pen" text="Poenis depressam" type="double"/>
        <underline language="LA" method="pen" text="fiet tamen illud quod futurum est. Sin autem id potest flecti, nullum est fatum: ita ne diuinatio quidem,              quoniam ea rerum futurarum est. Nihil autem est pro certo futurum quod potest aliqua procuratione accidere ne fiat. Atque ego ne vtilem quidem arbitror esse nobis              futuraru[m] rerum scientiam. Quae enim" type="double"/>
        <underline language="LA" method="pen" text="à fabulis" type="double"/>
        <underline language="LA" method="pen" text="ipse cooptasset" type="double"/>
        <underline language="LA" method="pen" text="Quid enim posset iis esse laetum, exitus suos cogitantibus? Ita" type="double"/>
        <underline language="LA" method="pen" text="eorum solertia" type="double"/>
        
