    <annotation>
        
        <marginalia anchor_text="leuis armaturae prima orationis excursio" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>Leuis armatur[a]e p[r]ima excursio.</marginalia_text>
                </position>
            </language>
            <translation>The first part of my discussion [has been attacking] with light infantry.</translation>
        </marginalia>
        
        <marginalia anchor_text="leuius casura rebus diuinis procuratis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Quod leuius no[n] sint Casura</marginalia_text>
                </position>
            </language>
            <translation>What is lighter may not befall.</translation>
        </marginalia>
        
        <marginalia anchor_text="Homerus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Homerus</marginalia_text>
                    <person name="Homer"/>
                </position>
            </language>
            <translation>Homer.</translation>
        </marginalia>
        
        <marginalia anchor_text="Si enim prouideri nihil potest futurum esse eorum quae casu fiunt, quia esse certa non possunt: diuinatio nulla est. Sin autem idcirco possunt prouideri,              quia certe sunt &amp; fataliaL rusus diuinatio nulla est, eam enim tu fortuitaru[m] rerum esse dicebas." hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Dilemma</marginalia_text>
                </position>
            </language>
            <translation>A dilemma.</translation>
        </marginalia>
        
        <marginalia anchor_text="Duo enim genera diuinandi" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Duo diuinandi g[e]n[er]as.</marginalia_text>
                </position>
            </language>
            <translation>Two kinds of divination.</translation>
        </marginalia>
        
        <marginalia anchor_text="extrinsecus ex diuinitate, vnde omnes animos haustos aut acceptos, aut libatos haberemus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Ex Diuinitate extrinsecus o[mn]es animos haustos habemus</marginalia_text>
                </position>
            </language>
            <translation>We all have souls drawn off from a source outside itself [Loeb 154, 399].</translation>
        </marginalia>
        
        <marginalia anchor_text="diuinationem omne[m] à tribus rebus, à deo, à fato, à natura" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>O[mn]is Diuniatio à tribus rebus à Deo, à fato, a natura.</marginalia_text>
                </position>
            </language>
            <translation>All divination comes from three things: God, fate, and nature.</translation>
        </marginalia>
        
        <marginalia anchor_text="Argumentis &amp; rationibus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Argume[n]ta &amp; r[ati]o[n]es.</marginalia_text>
                </position>
            </language>
            <translation>Arguments and reasons.</translation>
        </marginalia>
        
        <marginalia anchor_text="extis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>De extis.</marginalia_text>
                </position>
            </language>
            <translation>Concerning entrails.</translation>
        </marginalia>
        
        <marginalia anchor_text="nec esse vnam omnium disciplinam" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>No[n] est vna o[mn]iu[m] disciplina in extis explicandis</marginalia_text>
                </position>
            </language>
            <translation>There is not one practice for everyone regarding interpreting entrails. </translation>
        </marginalia>
        
        <marginalia anchor_text="diuina cum rerum natura" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Diuina natura.</marginalia_text>
                </position>
            </language>
            <translation>Divine nature.</translation>
        </marginalia>
        
        <marginalia anchor_text="Democritus tamen non inscite nugatur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Democriti Dictum.</marginalia_text>
                    <person name="Democritus"/>
                </position>
            </language>
            <translation>Democritus' saying.</translation>
        </marginalia>
        
        <marginalia anchor_text="O mortalem beatum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>ò mortale[m] beatu[m]</marginalia_text>
                </position>
            </language>
            <translation>O happy mortal.</translation>
        </marginalia>
        
        <marginalia anchor_text="Pherecydeum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
