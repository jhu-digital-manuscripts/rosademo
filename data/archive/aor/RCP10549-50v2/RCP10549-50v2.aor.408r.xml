    <annotation>
        
        <marginalia anchor_text="cuiusda[m]modi" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>cuiusdammodi</marginalia_text>
                </position>
            </language>
            <translation>Of whatever kind.</translation>
        </marginalia>
        
        <marginalia anchor_text="lucello meo, aut meus quaesticulus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>lucellum qu[a]esticulus</marginalia_text>
                </position>
            </language>
            <translation>Small gain and small profit</translation>
        </marginalia>

        <marginalia anchor_text="credere omnia vide ne non sit necesse" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Credere o[mn]ia no[n] est neccesse</marginalia_text>
                </position>
            </language>
            <translation>It is not necessary to believe everything.</translation>
        </marginalia>
        
        <marginalia anchor_text="Ab acqua, aut ab igni pericula monent" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Hydromantεια. Pyroma[n]teia.</marginalia_text> 
                </position>
            </language>
            <translation>Hydromancy, pyromancy.</translation>
        </marginalia>
        
        <marginalia anchor_text="ars vlla rerum incognitarum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Ars an sit reru[m] incognitaru[m]</marginalia_text>
                </position>
            </language>
            <translation>Whether there may be an art of unknown things.</translation>
        </marginalia>
        
        <marginalia anchor_text="omne quod esset, vnum esse dixerunt" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Omnia vnum.</marginalia_text>
                </position>
            </language>
            <translation>All things are one.</translation>
        </marginalia>
        
        <marginalia anchor_text="rerum aute[m] natura, quam cognationem" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Reru[m] naturaliu[m] cognatio</marginalia_text>
                </position>
            </language>
            <translation>The relationship of natural things.</translation>
        </marginalia>
        
        <marginalia anchor_text="musculorum iescula" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Musculoru[m] iecuscula.</marginalia_text>
                </position>
            </language>
            <translation>The livers of mice.</translation>
        </marginalia>
        
        <marginalia anchor_text="conchiliis omnibus contingere" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Conchiliu[m] natura</marginalia_text>
                </position>
            </language>
            <translation>The nature of shellfish.</translation>
        </marginalia>
        
        <marginalia anchor_text="acessus &amp; recessus, lunae" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Accessus et recessus maris a ☾</marginalia_text>
                    <symbol_in_text name="Moon"/>
                </position>
            </language>
            <translation>The ebbs and flows of the sea by the moon.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary"> 
            <language ident="EL">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>συμπάθεια</marginalia_text>
                </position>
            </language>
            <translation>Affinity.</translation>
        </marginalia>
        
        <marginalia anchor_text="vim quandam sentientem atque diuinam, quae tota confusa mundo" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Vis diuina tota co[n]fusa mu[n]do</marginalia_text>
                </position>
            </language>
            <translation>The entire divine force that pervades the universe.</translation>
        </marginalia>
        
        <marginalia anchor_text="deorum enim numini parêre omnia" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>omnia Deoru[m] numini pare[n]t</marginalia_text>
                </position>
            </language>
            <translation>All things are obedient to the divine will of the gods.</translation>
        </marginalia>
        
        <marginalia anchor_text="tauri opimi extis immolante Caesare cor" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Cor tauri im[m]ola[n]te ca[e]sare</marginalia_text>
                    <person name="Julius Caesar"/>
                </position>
            </language>
            <translation>The heart of a bull when Caesar was offering a sacrifice.</translation>
        </marginalia>
        
        <marginalia anchor_text="Physiologiam" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Physiologia.</marginalia_text>
                </position>
            </language>
            <translation>Physiology.</translation>
        </marginalia>
        
        <marginalia anchor_text="ex nihilo oriatur, aut in nihilum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Ex nihilo fit nihil</marginalia_text>
                </position>
            </language>
            <translation>Nothing comes from nothing.</translation>
        </marginalia>
        
        <marginalia anchor_text="dissolui" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>dissolui.</marginalia_text>
                </position>
            </language>
            <translation>To be dissolved.</translation>
        </marginalia>
        
        <marginalia anchor_text="immolatoris" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>immolator</marginalia_text>
                </position>
            </language>
            <translation>A sacrificer.</translation>
        </marginalia>
        
        <marginalia anchor_text="exile &amp; exiguum &amp; vietum cor" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>cor exile exiguu[m] vietum</marginalia_text>
                </position>
            </language>
            <translation>Heart: feeble, small and withered.</translation>
        </marginalia>
        
        <marginalia anchor_text="Concedam hoc ipsum si vis, esti magnam iacturam causae fecero" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>co[n]ceda[m] hoc ipsu[m] si vis, esti magna[m] caus[a]e iactua[m] fecero.</marginalia_text>
                </position>
            </language>
            <translation>'I will concede even this if you wish, though it will greatly weaken my case' [Loeb 154, 409].</translation>
        </marginalia>
        
        <marginalia anchor_text="" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>alea fortuna temeritas</marginalia_text>
                </position>
            </language>
            <translation>A game of dice, fortune, temerity.</translation>
        </marginalia>
        
        <marginalia anchor_text="nihil videtur esse dirius" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>nihil dirius</marginalia_text>
                </position>
            </language>
            <translation>Nothing more horrible.</translation>
        </marginalia>
        
        <marginalia anchor_text="Vrbem philosophiae mihi crede proditis, dum castella defenditis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>vrbe[m] philosophi, mihi crede p[ro]ditis, du[m] castella defe[n]ditis</marginalia_text>
                </position>
            </language>
            <translation>'Upon my word you [Stoics] surrender the very city of philosophy while defending its outworks' [Loeb 154, 411].</translation>
        </marginalia>
        
        <marginalia anchor_text="co[n]centu, atque co[n]sensu, qua[m] συμπάθειαν Graeci appellant, co[n]uenire" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>co[n]sensus co[n]venie[n]tia co[n]iunctio cognatio. [co[n]vene[n]tia - deleted] contagio</marginalia_text>
                </position>
            </language>
            <translation>Consensus, covenant, conjunction, affinity, [covenant - deleted], contact.</translation>
        </marginalia>
        
        <marginalia anchor_text="naturae, &amp; quasi co[n]centu, atque co[n]sensu qua[m] συμπάθειαν" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>natura[e]</marginalia_text>
                </position>
            </language>
            <language ident="EL">
                <position book_orientation="0" place="tail">
                    <marginalia_text>συμπάθειαν</marginalia_text> <!-- M.B. Greek should be checked. -->
                </position>
            </language>
            <translation>Nature.</translation>
        </marginalia>
        
