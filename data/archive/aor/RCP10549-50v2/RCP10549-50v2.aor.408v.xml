        <marginalia anchor_text="iactúsque fulminum exctimuissent, credidisse ea efficere rerum onium praepotentem Iouem" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>fulminu[m] iactus à Ioue credebatur fiéri.</marginalia_text>
                    <person name="Jupiter"/>
                </position>
            </language>
            <translation>It is believed that lightining is made by Jupiter.</translation>
        </marginalia>
        
        <marginalia anchor_text="quidquam certi significari rebus incertis" hand="English_secretary">
            <language ident="LA">
