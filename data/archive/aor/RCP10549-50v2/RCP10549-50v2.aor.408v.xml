        <marginalia anchor_text="iactúsque fulminum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>Iactus fulminu[m]</marginalia_text>
                </position>
            </language>
            <translation>Lightening bolts.</translation>
        </marginalia>
        
        <marginalia anchor_text="inducitis in eas captiones" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>inducere in captiones. se mirifica[m] in latebra[m] co[n]iecisti premere alique[m].</marginalia_text>
                </position>
            </language>
            <translation>To lead into these sophistries. You throw yourself in a wonderful hiding place. To overwhelm someone.</translation>
        </marginalia>
        
        <marginalia anchor_text="quàm quu[m] foruito hostiae adducantur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Quid diuini sit in hostia</marginalia_text>
                </position>
            </language>
            <translation>What of divinity may there be in a sacrifice?</translation>
        </marginalia>
        
        <marginalia anchor_text="agnum" hand="English_secretary">
