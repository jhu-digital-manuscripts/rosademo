        <underline language="LA" method="pen" text="perinde" type="double"/>
        <underline language="LA" method="pen" text="vtcunque temperatus sit aer, ita pueros orientes animari atque formari ex eóque ingenia, mores,              animum corpus, actionem vitae, casus cuiusque euentúsque fingi" type="straight"/>
