    <annotation>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>ho[m]i[nu]m ortus p[ro]creatio</marginalia_text>
                </position>
            </language>
            <translation>The birth and procreation of men.</translation>
        </marginalia>
        
        <marginalia anchor_text="astro ingenerata &amp; tradita" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>astro ingenerata tradita</marginalia_text>
                </position>
            </language>
            <translation>Implanted and handed over to a star.</translation>
        </marginalia>
        
        <marginalia anchor_text="linguae sic inhaererent" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>lingua inha[e]ret</marginalia_text>
                </position>
            </language>
            <translation>He is 'tongue-tied' [Loeb 154, 479].</translation>
        </marginalia>
        
        <marginalia anchor_text="multi etiam naturae vitium meditatione atque exercitatione sustulerunt" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Natura[e] vitiu[m] meditatio[n]e atq[ue] exercitatione multi sustuleru[n]t</marginalia_text>
                </position>
            </language>
            <translation>'Many more have corrected a natural defect by intelligent exertion' [Loeb 154, 479].</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Ptoloma[e]us concedit libro. 2⁰</marginalia_text>
                    <person name="Ptolemy"/>
                </position>
            </language>
            <translation>Ptolemy conceded [this] in book two.</translation>
        </marginalia>
        
        <marginalia anchor_text="plus terrarum situs, quàm lunae ad nascendum valere" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Plus terrar[u]m situs q[uam] ☾ tractus ad nascendu[m] valit.</marginalia_text>
                    <symbol_in_text name="Moon"/>
                </position>
            </language>
            <translation>'One’s birth is more affected by local environment than by the condition of the moon' [Loeb 154, 479].</translation>
        </marginalia>
        
        <marginalia anchor_text="Panaetius" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Pana[e]tius</marginalia_text>
                    <person name="Panaetius"/>
                </position>
            </language>
            <translation>Panaetius.</translation>
        </marginalia>
        
        <marginalia anchor_text="" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Excipiunt huiusmodi causas generales</marginalia_text>
                </position>
            </language>
            <translation>They receive the general causes of this kind.</translation>
        </marginalia>
        
        <marginalia anchor_text="exitus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Varij exitus</marginalia_text>
                </position>
            </language>
            <translation>Various solutions.</translation>
        </marginalia>
        
        <marginalia anchor_text="similis nemo Homeri" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>similis Nemo Homeri.</marginalia_text>
                    <person name="Homer"/>
                </position>
            </language>
            <translation>No one is similiar to Homer.</translation>
        </marginalia>
        
        <marginalia anchor_text="L. quidem Tarutius Firmanus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>L. Tarutius Firmianus</marginalia_text>
                    <person name="Lucius Tarutius Firmanus"/>
                </position>
            </language>
            <translation>Lucius Tarutius Firmanus.</translation>
        </marginalia>
        
        <marginalia anchor_text="vrbis etiam nostrae natalem diem repetebat ab iis Parilibus, quibus eam à Rumolo conitam accepimus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Vrbis fundame[n]ta qua[n]do p[ri]mo iaciuntur</marginalia_text>
                </position>
            </language>
            <translation>The foundations of the city when they are cast at first.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Vrbis Dies Natalis.</marginalia_text>
                </position>
            </language>
            <translation>The birthday of Rome.</translation>
        </marginalia>
        
        <marginalia anchor_text="in puero referre ex qua affectione coeli primum spiritum duxerit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>refert in puero ex qua affictione coeli spritu[m] p[ri]mu[m] duxerit</marginalia_text>
                </position>
            </language>
            <translation>'It matters in the case of a child under what arrangement of the heavenly bodies it draws its first breath' [Loeb 154, 481-3].</translation>
        </marginalia>
        
        <marginalia anchor_text="credat iis, quorum praedicta cotidie videat, re &amp; euentis refelli" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Cur ijs creditur q[uo]ru[m] pra[e]dictare &amp; eue[n]tis refelluntur?</marginalia_text>
                </position>
            </language>
            <translation>Why does one believe in those [men] whose predictions are refuted?</translation>
        </marginalia>
        
        <marginalia anchor_text="Storicorum conclusio rationis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Stoicoru[m] r[ati]o qua probare velint oraculoru[m] &amp; somnioru[m] Diuinatione[m] esse</marginalia_text>
                </position>
            </language>
            <translation>The method of the Stoics by which they wish to show that there is a prognostication by oracles and dreams.</translation>
        </marginalia>
        
        <marginalia anchor_text="ad ipsis co[n]stituta" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>fata à Dijs co[n]stituta.</marginalia_text>
                </position>
            </language>
            <translation>Destinies constituted by the gods.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Exp[er]ie[n]tia</marginalia_text>
                </position>
            </language>
            <translation>Experience.</translation>
        </marginalia>
        
        <marginalia anchor_text="" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Halij hijs respondet lib. 6 [...] in Ptolom[a]eu[m], contra Albumasar fo 4.</marginalia_text> <!-- FSF: one word missing -->
                    <person name="Albumasar"/>
                    <person name="Abu Ja'far Ahmad ibn Yusuf"/> 
                    <!--JG: https://books.google.co.uk/books?id=Gy3Vp7TurVUC&pg=PA188&lpg=PA188&dq=astrologist+ptolemy+halii&source=bl&ots=EUrsfgCNDD&sig=-6MRzklGxFj-FUJMxxQuGMb_gKM&hl=en&sa=X&ved=0ahUKEwjg48u5sI_bAhVrAcAKHQoOC0sQ6AEIbTAM#v=onepage&q=astrologist%20ptolemy%20halii&f=false--> <!--JG: https://ptolemaeus.badw.de/work/24-->
                    <person name="Ptolemy"/>
                </position>
            </language>
            <translation>Abu Ja'far Ahmad ibn Yusuf responded to these things in book 6 against Ptolomey, against Albumasar folio 4.</translation>
        </marginalia>
        
        <marginalia anchor_text="moueatur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>moveri solute libiri</marginalia_text>
                </position>
            </language>
            <translation>To be move unrestrained [and] freely.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>quibus r[ati]o[n]ib[us] h[a]ec infirmas?</marginalia_text>
                </position>
            </language>
            <translation>For which reasons do you weaken this?</translation>
        </marginalia>
        
        <marginalia anchor_text="praesignificare" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>p[ra]esignificare</marginalia_text>
                </position>
            </language>
            <translation>To presignify.</translation>
        </marginalia>
        
