<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v2.413v.tif" pagination="278" reader="John Dee" signature="[Ss.iii.[v]]"/> 
    <annotation>
        
        <marginalia anchor_text="quod dubium controuersúmque sit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>hoc { dubiu[m] co[n]troversu[m]q[ue] est.</marginalia_text>
                </position>
            </language>
            <translation>This is dubious and controversial.</translation>
        </marginalia>
        
        <marginalia anchor_text="hebetem &amp; rudem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>hebes rudis</marginalia_text>
                </position>
            </language>
            <translation>Blunt. Coarse.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Qua[e] est conclusio bona</marginalia_text>
                </position>
            </language>
            <translation>This is a fine conclusion.</translation>
        </marginalia>
        
        <marginalia anchor_text="omne esse dicimus, infinitum esse co[n]cluserit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>omne infinitu[m] est.</marginalia_text>
                </position>
            </language>
            <translation>Everything is infinite.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Pra[e]missa[e].</marginalia_text>
                </position>
            </language>
            <translation>Premisses.</translation>
        </marginalia>
        
        <marginalia anchor_text="Ennius" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Ennius.</marginalia_text>
                    <person name="Ennius"/>
                </position>
            </language>
            <translation>Ennius.</translation>
        </marginalia>
        
        <marginalia anchor_text="nihil deos ignorare" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Dij nihil ignora[n]t.</marginalia_text>
                </position>
            </language>
            <translation>The gods ignore nothing.</translation>
        </marginalia>
        
        <marginalia anchor_text="melius esse, quàm scire quae euentura sint" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Eve[n]tura, nescire melius q[uam] scire.</marginalia_text>
                </position>
            </language>
            <translation>It is better not to know than to know what will happen.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Deos ignorare.</marginalia_text>
                </position>
            </language>
            <translation>To ignore the gods.</translation>
        </marginalia>
        
        <marginalia anchor_text="Sunt autem dii" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>No[n] o[mn]es, e[ss]e Deos fatentur.</marginalia_text>
                </position>
            </language>
            <translation>Not all acknowledge to be gods.</translation>
        </marginalia>
        
        <marginalia anchor_text="Fac dare deos, quod absurdum est: quid refert si accipere no[n] possumus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Multa dij da[n]t, qua[e] nos accipere non possumus.</marginalia_text>
                </position>
            </language>
            <translation>The gods give many things which we cannot receive.</translation>
        </marginalia>
        
        <marginalia anchor_text="Ex falsis enim, vt ab ipsis didicimus, verum effici non potest" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Ex falsis veru[m] sequi no[n] p[otes]t</marginalia_text>
                </position>
            </language>
            <translation>The truth cannot follow from falsehoods.</translation>
        </marginalia>
        
        <marginalia anchor_text="Cratippu[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Cratippi r[ati]o</marginalia_text>
                    <person name="Cratippus of Pergamon"/>
                </position>
            </language>
            <translation>Cratippus of Pergamon's reasoning.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Co[n]tra cratippi r[ati]o[n]em.</marginalia_text>
                    <person name="Cratippus of Pergamon"/>
                </position>
            </language>
            <translation>Against Cratippus of Pergamon's reasoning.</translation>
        </marginalia>
        
        <marginalia anchor_text="λήμματα" hand="Italian">
            <language ident="EL">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>λήμματα</marginalia_text>
                </position>
            </language>
            <translation>Assumptions.</translation>
        </marginalia>
        
        <marginalia anchor_text="assumptione non concessa, nulla conclusio est" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Assu[m]ptione no[n] con[c]essa, nulla conclusio est.</marginalia_text>
                </position>
            </language>
            <translation>'The minor premise is denied the conclusion fails' [Loeb 154, 493].</translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="verbis confectum negociu[m]" type="straight"/>
        <underline language="LA" method="pen" text="probanda est, in qua ex rebus non dubiis, id quod dubitatur efficitur" type="straight"/>
        <underline language="LA" method="pen" text="hebetem &amp; rudem" type="straight"/>
        <underline language="LA" method="pen" text="vt ad rem dubiam concessis" type="straight"/>
        <underline language="LA" method="pen" text="non sumitis ad concludendu[m] quae ab omnibus concedantur, sed ea sumitis, quibus concessis, nihilo magis efficiatis quod              velitis. Primu[m]" type="straight"/>
        <underline language="LA" method="pen" text="pro certo, quod dubium controuersúmque sit" type="straight"/>
        <underline language="LA" method="pen" text="Deinde contorquent, &amp; ita concludunt, Non" type="straight"/>
        <underline language="LA" method="pen" text="Ex falsis enim, vt ab ipsis didicimus, verum" type="straight"/>
        <underline language="LA" method="pen" text="Festiue, &amp; breuiter" type="straight"/>
        <underline language="LA" method="pen" text="somnia pro somniis" type="straight"/>
        <underline language="LA" method="pen" text="controuersia: iam assumptione non concessa, nulla conclusio" type="straight"/>
        
