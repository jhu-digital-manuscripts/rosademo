    <page filename="RCP10549-50v2.414r.tif" pagination="279" reader="John Dee" signature="Ss.iiii."/> 
    <annotation>
        
        <marginalia anchor_text="fulguratores" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>fulguratores</marginalia_text>
                </position>
            </language>
            <translation>Priests who interpreted omens from lightning.</translation>
        </marginalia>
        
        <marginalia anchor_text="hominum fama" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>ho[m]i[nu]m fama.</marginalia_text>
                </position>
            </language>
            <translation>The fame of men.</translation>
        </marginalia>
        
        <marginalia anchor_text="Adhibuit etiam latebram obscuritatis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>adhibere latebra[m] obscuritatis</marginalia_text>
                </position>
            </language>
            <translation>'To employ a maze of obscurity' [LCL 154, 497].</translation>
        </marginalia>
        
        <marginalia anchor_text="varietas quae est propria fortunae" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Varietas fortuna[e] p[ro]p[ri]a</marginalia_text>
                </position>
            </language>
            <translation>The particular variety of fortune.</translation>
        </marginalia>
        
        <marginalia anchor_text="aliquid sicut praedictum sit euaserit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>aliquid vere p[rae]dictum est.</marginalia_text>
                </position>
            </language>
            <translation>Something is foretold truly.</translation>
        </marginalia>
        
        <marginalia anchor_text="furor iste, quem diuinum vocatis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Diuninus furor.</marginalia_text>
                </position>
            </language>
            <translation>Divine madness.</translation>
        </marginalia>
        
        <marginalia anchor_text="Regem" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>forsa[n] de CHR[IST]O</marginalia_text>
                    <person name="Jesus Christ"/>
                </position>
            </language>
            <translation>Perhaps on Christ.</translation>
        </marginalia>
        
        <marginalia anchor_text="Callide enim qui illa composuit perfecit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Callide sybylla scipsit.</marginalia_text>
                    <person name="Sibyl"/>
                </position>
            </language>
            <translation>The Sibyl wrote skilfully.</translation>
        </marginalia>
        
        <marginalia anchor_text="praedictum videretur hominum &amp; temporum definitione sublata" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Ho[m]i[nu]m temporu[m]q[ue] Definitio addatur p[rae]dictis</marginalia_text>
                </position>
            </language>
            <translation>The end of men and times is also said in the predictions.</translation>
        </marginalia>
        
        <marginalia anchor_text="ἀκροστιχίς" hand="English_secretary">
            <language ident="EL">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>ἀκροστιχίς</marginalia_text>
                    <book title="Sibylline oracles"/>
                </position>
            </language>
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>sibylla[e]</marginalia_text>
                    <person name="Sibyl"/>
                </position>
            </language>
            <translation>The acrostic poem of Sibyl.</translation>
        </marginalia>
        
        <marginalia anchor_text="Cassandra" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Cassandra.</marginalia_text>
                    <person name="Cassandra"/>
                </position>
            </language>
            <translation>Cassandra.</translation>
        </marginalia>
        
        <marginalia anchor_text="fabulis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Fabula[e]</marginalia_text>
                </position>
            </language>
            <translation>Fables.</translation>
        </marginalia>
        
        <marginalia anchor_text="exercitus Caesaris" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Ca[e]saris exercitus</marginalia_text>
                    <person name="Julius Caesar"/>
                </position>
            </language>
            <translation>Caesar's army.</translation>
        </marginalia>
        
        <marginalia anchor_text="praelii nemo nostrum erat quin timeret, sed ita vt constantibus hominibus par erat, non aperte" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Casus p[rae]lij nemimi co[n]sta[n]ti timendus aperte est</marginalia_text>
                </position>
            </language>
            <translation>The plight of the battle must be feared openly by no one who is steadfast.</translation>
        </marginalia>
        
        <marginalia anchor_text="timeret" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Timor</marginalia_text>
                </position>
            </language>
            <translation>Fear.</translation>
        </marginalia>
        
        <marginalia anchor_text="Apollo, qui vmbilicum certum terrarum obsides" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Apollo vmbilicu[m] terra[e] obsidet</marginalia_text>
                </position>
            </language>
            <translation>Apollo takes possession of the earth's core.</translation>
        </marginalia>
        
        <marginalia anchor_text="Croesus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Cro[e]sus</marginalia_text>
                    <person name="Croesus"/>
                </position>
            </language>
            <translation>Croesus.</translation>
        </marginalia>
        
        <marginalia anchor_text="oraculis Chrysippus totum volumen impleuit, partim falsis, vt ego opinor, partim casu veris, vt fit in omni oratione saepissime:              partim flexiloquis &amp; obscuris, vt interpres egeat interprete, &amp; sors ipsa ad sortes referenda sit: ambiguis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>oracula falsa casu vera flexiloqua obscura ambigua</marginalia_text>
                </position>
            </language>
            <translation>False, accidentally true, equivocal, obscure, and ambiguous prophecies.</translation>
        </marginalia>
        
        <marginalia anchor_text="opertis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>op[er]tis</marginalia_text>
                </position>
            </language>
            <translation>Hidden.</translation>
        </marginalia>
        
        <marginalia anchor_text="partim ficta aperte, partim effutita temere" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>partim ficta ap[er]te, partim effutita temere</marginalia_text>
                </position>
            </language>
            <translation>Partly openly feigned, partly rashly blurted out.</translation>
        </marginalia>
        
