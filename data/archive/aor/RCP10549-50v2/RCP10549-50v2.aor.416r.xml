    <page filename="RCP10549-50v2.416r.tif" pagination="283" reader="John Dee"/> 
    <annotation>
        
        <marginalia anchor_text="multa omnibus modis confusa &amp; variata" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>multis modis co[n]fusa variata</marginalia_text>
                </position>
            </language>
            <translation>Confused and varied in many ways.</translation>
        </marginalia>
        
        <marginalia anchor_text="inusitata, tam nulla" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>ta[m] invsitata ta[m] nulla</marginalia_text>
                </position>
            </language>
            <translation>So unseen and so void.</translation>
        </marginalia>
        
        <marginalia anchor_text="superstitione &amp; deprauatione" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>sup[er]stitio deprauatio.</marginalia_text>
                </position>
            </language>
            <translation>Superstition and depravity.</translation>
        </marginalia>
        
        <marginalia anchor_text="Plena sunt imaginum omnia" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Plena imaginu[m] o[mn]ia.</marginalia_text>
                </position>
            </language>
            <translation>All things are full of phantoms.</translation>
        </marginalia>
        
        <marginalia anchor_text="informata habeamus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>informata habem[us] multa.</marginalia_text>
                </position>
            </language>
            <translation>We have fashioned many things.</translation>
        </marginalia>
        
        <marginalia anchor_text="omnia igitur quae volumus, nota nobis esse possunt." hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>O[mn]ia qua[e] volum[us] nota nobis esse possu[n]t</marginalia_text>
                </position>
            </language>
            <translation>It is possible for us to know everything we wish to know [Loeb 154, 525].</translation>
        </marginalia>
        
        <marginalia anchor_text="Animorum est ea vis eáque natura, vt vigeant vigilantes nullo adue[n]titio pulsu, sed suo motu" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Animoru[m] motus</marginalia_text>
                </position>
            </language>
            <translation>The disturbance of souls.</translation>
        </marginalia>
        
        <marginalia anchor_text="maximaéque reliquiae reru[m] earum mouentur in animis &amp; agitanturm de quibus vigilantes aut cogitauimus aut egimus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Maxime reliquia[e] earu[m] reru[m] moue[n]tur in ani[mi]s de q[ui]b[us] vigila[n]tos aut cogitamim[us] aut egim[us]</marginalia_text>
                </position>
            </language>
            <translation>The remnants of our waking thoughts in particular, move and excite the soul [Loeb 154, 527].</translation>
        </marginalia>
        
        <marginalia anchor_text="animis vigilantium cogitationum vestigia" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Insu[n]t in a[nim]is vigila[n]tiu[m] cogitationu[m] vestigia</marginalia_text>
                </position>
            </language>
            <translation>Within our souls are traces of our waking thoughts.</translation>
        </marginalia>
        
        <marginalia anchor_text="" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Lepide</marginalia_text>
                </position>
            </language>
            <translation>Agreeably.</translation>
        </marginalia>
        
