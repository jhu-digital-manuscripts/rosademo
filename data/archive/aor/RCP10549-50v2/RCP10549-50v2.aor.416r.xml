                    <person name="Alcibiades"/>
                </position>
            </language>
            <translation>The dream of Alcibiades.</translation>
        </marginalia>
        
        <marginalia anchor_text="meridiationes" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>meridianationes</marginalia_text>
                </position>
            </language>
            <translation>Afternoon naps.</translation>
        </marginalia>
        
        <marginalia anchor_text="lucubrationes" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>lucubrationes</marginalia_text>
                </position>
            </language>
            <translation>Nocturnal studies.</translation>
        </marginalia>
        
        <marginalia anchor_text="pleni enectíve" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>pleni enective</marginalia_text>
                </position>
            </language>
            <translation>Excessively corpulent.</translation>
        </marginalia>
        
        <marginalia anchor_text="deos" hand="English_secretary">
            <language ident="LA">
