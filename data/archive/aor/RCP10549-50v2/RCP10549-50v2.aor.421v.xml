            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>pra[e]des.</marginalia_text>
                </position>
            </language>
            <translation>Sureties.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>nouare.</marginalia_text>
                </position>
            </language>
            <translation>To refresh.</translation>
        </marginalia>
        
        <marginalia anchor_text="scribis morderi" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>morderi.</marginalia_text>
                </position>
            </language>
            <translation>To be bitten.</translation>
        </marginalia>
        
        <marginalia anchor_text="dilaudas" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>dilaudare.</marginalia_text>
                </position>
            </language>
            <translation>To praise highly.</translation>
        </marginalia>
        
        <marginalia anchor_text="scribis morderi" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>serere mores.</marginalia_text>
                </position>
            </language>
            <translation>To weave morals.</translation>
        </marginalia>
        
        <marginalia anchor_text="repetam stirpe[m] iuris à natura" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>repeta[m] stirpe[m] iuris a natura</marginalia_text>
                </position>
            </language>
            <translation>I will claim the root of law from nature.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>natura[e] adunctio</marginalia_text>
                </position>
            </language>
            <translation>A conjunction of nature.</translation>
        </marginalia>
        
        <marginalia anchor_text="nec scriptis omnia sancienda" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>o[mn]ia scriptis no[n] sancie[n]da</marginalia_text>
                </position>
            </language>
            <translation>Everything must not be confirmed by writing.</translation>
        </marginalia>
        
        <marginalia anchor_text="magistratuum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Magistratus.</marginalia_text>
                </position>
            </language>
            <translation>Magistracy.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="EL">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>τὸ γὰρ εὖ μετ' ἐμοῦ</marginalia_text>
                    <X-ref book_title="Acharnians" person="Aristophanes"/>
                </position>
            </language>
            <translation>"For the right is on my side".</translation>
        </marginalia>
        
        <marginalia anchor_text="foenus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>fo[e]nus.</marginalia_text>
                </position>
            </language>
            <translation>Profit</translation>
        </marginalia>
        
        <marginalia anchor_text="nihil esse bonum, nisi quod honestum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Nihil bonu[m] nisi q[uod] honestum.</marginalia_text>
                </position>
            </language>
            <translation>Nothing is good except that which is honest.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="EL">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>φυσικὴ πρὸς τὰ τέκνα</marginalia_text>
                </position>
            </language>
            <translation>Physics (i.e. the science of origination) is about the children.</translation>
        </marginalia>
        
        <marginalia anchor_text="accommodabimus hoc tempore leges" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Cui rei acco[m]moda[n]da[e] leges</marginalia_text>
                </position>
            </language>
            <translation>To which matter the laws must be accommodated.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Physica.</marginalia_text>
                </position>
            </language>
            <translation>Natural philosophy.</translation>
        </marginalia>
        
        <marginalia anchor_text="homines deorum agnatione &amp; gente teneantur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>omnis deoru[m] agnatione et gente teneatur</marginalia_text>
                </position>
            </language>
            <translation>Everything may be held with consanguinity and by the people of the gods.</translation>
        </marginalia>
        
        <marginalia anchor_text=" vtiis infici solet ciuitas" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>vt infici ciuitas solet.</marginalia_text>
                </position>
            </language>
            <translation>As the community is accustomed to be corrupted.</translation>
        </marginalia>
        
        <marginalia anchor_text="animos mihi addidisti" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>animos addere</marginalia_text>
                </position>
            </language>
            <translation>To add souls.</translation>
        </marginalia>
        
