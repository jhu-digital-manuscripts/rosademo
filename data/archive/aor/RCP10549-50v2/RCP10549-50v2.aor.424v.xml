    <annotation>
        
        <marginalia anchor_text="virtus honore[m] nec est virtutis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Virtutis merces, honor.</marginalia_text>
                </position>
            </language>
            <translation>Honour is the reward of virtue.</translation>
        </marginalia>
        
        <marginalia anchor_text="ignorantia veri facit incertam labantémque sententiam" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Ignora[n]tia incerta[m] laba[n]te[m]que sente[n]tiam facit.</marginalia_text>
                </position>
            </language>
            <translation>Ignorance produces an uncertain and wavering opinon.</translation> 
        </marginalia>
        
        <marginalia anchor_text="praemium virtus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Virtutis pra[e]ium.</marginalia_text>
                </position>
            </language>
            <translation>The prize of virtue.</translation>
        </marginalia>
        
        <marginalia anchor_text="mundus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Mundus.</marginalia_text>
                </position>
            </language>
            <translation>The world.</translation>
        </marginalia>
        
        <marginalia anchor_text="vera lex" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Vera. Lex</marginalia_text>
                </position>
            </language>
            <translation>True law.</translation>
        </marginalia>
        
        <marginalia anchor_text="vir" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Vir { Probus improbus</marginalia_text>
                </position>
            </language>
            <translation>Man: honest, immoderate. </translation>
        </marginalia>
        
        <marginalia anchor_text="bellum suscipi" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Belli causa[e] qu[a]e essent.</marginalia_text>
                </position>
            </language>
            <translation>Which are the causes of war.</translation>
        </marginalia>
        
        <marginalia anchor_text="mortis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Mors.</marginalia_text>
                </position>
            </language>
            <translation>Death.</translation>
        </marginalia>
        
        <marginalia anchor_text="ciuitas, vt aternea" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Ciuitas a[e]terna</marginalia_text>
                </position>
            </language>
            <translation>The eternal city.</translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="Furius aiebat, rapiatur, vexetur, exterminetur, egeat, aufera[n]tur ei manus, effodiantur ei oculi              damnetur, vinciatur, vratur, miseris etiam modis necetur: perdétne suu[m] praemium virtus, an" type="straight"/>
        <underline language="LA" method="pen" text="recta ratio naturae congruens, diffusa in omnes, constans, sempiterna, quae vocet ad officium iubendo:              vetando, à fraude deterreat: quae tamen neque probos frustra iubet aut vetat, nec improbos iubendo aut vetando mouet. Huic" type="straight"/>
        <underline language="LA" method="pen" text="Debet enim constituta sic esse ciuitas, vt aeterna" type="straight"/>
        
