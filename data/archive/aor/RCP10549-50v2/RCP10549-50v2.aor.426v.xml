    <annotation>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>suspicere in coelu[m]</marginalia_text>
                </position>
            </language>
            <translation>To admire in heaven.</translation>
        </marginalia>
        
        <marginalia anchor_text="ex animo meo discedit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>ex a[m]i[n]o dicedere.</marginalia_text>
                </position>
            </language>
            <translation>To go away from the mind.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>collacrymare</marginalia_text>
                </position>
            </language>
            <translation>To weep together.</translation>
        </marginalia>
        
        <marginalia anchor_text="moderatori Reipub. beata ciuium vita propostita" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Quid Reip[ublicae] moderatori sit propositum.</marginalia_text>
                </position>
            </language>
            <translation>What may be proposed to the Republic's governour.</translation>
        </marginalia>
        
        <marginalia anchor_text="fortitudo" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>fortitudo.</marginalia_text>
                </position>
            </language>
            <translation>Strength.</translation>
        </marginalia>
        
        <marginalia anchor_text="Scipio dixisset, admodum probans: nimis enima erat odio rhetoru[m] imbutus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Scipio odio Rhetorum imbutus</marginalia_text>
                    <person name="Scipio Africanus"/>
                </position>
            </language>
            <translation>Scipio Africanus was soaked with the hatred of the rethoricians.</translation>
        </marginalia>
        
        <marginalia anchor_text="Masinissam" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Masinissa.</marginalia_text>
                    <person name="Masanissa"/>
                </position>
            </language>
            <translation>Masanissa.</translation>
        </marginalia>
        
        <marginalia anchor_text="Aphricano" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Aphricanus</marginalia_text>
                    <person name="Scipio Africanus"/>
                </position>
            </language>
            <translation>Scipio Africanus.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>somnia vnde oriantur</marginalia_text>
                </position>
            </language>
            <translation>Whence dreams arise.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Aphricanus ad scipione[m]</marginalia_text>
                    <person name="Scipio Africanus"/>
                    <person name="Scipio Aemilianus"/>
                </position>
            </language>
            <translation>Scipio Africanus towards Scipio Aemilianus.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Co[e]lum</marginalia_text>
                </position>
            </language>
            <translation>Heaven.</translation>
        </marginalia>
        
        <marginalia anchor_text="lumen animi ingenii, consiliíque" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Lumen a[nim]i consilijq[ue]</marginalia_text>
                </position>
            </language>
            <translation>The light of the mind and advice.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Circuitu naturali summa fatalis nobis conficitur</marginalia_text>
                </position>
            </language>
            <translation>The highest destined [matter] is composed with a natural circle.</translation>
        </marginalia>
        
        <marginalia anchor_text="pax sit rebus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Pax sit rebus.</marginalia_text>
                </position>
            </language>
            <translation>May peace be to things.</translation>
        </marginalia>
        
        <marginalia anchor_text="nihil mihi potius fuit" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>nihil mihi potius fuit.</marginalia_text>
                </position>
            </language>
            <translation>Nothing was better to me.</translation>
        </marginalia>
        
        <marginalia anchor_text="iustis de causis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>iustis de caußis</marginalia_text>
                </position>
            </language>
            <translation>On just causes.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>renovare p[r]istina bella.</marginalia_text>
                </position>
            </language>
            <translation>To renovate former beauty.</translation>
        </marginalia>
        
        <marginalia anchor_text="cuius ego nomine ipse" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>cui[us] ego no[m]i[n]e ip[s]e</marginalia_text>
                    <emphasis method="pen" text="ego ip[s]e" type="straight"/>
                </position>
            </language>
            <translation>Of which I myself am named.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>excelsus &amp; plenus stellaru[m], illustris et clarus q[ui]da[m] locus. Sempiterni illi ignes, qu[a]e sydera et stellas vocatis 
                        splendissimo caudare inter flammas circus elucens.</marginalia_text>
                </position>
            </language>
            <translation>Lofty and full of the stars, a certain bright and clear place. These fires of the everlasting you call constellations and stars.
            A brightly shining orbit with a most splendid tail among flames.</translation> <!--JG: Unsure-->
        </marginalia>
        
        <marginalia anchor_text="Tu eris vnus in quo nitatur ciuitatis salus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>Tu vnus es in q[uo] nititur ciuitatis salus</marginalia_text>
                </position>
            </language>
            <translation>You are on in which the health of the community is advanced.</translation>
        </marginalia>
        
