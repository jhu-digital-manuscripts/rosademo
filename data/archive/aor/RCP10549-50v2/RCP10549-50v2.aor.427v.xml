        <marginalia anchor_text="sermonibus vulgi dederis te" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Sermonibus vulgi ne dederis te.</marginalia_text>
                </position>
            </language>
            <translation>You shall not surrender yourself to the rumours of the people.</translation>
        </marginalia>
        
