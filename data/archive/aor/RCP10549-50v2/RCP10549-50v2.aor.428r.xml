<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v2.428r.tif" pagination="307" reader="John Dee" signature="Vv.ii"/> 
    <annotation>
        
        <marginalia anchor_text="vigila[n]tius" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>vigilati[us]</marginalia_text>
                </position>
            </language>
            <translation>More vigilantly.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>benemeritis</marginalia_text>
                </position>
            </language>
            <translation>Well-deserved.</translation>
        </marginalia>
        
        <marginalia anchor_text="perennis fuit, &amp; obruitur hominum interitu" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Nulla inter ho[m]i[n]es p[er]ennis no[min]is celebritas</marginalia_text>
                </position>
            </language>
            <translation>No reputation is everlasting in name among men.</translation>
        </marginalia>
        
        <marginalia anchor_text="nunc tamen tanto praemio proposito multo vigilantius" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Pra[e]m[un]ire propositu[m] recte acta[e] vita[e]</marginalia_text> <!--JG: Unsure about 'praemunire'--> <!-- FSF: not sure either -->
                </position>
            </language>
            <translation>To correctly strenghten a proposition of an act of life.</translation> <!--JG: Unsure-->
        </marginalia>
        
        <marginalia anchor_text="deus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Deus.</marginalia_text>
                </position>
            </language>
            <translation>God.</translation>
        </marginalia>
        
        <marginalia anchor_text="nundum ex quadam parte mortalem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Mundus mortalis. a[n]i[m]us i[m]mort[alis]</marginalia_text>
                </position>
            </language>
            <translation>Mortal world. Immortal mind.</translation>
        </marginalia>
        
        <marginalia anchor_text="hic fons, hoc principium est mouendi" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Animus mouendi fons</marginalia_text>
                </position>
            </language>
            <translation>The mind, the source for moving.</translation>
        </marginalia>
        
        <marginalia anchor_text="Principium" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>principium.</marginalia_text>
                </position>
            </language>
            <translation>Beginning.</translation>
        </marginalia>
        
        <marginalia anchor_text="à principio oriri omnia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>À principio oriu[n]tur omnia.</marginalia_text>
                </position>
            </language>
            <translation>Everything arises from a beginning.</translation>
        </marginalia>
        
        <marginalia anchor_text="Inanimum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Inamimu[m] quid</marginalia_text>
                </position>
            </language>
            <translation>What is inanimate.</translation>
        </marginalia>
        
        <marginalia anchor_text="animal" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Animal</marginalia_text>
                </position>
            </language>
            <translation>Animal.</translation>
        </marginalia>
        
        <marginalia anchor_text="exerce in optimis rebus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Anima[m] exerce in optimis reb[us]</marginalia_text>
                </position>
            </language>
            <translation>Exercise the soul in the best matters.</translation>
        </marginalia>
        
        <marginalia anchor_text="contemplans" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Contemplatio</marginalia_text>
                </position>
            </language>
            <translation>Contemplation.</translation>
        </marginalia>
        
        <marginalia anchor_text="animi" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Animoru[m] errores</marginalia_text>
                </position>
            </language>
            <translation>The errors of the minds.</translation>
        </marginalia>
        
        <marginalia anchor_text="ciuis ita necesse est" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Ciuis qualis esset.</marginalia_text>
                </position>
            </language>
            <translation>What kind of citizen one may be.</translation>
        </marginalia>
        
        <marginalia anchor_text="boni plus, quà, multi valent" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Boni plusq[uam] multi valent</marginalia_text>
                </position>
            </language>
            <translation>Quality is worth more than quantity.</translation>
        </marginalia>
        
        <marginalia anchor_text="seditio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Seditio</marginalia_text>
                </position>
            </language>
            <translation>Sedition.</translation>
        </marginalia>
        
        <marginalia anchor_text="libidines" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Libidines</marginalia_text>
                </position>
            </language>
            <translation>Desires.</translation>
        </marginalia>
        
        <marginalia anchor_text="Firmiter enim maiores nostris stabilita matrimonia esse" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Matrimonia firma.</marginalia_text>
                </position>
            </language>
            <translation>A strong marriage.</translation>
        </marginalia>
        
        <marginalia anchor_text="desubito" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>desubito</marginalia_text>
                </position>
            </language>
            <translation>Suddenly.</translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="obruitur hominum interitu" type="straight"/>
        <underline language="LA" method="pen" text="&amp; obliuione" type="straight"/>
        <underline language="LA" method="pen" text="limes ad caeli aditum" type="straight"/>
        <underline language="LA" method="pen" text="vestigiis ingressus partriis" type="straight"/>
        <underline language="LA" method="pen" text="tamen tanto praemio" type="straight"/>
        <underline language="LA" method="pen" text="non esse te mortale[m], sed corpus hoc. Nec" type="straight"/>
        <underline language="LA" method="pen" text="is es quem forma ista declarat, sed mens cuiusque is est quisque, non ea figura quae digito demonstrari potest. Deum" type="straight"/>
        <underline language="LA" method="pen" text="sic fragile corpus animus sempiternus mouet" type="straight"/>
        <underline language="LA" method="pen" text="quin etiam caeteris quae mouentur, hic fons, hoc principium est mouendi" type="straight"/>
        <underline language="LA" method="pen" text="principium extinctum, nec ipsum ab alio renascetur, nec ex se aliud creabit" type="straight"/>
        <underline language="LA" method="pen" text="necesse est à principio oriri omnia" type="straight"/>
        <underline language="LA" method="pen" text="vim vllam nanciseatur" type="straight"/>
        <underline language="LA" method="pen" text="est, hanc tu exerce in optimis rebus. Sunt" type="straight"/>
        <underline language="LA" method="pen" text="curae de salute patriae" type="straight"/>
        <underline language="LA" method="pen" text="velocius in hanc sedem &amp; domum suam peruolabit. Idque" type="straight"/>
        <underline language="LA" method="pen" text="eminebit foras, &amp; ea quae extra erunt, contemplans, quàm maxime se à corpore abstrahet" type="straight"/>
        <underline language="LA" method="pen" text="desubito" type="straight"/>
        
