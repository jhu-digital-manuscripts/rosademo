    <annotation>
        
        <marginalia anchor_text="pertubat nos opinionum varietas, hominúmque dissensio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Pertubat nos Opinionu[m] varietas ho[m]i[nu]mq[ue] disse[n]sio</marginalia_text>
                </position>
            </language>
            <translation>We are confused by the variety of opinions and disagreement of men.</translation>
        </marginalia>
        
        <marginalia anchor_text="sensus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Sensus.</marginalia_text>
                </position>
            </language>
            <translation>Senses.</translation>
        </marginalia>
        
        <marginalia anchor_text="quae penitus in omni sensu implicata insidet imitatrix boni, voluptas: malorum autem mater omnium quoius blanditiis              corrupta qua natura bona sunt" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Vnde deprana[n]tur ho[m]i[n]es</marginalia_text>
                </position>
            </language>
            <translation>From where men are perverted.</translation>
        </marginalia>
        
        <marginalia anchor_text="ius, &amp; omne honestu[m] sua sponte esse expetendum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Ius o[mn]e &amp; honestu[m] sua spo[n]te expetendum est.</marginalia_text>
                </position>
            </language>
            <translation>We should voluntarily strive for every right and honourable thing.</translation>
        </marginalia>
        
        <marginalia anchor_text="liberalitas" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Liberalitas.</marginalia_text>
                </position>
            </language>
            <translation>Generosity.</translation>
        </marginalia>
        
        <marginalia anchor_text="malitia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Malitia</marginalia_text>
                </position>
            </language>
            <translation>Malice.</translation>
        </marginalia>
        
        <marginalia anchor_text="beneficus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>beneficus</marginalia_text>
                </position>
            </language>
