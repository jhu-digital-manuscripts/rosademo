        </marginalia>

        <marginalia anchor_text="amicitia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Amicus</marginalia_text>
                </position>
            </language>
            <translation>Friendly.</translation>
        </marginalia>
        
        <marginalia anchor_text="iustitiae mercedem quaerere" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Iustitia[e], ne qua[e]rat[ur] mer[ces]</marginalia_text>
                </position>
            </language>
            <translation>So that no reward is sought of justice.</translation>
        </marginalia>
                
        <marginalia anchor_text="infammiae metu, pudicos dicere, cum ipsa infamia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Infamia[e] metus, infamia</marginalia_text>
                </position>
            </language>
            <translation>Fear of disgrace or disgrace.</translation>
        </marginalia>
        
        <marginalia anchor_text="animi deformitas non habebit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Animoru[m] pravitates.</marginalia_text>
                </position>
            </language>
            <translation>Deformities of the soul.</translation>
        </marginalia>
        
        <marginalia anchor_text="melius sse aliquid, quàm virtutem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Virtute nihil melius</marginalia_text>
                </position>
            </language>
            <translation>Virtute is better than nothing.</translation>
        </marginalia>
        
