        <underline language="LA" method="pen" text="nam qui se ipse norit, primùm aliquid se habere sentiet diuinum: ingenium in se suum sicut simulacrum aliquod dedicatum              putabit, tantóque munere deorum semper dignum aliquid &amp; faciet &amp; sentiet: &amp;" type="straight"/>
        <underline language="LA" method="pen" text="natura subornatus" type="straight"/>
        <underline language="LA" method="pen" text="vitam venerit" type="straight"/>
        <underline language="LA" method="pen" text="quasi adumbratas intelligentias animo ac mente conceperit: quibus illustratus, sapientia duce, bonum virum, &amp; ob eam ipsam causam              cernat se beautu[m] fore." type="straight"/>
        <underline language="LA" method="pen" text="obsequio indulge[n]tiáque" type="straight"/>
        <underline language="LA" method="pen" text="voluptatémque sicut labem aliqua[m] decoris" type="straight"/>
