        <underline language="LA" method="pen" text="nisi vetustas ea certa esse docuisset" type="straight"/>
        <underline language="LA" method="pen" text="euanuerit iam &amp;" type="straight"/>
        <underline language="LA" method="pen" text="in nostro collegio fuisse: neque illi, qui esse etiam nunc" type="straight"/>
        <underline language="LA" method="pen" text="suscipiendo &amp; gerendo, &amp; deponendo" type="straight"/>
        <underline language="LA" method="pen" text="immaníque vita exculti ad humanitatem &amp; mitigati sumus" type="straight"/>
        <underline language="LA" method="pen" text="cogitatam libidinem intulit, quò ne imprudentiam quidem oculorum adiici fas fuit" type="straight"/>
        <underline language="LA" method="pen" text="initientúrque eo ritu Cereri, quo Romae initiantur" type="straight"/>
        <underline language="LA" method="pen" text="peruigilationes" type="straight"/>
        <underline language="LA" method="pen" text="sunt cauea, circóque diuisi, sint corporum certationes" type="straight"/>
        
