        <marginalia anchor_text="ardentes cum cupiditate, tum metu, tum conscientia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Ardentes { Cupiditate. Metu. Conscie[n]tia.</marginalia_text>
                </position>
            </language>
            <translation>Burning by desire, fear, [and] conscience.</translation>
        </marginalia>
        
        <marginalia anchor_text="duplice[m] poenam esse diuinam" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Diuina po[e]na duplex</marginalia_text>
                </position>
            </language>
            <translation>Twofold divine punishment.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Quid Dijs co[n]secrari debet: ex Platone</marginalia_text>
                    <person name="Plato"/>
                </position>
            </language>
            <translation>What ought to be consecrate to the Gods, from Plato.</translation>
        </marginalia>
        
        <marginalia anchor_text="non solum scire aliquid artis est, sed quaedam ars etiam doce[n]di" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>H[om]o solu[m] scire aliquid artis est sed etiam qua[e]da[m] est docendi ars</marginalia_text>
                </position>
            </language>
            <translation>Man is only to know something of art, but also the art of teaching something.</translation>
        </marginalia>
        
        <marginalia anchor_text="cum religione" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Religio</marginalia_text>
                </position>
            </language>
            <translation>Devotion.</translation>
        </marginalia>
        
        <marginalia anchor_text="perparua sint: de sacris" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>perpetua sint sacra</marginalia_text>
                </position>
            </language>
            <translation>Perpetual things are sacred.</translation>
        </marginalia>
        
        <marginalia anchor_text="Haeredum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Ha[e]res</marginalia_text>
