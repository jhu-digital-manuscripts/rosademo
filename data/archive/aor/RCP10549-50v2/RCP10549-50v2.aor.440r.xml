    <annotation>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>Obseri</marginalia_text> 
                </position>
            </language>
            <translation>To bar.</translation>
        </marginalia>
        
        <marginalia anchor_text="NENIAE" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Nenia[e]</marginalia_text>
                </position>
            </language>
            <translation>A funeral song.</translation>
        </marginalia>
        
        <marginalia anchor_text="sepulcrorum magnificentia[m] esse minuendam" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Sepulchrou[m] magnifice[n]tia minuenda.</marginalia_text>
                </position>
            </language>
            <translation>The greatness of tombs is diminished.</translation>
        </marginalia>
        
        <marginalia anchor_text="corpus terra huma[n]di" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Corpora terra humanda</marginalia_text>
                </position>
            </language>
            <translation>For the burial of the body in the earth.</translation>
        </marginalia>
        
        <marginalia anchor_text="epulae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Epula[e]</marginalia_text>
                </position>
            </language>
            <translation>A feast.</translation>
        </marginalia>
        
        <marginalia anchor_text="sumptuosa fieri funera &amp; lamentabilia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>funera sumptuosa lame[n]tabiliáq[ue]</marginalia_text>
                </position>
            </language>
            <translation>A funeral, extravagant and lamentable.</translation>
        </marginalia>
        
        <marginalia anchor_text="TYMOBON" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>TYMBOS</marginalia_text>
                </position>
            </language>
            <translation>Burial Mound.</translation> 
        </marginalia>
        
        <marginalia anchor_text="Ceramico" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Ceramicum</marginalia_text>
                    <location name="Ceramus"/>
                </position>
            </language>
            <translation>Ceramus.</translation>
        </marginalia>
        
        <marginalia anchor_text="amplitudines sepulcrorum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Sepulchroru[m] amplitudo</marginalia_text>
                </position>
            </language>
            <translation>Enormous tombs.</translation>
        </marginalia>
        
        <marginalia anchor_text="celebritas virorum, ac mulierum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Celebritas Viroru[m], mulieru[m]q[ue]</marginalia_text>
                </position>
            </language>
            <translation>A feast for men and women.</translation>
        </marginalia>
        
        <marginalia anchor_text="ante lucuem enim iussit efferri" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Ante luce[m] efferenda funera</marginalia_text>
                </position>
            </language>
            <translation>Funerals should be carried out before daybreak.</translation>
        </marginalia>
        
        <marginalia anchor_text="fruges" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>fruges</marginalia_text>
                </position>
            </language>
            <translation>Crops</translation>
        </marginalia>
        
        <marginalia anchor_text="mater" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>terra m[ate]r</marginalia_text>
                </position>
            </language>
            <translation>Mother earth.</translation>
        </marginalia>
        
        <marginalia anchor_text="laudem mortui incisam" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Laus mortui incisa</marginalia_text>
                </position>
            </language>
            <translation>The inscribed praise of the deceased.</translation>
        </marginalia>
        
        <marginalia anchor_text="post mortem tranquillitate bonorum, poenis impiorum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Tranquillitas post morte[m] bonoru[m], po[e]n[a]e impioru[m].</marginalia_text>
                </position>
            </language>
            <translation>The tranquil after the death of the good and the punishments of the wicked.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>pra[e]finiri</marginalia_text>
                </position>
            </language>
            <translation>To be prescribed.</translation>
        </marginalia>
        
