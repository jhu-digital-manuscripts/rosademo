    <annotation>
        
        <marginalia anchor_text="diuinum illum virum ... Platone[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Plato vir diuinus.</marginalia_text>
                    <person name="Plato"/>
                </position>
            </language>
            <translation>Plato was a divine man.</translation>
        </marginalia>
        
        <marginalia anchor_text="difficillimam illam societatem grauitatis cum humanitate" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Difficillima societas, grauitatis cum humanitate.</marginalia_text>
                </position>
            </language>
            <translation>The most difficult combination of dignity with refinement.</translation>
        </marginalia>
        
        <marginalia anchor_text="magistratus hanc esse vim" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Magistratus Vis.</marginalia_text>
                </position>
            </language>
            <translation>The power of an office.</translation>
        </marginalia>
        
        <marginalia anchor_text="ius, conditionémque naturae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Ius conditioq[ue] natura[e], Lex</marginalia_text>
                </position>
            </language>
            <translation>Law is a right and an agreement of nature.</translation>
        </marginalia>
        
        <marginalia anchor_text="hic Deo paret" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Mundus Deo paret.</marginalia_text>
                </position>
            </language>
            <translation>The world is subject to God.</translation>
        </marginalia>
        
        <marginalia anchor_text="regibus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Reges.</marginalia_text>
                </position>
            </language>
            <translation>Kings.</translation>
        </marginalia>
        
        <marginalia anchor_text="regia potestas" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Regalis potestas</marginalia_text>
                </position>
            </language>
            <translation>Regal power.</translation>
        </marginalia>
        
        <marginalia anchor_text="Magistratibus igitur opus est" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Cur creantur Magistrat[us]</marginalia_text>
                </position>
            </language>
            <translation>Why offices are created.</translation>
        </marginalia>
        
        <marginalia anchor_text="Charondas" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Charondas</marginalia_text>
                    <person name="Charondas"/>
                </position>
            </language>
            <translation>Charondas.</translation>
        </marginalia>
        
        <marginalia anchor_text="aduersentur magistratibus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Aduersari Magistratib[us] contra ius fasq[ue]</marginalia_text>
                </position>
            </language>
            <translation>To oppose the magistrates against right and what is lawful.</translation>
        </marginalia>
        
        <marginalia anchor_text="CIVES" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Ciues</marginalia_text>
                </position>
            </language>
            <translation>Citizens.</translation>
        </marginalia>
        
        <marginalia anchor_text="MULTA" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Mulcta.</marginalia_text>
                </position>
            </language>
            <translation>Fine.</translation>
        </marginalia>
        
        <marginalia anchor_text="IMPERABIT" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Imperator</marginalia_text>
                </position>
            </language>
            <translation>Emperor.</translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="nimis valde vnquam, nec nimis saepe laudaueris" type="straight"/>
        <underline language="LA" method="pen" text="hanc esse vim, vt praesit, praescribátque recta &amp; vtilita &amp; coniuncta cum legibus. Vt enim              magistratibus leges, ita populo praesunt magistratus: veréque dici potest, magristratum, legem esse loquentem: legem autem, mutum" type="straight"/>
        <underline language="LA" method="pen" text="tam aptum" type="straight"/>
        <underline language="LA" method="pen" text="Nam &amp; hic Deo paret, &amp; huic obediunt maria, terrá[e]que, &amp; hominum vita iussis supremae legis obtemperat" type="straight"/>
        <underline language="LA" method="pen" text="homines iustissimos &amp; sapientissimos" type="straight"/>
        <underline language="LA" method="pen" text="non ii nemini, sed no[n] semper vni parêre voluerunt" type="straight"/>
        <underline language="LA" method="pen" text="prudentia ac diligentia esse ciuitas non potest, quorúmque descriptione omnis reip. moderatio continetur. Neque solum iis              praescribendus est imperandi, sed etiam ciuibus obtemperandi modus. Nam &amp; qui bene imperat, paruerit aliquando necesse est" type="straight"/>
        <underline language="LA" method="pen" text="paret, videtur, qui aliquando imperet, dignus esse. Itaque oportet &amp; eum qui paret, sperare, se aliquo tempore imperaturum: &amp;              illum qui imperat, cogitare, breui tempore sibi esse paredum. Nec verò solum vt obtemperent obediántque magistratibus, sed etiam vt eos colant, diligántque, praescribimus:              vt Charondas in suis facit leigibus. Noster verò" type="straight"/>
        <underline language="LA" method="pen" text="Titanu[m] è genere statuit eos, qui vt illi caelestibus, sic hi aduersentur" type="straight"/>
        <underline language="LA" method="pen" text="RECVSATIONE" type="straight"/>
        
