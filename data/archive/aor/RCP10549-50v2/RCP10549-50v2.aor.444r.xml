    <page filename="RCP10549-50v2.444r.tif" pagination="339" reader="John Dee" signature="Yy.ii."/> 
    <annotation>
        
        <marginalia anchor_text="legem antiquastis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>Lege[m] antiquare.</marginalia_text>
                </position>
            </language>
            <translation>To reject a bill.</translation>
        </marginalia>
        
        <marginalia anchor_text="quantum è vultu eius intelligo" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>qua[n]tu[m] è vultu ei[us] intelligo.</marginalia_text>
                </position>
            </language>
            <translation>As much as I can judge from his expression.</translation>
        </marginalia>
        
        <marginalia anchor_text="recognoscimus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>recognosco.</marginalia_text>
                </position>
            </language>
            <translation>I recognise.</translation>
        </marginalia>
        
        <marginalia anchor_text="Cassiae legis culpa[m] " hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Lex Cassia sublata.</marginalia_text>
                </position>
            </language>
            <translation>Abolished the Cassian law.</translation>
        </marginalia>
        
        <marginalia anchor_text="nihil vnquam populare placuit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Nihil populare placeat.</marginalia_text>
                </position>
            </language>
            <translation>No popular pleasure.</translation>
        </marginalia>
        
        <marginalia anchor_text="optimorum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Aristocratio optima.</marginalia_text>
                </position>
            </language>
            <translation>The best aristocracy.</translation>
        </marginalia>
        
        <marginalia anchor_text="tabella condemnet, quàm solebat vox" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Tabella. vox.</marginalia_text>
                </position>
            </language>
            <translation>Contract, voice.</translation>
        </marginalia>
        
        <marginalia anchor_text="largitione corrupta suffragia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Largitio[n]e corrupta suffragia</marginalia_text>
                </position>
            </language>
            <translation>The corrupting effects of bribery on votes.</translation>
        </marginalia>
        
        <marginalia anchor_text="Actor" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Actor.</marginalia_text>
                </position>
            </language>
            <translation>Actor.</translation>
        </marginalia>
        
        <marginalia anchor_text="iussa tria sunt" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Senatori tria iussa.</marginalia_text>
                </position>
            </language>
            <translation>Three senatorial injunctions..</translation>
        </marginalia>
        
        <marginalia anchor_text="breuitas non modo Senatoris, sed etiam oratoris magna laus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Breuitatis magna laus</marginalia_text>
                </position>
            </language>
            <translation>A great praise of brevity.</translation>
        </marginalia>
        
        <marginalia anchor_text="longa oratione vtendum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Quando Longa est vte[n]du[m] or[ati]o[n]e</marginalia_text>
                </position>
            </language>
            <translation>When a long oration should be used.</translation>
        </marginalia>
        
        <marginalia anchor_text="Cato" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Cato.</marginalia_text>
                    <person name="Cato the Elder"/>
                </position>
            </language>
            <translation>Cato.</translation>
        </marginalia>
        
        <marginalia anchor_text="CAVSAS POPVLI" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Caussa[e] Pop[uli]</marginalia_text>
                </position>
            </language>
            <translation>Cause of the people.</translation>
        </marginalia>
        
        <marginalia anchor_text="scientiae, diligentiae, memoriae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Scie[n]tia. dilige[n]tia. Memoria.</marginalia_text>
                </position>
            </language>
            <translation>Knowledge, industry and memory.</translation>
        </marginalia>
        
        <marginalia anchor_text="VIS ABESTO" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Vis abesto.</marginalia_text>
                </position>
            </language>
            <translation>No violence shall be used.</translation>
        </marginalia>
        
        <marginalia anchor_text="actoris esse iubeo fraudem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Actoris fraus</marginalia_text>
                </position>
            </language>
            <translation>The guile of the actor.</translation>
        </marginalia>
        
        <marginalia anchor_text="nihil est enim exitiosius ciuitatibus, nihil tam contrarium iuri &amp; legibus, nihil minus ciuile est &amp; humanum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>nihil exitiosius est. ta[m] co[n]trariu[m] iuri &amp; legib[us] min[us] ciuile &amp; humanu[m]</marginalia_text>
                </position>
            </language>
            <translation>Nothing is more destructive, more hostile than the law and principles, [nothing is] less civil and humane.</translation>
        </marginalia>
        
