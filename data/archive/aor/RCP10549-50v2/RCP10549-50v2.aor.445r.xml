        </marginalia>
        
        <marginalia anchor_text="Demetrius Phalereus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Demetrius</marginalia_text>
                    <person name="Demetrius of Phalerum"/>
                </position>
            </language>
            <translation>Demetrius.</translation>
        </marginalia>
        
        <marginalia anchor_text="grauissime &amp; copiosissime potuisse dicere" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>dicere grauissime copiosissimèq[ue]</marginalia_text>
                </position>
            </language>
            <translation>To speak most powerfully and most copiously.</translation>
        </marginalia>
        
        <marginalia anchor_text="ornatè spendidéque" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>facere ornato splendidi</marginalia_text>
                </position>
            </language>
            <translation>To make gracefully and elegantly.</translation>
        </marginalia>
        
        <marginalia anchor_text="accuratè copioséque à philosophis disputata" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>disp[utare] accurate copiose</marginalia_text>
                </position>
            </language>
            <translation>To argue accurately and copiously.</translation>
        </marginalia>
        
        <marginalia anchor_text="id mihi assumo, videor id meo iure quoda[n]modo vendicare" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>assumo vindico.</marginalia_text>
                </position>
            </language>
            <translation>I assume, I claim.</translation>
        </marginalia>
        
        <marginalia anchor_text="officii praecepta firma, stabilia, co[n]iuncta naturae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>officij pr[aec]epta { firma stabilia co[n]iuncta natur[a]e</marginalia_text>
                </position>
            </language>
            <translation>Precepts on duties { solid, constant, steadfast to nature.</translation>
        </marginalia>
        
        <marginalia anchor_text="si sibi consentaneae velint esse" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>conse[n]tire sibi si sibi co[n]se[n]taneus esse veli[nt]</marginalia_text>
                </position>
            </language>
            <translation>Consistent with, if it would be consistent with itself.</translation> 
        </marginalia>
       
