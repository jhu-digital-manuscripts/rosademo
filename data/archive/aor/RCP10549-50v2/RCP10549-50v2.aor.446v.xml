    <annotation>
        
        <marginalia anchor_text="beneficentia, quam eandem vel benignitatem vel liberalitatem" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>benefice[n]ta benignitas liberalitas</marginalia_text>
                </position>
            </language>
            <translation>Beneficence, benevolence, liberality.</translation>
        </marginalia>
        
        <marginalia anchor_text="propulsant iniuriam. Nam qui iniuste impetum in quempiam facit, aut ira, aut aliqua perturbatione incitatus, is quasi manus videtur afferre socio: qui              aute[m] nec defendit, nec obsistit si potest iniuriae" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>defendere propulsare iniuria[m] obsistere iniuria[e] tutari.</marginalia_text>
                </position>
            </language>
            <translation>To defend, repel injury, oppose injuries, protect. </translation>
        </marginalia>
        
        <marginalia anchor_text="iustitia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Iustitia</marginalia_text>
                </position>
            </language>
            <translation>Justice.</translation>
        </marginalia>
        
        <marginalia anchor_text="iustitiae primum munus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Iustitia[e] munera</marginalia_text>
                </position>
            </language>
            <translation>The obligations of justice.</translation>
        </marginalia>
        
        <marginalia anchor_text="priuata natura nulla" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Priuata nulla, natura</marginalia_text>
                </position>
            </language>
            <translation>No private things by nature.</translation>
        </marginalia>
        
        <marginalia anchor_text="Non nobis solum nati sumus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Non nobis solu[m] nati sum[us]</marginalia_text>
                </position>
            </language>
            <translation>We are not born only for ourselves.</translation>
        </marginalia>
        
        <marginalia anchor_text="communes vtilitates in mediu[m] afferrem mutatione officiorum, dando, accipiendo: tum artibus, tum opera, tum faultatibus deuincire              hominum inter homines societatem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Ho[m]i[nu]m societas &amp; vita[e] communitas.</marginalia_text>
                </position>
            </language>
            <translation>The society of men and the community of life.</translation>
        </marginalia>
        
        <marginalia anchor_text="fidem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Fides.</marginalia_text>
                </position>
            </language>
            <translation>Faith.</translation>
        </marginalia>
        
        <marginalia anchor_text="iniustitiae genera" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Iniustita[e] genera.</marginalia_text>
                </position>
            </language>
            <translation>Kinds of injustice.</translation>
        </marginalia> 
        
        <marginalia anchor_text="metu" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Metus.</marginalia_text>
                </position>
            </language>
            <translation>Fear.</translation>
        </marginalia>
        
        <marginalia anchor_text="auaritia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Avaritia.</marginalia_text>
                </position>
            </language>
            <translation>Avarice.</translation>
        </marginalia>
        
        <marginalia anchor_text="diuitiae tum ad vsus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Diuitiaru[m] vsus.</marginalia_text>
                </position>
            </language>
            <translation>The purpose of riches.</translation>
        </marginalia>
        
        <marginalia anchor_text="Marcus Crassus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>M[arcus] Crassus</marginalia_text>
                    <person name="Marcus Licinius Crassus"/>
                </position>
            </language>
            <translation>Marcus Licinius Crassus.</translation>
        </marginalia>
        
        <marginalia anchor_text="est vt infinita pecuniae cupiditas" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Infinita pecunia[e] cupiditas est.</marginalia_text>
                </position>
            </language>
            <translation>The desire of riches is insatiable.</translation>
        </marginalia>
        
        <marginalia anchor_text="Maxime autem adducuntur plerique vt eos iustitiae capiat obliuio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Iustitia[e] obliuio q[uos] capit maxime</marginalia_text> <!-- FSF: the first word as it is written here is definitely not iustitia -->
                </position>
            </language>
            <translation>Especially when a neglect of justice has seized them.</translation>
        </marginalia>
        
        <marginalia anchor_text="excellere, in eo fit plerunque tanta contentio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Contentio ob excelle[n]tiam</marginalia_text>
                </position>
            </language>
            <translation>The contest towards excellence.</translation>
        </marginalia>
        
        <marginalia anchor_text="C, Caesaris" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>.C[aius] Ca[e]sar.</marginalia_text>
                    <person name="Julius Caesar"/>
                </position>
            </language>
            <translation>Caius Caesar.</translation>
        </marginalia>
        
        <marginalia anchor_text="gloria cupiditates" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>gloria[e] cupiditas</marginalia_text>
                </position>
            </language>
            <translation>The desire for glory.</translation>
        </marginalia>
        
        <marginalia anchor_text="Leuiora enim sunt ea quae repentino aliqio motu accidunt, quàm ea quae meditata &amp; praeparata" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>subita / p[rae]paratis leuiora</marginalia_text>
                </position>
            </language>
            <translation>The approached are shorter when prepared.</translation>
        </marginalia>
        
        <marginalia anchor_text="possunt, non propulsant iniuriam" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Cur cu[m] possim[us] non propulsam[us] iniurias</marginalia_text>
                </position>
            </language>
            <translation>Why I may not be able to repel injuries.</translation>
        </marginalia>
        
        <marginalia anchor_text="co[n]sultato &amp; cogitato fiat iniuria. Leuiora enim sunt ea quae repentino aliquo motu " hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>{ co[n]sulto cogitatio data op[er]a repentino motu</marginalia_text>
                </position>
            </language>
            <translation>Deliberate, considered, offered work, sudden impulse.</translation>
        </marginalia>
        
