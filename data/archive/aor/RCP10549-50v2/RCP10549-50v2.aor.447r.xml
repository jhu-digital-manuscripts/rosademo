        <marginalia anchor_text="" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>madere in incurrere in } vitiu[m]</marginalia_text>
                </position>
            </language>
            <translation>Abounding in crime.</translation>
        </marginalia>
        
        <marginalia anchor_text="maximos in luctus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>madere in luctus</marginalia_text>
                </position>
            </language>
            <translation>Abounding in grief.</translation>
        </marginalia>
        
        <marginalia anchor_text="studio impediti, quos tueri debent, deserunt" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Philosophi discendi studio impediti quos tueri debent deserunt</marginalia_text>
                </position>
            </language>
            <translation>Prevented by the eagerness of learning philosophers desert those whom they should protected.</translation>
        </marginalia>
        
        <marginalia anchor_text="iustum est quod recte sit, si est voluntariu[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Iustu[m] o[mn]e voluntarium</marginalia_text>
                </position>
            </language>
            <translation>The just is always voluntary.</translation>
        </marginalia>
        
        <marginalia anchor_text="deserunt enim vitae societatem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>vita[e] societas deserta.</marginalia_text>
                </position>
            </language>
            <translation>They deserted the society of the living.</translation>
        </marginalia>
        
        <marginalia anchor_text="cuiusque temporis officium" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Officiu[m] cuiusq[ue] temporis</marginalia_text>
                </position>
            </language>
            <translation>Our duty on every occasion.</translation>
        </marginalia>
        
        <marginalia anchor_text="difficilis cura rerum alienaru[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Difficilis cura reru[m] alienaru[m]</marginalia_text>
                </position>
            </language>
            <translation>The charge of other people's affairs is a difficult matter.</translation>
        </marginalia>
        
        <marginalia anchor_text="quidquam agere quod dubites aequum sit an iniquum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Nihil agas q[uod] dubites a[e]quu[m] sit an iniquu[m].</marginalia_text>
                </position>
            </language>
            <translation>You do should do nothing about which you have doubt about whether it be just or unjust.</translation>
        </marginalia>
        
        <marginalia anchor_text="fundamenta iustitiae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Fundame[n]ta iustitia[e]</marginalia_text>
                </position>
            </language>
            <translation>Foundations of justice.</translation>
        </marginalia>
        
        <marginalia anchor_text="officium, &amp; non semper est idem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>officiu[m] non semper est ide[m].</marginalia_text>
                </position>
            </language>
            <translation>The duty is not always the same.</translation>
        </marginalia>
        
        <marginalia anchor_text="Neptunus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Neptunus</marginalia_text>
                    <person name="Neptune"/>
                </position>
            </language>
            <translation>Neptune</translation>
        </marginalia>
        
        <marginalia anchor_text="promissa igitur seruanda" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Promissa vt serua[n]da</marginalia_text>
                </position>
            </language>
            <translation>Promises are to be kept.</translation>
        </marginalia>
        
        <marginalia anchor_text="promissis standum non esse quis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Quibus promissis no[n] standu[m]</marginalia_text>
                </position>
            </language>
            <translation>Which promises does one not have to keep.</translation>
        </marginalia>
        
        <marginalia anchor_text="Summu[m] ius, summa iniuria" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>summu[m] ius su[m]ma iniuria</marginalia_text>
                </position>
            </language>
            <translation>The strictest law is the greatest injustice.</translation>
        </marginalia>
        
        <marginalia anchor_text="Q. Fabiu[m] Labeonem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Q[uintus] F[abius] Labeo.</marginalia_text>
                    <person name="Quintus Fabius Labeo"/>
                </position>
            </language>
            <translation>Quintus Fabius Labeo.</translation>
        </marginalia>
        
        <marginalia anchor_text="officia etiam aduersus eos seruanda, à quibus inuriam acceperis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>officia aduersus eos à q[ui]bus iniuria[m] acceperis serua[n]da</marginalia_text>
                </position>
            </language>
            <translation>Duties towards those, from whom you have received injuries, are to be kept.</translation>
        </marginalia>
        
        <marginalia anchor_text="duo genera decerta[n]di" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Duo decerta[n]di genera.</marginalia_text>
                </position>
            </language>
            <translation>Two kinds of contention.</translation>
        </marginalia>
        
        <marginalia anchor_text="Quare suscipie[n]da quidem bella" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Cur. bellu[m] suscipiendu[m]</marginalia_text>
                </position>
            </language>
            <translation>Why wars are to be undertaken.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>mutari</marginalia_text>
                </position>
            </language>
            <translation>Remove</translation>
        </marginalia>
        
        <marginalia anchor_text="regredi" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>regredi</marginalia_text>
                </position>
            </language>
            <translation>To go back.</translation>
        </marginalia>
        
        <marginalia anchor_text="" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>terminare fines</marginalia_text>
                </position>
            </language>
            <translation>To mark the boundaries of the ends.</translation>
        </marginalia>
        
        <marginalia anchor_text="aliqua[n]tum agri in medio relictum est. Itaqye illoru[m] fines, sicut ipso dixerant terminauit: in medio relictu[m] quod erat, Po. Ro. adiudicauit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>adiudicare alicui, aliquid</marginalia_text>
                </position>
            </language>
            <translation>To award someone something.</translation>
        </marginalia>
        
        <marginalia anchor_text="destitutum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>destitutus desertus</marginalia_text>
                </position>
            </language>
            <translation>Destitute, deserted</translation>
        </marginalia>
        
        <marginalia anchor_text="nihil praeter auditum habeo" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>nihil p[rae]ter auditu[m] habeo</marginalia_text>
                </position>
            </language>
            <translation>I have nothing but a report.</translation>
        </marginalia>
        
