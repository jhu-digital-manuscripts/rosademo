<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v2.448r.tif" pagination="347" reader="John Dee" signature="[Yy.vi.]"/> 
    <annotation>
        
        <marginalia anchor_text="beneficentia ac liberalitate dicatur, qua quidem nihil est naturae hominis acco[m]modatius. Sed habet multas cautiones. Videndum est enim primùm ne obsit              benignitas" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>benifice[n]tia benignitas liberalitas</marginalia_text>
                </position>
            </language>
            <translation>Beneficence, kindness, liberality.</translation>
        </marginalia>
        
        <marginalia anchor_text="capitalior" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>capitalior</marginalia_text>
                </position>
            </language>
            <translation>More pernicious.</translation>
        </marginalia>
        
        <marginalia anchor_text="beneficentia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Benificentia.</marginalia_text>
                </position>
            </language>
            <translation>Beneficence.</translation>
        </marginalia>
        
        <marginalia anchor_text="cautiones" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Cautiones</marginalia_text>
                </position>
            </language>
            <translation>Cautions.</translation>
        </marginalia>
        
        <marginalia anchor_text="assentatores" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Assentatores</marginalia_text>
                </position>
            </language>
            <translation>Sycophants.</translation>
        </marginalia>
        
        <marginalia anchor_text="cupidi splendoris &amp; gloriae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Cupidi splendoris &amp; glor[iae]</marginalia_text>
                </position>
            </language>
            <translation>Ambitions of grandeur and glory.</translation>
        </marginalia>
        
        <marginalia anchor_text="Videndum est igitur vt ea liberalitate" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>qua vtendu[m] liberalitate est</marginalia_text>
                </position>
            </language>
            <translation>Is so far as liberality is employed.</translation>
        </marginalia>
        
        <marginalia anchor_text="benigniores esse volunt quàm res patitur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>benigniores q[uam] res patit[ur]</marginalia_text>
                </position>
            </language>
            <translation>More generous than ability admits.</translation>
        </marginalia>
        
        <marginalia anchor_text="gloria ductos" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>GLoria Ducti</marginalia_text>
                </position>
            </language>
            <translation>Those led by glory.</translation>
        </marginalia>
        
        <marginalia anchor_text="simulacra virtutis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Simulacra virtutis</marginalia_text>
                </position>
            </language>
            <translation>Representations of virtue.</translation>
        </marginalia>
        
        <marginalia anchor_text="virtutibus his leuioribus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Leuiores virtutes</marginalia_text>
                </position>
            </language>
            <translation>Meeker virtues.</translation>
        </marginalia>
        
        <marginalia anchor_text="beneuolentia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Benivolentia</marginalia_text>
                </position>
            </language>
            <translation>Benevolence.</translation>
        </marginalia>
        
        <marginalia anchor_text="Nullum enim officium referenda gratia magis necessarium" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Nullu[m] officiu[m] referenda gr[ati]a magis necessariu[m]</marginalia_text>
                </position>
            </language>
            <translation>No duty is more necessary than in requiring kindness.</translation>
        </marginalia>
        
        <marginalia anchor_text="agros fertiles" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Agri fertiles.</marginalia_text>
                </position>
            </language>
            <translation>Fertile lands.</translation>
        </marginalia>
        
        <marginalia anchor_text="duo genera liberalitatis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Duo genera liberalitatis</marginalia_text>
                </position>
            </language>
            <translation>Two kinds of generosity.</translation>
        </marginalia>
        
        <marginalia anchor_text="Acceptorum aute[m] beneficioru[m] sunt delectus habendi" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Acceptoru[m] Beneficiorum Delectus habendi.</marginalia_text>
                </position>
            </language>
            <translation>Selections of benefits received are to be considered.</translation>
        </marginalia>
        
        <marginalia anchor_text="maxime officii est, vt quisque maxime opis indigeat, ita ei potissimum opitulari" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Qui maxime opis indigeat ei opitulandum maxime</marginalia_text>
                </position>
            </language>
            <translation>Who wants assistance the most we ought to assist.</translation>
        </marginalia>
        
