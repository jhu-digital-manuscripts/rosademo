<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v2.449v.tif" pagination="350" reader="John Dee" signature="[Yy.vii.[v]]"/> 
    <annotation>
        
        <marginalia anchor_text="splendor omnis &amp; amplitudo" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>Splendor amplitudo</marginalia_text>
                </position>
            </language>
            <translation>Eminence, dignity.</translation>
        </marginalia>
 
        <marginalia anchor_text="fortis animi magníque ducedum est: &amp; ea quae videntur acerba, quae multa &amp; varia in hominum vita fortunáque versantur,              ita ferre, vt nihil á statu naturae discedas, nihil à dignitae sapie[n]tis, robusti animi est, magnaéque constantiae" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>animus altus elatus magnus fortis robustus exelsus magnificus</marginalia_text>
                </position>
            </language>
            <translation>A mind, high, elevated, great, brave, lofty, noble. </translation>
        </marginalia>
        
        <marginalia anchor_text="angusti animi támnque parui" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>angust[us] parus</marginalia_text>
                </position>
            </language>
            <translation>Contracting, small.</translation>
        </marginalia>
        
        <marginalia anchor_text="vera autem &amp; sapiens animi magnitudo" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>vera sapiens ] magnitudo a[nim]i</marginalia_text>
                </position>
            </language>
            <translation>True and genuine magnanimity of mind.</translation>
        </marginalia>
        
        <marginalia anchor_text="magnanimi" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Magnanimi.</marginalia_text>
                </position>
            </language>
            <translation>Magnanimous.</translation>
        </marginalia>
        
        <marginalia anchor_text="principémque se esse mauult quàm videri" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Principe[m] esse melius q[uam] videri. est.</marginalia_text>
                </position>
            </language>
            <translation>Better he were a ruler than to appear so.</translation>
        </marginalia>
        
        <marginalia anchor_text="gloiae cupido" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>gloria[e] cupiditas</marginalia_text>
                </position>
            </language>
            <translation>A desire for glory.</translation>
        </marginalia>
        
        <marginalia anchor_text="animus &amp; magnus duabus rebus maxime cernitur" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Duabus in rebus fortis a[n]i[m]us maxime cernitur.</marginalia_text>
                </position>
            </language>
            <translation>A brave mind is chiefly discerned in two things.</translation>
        </marginalia>
        
        <marginalia anchor_text="excellentes animos &amp; humana co[n]temnentes" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Animus excellens humana omnia contemnit.</marginalia_text>
                </position>
            </language>
            <translation>An elevated mind disregards human affairs.</translation>
        </marginalia>
        
        <marginalia anchor_text="pecuniae fugienda cupiditas" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Pecunia[e] Cupiditas</marginalia_text>
                </position>
            </language>
            <translation>The desire for money.</translation>
        </marginalia>
        
        <marginalia anchor_text="Cauenda est etiam gloriae cupiditas" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Gloria[e] cupiditas cavenda.</marginalia_text>
                </position>
            </language>
            <translation>A desire for glory is to be avoided.</translation>
        </marginalia>
        
        <marginalia anchor_text="Vacandum autem est omni animi pertubatione" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Pertubationib[us] vacanda[m].</marginalia_text>
                </position>
            </language>
            <translation>Free from pertubations.</translation>
        </marginalia>
        
        <marginalia anchor_text="tranquillitas" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Tranquillitas.</marginalia_text>
                </position>
            </language>
            <translation>Tranquillity.</translation>
        </marginalia>
        
        <marginalia anchor_text="principu[m] mores" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Principu[m] mores.</marginalia_text>
                </position>
            </language>
            <translation>The manners of directors.</translation>
        </marginalia>
        
        <marginalia anchor_text="libertate vterentur, cuius proprium" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Libertatis proprium.</marginalia_text>
                </position>
            </language>
            <translation>Liberty is proper.</translation>
        </marginalia>
         
        <marginalia anchor_text="philosophi, li[n]géque principes" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Vita | Philosophoru[m] principu[m]</marginalia_text>
                </position>
            </language>
            <translation>Life of philosophers and leaders.</translation>
        </marginalia>
        
        <marginalia anchor_text="Remp. no[n] capessentibus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Quale[m] afferre debet ph[ilosoph]us caussa[m] cur remp[ublicam] non capesset</marginalia_text> 
                </position>
            </language>
            <translation>A philosopher must give a reason for why he did not understand the state.</translation>
        </marginalia>
        
        <marginalia anchor_text="no[n] satis constanter" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Inconstantia</marginalia_text>
                </position>
            </language>
            <translation>Inconstancy.</translation>
        </marginalia>
        
        <marginalia anchor_text="in eo quòd" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>in eo quòd</marginalia_text>
                </position>
            </language>
            <translation>In that because.</translation>
        </marginalia>
        
