    <annotation>
        
        <marginalia anchor_text="imperium Lacedaemonis dilitatum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>imp[er]iu[m] dilatare.</marginalia_text>
                </position>
            </language>
            <translation>To enlarge the empire.</translation>
        </marginalia>
        
        <marginalia anchor_text="parentiores habuerunt exercitus &amp; fortiores" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>exercitus pare[n]tiores fortiores</marginalia_text>
                </position>
            </language>
            <translation>Armies, better disciplined, braver.</translation>
        </marginalia>
        
        <marginalia anchor_text="adiumenta" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>adiume[n]tu[m]</marginalia_text>
                </position>
            </language>
            <translation>Assistance.</translation>
        </marginalia>
        
        <marginalia anchor_text="singularis &amp; vir &amp; Impertator" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>Singularis et vir imp[er]ator</marginalia_text>
                </position>
            </language>
            <translation>Extraordinary man and general.</translation>
        </marginalia>
        
        <marginalia anchor_text="vir abundans bellicis laudibus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>vir abundans bellicis laudib[us]</marginalia_text>
                </position>
            </language>
            <translation>A man abounding with military honours.</translation>
        </marginalia>
       
        
        <marginalia anchor_text="abiecta omni cunctatione" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>abiecta o[mn]i cunctatio[n]e</marginalia_text>
                </position>
            </language>
            <translation>All hesitation cast aside.</translation>
        </marginalia>
        
        <marginalia anchor_text="philosophis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Philosophi.</marginalia_text>
                </position>
            </language>
            <translation>Philosophers.</translation>
        </marginalia>
        
        <marginalia anchor_text="magnificentia &amp; despicientia abhibenda sit rerum humanurum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Magnifice[n]tia, cu[m] rerum humanaru[m] despicie[n]tia.</marginalia_text>
                </position>
            </language>
            <translation>Greatness with contempt for humane things.</translation>
        </marginalia>
        
        <marginalia anchor_text="mangitudo animi est adhibe[n]da, &amp; vacuitas an angoribus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Magnitudo a[nim]i &amp; vacuitas ab angoribus abhibenda.</marginalia_text>
                </position>
            </language>
            <translation>Elevation of the mind and exemption from being possessed by perplexities.</translation>
        </marginalia>
        
        <marginalia anchor_text="negociis prius quàm aggrediare, abhidenda est praeparatio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>pra[e]paratio a[nim]i ad o[mn]e negocu[m]</marginalia_text>
                </position>
            </language>
            <translation>Preparation of the mind in all affairs.</translation>
        </marginalia>
        
        <marginalia anchor_text="res bellicas maiores esse quàm vrbanas" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Vrbanas res Bellicis maiores esse</marginalia_text>
                </position>
            </language>
            <translation>Military things are greater than civil ones.</translation>
        </marginalia>
        
        <marginalia anchor_text="Themistocles iure laudetur, &amp; sit eius nomen quàm Solonis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Themistocles. Solon.</marginalia_text>
                    <person name="Themistocles"/>
                    <person name="Solon"/>
                </position>
            </language>
            <translation>Themistocles, Solon. </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>A te[m]poris diuturnitate</marginalia_text>
                </position>
            </language>
            <translation>Since a long duration of time.</translation>
        </marginalia>
        
        <marginalia anchor_text="Pausania Lysandróque" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Pausanias. Lysander</marginalia_text>
                    <person name="Pausanias of Sparta"/>
                    <person name="Lysander"/>
                </position>
            </language>
            <translation>Pausanias, Lysander.</translation>
        </marginalia>
        
        <marginalia anchor_text="Lycurgi" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Lycurgus.</marginalia_text>
                    <person name="Lycurgus of Sparta"/>
                </position>
            </language>
            <translation>Lycurgus.</translation>
        </marginalia>
        
        <marginalia anchor_text="M. Scaurus C. Mario, neque quu[m] versaremur in Rep. Q. Catulus Cn. Pompeio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>M. Scaurus. C. Marius Q Catulus. Cn. Pompeius</marginalia_text>
                    <person name="Marcus Aemilius Scaurus"/>
                    <person name="Gaius Marius"/>
                    <person name="Quintus Lutatius Catulus"/>
                    <person name="Pompey"/>
                </position>
            </language>
            <translation>Marcus Aemilius Scaurus, Caius Marius, Quintus Lutatius Catulus, Pompey.</translation>
        </marginalia>
        
        <marginalia anchor_text="P. Nascia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Scipio. P. Nascia</marginalia_text>
                    <person name="Publius Cornelius Scipio Nasica Serapio"/>
                </position>
            </language>
            <translation>Scipio Publicus Nasica.</translation>
        </marginalia>
        
        <marginalia anchor_text="Ti. Gracchu[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>T. Gracchus</marginalia_text>
                    <person name="Tiberius Gracchus"/>
                </position>
            </language>
            <translation>Tiberius Gracchus.</translation>
        </marginalia>
        
        <marginalia anchor_text="Cedant arma togae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Ca[e]dant arma toga[e]</marginalia_text>
                </position>
            </language>
            <translation>Arms yield to the gown.</translation>
        </marginalia>
        
        <marginalia anchor_text="haereditsa huius gloriae, &amp; factorum imitatio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Gloria[e] ha[e]reditas. factorum imitatio.</marginalia_text>
                </position>
            </language>
            <translation>The heritage of this glory and the imitation of my actions.</translation>
        </marginalia>
        
        <marginalia anchor_text="Cn. Pompeius" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Cn. Pompeius</marginalia_text>
                    <person name="Pompey"/>
                </position>
            </language>
            <translation>Cnaeus Pompeius.</translation>
        </marginalia>
        
        <marginalia anchor_text="ex animo excelso magnificòque quaerimus, animi efficitur, no[n] corporis viribus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Vires { Animi Corporis</marginalia_text>
                </position>
            </language>
            <translation>Powers of the mind and body.</translation>
        </marginalia>
        
        <marginalia anchor_text="Exercendu[m] tamen corpus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Corporis exercitatio qualis</marginalia_text>
                </position>
            </language>
            <translation>What kind of exercise of the body</translation>
        </marginalia>
        
        <marginalia anchor_text="M. Catonis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>M. Ca[e]to.</marginalia_text>
                    <person name="Cato the Elder"/>
                </position>
            </language>
            <translation>Marcus Cato.</translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="abiecta omni cunctatione adipiscendi" type="straight"/>
        <underline language="LA" method="pen" text="minus multa patent in eorum vita quae fortuna feriat, &amp; quo" type="straight"/>
        <underline language="LA" method="pen" text="Ad rem gerendam aute[m] qui accedit, caueat ne id modo consideret, quàm illa res honesta sit, sed etia[m] vt habeat              efficiendi facultate[m]. In quo" type="straight"/>
        <underline language="LA" method="pen" text="enim bella saepe quaesiuerunt propter" type="straight"/>
        <underline language="LA" method="pen" text="vrbanae maiores clariorésque" type="straight"/>
        <underline language="LA" method="pen" text="no[n] minus praeclarum" type="straight"/>
        <underline language="LA" method="pen" text="quanquam imperium" type="straight"/>
        <underline language="LA" method="pen" text="dilatatum putatur" type="straight"/>
        <underline language="LA" method="pen" text="causas &amp; parentiores" type="straight"/>
        <underline language="LA" method="pen" text="neque pueris nobis" type="straight"/>
        <underline language="LA" method="pen" text="Parua enim sunt foris arma, nisi est consilium domi" type="straight"/>
        <underline language="LA" method="pen" text="res no[n] solum ex domestica est ratione, sed attingit etia[m]" type="straight"/>
        <underline language="LA" method="pen" text="solere ad inuidis &amp; improbis audito" type="straight"/>
        <underline language="LA" method="pen" text="nostra celeriter de manibus audacissimoru[m] ciuium delapsa arma ipsa ceciderunt" type="straight"/>
        <underline language="LA" method="pen" text="Marce fili" type="straight"/>
        <underline language="LA" method="pen" text="abundans bellicis laudibus" type="straight"/>
        <underline language="LA" method="pen" text="triumphu[m] deportaturum fuisse" type="straight"/>
        <underline language="LA" method="pen" text="his operae studiíque ponendu[m] est. Omnino" type="straight"/>
        <underline language="LA" method="pen" text="Exercendu[m] tamen corpus, &amp; ita efficiendum est, vt obedire consilio rationíque possit in exequendis negociis,              &amp; in labore tolerando. Honestum" type="straight"/>
        
