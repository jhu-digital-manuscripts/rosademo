<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v2.451v.tif" pagination="354" reader="John Dee" signature="[Z.z.i[v]]"/>
    <annotation>
        
        <marginalia anchor_text="separari" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>distingui separari.</marginalia_text>
                </position>
            </language>
            <translation>To be distinguished, to be separated.</translation>
        </marginalia>
        
        <marginalia anchor_text="cum virtute confusum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>confusu[m] cu[m] virtute</marginalia_text>
                </position>
            </language>
            <translation>Combined with virtue.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>hoc ip[s]o, quòd in eo, in q[uo].</marginalia_text>
                </position>
            </language>
            <translation>This itself, which, in it, in which.</translation>
        </marginalia>
        
        <marginalia anchor_text="reco[n]dita quadam ratione" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>reco[n]dita r[ati]o[n]e cerni</marginalia_text>
                </position>
            </language>
            <translation>To be examined by a hidden reasoning.</translation>
        </marginalia>
        
        <marginalia anchor_text="cum specie quadam liberali" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>cu[m] sp[eci]e q[ua]da[m] liberali</marginalia_text> 
                </position>
            </language>
            <translation>With a certain generous hope.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="EL">
                <position book_orientation="0" place="head">
                    <marginalia_text>τὸ πρέπον</marginalia_text> 
                </position>
            </language>
            <translation>That which is appropriate.</translation>
        </marginalia>
        
        <marginalia anchor_text="quid deceat apparet" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Quid decet, quid dedecet</marginalia_text>
                </position>
            </language>
            <translation>What is fitting, what is unsuitable.</translation>
        </marginalia>
        
        <marginalia anchor_text="venustas; valetudine" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>valetudo. venustas.</marginalia_text>
                </position>
            </language>
            <translation>Good health. Attractiveness.</translation>
        </marginalia>
        
        <marginalia anchor_text="duplex. nam &amp; generale quoddam decorum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Decorum duplex.</marginalia_text>
                </position>
            </language>
            <translation>The divide of beaty.</translation>
        </marginalia>
        
        <marginalia anchor_text="" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Decorum generale &amp; particulare</marginalia_text>
                </position>
            </language>
            <translation>The general and particular of beauty.</translation>
        </marginalia>
        
        <marginalia anchor_text="seruare illud poetas quod deceat" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Poeta[e] decorum serua[n]t</marginalia_text>
                </position>
            </language>
            <translation>The poets of beauty watch over.</translation>
        </marginalia>
        
        <marginalia anchor_text="Nobis autem personam imposuit ipsa natura" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Nobis p[er]sona[m] imposuit Na[m]</marginalia_text>
                </position>
            </language>
            <translation>For [nature itself] imposed a mask on us.</translation>
        </marginalia>
        
        <marginalia anchor_text="Nobis autem quum à naturam constantiae, moderationism tempera[n]tiae, verecundiae, partes datae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Qua[e] partes nobis su[n]t à natura data[e].</marginalia_text>
                </position>
            </language>
            <translation>Which parts are given by nature to us.</translation>
        </marginalia>
        
        <marginalia anchor_text="pulchritudo corporis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Comparatio pulchritudinis &amp; temperantia[e]</marginalia_text>
                </position>
            </language>
            <translation>A combination of beauty and temperance.</translation>
        </marginalia>
        
        <marginalia anchor_text="reuerentia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Reuerentia</marginalia_text>
                </position>
            </language>
            <translation>Reverence.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Fama &amp; nome[n] bonu[m]</marginalia_text> 
                </position>
            </language>
            <translation>Fame and a good name.</translation>
        </marginalia>
        
        <marginalia anchor_text="Iustitiae partes sunt, non violare homines: verecundiae" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Iustiti[a]e et et verecundi[a]e discrimen.</marginalia_text>
                </position>
            </language>
            <translation>The dividing line of justice and modesty.</translation>
        </marginalia>
        
        <marginalia anchor_text="conuenientiam co[n]seruationémque naturae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Convenientia &amp; conservatio natura[e].</marginalia_text>
                </position>
            </language>
            <translation>The harmony and preservation of nature.</translation>
        </marginalia>
        
        <marginalia anchor_text="Duplex est enim animoru[m] vis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Duplex animoru[m] vis</marginalia_text>
                </position>
            </language>
            <translation>The twofold force of mind.</translation>
        </marginalia>
        
        <marginalia anchor_text="ratio praesit, appetitus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Appetitus. R[ati]o.</marginalia_text>
                </position>
            </language>
            <translation>Desire. Reason.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>approbatio</marginalia_text>
                </position>
            </language>
            <translation>Approbation.</translation>
        </marginalia>
        
        <marginalia anchor_text="consociationem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>co[n]sociatio</marginalia_text>
                </position>
            </language>
            <translation>Association.</translation>
        </marginalia>
        
        <marginalia anchor_text="aduersus homines" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>aduersus ho[m]i[n]es</marginalia_text>
                </position>
            </language>
            <translation>Against men.</translation>
        </marginalia>
        
        <marginalia anchor_text="acutum &amp; perspicax natura est, &amp; id quod ad hominum consociationem accomoodatum est, &amp; id quod vehemens atque forte" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>acutu[m] p[er]spicax vehemens forte</marginalia_text>
                </position>
            </language>
            <translation>Sharpened. Penetrating. Vehement. Strong.</translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="facilius intelligi quàm explanari potest" type="straight"/>
        <underline language="LA" method="pen" text="&amp; agere quod agas considerate" type="straight"/>
        <underline language="LA" method="pen" text="dedecet, quàm delirare &amp; mente captu[m] esse" type="straight"/>
        <underline language="LA" method="pen" text="pertinet, vt non reco[n]dita quadam ratione" type="straight"/>
        <underline language="LA" method="pen" text="quod cogitatione magis à virtute potest, quàm re separari" type="straight"/>
        <underline language="LA" method="pen" text="cum virtute consusum" type="straight"/>
        <underline language="LA" method="pen" text="Decorum id esse, quod consentaneum sit hominis excellentiae in eo, in quo natura eius à reliquis animantibus differat" type="straight"/>
        <underline language="LA" method="pen" text="cum specie quadam" type="straight"/>
        <underline language="LA" method="pen" text="sona dignum est, &amp; fit" type="straight"/>
        <underline language="LA" method="pen" text="personam imposuit ipsa natura" type="straight"/>
        <underline language="LA" method="pen" text="magna cum excellentia praestantiáque animantium reliquorum" type="straight"/>
        <underline language="LA" method="pen" text="constantiae" type="straight"/>
        <underline language="LA" method="pen" text="moderationis" type="straight"/>
        <underline language="LA" method="pen" text="tempera[n]tiae, verecundiae, partes" type="straight"/>
        <underline language="LA" method="pen" text="aduersus homines" type="straight"/>
        <underline language="LA" method="pen" text="apta compositione membrorum" type="straight"/>
        <underline language="LA" method="pen" text="Nam negliere quid de se quisque sentiat, non solum arrogantis est, sed etiam omnino dissoluti" type="straight"/>
        <underline language="LA" method="pen" text="conuenientiam co[n]seruationémque naturae" type="straight"/>
        
        <symbol name="Jupiter" place="left_margin"/>
