    <annotation>
        
        <marginalia anchor_text="summissum &amp; abiectum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>summissus abiectus</marginalia_text>
                </position>
            </language>
            <translation>Quiet. Humble.</translation>
        </marginalia>
        
        <marginalia anchor_text="se effere[n]tem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>se effere[n]s dedecus co[n]cipere.</marginalia_text>
                </position>
            </language>
            <translation>Busying themselves to contain disgrace.</translation>
        </marginalia>
        
        <marginalia anchor_text="relaxare animos, &amp; dare se iucunditati" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>relaxare a[n]i[m]os dare se iucunditati.</marginalia_text>
                </position>
            </language>
            <translation>To relaxe the minds, to give themselves to pleasantness.</translation>
        </marginalia>
        
        <marginalia anchor_text="languori se desidiaéque" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>languori desidi[a]e } dedere se</marginalia_text>
                </position>
            </language>
            <translation>To give themselves to feebleness and idleness.</translation>
        </marginalia>
        
        <marginalia anchor_text="adolescentis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Adolescentu[m] officiu[m]</marginalia_text>
                </position>
            </language>
            <translation>The duty of the adolescents.</translation>
        </marginalia>
        
        <marginalia anchor_text="senum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Senes.</marginalia_text>
                </position>
            </language>
            <translation>Old men.</translation>
        </marginalia>
        
        <marginalia anchor_text="Senibus aute[m] labores" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Senu[m] officiu[m]</marginalia_text>
                </position>
            </language>
            <translation>The duty of old men.</translation>
        </marginalia>
        
        <marginalia anchor_text="Luxuria" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Luxuria</marginalia_text>
                </position>
            </language>
            <translation>Luxury.</translation>
        </marginalia>
        
        <marginalia anchor_text="libidinum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Libido</marginalia_text>
                </position>
            </language>
            <translation>Desire.</translation>
        </marginalia>
        
        <marginalia anchor_text="magistratuum, de priuatorum, de ciuium, de peregrinoru[m] officiis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Magistratus officiu[m]</marginalia_text>
                </position>
            </language>
            <translation>The duty of a magistrate.</translation>
        </marginalia>
        
        <marginalia anchor_text="Priuatum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Priuatus.</marginalia_text>
                </position>
            </language>
            <translation>Private.</translation>
        </marginalia>
        
        <marginalia anchor_text="Peregrini" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Peregrinus</marginalia_text>
                </position>
            </language>
            <translation>Foreigner.</translation>
        </marginalia>
        
        <marginalia anchor_text="constantiam" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Constantia</marginalia_text>
                </position>
            </language>
            <translation>Perseverance.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Corpus n[ost]r[u]m.</marginalia_text> 
                </position>
            </language>
            <translation>Our body.</translation>
        </marginalia>
        
        <marginalia anchor_text="fabricam imitata est hominu[m] verecundia" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Ho[m]i[nu]m verecu[n]dia, natura[e] fabrica[m] est imitata</marginalia_text>
                </position>
            </language>
            <translation>'Man's modesty has followed this careful contrivance of nature' [Walter Miller, 1913].</translation>
        </marginalia>
        
        <marginalia anchor_text="petulantia vacat, nec orationis obscoenitas" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Petula[n]tia. or[ati]o[n]is obsco[e]nitas</marginalia_text>
                </position>
            </language>
            <translation>Insolence. The indecency of speech.</translation>
        </marginalia>
        
        <marginalia anchor_text="Cynici" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Cynici.</marginalia_text>
                </position>
            </language>
            <translation>Cynics.</translation>
        </marginalia>
        
        <marginalia anchor_text="Staus, incessus, sessio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>status. incessus. sessio.</marginalia_text>
                </position>
            </language>
            <translation>Standing still, walking, setting.</translation>
        </marginalia>
        
        <marginalia anchor_text="histrionibus oratoribúsque concedendum est, vt his haec apta sint, nobis dissoluta. Scenicorum quidem mos tanta[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Ergo histrio hoc videbit in scena q[uod] sapiens vir non videbit in vita?</marginalia_text>
                </position>
            </language>
            <translation>Therefore this actor will see in the theater what a wise man will not see in life.</translation>
        </marginalia>
        
        <marginalia anchor_text="verecundia[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Verecundia</marginalia_text>
                </position>
            </language>
            <translation>Shame.</translation>
        </marginalia>
        
        <marginalia anchor_text="pulchritudinis duo genera" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Pulchritudinis duo genera</marginalia_text>
                </position>
            </language>
            <translation>Two kinds of excellence.</translation>
        </marginalia>
        
        <marginalia anchor_text="abhorret ab oculorum auriúmque approbatione" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>contego abdo. occulto. removere ab oculis abhorret ab oculor[um] auriumq[ue] approbatione</marginalia_text>
                </position>
            </language>
            <translation>To conceal, head, cover up. To remove from [one's] eyes. 'Is offensive to our eyes and ears' [Walter Miller, 1913].</translation>
        </marginalia>
        
        <marginalia anchor_text="effoeminatum aut molle, &amp; nequid durum aut rusticum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>effo[e]minatu[m] molle duru[m] rusticu[m].</marginalia_text>
                </position>
            </language>
            <translation>Effeminate, over-nice, coarse, boorish [Walter Miller, 1913].</translation>
        </marginalia>
        
        <marginalia anchor_text="venustatem" hand="Italian">
            <language ident="EN">
                <position book_orientation="0" place="intext">
                    <marginalia_text>beawtie</marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <marginalia anchor_text="dignitatem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="intext">
                    <marginalia_text>cumlines</marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <underline language="LA" method="pen" text="distictione" type="straight"/>
        <underline language="LA" method="pen" text="Ineuntis enim aetatis inscitia, senum constituenda &amp; regenda prudentia" type="straight"/>
        <underline language="LA" method="pen" text="à libidinibus arcenda est, exerce[n]dáque in labore patientiáque &amp; animi &amp; corporis: vt eorum" type="straight"/>
        <underline language="LA" method="pen" text="quod erit facilius, si in eiusmodi quidem rebus maiores natu velint interesse" type="straight"/>
        <underline language="LA" method="pen" text="languori se desidiáeque dedat" type="straight"/>
        <underline language="LA" method="pen" text="dedecus concipit" type="straight"/>
        <underline language="LA" method="pen" text="impudentiorem intempera[n]tiam" type="straight"/>
        <underline language="LA" method="pen" text="deberéque eius dignitatem &amp; decus sustinere" type="straight"/>
        <underline language="LA" method="pen" text="summissum &amp; abiectum" type="straight"/>
        <underline language="LA" method="pen" text="nihil praeter suum negocium agere, nihil de alieno inquirere, miniméque esse in aliena repub. curiosum" type="straight"/>
        <underline language="LA" method="pen" text="esset species honesta, eam" type="straight"/>
        <underline language="LA" method="pen" text="eas contexit atque abdidit. Hanc" type="straight"/>
        <underline language="LA" method="pen" text="occultissime pareant" type="straight"/>
        <underline language="LA" method="pen" text="quódque facere turpe non est, modo occulte, id dicere, obscoenum est" type="straight"/>
        <underline language="LA" method="pen" text="liberis dare operam" type="straight"/>
        <underline language="LA" method="pen" text="nequid effoeminatum aut molle, &amp; nequid durum aut rusticum" type="straight"/>
        <underline language="LA" method="pen" text="vt his haec apta sint, nobis dissoluta" type="straight"/>
        
