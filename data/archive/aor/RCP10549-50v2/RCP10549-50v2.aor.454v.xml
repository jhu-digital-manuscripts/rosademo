            <language ident="EL">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>εὐκαιρία</marginalia_text>
                </position>
            </language>
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>occasio.</marginalia_text>
                </position>
            </language>
            <translation>A timeless occasion.</translation>
        </marginalia>
        
        <marginalia anchor_text="prudentiae" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Prudentia.</marginalia_text>
                </position>
            </language>
            <translation>Prudence.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Ob.</marginalia_text>
                </position>
            </language>
            <translation>Against.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Sol.</marginalia_text>
                </position>
            </language>
            <translation>Sun.</translation>
        </marginalia>
        
        <marginalia anchor_text="order actionum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>ordo actionu[m]</marginalia_text>
                </position>
            </language>
            <translation>Orderliness of conduct.</translation>
        </marginalia>
        
