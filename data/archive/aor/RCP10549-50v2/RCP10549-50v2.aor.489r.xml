        <underline language="LA" method="pen" text="igni vacuu[m]" type="straight"/>
        <underline language="LA" method="pen" text="conferatur: id quod medium est, tum primu[m] fit, tum postremum" type="straight"/>
        <underline language="LA" method="pen" text="si vniuersitatis corpus planum &amp; aequabile explicaretur, neque" type="straight"/>
        <underline language="LA" method="pen" text="colligaret" type="straight"/>
        <underline language="LA" method="pen" text="terrae proportione redderet" type="straight"/>
        <underline language="LA" method="pen" text="rebus numero quatuor" type="straight"/>
        <underline language="LA" method="pen" text="est corpus" type="straight"/>
        <underline language="LA" method="pen" text="co[n]strictus comparatione" type="straight"/>
        <underline language="LA" method="pen" text="quo ipse se concordi quadam amicitia &amp; charitate complectitur: atque" type="straight"/>
        <underline language="LA" method="pen" text="morbus eum possit, aut senectus attingere" type="straight"/>
        <underline language="LA" method="pen" text="coagme[n]tatio corporis, vel" type="straight"/>
        <underline language="LA" method="pen" text="effector" type="straight"/>
        <underline language="LA" method="pen" text="molitor" type="straight"/>
        <underline language="LA" method="pen" text="forma figurauit, qua" type="straight"/>
        <underline language="LA" method="pen" text="vocant, cuius omnis extremitas paribus à medio radiis attingitur. ídque ita tornauit, vt nihil effici possit rotu[n]dius, nihil vt asperitatis              haberet nihil offensionis, nihil inclusum angulis, nihil anfractibus, nihil eminens, nihil lacunosum, omnésque partes simillimae omnium, quoad" type="straight"/>
        <underline language="LA" method="pen" text="Omni aute[m]" type="straight"/>
        <underline language="LA" method="pen" text="anima" type="straight"/>
        <underline language="LA" method="pen" text="aut detractionem confecti &amp; consumpti" type="straight"/>
        <underline language="LA" method="pen" text="decessio fieri poterat neque accessio" type="straight"/>
        <underline language="LA" method="pen" text="qui ista iunxit, &amp; condidit: ipsum" type="straight"/>
        <underline language="LA" method="pen" text="Motum enim dedit caelo, eum qui figuratus sit aptissimus, qui vnus ex septem motibus mentem atque intelligentiam cieret: maxime" type="straight"/>
        <underline language="LA" method="pen" text="erratione" type="straight"/>
        
