<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="RCP10549-50v2.489r.tif" pagination="429" reader="John Dee" signature="[DDD.vii.]"/>   
    <annotation>
        
        <marginalia anchor_text="mundum atque vnígenam" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>Mundus vnus vnigena</marginalia_text>
                </position>
            </language>
            <translation>The world, one and only-begotten.</translation>
        </marginalia>
        
        <marginalia anchor_text="colligaret ... copulentur ... coniungeret ... co[n]strictus" hand="English_secretary"> 
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>copulari colligare co[n]iungere constringere</marginalia_text> 
                </position>
            </language>
            <translation>To unite, gather, marry, bind.</translation>
        </marginalia>

        <marginalia anchor_text="Corporeum autem &amp; aspectabile, itémque tractabile omne necesse est esse quod natum est" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Quicquid natu[m] est Corporeu[m] et aspectabile erit.</marginalia_text>
                </position>
            </language>
            <translation>Whatever is born shall be physical and visible.</translation>
        </marginalia>
        
        <marginalia anchor_text="Nihil porro igni vacuu[m] videri potest, nec verò tangi" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Nihil sine igne videtur, aut sine quantitate tangitur</marginalia_text>
                </position>
            </language>
            <translation>Nothing is seen without brightness or is touched without quantity.</translation>
        </marginalia>
        
        <marginalia anchor_text="proportióve" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Proportio</marginalia_text>
                </position>
            </language>
            <translation>Proportion.</translation>
        </marginalia>
        
        <marginalia anchor_text="omnia sint vnum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Omnia vnu[m]</marginalia_text>
                </position>
            </language>
            <translation>All [is] one.</translation>
        </marginalia>
        
        <marginalia anchor_text="solida aute omnia vno medio nunqua[m], duobus semper copulentur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>In sup[er]ficiebus vnu[m] mediu[m] proportionis at in solidis duo</marginalia_text>
                </position>
            </language>
            <translation>There is one center of proportion in areas and two in solid things.</translation>
        </marginalia>
        
        <marginalia anchor_text="inter ignem atque terram, aquam deus animámque poneret eáque inter se compararet, &amp; proportinone coniungeret" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Ignis anima aqua terra } Omnia eleme[n]ta apta proportio[n]e colligata</marginalia_text>
                </position>
            </language>
            <translation>Fire, air, water, earth } All elements are united by a suitable proportion.</translation>
        </marginalia>
        
        <marginalia anchor_text="mundi est corpus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Eleme[n]ta sunt mundi corp[us].</marginalia_text>
                </position>
            </language>
            <translation>The elements are the body of the world.</translation>
        </marginalia>
        
        <marginalia anchor_text="" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Mundus interitus no[n] est naturalis sed Dei nutu.</marginalia_text>
                </position>
            </language>
            <translation>The ruin of the world is not natural but [done] by God's will.</translation>
        </marginalia>
        
        <marginalia anchor_text="morbus eum possit, aut senectus attingere" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Nec Morbus nec senectus p[ossi]t mundum attingere.</marginalia_text>
                </position>
            </language>
            <translation>Neither disease nor old age can touch the earth.</translation>
        </marginalia>
        
        <marginalia anchor_text="formam" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Mu[n]di forma</marginalia_text>
                </position>
            </language>
            <translation>The form of the world.</translation>
        </marginalia>
        
        <marginalia anchor_text="Nec enim oculis egebat, quia nihil extra, quod cerni posset, relictum erat: nec auribus, quia ne quod audiretur quidem: neque erant anima circunfusa extremamundi, vt respirationem requireret. Nec verò desiderabat aut alimenta corporis, aut detractionem confecti &amp; consumpti cibi. neque enim vlla decessio fieri poterat neque accessio, nec verò erat vnde" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Mundus nec oculos nec aures, nec pulmone[m] &amp;c h[abet] nec stomachu[m] nec [...].</marginalia_text> <!--JG: Can't figure out last word-->
                    <!-- FSF: the last word, I suppose, captures "neque enim vlla decessio fieri poterat neque accessio, nec ver&ograve; erat vnde", the last bit in the sequence not summarised yet; it contains an a  -->
                </position>
            </language>
            <translation>The world [does not have] eyes, nor ears, nor lungs, nor a stomach, nor [...] et cetera.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Mundi pabulum</marginalia_text>
                </position>
            </language>
            <translation>The fuel for the world.</translation>
        </marginalia>
        
        <marginalia anchor_text="Motum enim dedit caelo" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Co[e]li Motus.</marginalia_text>
                </position>
            </language>
            <translation>The motion of the sky.</translation>
        </marginalia>
        
        <marginalia anchor_text="mundi est corpus effectum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Corpus Mu[n]di</marginalia_text>
                </position>
            </language>
            <translation>The body of the world.</translation>
        </marginalia>
        
        <marginalia anchor_text="quoad" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>nota hoc quoad.</marginalia_text>
                </position>
            </language>
            <translation>Note until here.</translation>
        </marginalia>

        <marginalia anchor_text="anima" hand="Italian">
            <language ident="EN">
                <position book_orientation="0" place="intext">
                    <marginalia_text>aere</marginalia_text>
                </position>
            </language>
        </marginalia>

        <marginalia anchor_text="qui vnus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="intext">
                    <marginalia_text>factus</marginalia_text>
                </position>
            </language>
            <translation>Deed.</translation>
        </marginalia>
        
        <marginalia anchor_text="septem motibus mentem" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="intext">
                    <marginalia_text>7 planetar[um] sc[ilicet]</marginalia_text>
                </position>
            </language>
            <translation>Of the seven planets, that is</translation>
        </marginalia>
        
        <marginalia anchor_text="decessio fieri poterat neque accessio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>deceßio At acceßio. ]</marginalia_text> 
                </position>
            </language>
            <translation>Departure and approach.</translation>
        </marginalia>
        
