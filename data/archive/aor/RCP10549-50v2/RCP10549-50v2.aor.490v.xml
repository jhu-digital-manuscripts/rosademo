    <page filename="RCP10549-50v2.490v.tif" pagination="432" reader="John Dee" signature="[DDD.viii.[v]]"/>   
    <annotation>

        <marginalia anchor_text="[diuinis duo gene]ra motus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Diuinis a[n]i[m]alib[us] duo insunt genera motus</marginalia_text>
                </position>
            </language>
            <translation>The divine beings have two kinds of emotions.</translation>
        </marginalia>
        
        <marginalia anchor_text="quae infixa caelo non mounetur loco" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>fixa sydera</marginalia_text>
                </position>
            </language>
            <translation>The fixed stars.</translation>
        </marginalia>
        
        <marginalia anchor_text="" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>De planetis loq[ui]tur et eor[um] ut vulgo dicunt passionibus</marginalia_text>
                </position>
            </language>
            <translation>It is said of the planets and theirs as if they usually speak with passions.</translation>
        </marginalia>
        
        <marginalia anchor_text="δαίμουας" hand="English_secretary">
            <language ident="EL">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>δαίμονες</marginalia_text>
                </position>
            </language>
            <translation>Demons.</translation>
        </marginalia>
        
        <marginalia anchor_text="Oceanum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Oceanus.</marginalia_text>
                </position>
            </language>
            <translation>Ocean.</translation>
        </marginalia>
        
        <marginalia anchor_text="deorum ortus" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Deoru[m] duplex genus</marginalia_text>
                </position>
            </language>
            <translation>Twofold kind of gods.</translation>
        </marginalia>
        
        <marginalia anchor_text="Vos qui deorum satu ori estis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Dei or[ati]o ad creaturas suas ca[e]listes.</marginalia_text>
                </position>
            </language>
            <translation>The speech of the god to his heavenly creatures.</translation>
        </marginalia>
        
        <marginalia anchor_text="quasi in currum vniuersitatis" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Co[e]lu[m], currus vniversitatis</marginalia_text>
                </position>
            </language>
            <translation>Heaven, chariot of the universe.</translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="in antiquam partem" type="straight"/>
        <underline language="LA" method="pen" text="ex quo genere ea sunt sidera, quae infixa caelo non mouentur loco, quae sunt animantia, eáque diuina, ob eámque causam" type="straight"/>
        <underline language="LA" method="pen" text="vt suprà" type="straight"/>
        <underline language="LA" method="pen" text="quae traiecto axi" type="straight"/>
        <underline language="LA" method="pen" text="generatores suos" type="straight"/>
        <underline language="LA" method="pen" text="quanquam nec argumentis" type="straight"/>
        <underline language="LA" method="pen" text="deorum satu orti estis" type="straight"/>
        <underline language="LA" method="pen" text="&amp; indissolubiles non potestis" type="straight"/>
        <underline language="LA" method="pen" text="Vt igitur mortali conditione genere[n]tur vos" type="straight"/>
        <underline language="LA" method="pen" text="illa gignatis," type="straight"/>
        <underline language="LA" method="pen" text="imiteminíque vim meam qua me &amp; in vestro ortu" type="double"/>
        <underline language="LA" method="pen" text="vsum esse meministis" type="straight"/>
        <underline language="LA" method="pen" text="erit immortale partem attexitote mortalem: ita orientur animantes quos" type="straight"/>
        <underline language="LA" method="pen" text="issdem, sed à diis secundum sumebat atque etiam tertium" type="straight"/>
        <underline language="LA" method="pen" text="atque ita quasi in currum vniuersitatis imposuit" type="straight"/>
       
