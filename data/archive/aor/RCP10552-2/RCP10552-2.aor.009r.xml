        </marginalia>
        
        <underline language="LA" method="pen" text="Princeps verò VValliae videns se regi Angliae quotidiè inualescenti non posse resistere, pacem petijt &amp; obtinuit              sub hac forma, videlicet, quòd omnes captiui quos hactenus ratione regis Angliae" type="straight"/>
        <underline language="LA" method="pen" text="quatuor cantredorum" type="straight"/>
        <underline language="LA" method="pen" text="Regi &amp; suis haeredibus cum" type="straight"/>
        <underline language="LA" method="pen" text="excepta Insula Angleseya" type="straight"/>
        <underline language="LA" method="pen" text="Insula enim Angleseya" type="straight"/>
        <underline language="LA" method="pen" text="concessa est Principi" type="double"/>
        <underline language="LA" method="pen" text="Et si Princeps sine haerede de corpore suo moreretur, Insula illa in regis Angliae possessione remaneret." type="straight"/>
        <underline language="LA" method="pen" text="quinque Baronum" type="straight"/>
        <underline language="LA" method="pen" text="quia se Principem" type="straight"/>
        <underline language="LA" method="pen" text="aliquos Barones" type="straight"/>
        <underline language="LA" method="pen" text="&amp; suis haeredibus in aeternum" type="straight"/>
        <underline language="LA" method="pen" text="Princeps" type="straight"/>
        <underline language="LA" method="pen" text="habuit enim tres fratres" type="straight"/>
        <underline language="LA" method="pen" text="Ovvenum &amp; Rodericum" type="straight"/>
        <underline language="LA" method="pen" text="Dauid fuga" type="straight"/>
        <underline language="LA" method="pen" text="morem gentis" type="double"/>
        <underline language="LA" method="pen" text="factus in ista" type="straight"/>
        <underline language="LA" method="pen" text="valorem mille librarum" type="straight"/>
        
        <mark method="pen" name="bracket" place="right_margin" text="Forma pacis inter Regem Angliae &amp; Principem VValliae."/>
        
