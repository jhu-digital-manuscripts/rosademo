        <underline language="LA" method="pen" text="sicut potest de iure facere tanquam dominus de feodo suo, quia nos ei homagium nostru[m]" type="straight"/>
        <underline language="LA" method="pen" text="plena potestate" type="straight"/>
        <underline language="LA" method="pen" text="voluntate reddimus ei terram Scotiae" type="double"/>
        <underline language="LA" method="pen" text="gentem totam cum homagijs" type="straight"/>
        <underline language="LA" method="pen" text="factoq[ue]; sigillo communi" type="straight"/>
        <underline language="LA" method="pen" text="Scotiae, praecedente eum semper per vnam dietam Episcopo Dunelmensi" type="straight"/>
        <underline language="LA" method="pen" text="Scotorum tempore coronationis" type="straight"/>
        <underline language="LA" method="pen" text="vti pro throno, vsque VVestmonasterium transtulit illum" type="straight"/>
        <underline language="LA" method="pen" text="caelebrantium cathedram sacerdotum" type="straight"/>
        <underline language="LA" method="pen" text="magnatum Scotiae fidelitates" type="straight"/>
        <underline language="LA" method="pen" text="homagia" type="straight"/>
        <underline language="LA" method="pen" text="hoc literas patentes eorum sigillis" type="straight"/>
        <underline language="LA" method="pen" text="promittimus pro nobis, &amp; haeredibus nostris, sub poena corporum nostrorum &amp; castellorum, ac omnium              quae habere possumus, quod nos seruiemus ei bene &amp; fideliter contra omnes gentes, quae viuere &amp; mori possunt, omnibus vicibus quibus" type="straight"/>
        <underline language="LA" method="pen" text="nostris, &amp; eos" type="straight"/>
        <underline language="LA" method="pen" text="Insuper &amp; iuramus tactis sacrosanctis Euangelijs, &amp; posteà nos omnes, &amp; quilibet nostrum," type="straight"/>
        <underline language="LA" method="pen" text="se secit homagium dicto domino nostro regi Angliae in haec verba." type="straight"/>
        <underline language="LA" method="pen" text="legalitatem seruabo Edvvardo regi Angliae &amp; haeredibus suis, de" type="straight"/> 
        <underline language="LA" method="pen" text="terreno honore, co[n]tra omnes qui possunt viuere &amp; mori:" type="straight"/>
        <underline language="LA" method="pen" text="arma" type="straight"/>
        <underline language="LA" method="pen" text="nec ero in consilio vel auxilio contra eum, vel haeredes suos" type="straight"/>
        <underline language="LA" method="pen" text="sed fideliter recognoscam, &amp; fideliter faciam seruitia, quae pertinent ad tenementum, quod de eo tenere clamito, sic" type="straight"/>
        <underline language="LA" method="pen" text="regni regis Angliae Edvvardi" type="straight"/>
        <underline language="LA" method="pen" text="domini nostri" type="double"/>
        <underline language="LA" method="pen" text="vicesimo quinto" type="straight"/>
        <underline language="LA" method="pen" text="cui mandauit rex, vt omnes tenentes de rege terras aliquas vocaret, &amp; eorum homagia fidelitatésque reciperet." type="straight"/>
        <underline language="LA" method="pen" text="quo[n]dam regem" type="double"/>
        <underline language="LA" method="pen" text="Londonias ad turrim" type="double"/>
        
