            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Robertus vocatur.</marginalia_text>
                    <person name="Rollo"/>
                </position>
            </language>
            <translation>He is called Robert.</translation>
        </marginalia>
         
        <marginalia anchor_text="conuolantes vnanimes ad baptismum" hand="Italian" method="pen">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Dani fiunt Christiani</marginalia_text>
                </position>
            </language>
            <translation>The Danes become Christians.</translation>
        </marginalia>
        
        <marginalia anchor_text="912" hand="Italian" method="pen">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>A[nn]⁰ 912. &amp;c.</marginalia_text>
                </position>
            </language>
            <translation>In the year 912, etc.</translation>
        </marginalia>
        
        <marginalia hand="Italian" method="pen">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>Northmannia et Brytannia tota, Rolloni, a Carolo R. Fr. concessa. A[nn]⁰ 912</marginalia_text>
                    <person name="Rollo"/>
                    <person name="Charles the Simple"/>
                    <location name="Normandy"/>
                    <location name="Brittany"/>
                    <location name="France"/>
                </position>
            </language>
            <translation>The whole of Normandy and Brittany conceded by Charles, King of France, to Rollo. In the year 912.</translation>
        </marginalia>
        
        <marginalia hand="Italian" method="pen">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>Northmania ia[m] sylvosa et vasta fere</marginalia_text>  
                    <location name="Normandy"/>
                </position>
            </language>
            <translation>Normandy is now forested and almost empty.</translation>
        </marginalia>
        
        <marginalia hand="Italian" method="pen">
            <language ident="LA">
                <position book_orientation="0" place="intext">
                    <marginalia_text>Ep[iscop]um sibar, fuga elapsu[m] no[n] rep[er]ie[n]tes </marginalia_text>  
                    <person name="Sibar, Bishop of Evreux"/>          
                </position>
            </language>
            <translation>Without capturing Bishop Sibar, who had escaped in flight.</translation>
        </marginalia>
        
