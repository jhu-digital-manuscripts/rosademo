            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Rihulfi fuga.</marginalia_text>
                    <person name="Rudolph of France"/>
                </position>
            </language>
            <translation>The flight of Rudolph.</translation>
        </marginalia>
        
        <marginalia anchor_text="Pratum belli" hand="Italian" method="pen">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Pratum belli</marginalia_text>
                </position>
            </language>
            <translation>The Meadow wars.</translation>  
            <!-- DO: Not sure if 'Pratum' refers to a specific location, but I can't find it... --> 
            <!-- MB. I think its referening to the name of a historic battle in Normandy. see: http://fmg.ac/Projects/MedLands/NORMANDY.htm. -->
        </marginalia>
        
        <marginalia anchor_text="Sprota" hand="Italian" method="pen">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Sprota Concubina Gulielmi.</marginalia_text>
                    <person name="Sprota"/>
                    <person name="William I Longsword"/>
                </position>
            </language>
            <translation>Sprota the concubine of William.</translation>
        </marginalia>
        
        <marginalia hand="Italian" method="pen">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>Gallica traductio dicit illa[m] fuisse filiam Heberti Comitis senliz</marginalia_text>  
                    <person name="Herbert I, Count of Vermandois"/>
                </position>
            </language>
            <translation>The French translation says that she was the daughter of Herbert, Count of Senlis.</translation>
        </marginalia>
        
        <marginalia anchor_text="Fistanuensis castri" hand="Italian" id="RCP10552-2:499:m0001" method="pen">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>Castru[m] Fistanuense vbi natus est Richardus primus Dux. Nor[thmanniae].</marginalia_text>
                    <person name="Richard I of Normandy"/>
                    <location name="Fecamp"/>
                </position>
            </language>
            <translation>Fecamp where Richard the first Duke of Normandy was born.</translation>
        </marginalia>
        
        <marginalia hand="Italian" method="pen">
            <language ident="LA">
                <position book_orientation="0" place="tail">
                    <marginalia_text>vulgariter hodie Feseant du[m] niter S[anc]t[us] Valeri et stable de grace.</marginalia_text>  <!-- DO: Not sure about this -->
                </position>
            </language>
            <translation>Commonly today </translation>
        </marginalia>
        
