                    <person name="Alan I"/>
                    <person name="Berengar II of Neustria"/>
                    <location name="Brittany"/>
                </position>
            </language>
            <translation>Alan, Berengar] Brittany.</translation>
        </marginalia>
        
        <marginalia anchor_text="terminos sui Ducatus per circuitum dilatare non desistens" hand="Italian" method="pen">
