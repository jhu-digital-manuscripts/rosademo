        <marginalia anchor_text="legem statuit, vt nullus assensum furi praeberet" hand="Italian" method="pen">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Roberti Ducis N[orthmanniae] lex co[n]tra furtum</marginalia_text>
                    <person name="Rollo"/>
                    <location name="Normandy"/>
                </position>
            </language>
            <translation>Robert, Duke of Normandy's law against theft.</translation>
        </marginalia>
        
        <marginalia anchor_text="Gisla sine liberis" hand="Italian" method="pen">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Gisla sine liberis</marginalia_text> 
                    <person name="Gisela of France"/>
                </position>
            </language>
            <translation>Gisela without liberties.</translation>
        </marginalia>
        
        <marginalia anchor_text="iterum repetens sibi copulauit" hand="Italian" method="pen">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Popa denuo recepta ad Ducis thorum.</marginalia_text> 
                    <person name="Poppa of Bayeux"/>
                </position>
            </language>
            <translation>Poppa again retaken to the Duke's bed.</translation>
        </marginalia>
        
        <marginalia anchor_text="Alano &amp; Berengario Britonibus" hand="Italian" method="pen">
