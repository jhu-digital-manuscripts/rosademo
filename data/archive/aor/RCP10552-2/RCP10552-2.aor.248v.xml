        <marginalia hand="Italian" method="pen">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>ID. gallica traductio hanc hugonis magni filiam vbiq[ue] nominat Aninam [est] Agnet</marginalia_text> 
                    <person name="John Dee"/>
                    <person name="Hugh the Great"/>
                    <person name="Emma of Paris"/>
                </position>
            </language>
            <translation>JD. The Gallic translation names everywhere this daughter of Hugh the Great as Anina [is] Agnet.</translation>
        </marginalia>
        
        
        <marginalia anchor_text="filium suum nomine Hugonem" hand="Italian" method="pen">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Hugo filius Hugonis magni dictus Hugo Capetus.</marginalia_text>
                    <person name="Hugh the Great"/>
                    <person name="Hugh Capet"/>
                </position>
            </language>
            <translation>Hugh, the son of Hugh the Great, was called Hugh Capet.</translation>
        </marginalia>
        
        
