        <marginalia anchor_text="Rodulphi Comitis" hand="Italian" method="pen">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>Rodulphus Comes.</marginalia_text>
                    <person name="Rodulf of Ivry"/>
