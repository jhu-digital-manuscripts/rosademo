        <marginalia anchor_text="lex ueluti una sola dictio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>[L]ex velut [u]na dictio.</marginalia_text>
                </position>
            </language>
