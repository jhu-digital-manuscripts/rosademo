    <annotation>
        
        <marginalia anchor_text="Aeneas" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>
                        Vide omnino co[m]me[n]taria Seruij in Æneida virgilij
                    </marginalia_text>
                    <person name="Virgil"/>
                    <person name="Maurus Servius Honoratus"/>
                    <book title="Aeneid"/>
                    <book title="In Vergilii Aeneidem commentarii"/>
                </position>
            </language>
            <translation>
                See entirely [Maurus] Servius's [Honoratus] commentaries on Virgil's Aeneid.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Ascanius" hand="Italian">  
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Aliter dictus Euritala Dionysio Halicarnasseo 
                    </marginalia_text>
                    <person name="Ascanius"/>
                    <person name="Dionysius of Halicarnassus"/>
                </position>
            </language>
            <translation>
                Otherwise called Euritala by Dionysius of Halicarnassus.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Hic condidit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="intext">
                    <marginalia_text>
                        dubito
                    </marginalia_text>
                </position>
            </language>
            <translation>
                I doubt [it].
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        sine liberis. Vt servus vel, <!-- JG: Checked original, note does not continue hereafter -->
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Without children. As a servant or.
            </translation>
        </marginalia>
        
