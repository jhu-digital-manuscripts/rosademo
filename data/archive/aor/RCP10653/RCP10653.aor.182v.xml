    <page filename="RCP10653.182v.tif" pagination="356" reader="John Dee" signature="[Gg iiij[v]]"/>
    <annotation>
        
        <marginalia anchor_text="videtur ad historiae claritatem, vt eius genealogiam breui epilogio perstringamus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Historia illustratur, viroru[m] nobiliu[m] descriptis genealogijs    
                    </marginalia_text>
                </position>
            </language>
            <translation>
                History is illuminated by tracing the genealogies of noble men. <!-- NBW: This last part is likely an ablative absolute, so a more literal translation would be "the genealogies of noble men having been recorded, history is illutrated" -->
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Amazones" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Amazones.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Amazons.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ex Scanza insula, cum suo rege Berig nomine" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Berig Rex Scanza.
                    </marginalia_text>
                    <person name="Berig"/>
                    <location name="Scandza"/>
                </position>
            </language>
            <translation>
                Berig, King in Scandza.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Troianos ex sua stirpe processisse" hand="Italian"> 
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Troianos ex hijs fluxisse  
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The Trojans descended from these.
            </translation>
        </marginalia>
        
