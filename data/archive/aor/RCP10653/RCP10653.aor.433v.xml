    <annotation>
        
        <underline language="LA" method="pen" text="Robertus archiepiscopus Cantuar. pro clero nihil voluit concedere, neque de temporalitate annexa ecclesiae, sine licentia summi pontificis speciali" type="straight"/>
        <underline language="LA" method="pen" text="Rex dedit Eadvvardo filio suo principatum VValliae, quod VVallensibus multum placuit, eò quòd &amp; in VVallia natus esset similiter &amp; Comitatum" type="straight"/>
        <underline language="LA" method="pen" text="per triennium vsurpauit" type="straight"/>
        <underline language="LA" method="pen" text="illis diebus sal terrae" type="straight"/>
        <underline language="LA" method="pen" text="&amp; caput vulgi, in magnam hydropisim ceciderunt" type="straight"/>
        
