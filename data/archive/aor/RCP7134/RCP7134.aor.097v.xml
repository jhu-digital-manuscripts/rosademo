            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Regula[e] pulcherr[ima]e. vide seg.3. aph.93 et seg.6. aph. 60</marginalia_text>
                    <internal_ref>
                        <target ref="RCP7134:495" text="aph.93"/>
                        <target ref="RCP7134:564:m0001" text="aph. 60"/>
