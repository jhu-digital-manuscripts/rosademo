    <annotation>
        
        <marginalia anchor_text="per damnationem" hand="English_secretary"> 
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Damnatio, vt hic capitur.</marginalia_text>
                </position>
            </language>
            <translation>Damnation, as it is captured here.</translation>
        </marginalia>
        
        <marginalia anchor_text="Et scias, quod non negatur transitus de geniture ad mensem, uel ad diem, sine anno, &amp; ita in reliquis, cum damnatio fuerit ualida, aliter non" hand="English_secretary"> 
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Transitus alioq[ui] ordine profligato, no[n] admitteretur //.</marginalia_text>
                </position>
            </language>
            <translation>In some respects the crossing to a deprived order was not allowed.</translation>
        </marginalia>
        
        <marginalia anchor_text="sub terra" hand="English_secretary"> 
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Sub terra/.</marginalia_text>
                </position>
            </language>
            <translation>Under the earth.</translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="ille erat in reuolutione anni robustissimus, &amp; ex quartis illa, in qua damnabitur dispositor loci Lunae" type="straight"/>
