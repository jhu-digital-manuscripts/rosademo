    <page filename="RCP7134.155v.tif" pagination="[148v]" reader="John Dee"/>
    <annotation>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="intext">
                    <marginalia_text>Alij ponunt 20 m[inutae] te[m]poris tardius sed Cardanus fortissimu[m] se posuisse putat.</marginalia_text>
                    <person name="Girolamo Cardano"/>
                </position>
            </language>
            <translation>Others add 20 minutes later, but Cardano reckons that the strongest did it himself .</translation> 
        </marginalia>
        
        <marginalia anchor_text="reueretur, fixa enim adiacet" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>reversa omnino est anno 1554 in [amita] anni</marginalia_text> <!--JG: Unsure--> <!-- FSF: I am not sure either; I think anni is fine -->
                </position>
            </language>
            <translation>It completely turned in the year 1554 in [...] of a year.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="90" place="tail">
                    <marginalia_text>1516. 18.[Feb]. hor </marginalia_text> 
                </position>
                <position book_orientation="90" place="left_margin">
                    <marginalia_text>8. fere</marginalia_text>
                </position>
            </language>
            <translation>February 18, 1516, eight hours, almost.</translation>
        </marginalia>
        
