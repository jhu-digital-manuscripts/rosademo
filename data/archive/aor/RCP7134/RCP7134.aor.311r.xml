    <page filename="RCP7134.311r.tif" pagination="304" reader="John Dee"/>
    <annotation>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>☉ in loco ☾a[e] radicis</marginalia_text>
                    <symbol_in_text name="Sun"/>
                    <symbol_in_text name="Moon"/>
                </position>
            </language>
            <translation>The Sun in the place of the root of the Moon.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>☾ in [septima] do[mus?] hora[e] casus</marginalia_text> <!--JG: Unsure about the word before 'horae'--> <!-- FSF: could it be a do as an abbreviation for domu?; horae is right, I think -->
                    <symbol_in_text name="Moon"/>
                </position>
            </language>
            <translation>The moon in the seventh...</translation> <!--JG: Unsure..-->
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>☉ ♀ ☿ in [octava].</marginalia_text>
                    <symbol_in_text name="Sun"/>
                    <symbol_in_text name="Venus"/>
                    <symbol_in_text name="Mercury"/>
                </position>
            </language>
            <translation>The Sun, Venus, and Mercury in the eight [house].</translation> 
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Asce[n]dit [octava] revolutio[n]is </marginalia_text>
                </position>
            </language>
            <translation>The eight [house] of the revolution ascends.</translation>  
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>☿ o[mn]i tempore infortunatus erat.</marginalia_text>
                    <symbol_in_text name="Mercury"/>
                </position>
            </language>
            <translation>Venus was infortunate in all times.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>D[omi]na as[cendentis]. sub [re] radijs ☉ cum [i]nfortunate ☿i.</marginalia_text> 
                    <symbol_in_text name="Sun"/>
                    <symbol_in_text name="Mercury"/> 
                </position>
            </language>
            <translation>The lady of the ascendant under the [..] rays of the Sun with the misfortune of Mercury.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Signa Infortunij et mortis hoc anno. octava radicis, as.</marginalia_text> 
                </position>
            </language>
            <translation>The signs of misfortune and death in this year. The eigth of the root of the ascendant.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>☾ in [sexta] radicis</marginalia_text>
                    <symbol_in_text name="Moon"/>
                </position>
            </language>
            <translation>The Moon in the sixth of the root.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>♃ in radice et revolutione in [duodecima].</marginalia_text>
                    <symbol_in_text name="Jupiter"/>
                </position>
            </language>
            <translation>Jupiter in the root and revolution in the twelfth.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>□ ☾ et ☉ et ☾ [in sublimi - deleted] loco</marginalia_text> 
                    <symbol_in_text name="Square"/>
                    <symbol_in_text name="Moon"/>
                    <symbol_in_text name="Sun"/>
                </position>
            </language>
            <translation>A square of the Moon and the Sun, and the Moon in the place.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>et ♄ d[omi]n[u]s asce[n]de[n]t[i]s elevatus in figurali tetragono ♀ in in ascendete.</marginalia_text>
                    <symbol_in_text name="Saturn"/>
                    <symbol_in_text name="Venus"/>
                </position>
            </language>
            <translation>Saturn, the lord of the ascendant, elevated in a square shape; Venus in [a square shape] in the ascendant.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>♀ d[omi]na octava[e] in asce[n]dente [pulsata] d[omi]no asce[n]dentis</marginalia_text>
                    <symbol_in_text name="Venus"/>
                </position>
            </language>
            <translation>Venus, the lady of the eight in the ascendant, beaten by the lord of the ascendant.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>♂ circa [septimam] radicis</marginalia_text>
                    <symbol_in_text name="Mars"/>
                </position>
            </language>
            <translation>Mars around the seventh of the root.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>♂ cadit in □⁰ radicalis veneris.</marginalia_text> 
                    <symbol_in_text name="Mars"/>
                    <symbol_in_text name="Square"/>
                </position>
            </language>
            <translation>Mars falls in the square of Venus's root.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>♒ etia[m] signu[m] protectionati est, et [octavii] radicis.</marginalia_text> <!--JG: Unsure about protectionati-->
                    <symbol_in_text name="Aquarius"/>
                </position>
            </language>
            <translation>Even Aquarius is the sign of protection and of the root of the eighth [house].</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Ascendit quadrat[u]s Loci ☾ radicis in 8.</marginalia_text>
                    <symbol_in_text name="Moon"/>
                </position>
            </language>
            <translation>The squared [?] of the place of the root of the Moon ascends in eight.</translation> <!--JG: Very unsure--> <!-- FSF: I don't understand this -->
        </marginalia>
         
