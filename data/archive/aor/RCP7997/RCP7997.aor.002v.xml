        <marginalia anchor_text="demo[n]stratio[n]e" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Demo[n]stratio {in re. circa rem. secu[n]du[m] rem.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Demonstration in a thing, concerning a thing, according to a thing.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="vndecim distinctio[n]es c[on]tinue qua[n]titat[is]" hand="Italian">
