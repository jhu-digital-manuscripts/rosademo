            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Quatuor famosa[e] distinctio[n]es Numeri.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                4 famous distinctions of a number.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="themata" hand="Italian">
