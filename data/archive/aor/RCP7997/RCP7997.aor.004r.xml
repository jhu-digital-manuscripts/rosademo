    <annotation>
        
        <marginalia anchor_text="co[mmun]em termin[um]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Co[mmun]is terminu[s]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A common end.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="linee" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Linea
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Line.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="lo[n]gitudinem sine latitudine cui[us] quide[m] extremitates sunt duo pu[n]cta quor[um] primu[m]             appellat[ur] punctu[m] creat[i]o[n]is. Et alteru[m] lineatio[n]is" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Punctu[m] creatio[n]is lineatio[n]is
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The point of creation and lineation.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ante [et] retro" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Ante:retro
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Before, after.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="S[ed] s[ecundu]m situ[m] [et] positio[n]e[m] motuu[m] variat[ur] respect[us]             sc[ilicet] q[uo]d linea flectit[ur] ad latu[m] ad p[ro]fundu[m] vel ad altu[m] s[ecundu]m q[uo]d postea dicet[ur]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Line[a]e situs [et] positio varia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The various positions and places of a line.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Et hec q[ua]ttuor mot[us] corp[or]is pande[n]s sc[ilicet] ante retro dextru[m] [et] sinistru[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Ante: retro: dextru[m]: sinistru[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Before, after, right, left.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Diffinit[ur] aut[em] a mathematicis h[abe]ns lo[n]gitudine[m] [et] latitudine[m] sine p[ro]funditate cui[us] t[er]mini quida[m] linee sunt" hand="Italian">  <!-- DO: I would just anchor 'sup[er]ficies'? -->
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Superficies.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Area.          <!-- DO: More like 'surface'? --> <!-- FSF: my impression is that superficies is a technical term for area -->
            </translation>
        </marginalia>
        
        <marginalia anchor_text="sup[er]ficies siue plana" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Planum
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Plain.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="a q[ua]ttuor motib[us] corp[or]is" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        infra
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Below.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="q[ua]re quattuor recti in plano no[n] posse surgere in sublimi" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Quatuor recti in plano no[n] possu[n]t surgere in sublimi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Four right [angles] in the plane cannot rise up.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="sc[ilicet] ante retro sinistru[m] dextru[m] sursum [et] deorsum quoru[m] q[ua]ttuor             p[rae]dicti pandu[n]t sup[er]ficie[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Ante retro dextru[m] sinistru[m] sursu[m] deorsu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Before, after, right, left, up, down.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="corpus phisicu[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Corpus physicu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A physical body.       <!-- DO: Not sure about translation. I'd say something like 'a physical structure'?? -->
            </translation>
        </marginalia>
        
        <marginalia anchor_text="viginti a[ua]ttuor rectos" hand="Italian"> 
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        24 recti a[n]guli circu[m]sta[n]tes
                    </marginalia_text>
                </position>
            </language>
            <translation>
                24 right angles all around.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="ad repletione[m] loci" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Loci repletio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Repletion of the place.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="corp[or]is p[ar]ticule sunt sup[er]ficies" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Particula corporis su[n]t corpora: vt line[a]e partes linea[e], na[m] nec ex punctis linea aut ex sup[er]ficiebus corpus.
                        Vt est notissimu[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The components of bodies are bodies, as the parts of a line are lines, for a line does not derive from points, nor does a structure from areas, as is most noteworthy  
                <!-- DO: Technically also 'the parts of a body'. Not sure about the end part, is it something like 'for a line does not derive from a point, or a body from surfaces'? Doesn't really make sense though... I'm also wondering if the printed text is perhaps using 'corpus' as 'structure' in general, hence the picture on the right?-->
            </translation>
        </marginalia>
        
