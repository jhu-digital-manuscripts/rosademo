        <underline language="LA" method="pen" text="positione[m] situm et ordinem" type="double"/>
        <underline language="LA" method="pen" text="ordine[m] na[m]q[ue] habet cu[m] p[ri]mu[m] et vltimu[m] copulant[ur] per t[er]minu[m] co[m]mune[m] qui vtriusq[ue] est co[m]mu[n]e et neutrius per se" type="straight"/>
        <underline language="LA" method="pen" text="na[m] situs e[st] habentiu[m] locum p[er] p[ri]mu[m] mediu[m] et postremu[m] s[e]c[un]d[u]m dime[n]siones siue dista[n]cias" type="straight"/>
        <underline language="LA" method="pen" text="et p[er]manentia[m] p[ar]tiu[m]" type="straight"/>
