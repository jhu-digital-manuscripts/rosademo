    <annotation>
        
        <marginalia anchor_text="magnu[m] [et] paruu[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        magnu[m] et paruu[m] su[n]t relatiua.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Large and small are relative.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="opinione[m] antiquoru[m] ph[ilosoph]or[um]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Antiq[uo]ru[m] philosophoru[m] opinio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The opinion of the old philosophers.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="s[ecundu]m q[uod] dixim[us] locum esse phisice co[n]sideratio[n]is" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Locus physicus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Physical space.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Locus aut[em] mathematice c[on]siderationis p[ro]ut trib[us] dime[n]sio[n]ibus              s[ecundu]m corporis locati dime[n]sio[n]es me[n]surat[ur] no[n] habet c[on]trariu[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Locus mathematicus.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A mathematical locus.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="in de celo" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        In lib. de ca[e]lo
                    </marginalia_text>
                </position>
            </language>
            <translation>
                <!-- FSF: I haven't found it -->
            </translation>
        </marginalia>
        
        <marginalia anchor_text="qua[n]titas p[er] se absoluta [et] no[n] respectiua" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        qua[n]titas {p[er] se absoluta respectiua
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The absolute quantity itself or respectively.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="c[on]traria sibimet deno[m]i[n]atio[n]e" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Co[n]traria denominatione
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A contrary denomination.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Est nanqu[am] inequale in maius [et] minus q[uae] c[on]traria sibimet deno[m]i[n]atio[n]e fingunt[ur]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Quantitas respectiue d[icitu]r a[e]qualis vel ina[e]qualis et no[n] absolute p[er] se, 
                        sed semp[er] in co[m]paratione ad alia[m] qua[n]titate[m] [etc].
                    </marginalia_text>
                </position>
            </language>
            <translation>
               A quantity respectively is said to be equal or unequal and not absolute in itself, but always in comparison to other quantities etc.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Fundant[ur] igit[ur] hec relatiua circa equale [et] ineq[ua]le" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Relatiua fundata circa a[e]quale et ina[e]quale.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Relatives established around the equal and unequal.
            </translation>
        </marginalia>
        
