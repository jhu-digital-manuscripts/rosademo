        <underline language="LA" method="pen" text="si est scibilis" type="straight"/>
        <underline language="LA" method="pen" text="ex quindecima tercij geometrie" type="straight"/>
        <underline language="LA" method="pen" text="quia est" type="straight"/>
        <underline language="LA" method="pen" text="Si quadratura circuli scibilis est necessario ab aduersario dicat[ur] curuum recto vniri per lieam" type="straight"/>
        <underline language="LA" method="pen" text="media et non per equalem angulum rectum" type="straight"/>
        <underline language="LA" method="pen" text="et itur per omnia media et nunq[uam] per equale. S[ed]" type="straight"/>
        <underline language="LA" method="pen" text="si angulus contingentie esset diuisibilis per lineam rectam aliquod scibile esse de quadratura quare dicit ph[ilosoph]us in p[ro]posito si est scibilis" type="straight"/>
        <underline language="LA" method="pen" text="q[uo]d tame[n] non est" type="straight"/>
        <underline language="LA" method="pen" text="multi male opinant[ur] de quadratura et diuersas opiniones" type="straight"/>
        <underline language="LA" method="pen" text="fieri per lineam elicam" type="straight"/>
        <underline language="LA" method="pen" text="cuse" type="straight"/>
        <underline language="LA" method="pen" text="differunt speciebus. Ita etiam figuris" type="straight"/>
        <underline language="LA" method="pen" text="etia[m] est vna figura" type="straight"/>
        <underline language="LA" method="pen" text="Quadratura negati[u]e euclidis" type="straight"/>
        
