        <underline language="LA" method="pen" text="p[ro]pter q[ui]d" type="straight"/>
        <underline language="LA" method="pen" text="esse a priori" type="straight"/>
        <underline language="LA" method="pen" text="p[re]supposit[ur]" type="straight"/>
        <underline language="LA" method="pen" text="eande[m] est impossibile. q[ua]re o[mn]es anguli semicirc[u]lor[um] sunt eq[ua]les" type="straight"/>
        <underline language="LA" method="pen" text="q[uo]d equi arcus equas ang[u]los forma[n]t seu" type="straight"/>
