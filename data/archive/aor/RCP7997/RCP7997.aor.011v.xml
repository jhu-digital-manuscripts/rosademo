    <annotation>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Per sensibiles formas Geometer demo[n]strat suas intellectuales co[n]clusio[n]es
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Through sensible forms the geometer demonstrates its intellectual conclusions.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="mathematic[us] abstrahit" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Mathematica abstractio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Mathematical abstraction.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="termini mathematicales in silogisticis r[ati]onibus, co[n]notatiue recipiu[n]t[ur]" hand="Italian">  
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Termini mathematicales in syllogismis co[n]notatiue accipiu[n]tur
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Mathematical terms are connotatively accepted by syllogisms.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="petitio ei[us] q[uo]d in p[ri]ncipio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Petitio eius q[uod] est in principio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A petition of what has been in the beginning.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="diffinitu[m] aut[em] c[aus]a diffinit[i]o[n]is" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        definitu[m] causa definitionis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Defined by the cause of the definition.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="in abstracto" type="straight"/>
