        <underline language="LA" method="pen" text="quare termini mathematicales in silogisticis r[ati]o[ni]bus. co[n]notatiue recipiu[n]t[ur] na[m] significa[n]t tria[n]gulu[m] hic descriptu[m]" type="straight"/>
        <underline language="LA" method="pen" text="cu[m] solu[m]m[od]o c[on]notat formale[m] dispositionem materie" type="straight"/>
        <underline language="LA" method="pen" text="s[ecundu]m rem [et] no[n] t[a]m[en] s[ecundu]m c[on]cessione[m]" type="straight"/>
