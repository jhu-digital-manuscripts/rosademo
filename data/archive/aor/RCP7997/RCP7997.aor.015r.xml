    <page filename="RCP7997.015r.tif" reader="John Dee" signature="Ciij"/> 
    <annotation>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Per se inesse
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Connecting to itself.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="loco corpus quonia[m] illud in est p[er] se" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Loco corpus inest p[er] se
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The body itself is in a place.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Locus quid
                    </marginalia_text>
                </position>
            </language>
            <translation>
                What place.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="De secundo m[od]o p[er] se" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Secundus modus de p[er] se inesse  
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The second mode to be connected to one's self.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="p[er]fect[us] est nu[m]erus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        perfect[us] Num[erus] 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A perfect number.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="triangulus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Triangulus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A triangle.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Scaleno[n], Isosceles quod accipiuntur se passionibus et no[n] p[ro] speb[us] in hac r[ati]o[n]e dice[n]di p[er] se.
                    </marginalia_text>
                </position>
            </language>
            <translation>
              A scalene triangle [and] and isoscele triangle, which are grasped themselves by passions and not for the hope in this reason of saying through themselves. <!-- Very unsure -->
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="et om[n]i loco corpus q[ua]re" type="straight"/>
        <underline language="LA" method="pen" text="insunt o[mn]i et semp[er] in p[ri]mo modo dicendi per se" type="straight"/>
        <underline language="LA" method="pen" text="est q[ua]n[do] totu[m] p[re]dicatu[m] est q[uod] quid est v[e]l totu[m] vel pars quidditatis subiecti id est" type="straight"/>
        <underline language="LA" method="pen" text="sic linea p[er] se inest triangulo" type="straight"/>
        <underline language="LA" method="pen" text="om[n]i hic" type="straight"/>
        <underline language="LA" method="pen" text="est dici p[er] se" type="straight"/>
        <underline language="LA" method="pen" text="et loco corpus quonia[m] illud inest p[er] se q[uo]d ingredit[ur] eius diffinitione[m] quidditatiua[m] vt linea ingredit[ur]" type="straight"/>
        <underline language="LA" method="pen" text="locus vltima superficies corporis continentis" type="straight"/>
