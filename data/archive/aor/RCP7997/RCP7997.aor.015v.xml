    <annotation>
        
        <marginalia anchor_text="att[ame]n nescim[us] no[m]i[n]are proprios species tria[n]guli, no[m]i[n]am[us] eas p[er] huiusmo[d]i passio[n]es" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Isoceles Isopleuros scalenos} Tria[n]guli su[n]t passiones an numeri
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Isosceles, Equilateral, Scalene} Triangles are properties or numbers.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="sine disiu[n]ctione co[n]uenit am[n]i tria[n]gulo Aliq[ua]n[do] v[er]o p[er] disiu[n]ctione[m] v[el] opposita sicut par v[el] impar dicit[ur] p[ro]pria passio nu[m]eri" hand="Italian"> 
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Prop[ri]a passio {simpl[icite]r, sine disiunctione p[re]dicat[ur] p[er] disiunctione[m] vel opposita 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                An individual property is asserted {simply, without separation; through separation or opposites.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="egidio" hand="Italian">
            <language ident="LA">
