        <underline language="LA" method="pen" text="par v[e]l impar" type="straight"/>
        <underline language="LA" method="pen" text="q[ua]n[do] no[m]i[n]at[ur] vno no[m]i[n]e v[e]l vniuoce" type="straight"/>
