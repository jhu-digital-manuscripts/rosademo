        </marginalia>
        
        <marginalia anchor_text="p[ri]mu[m] est" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        primo inesse
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be in the first one.
            </translation>
