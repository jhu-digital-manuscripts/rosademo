    <annotation>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Euclides in decimo tractavit Lineas, no[n] numeros vt q[ui]dam co[n]te[n]dit
                    </marginalia_text>
                    <person name="Euclid"/>
                </position>
            </language>
            <translation>
                In his tenth [book], Euclid treated lines, not numbers as one asserted.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="linea vt subiectu[m] passio[n]is r[ati]onalis et irr[ati]onalis co[n]siderat[ur] nu[m]ero" hand="Italian">  
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Linea[e] numeri { r[ati]onales irrationales  
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Lines are rational or irrational numbers.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="diuersa subiecta vt diuersa" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        diuersa subiecta vt diuersa
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Different subjects as different.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="genus subalternatu[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        genus subalternatu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A subaltern kind.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="methaphisicu[m]" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Metaphysic[us]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Metaphysics.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Duo cubi vn[us] cubus multiplicatione efficiu[n]tur
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Two cubes/one cube are effected by multiplication.
            </translation>
