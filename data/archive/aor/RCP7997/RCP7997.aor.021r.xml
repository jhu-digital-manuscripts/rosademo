        <underline language="LA" method="pen" text="co[m]mune scie[n]tia[m] q[uae] est methaphisica" type="straight"/>
        <underline language="LA" method="pen" text="hab[et] methaphisica ad om[n]es scie[n]tias qua[n]tu[m] ad hoc q[uod] demo[n]strat p[ri]ncipia co[m]munia" type="straight"/>
        <underline language="LA" method="pen" text="Quare co[n]cludit q[uod] demo[n]stratio no[n] erit ex p[ri]ncipijs extraneis nec ex co[m]munibus. sed ex p[ro]prijs q[uo] dictum est q[uod] no[n]" type="straight"/>
        <underline language="LA" method="pen" text="descendu[n]t in mechanicas v[e]l in machinatiuas" type="straight"/>
        <underline language="LA" method="pen" text="me[n]surantes co[n]tine[n]tia[m] dolioru[m]" type="straight"/>
        <underline language="LA" method="pen" text=" sunt de triangulis" type="straight"/>
        <underline language="LA" method="pen" text="Et hic simplicit[er] loque[n]do no[n] est descensus de genere in gen[us] diuersum. sed in ide[m] [et] subalt[er]nu[m]. p[ri]ncipia e[ni]m" type="straight"/>
        <underline language="LA" method="pen" text="simplicit[er] extranea p[ri]ncipijs" type="straight"/>
        <underline language="LA" method="pen" text="q[uod] illa pri[n]cipia sint resolubilia in p[ri]ma vera immediata" type="straight"/>
        <underline language="LA" method="pen" text="in q[ua]libet ergo scie[n]tia demo[n]stratiua accipit[ur] sine deminstratio[n]e quid significe[n]t p[er] nome[n] diffinitione[m] v[e]l expositione[m]. [et]" type="straight"/>
        <underline language="LA" method="pen" text="vt passio[n]is esse. est inesse subiecto. [et] hoc" type="straight"/>
        
