        <underline language="LA" method="pen" text="Na[m] par [et] i[m]par q[ua]dratus [et] cubicus sunt passiones ip[s]ius nu[m]eri q[ua]s supponit arismetric[us]" type="straight"/>
        <underline language="LA" method="pen" text="irrationalis [et] curuu[m] seu reflexu[m]" type="straight"/>
        <underline language="LA" method="pen" text="p[ro]ut terminat[ur] visibili. q[ua]re geomet[er] demo[n]strat reflexu[m]. quia est. [et] p[er]spectiu[us] p[ro]pter quid" type="straight"/>
        <underline language="LA" method="pen" text="tria[n]gulo q[ue] est [et] quid" type="straight"/>
        <underline language="LA" method="pen" text="S[e]c[un]do p[ro]pria[m] passione[m] subiecti" type="straight"/>
        <underline language="LA" method="pen" text="q[uod] co[n]clusio in vna demo[n]stratione potest esse p[ri]ncipiu[m] i[m]mediatu[m] in alia vt si sit c[on]clusio inte[n]ta p[ro]banda q[uod] om[n]is trianguli angulus" type="straight"/>
        <underline language="LA" method="pen" text="ergo p[ri]ncipia p[ro]pria possunt esse conclus[i]o[n]es demo[n]strationu[m]. ideo" type="straight"/>
        <underline language="LA" method="pen" text="quia p[er] huiusmo[d]i p[ri]ncipia co[m]munia [et] cu[m] p[ri]ncipijs que demo[n]strant[ur]. i[d est] cum p[ri]ncipijs p[ro]prijs demonstrat p[ro]prias passiones" type="straight"/>
        
