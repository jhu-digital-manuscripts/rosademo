    <page filename="RCP7997.024r.tif" reader="John Dee" signature="[Dvj]"/> 
    <annotation>
        
        <marginalia anchor_text="Hic ph[ilosoph]us/ph[isic]us ostendit co[n]seque[n]ter              q[uae] interrogationes sunt p[ro]prie vnicuiq[ue] scie[n]tie Et p[ri]mo ostendit hoc inqua[n]tu[m] assumunt[ur]              vt p[ro]positio[n]es. S[e]c[un]do inqua[n]tu[m] assumu[n]t[ur] vt c[on]clusiones" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Co[n]clusio interrogatio p[ro]p[ositio]        
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Conclusion, investigation, proposition.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="interrogatione[m] vt est co[n]clusio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Interrogatio geometrica vt co[n]clusio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Geometric investigation like a conclusion.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="p[ro]positio" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        vt p[ro]positio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Like a proposition.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="interrogationes sunt p[ro]prie vnicuiq[ue] scie[n]tie" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        Interrogationes quasdam esse proprias cuiusq[ue] scientia[e].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Certain enquiries are suitable for every science.  
            </translation>
        </marginalia>
        
        <marginalia anchor_text="p[ro]prie interrogationes geometrice ex quibus aliquid demo[n]strat[ur] in geometrica [...] S[ed] interrogatio[n]es imp[ro]prie" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        {Interrogationes geometrica[e] propriè loquendo. Geometrica[e] interrogationes impropriè
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Appropriate geometric enquiries, in speaking; inappropriate geometric enquiries.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="demo[n]stratione ad impossibile que[m]admodu[m] ex interrogatio[n]ibus fiunt silogismi q[ue] ex eo q[uod] int[er]rogat[ur] aduersarius [et] ex ea q[uod] aduersarius dat" type="straight"/>
