        <underline language="LA" method="pen" text="modus scie[n]di" type="straight"/>
        <underline language="LA" method="pen" text="vt duo semitonia iu[n]cta no[n] p[er]ficere integru[m] epogdou[m] i[d est] thonu[m]" type="straight"/>
        <underline language="LA" method="pen" text="q[ue] cu[m] sensibile sit auribus " type="straight"/>
        <underline language="LA" method="pen" text="p[ar]te rei scite et hec" type="straight"/>
        <underline language="LA" method="pen" text="Na[m] sepe aliquis cognoscit in v[niversa]li [et] t[ame]n nescit sing[u]laria p[ro]pter hoc q[uod] no[n] inte[n]dit" type="straight"/>
        <underline language="LA" method="pen" text="p[ro]pter sui altitudine[m] co[n]sidera[n]tes v[niversa]les causas. licet scia[n]t p[ro]pter quid ignora[n]t t[ame]n multos effectus [et] multa q[ue] q[uae] no[n] ignora[n]t" type="straight"/>
        <underline language="LA" method="pen" text="subalt[er]na[n]tes habe[n]t se vt v[niversa]le et subalt[er]nate vt p[ar]ticulare" type="straight"/>
        <underline language="LA" method="pen" text="q[ua]re aliq[ua]n[do] multi effect[us] nociores sunt causis v[niversal]ibus" type="straight"/>
        <underline language="LA" method="pen" text="ex p[ar]te scie[n]tie subalt[er]nate q[ue] e[st] materialior [et] sensibilior q[uam] subalt[er]nans" type="straight"/>
