    <page filename="RCP7997.029v.tif" reader="John Dee" signature="[Ev[v]]"/> 
    <annotation>
        
        <marginalia anchor_text="duplices sunt modi p[rae]dicandi p[er] se" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Duo modi predica[n]di p[er] se.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Two methods of predicting in itself.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Et huiusm[od]i silogistice rationes sunt in p[ri]mo m[od]o certitudinis              na[m] p[ro]cedu[n]t ex p[ri]oribus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        huiusmodi su[n]t in p[ri]mo modo certitudinis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Of this kind are [the reasons] in the first method of certainty.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="frequentissime" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>
                        frequentissim√®
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Most frequently.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="p[rae]dicatio v[niversa]lis s[e]c[un]d[u]m q[uod] sub se habet p[rae]dicatione[m] p[er] se" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Pra[e]dicatio vniversalis p[er] se
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Itself a universal prediction.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="nu[m]erus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Numerus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A number.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="multitudo" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Multitudo
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Amount.
            </translation>
        </marginalia>
        
        <underline language="LA" method="pen" text="triangulus causa passio[n]is" type="straight"/>
        <underline language="LA" method="pen" text="subiecto. Et huiusmo[d]i silogistice rationes sunt in p[ri]mo m[od]o certitudinis na[m] p[ro]cedu[n]t ex p[ri]oribus i[d est] a causis ad effectus" type="straight"/>
        <underline language="LA" method="pen" text="rectis q[ua]re p[er] causam scit mathematic[us] effectu[m]" type="straight"/>
        <underline language="LA" method="pen" text="vt effectus siue passio" type="straight"/>
        <underline language="LA" method="pen" text="frequentissime silogisantes in prima figura mathematici silogisant maxime scire" type="straight"/>
        <underline language="LA" method="pen" text="ide[m] analetice i[d est] p[er] rationes" type="straight"/>
        <underline language="LA" method="pen" text="i[d est] ad demo[n]stratiua[m] scie[n]tia[m] p[er]tinet p[rae]dicatio q[uae] est p[er] se. sed ad" type="straight"/>
        <underline language="LA" method="pen" text="p[rae]dicatio v[niuersa]lis" type="straight"/>
        <underline language="LA" method="pen" text="que p[er] se insunt rebus" type="straight"/>
        <underline language="LA" method="pen" text="vt par [et] impar p[rae]dicant[ur] de numero in secu[n]do m[od]o p[er] se" type="straight"/>
        <underline language="LA" method="pen" text="vt nu[m]erus est multitudo ex vnitatib[us] p[ro]fusa" type="straight"/>
        <underline language="LA" method="pen" text="tandu[m] q[uod] multitudo est qua[n]titas ex diuisibilib[us] sep[ar]atis coaceruata et differt a diuisibili s[e]c[un]d[u]m actu[m] Nam" type="straight"/>
        <underline language="LA" method="pen" text="decrescentia[m] multitudo qua[n]titas coaceruata diuisibile v[er]o qua[n]titas decresce[n]da v[e]l segrega[n]da" type="straight"/>
        <underline language="LA" method="pen" text="diuisibile p[ro]ponit actu[m] nu[m]er[um] m[u]ltitudo v[er]o nu[m]er[um] pote[n]ciale[m] augme[n]tandu[m]" type="straight"/>
        <underline language="LA" method="pen" text="Na[m] om[n]is demo[n]stratio h[abet] fieri in p[ri]mo aut secu[n]do m[od]o vt in p[re]missis est ostensum" type="straight"/>
        <underline language="LA" method="pen" text="in per se passio[n]ib[us]" type="straight"/>
        <underline language="LA" method="pen" text="q[uod] subiectu[m] sc[ilicet] nu[m]erus cui statim inest passio erit om[n]ib[us]" type="straight"/>
        <underline language="LA" method="pen" text="q[ue] quicq[ui]d est in r[ati]one diffinie[n]tis est in ratio[n]e diffiniti" type="straight"/>
        
