        <underline language="LA" method="pen" text="c[on]uertunt[ur]" type="triple"/>
        <underline language="LA" method="pen" text="cu[m] c[on]uertibili c[on]uertu[n]t[ur] etia[m] cu[m] eo q[uod] co[n]uertit[ur] cu[m] illo. nullu[m] aute[m]" type="straight"/>
        <underline language="LA" method="pen" text="c[on]tingit q[ui]a talia p[rae]dicata sunt de diffinitio[n]e subiecti [et] si" type="straight"/>
        <underline language="LA" method="pen" text="huiusmo[d]i aut[em] d[iffe]r[enti]am diffinie[n]te[m] no[n] c[on]tingit" type="straight"/>
        <underline language="LA" method="pen" text="c[on]stat ita n[ost]r[u]m p[ro]positu[m] si hoc est q[uod] est stat[us] in sursu[m] [et] deorsum tu[n]c necesse est deuenire ad aliq[uam] p[ri]ma p[ri]ncipia demonstrationu[m]             q[uae] sunt indemo[n]strabilia. q[ue] o[mn]is" type="straight"/>
        <underline language="LA" method="pen" text="o[mn]es passio[n]es ad hoc q[uod] c[on]stituit[ur] sup[er] data[m] linea[m] p[er] p[ri]ma[m] p[ri]mi elementor[um]              hec vltima passio p[er] p[ri]meua p[ri]ncipia sc[ilicet] p[er] diffinitio[n]es petitio[n]es" type="straight"/>
        <underline language="LA" method="pen" text="ani[m]co[n]ceptio[n]es p[ro]babit[ur] quib[us] p[ri]ncipijs vlterius est status in demo[n]stratio[n]e" type="straight"/>
        <underline language="LA" method="pen" text="q[uod] si aliquid insit aliquibus duob[us] paliquod co[m]mune non oportet illud co[m]mune eis inesse p[er] aliud co[m]mune [et]" type="straight"/>
        <underline language="LA" method="pen" text="Om[n]is aut[em] d[e]mo[n]stratio e[st] ex v[niversa]lib[us] vt dixi[m][us] [et] de his q[uae] su[n]t p[er] se [et] no[n] p[er] i[n]duc" type="straight"/>
        
