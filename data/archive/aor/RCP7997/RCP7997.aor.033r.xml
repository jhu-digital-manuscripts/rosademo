        <underline language="LA" method="pen" text="q[uae]stio medij" type="straight"/>
        <underline language="LA" method="pen" text="demo[n]strandu[m] potest accipi aliq[uo]d mediu[m]" type="straight"/>
        <underline language="LA" method="pen" text="questio[n]e quia q[uae]" type="straight"/>
        <underline language="LA" method="pen" text="est ip[s]i[us] medi[j] [et] de o[mn]ib[us] hijs paru[m] e[st] mathematici" type="straight"/>
