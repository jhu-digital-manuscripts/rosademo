                    <marginalia_text>A[n]i[m]a Mundi</marginalia_text>
                </position>
            </language>
            <translation>The soul of the world.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Animi Motus</marginalia_text>
                </position>
            </language>
            <translation>Movement of the soul.</translation>
        </marginalia>
        
