        <marginalia anchor_text="Roma capta dicitur à Senonibus Gallis, anno Vrbis 365" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Tempus Roma[e] Capta[e] per Brennu[m].</marginalia_text>
                    <person name="Brennus of the Senones"/>
                </position>
            </language>
            <translation>The time of the capture of Rome by Brennus.</translation>
        </marginalia>
        
        <marginalia anchor_text="Cyri Persarum regis tempus" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Cyri Persaru[m] R[egis] Tempus.</marginalia_text>
                    <person name="Cyrus the Great"/>
                </position>
            </language>
            <translation>The time of King Cyrus of Persia.</translation>
        </marginalia>
        
        <marginalia anchor_text="hîc annis erratum est centum" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>Error centum annorum</marginalia_text>
                </position>
            </language>
            <translation>An error of one hundred years.</translation>
        </marginalia>
        
