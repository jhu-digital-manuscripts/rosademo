        <underline language="LA" method="pen" text="Semae verò tertio Noe" type="straight"/>
        <underline language="LA" method="pen" text="fuerunt filij quinque, qui Asiam vsque ad Indicum Oceanum incoluerunt" type="straight"/>
        <underline language="LA" method="pen" text="Euphrate propagandae ditionis" type="straight"/>
        <underline language="LA" method="pen" text="suis vniuersum mundum" type="straight"/>
        <underline language="LA" method="pen" text="vsque ad Indiam" type="straight"/>
        <underline language="LA" method="pen" text="filio Iaphet obtigerunt" type="straight"/>
        <underline language="LA" method="pen" text="spectant ad Aquilonem" type="straight"/>
        <underline language="LA" method="pen" text="nihil iuuari" type="straight"/>
        <underline language="LA" method="pen" text="nihil de prima ista inter liberos Noe partitione" type="straight"/>
        <underline language="LA" method="pen" text="totius orbis partem obtinuit Iaphet" type="straight"/>
        <underline language="LA" method="pen" text="cum maiore Asiae" type="straight"/>
        <underline language="LA" method="pen" text="habitet in tabernaculis Sem. q. d." type="straight"/>        
        <underline language="LA" method="pen" text="Ecclesia collecta ex Iudaeis &amp; Gentibus, quatum" type="straight"/>
        <underline language="LA" method="pen" text="Iaphet" type="straight"/>
        <underline language="LA" method="pen" text="in Europam modò &amp; Asiam" type="straight"/>
        <underline language="LA" method="pen" text="Africam" type="double"/>
        <underline language="LA" method="pen" text="tanqua[m] falsum expungendu[m]" type="straight"/>
        
