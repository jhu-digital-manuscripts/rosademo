    <annotation>
        
        <marginalia anchor_text="de auro Schaba" hand="Italian" id="RCPD126.6:263:m0001">
            <language ident="LA">
                <position book_orientation="0" place="head">
                    <marginalia_text>Quid si fuerit de auro Ophir prope Schaba in Oreente?</marginalia_text> 
                    <location name="Ophir"/>
                    <location name="Saba"/>
                </position>
            </language>
            <translation>What if it was about the gold [of?] Ophir near Saba in the Orient?</translation>
        </marginalia>
        
        <marginalia anchor_text="Scheba &amp; Seba; Schaba &amp; Saba" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Schaba et Seba.</marginalia_text>
                    <location name="Saba"/>
                    <location name="Sheba"/>
                </position>
            </language>
            <translation>Saba and Sheba.</translation>
        </marginalia>
        
        <marginalia anchor_text="Iosephi" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Iosephus. Regina[m] Æthiopia[e] vult esse [Quae?] ad [?] venit</marginalia_text>  <!-- DO: Can't make some of this out, and not sure about translation --> 
                    <!-- FSF: I am not sure either; the first word missing ends with a d, maybe quid?, and the second one starts sat, I think -->  <!-- DO: Second one 'Salomonem' or 'Salomone[m]'? -->
                    <person name="Titus Flavius Josephus"/>
                    <person name="Queen of Sheba"/>
                    <person name="Solomon"/>
                </position>
            </language>
            <translation>Josephus. She wants to be Queen of Ethiopia, who came to Solomon.</translation>
        </marginalia>
        
        <marginalia anchor_text="Quàm etiam fallantur qui istam regionem Heuila, sunt interpretati Indiam" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Ista Seba no[n] est in India.</marginalia_text>
                    <location name="Sheba"/>
                    <location name="India"/>
                </position>
            </language>
            <translation>That Sheba is not in India.</translation>
        </marginalia>
        
        <marginalia anchor_text="Ptolomaeo" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="right_margin">
                    <marginalia_text>Ptoloma[e]o</marginalia_text>
                    <person name="Ptolemy"/>
                </position>
            </language>
            <translation>By Ptolemy.</translation>
        </marginalia>
        
