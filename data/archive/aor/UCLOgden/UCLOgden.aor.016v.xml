    <annotation>
        
        <marginalia anchor_text="unum masculinum est, aliud foemininum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Nota, semper altera[m] domor[um] masculina[m] esse alteram foemininam
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Note that there is always one of the houses masculine and another feminine.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="intext">
                    <marginalia_text>domicilia ♄</marginalia_text>
                    <symbol_in_text name="Saturn"/>
                </position>
            </language>
            <translation>The dwelling of Saturn.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="intext">
                    <marginalia_text>dom[us] ♃</marginalia_text>
                    <symbol_in_text name="Jupiter"/>
                </position>
            </language>
            <translation>The house of Jupiter.</translation>
        </marginalia>
        
        
