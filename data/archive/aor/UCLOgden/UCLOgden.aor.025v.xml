    <page filename="UCLOgden.025v.tif" pagination="34" reader="John Dee"/>
    <annotation>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Dispositor aliqua[n]do qua[m] magnu[m] vim habet.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                How sometimes a dispositor has a great force. 
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Comparatio status planetaru[m] ad n[ost]ros actus:
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Comparison of the status of the planets with our deeds.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="nocturnis" hand="Italian">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Diurna nocturnis, nocturna diurnis locis/
                    </marginalia_text>
                </position>
            </language>
            <translation>At day with nightly places, at night with daily places.</translation> <!--JG: Unsure-->
        </marginalia>
        
        <marginalia anchor_text="Genituarum qualitates, quae scilicet nobiles , quae'ue mediocres nuncupentur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Genitura mediocris
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Average birth.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="Prope autem deorum accedet felicitate[m], qui quatuor stellas, in domiciliis suis habeuerit constitutas, ultra hunc numerum, humani generis stubstantia no[n] patitur" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Praeter sole[m] et luna[m] vltra quatuor stellas in suis domibus esse humani generis substa[n]tia no[n] patetur.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Beyond the sun and the moon, it is not well not that there are four more stars in their houses with a substance of the human kind.
            </translation>
        </marginalia>
        
