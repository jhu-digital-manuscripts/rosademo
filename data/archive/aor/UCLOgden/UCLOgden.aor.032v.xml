    <annotation>
        
        <marginalia anchor_text="per MC in omnibus genituris possidet principatum" hand="Italian"> 
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        MC: in genituris principatum tenet.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Midheaven hold the highest point in the constellation.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Qua r[ati]o[n]e et platice sicut hic multa decesa et partiliter docet 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                For this reason and generally, it teaches here many settled things in the same way.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        Primogenitus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The first born.
            </translation>
        </marginalia>
        
        <marginalia anchor_text="♂ alium geniturae possederit cardinem, uel in anaphora cardinis fuerit, multorum malorum significat aduentum" hand="English_secretary">
            <language ident="LA">
                <position book_orientation="0" place="left_margin">
                    <marginalia_text>
                        ♂ in angulo aliq[uo] vel in anaphora.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Mars in some angle or in rising.
            </translation>
        </marginalia>
        
