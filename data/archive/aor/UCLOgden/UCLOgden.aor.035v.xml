        <underline language="LA" method="pen" text="♃, quodam subtili artificio saepe pollentes, aut aurifices" type="straight"/>
        <underline language="LA" method="pen" text="autem si fuerit partiliter in horosc. constitutus, sine radiatione ♃, faciet largiores uiros, plus uolentes" type="straight"/>
        <underline language="LA" method="pen" text="graues" type="straight"/>
        <underline language="LA" method="pen" text="qui quicquid ausi fuerint nulla potuerunt ratione co[m]plere" type="straight"/>
        <underline language="LA" method="pen" text="uxores etiam" type="straight"/>
