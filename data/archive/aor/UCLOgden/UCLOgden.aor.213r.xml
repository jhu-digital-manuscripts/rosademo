    <annotation>
        
        <underline language="LA" method="pen" text="coelum'q[ue] ac sidera seruant" type="straight"/>
        <underline language="LA" method="pen" text="Quis dubitet post haec hominem coniungere coelo?" type="straight"/>
        <underline language="LA" method="pen" text="Mitto quod certum est &amp; ineuitabile fatum. Materiae'q[ue] datum est cogi, sed cogere" type="straight"/>
        <underline language="LA" method="pen" text="mundo" type="double"/>
        <underline language="LA" method="pen" text="Aeternum &amp; stellis" type="double"/>
        <underline language="LA" method="pen" text="aduersus sidera bellum" type="straight"/>
        <underline language="LA" method="pen" text="Ni tantos animis oculos natura dedisset. Cognatam'q[ue] sibi mentem uertisset ad ipsum" type="straight"/>
        <underline language="LA" method="pen" text="Ipsa fides operi faciet pondus'q[ue] fidem'q[ue]. Nam neq[ue] decipitur ratio, nec decipit unquam. Rite secunda uia              est, ac ueris credita causis. Euentus'q[ue] datus, qualis praedicitur ante. Quod fortuna ratum faciat, quis dicere falsum Audeat? &amp; tantae" type="straight"/>
        <underline language="LA" method="pen" text="Hoc quoq[ue] fatorum est legem perdiscere fati" type="straight"/>
        
