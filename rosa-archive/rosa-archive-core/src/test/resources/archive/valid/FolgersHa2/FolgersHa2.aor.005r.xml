    	<underline method="pen" type="straight" language="IT" text="un galant'huomo"/>
    	<underline method="pen" type="straight" language="IT" text="per Borgomastro"/>
    	<underline method="pen" type="straight" language="IT" text="maggior puttaniere"/>
    	<underline method="pen" type="straight" language="IT" text="in quell'ufficio"/>
    	<underline method="pen" type="straight" language="IT" text="quel che il popolo diceua del Borgomastro"/>
    	<underline method="pen" type="straight" language="IT" text="quel che si facciano, o dicano gli altri"/>
    	<underline method="pen" type="straight" language="IT" text="lauorare nel campo"/>
    	<underline method="pen" type="straight" language="IT" text="priuato, amo la pace"/>
    	<underline method="pen" type="straight" language="IT" text="gli huomini saui di questo mondo"/>
    	<underline method="pen" type="straight" language="IT" text="piu uolte ingannato"/>
    	<underline method="pen" type="straight" language="IT" text="a uendere il cacio"/>
    	<underline method="pen" type="straight" language="IT" text="il Borgomastro"/>
    	<underline method="pen" type="straight" language="IT" text="e' non &egrave; molto"/>
    	<underline method="pen" type="straight" language="IT" text="da mia madre"/>
    	<underline method="pen" type="straight" language="IT" text="era un gran ruffiano"/>
    	<underline method="pen" type="straight" language="IT" text="uituperato una mia parente"/>
    	<underline method="pen" type="straight" language="IT" text="quei due barri"/>
    	<underline method="pen" type="straight" language="IT" text="non &egrave; molto che"/>
    	<underline method="pen" type="straight" language="IT" text="furono affogati"/>
    	<underline method="pen" type="straight" language="IT" text="le truffe, che"/>
    	<underline method="pen" type="straight" language="IT" text="il Borgomastro se n'and&ograve;"/>
     
