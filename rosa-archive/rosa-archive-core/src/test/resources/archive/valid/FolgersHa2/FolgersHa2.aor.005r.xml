    	<underline method="pen" type="straight" language="IT" text="dolendosi del cancelliere, con dire"/>
    	<underline method="pen" type="straight" language="IT" text="Il cancelliere, inform&ograve;"/>
    	<underline method="pen" type="straight" language="IT" text="cancelliere, dicendogli"/>
    	<underline method="pen" type="straight" language="IT" text="una carta di uillania"/>
    	<underline method="pen" type="straight" language="IT" text="cancelliere"/>
    	<underline method="pen" type="straight" language="IT" text="non rispose altro"/>
    	<underline method="pen" type="straight" language="IT" text="non"/>
    	<underline method="pen" type="straight" language="IT" text="due"/>
    	<underline method="pen" type="straight" language="IT" text="andosi"/>
    	<underline method="pen" type="straight" language="IT" text="Non si guadagna nulla"/>
    	<underline method="pen" type="straight" language="IT" text="trauagliarsi co' pazzi"/>
