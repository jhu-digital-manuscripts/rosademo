    <annotation>
        
        <!-- Marginalia -->
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                      Singula ex arte Decori.
                    </marginalia_text>
                </position>
            </language>
        	<translation>
        	From the single art of propriety.
        	</translation>
        </marginalia>
    	<marginalia hand="Italian">
    		<language ident="EN">
    			<position place="left_margin" book_orientation="90">
    				<marginalia_text>
    					Pronunciation most pliant to the matter vttered: &amp; the liueliest
    					interpreter, that euer explaned anie speach bie apt vtterance.
    				</marginalia_text>
    				<emphasis method="chalk" type="straight"/>
    			</position>
    		</language>
    	</marginalia>
    	<marginalia hand="Italian">
    		<language ident="LA">
    			<position place="intext" book_orientation="0">
    				<marginalia_text>
    					su[pra]
    				</marginalia_text>
    			</position>
    		</language>
    		<translation>
    			above
    		</translation>
    	</marginalia>
    	<marginalia hand="Italian">
    		<language ident="IT">
    			<position place="intext" book_orientation="0">
    				<marginalia_text>
    					/ 160. discretamente.
    				</marginalia_text>
    			</position>
    		</language>
    		<translation>
    			/ 160. discreetly.
    		</translation>
    	</marginalia>
    	
    	<marginalia hand="Italian">
    		<language ident="EN">
    			<position place="intext" book_orientation="0">
    				<marginalia_text>
    					) no more scribling: but enjoy the excellent, &amp; diuine notes,
    				</marginalia_text>
    				<emphasis method="chalk" type="straight"/>
    			</position>
    			<position place="right_margin" book_orientation="0">
    				<marginalia_text>
    					which you haue allreddi written.
    				</marginalia_text>
    			</position>
    		</language>
    	</marginalia>
    	<marginalia hand="Italian">
    		<language ident="LA">
    			<position place="tail" book_orientation="0">
    				<marginalia_text>
    					Quid pro quo,
    				</marginalia_text>
    			</position>
    		</language>
    		<language ident="EN">
    			<position place="tail" book_orientation="0">
    				<marginalia_text>
    					will supplie anie defects, anie
    					sooner doon, then saide, a sure course.
    				</marginalia_text>
    			</position>
    		</language>
    		<translation>
    			Something for something,
    		</translation>
    	</marginalia>
    	<marginalia hand="Italian">
    		<language ident="EN">
    			<position place="right_margin" book_orientation="270">
    				<marginalia_text>
    					His voice, a viue commentarie to whatsoeuer he speaketh,
    					or readeth. A speaking Glasse: or talking picture.
    				</marginalia_text>
    				<emphasis method="chalk" type="straight"/>
    			</position>
    		</language>
    	</marginalia>
        
        <!-- Underlines -->
        
    	<underline method="pen" type="straight" language="IT" text="Cardinal Rucellai che era"/>
