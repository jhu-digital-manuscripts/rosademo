                <position place="head" book_orientation="0">
                    <marginalia_text>
                      Lett Eutrapelus teach all men to speake sensibly;
                    </marginalia_text>
                	<person name="Eutrapelus"/>
                	<emphasis method="chalk" type="straight"/>
                </position>
            	<position place="right_margin" book_orientation="0">
