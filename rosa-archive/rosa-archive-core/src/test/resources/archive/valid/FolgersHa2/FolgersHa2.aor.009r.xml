    		<language ident="EN">
    			<position place="intext" book_orientation="0">
    				<marginalia_text>
    					Continual | emproouement of eueri part. 
    				</marginalia_text>
    				<emphasis method="chalk" type="straight"/>
    			</position>
    		</language>
    	</marginalia>
