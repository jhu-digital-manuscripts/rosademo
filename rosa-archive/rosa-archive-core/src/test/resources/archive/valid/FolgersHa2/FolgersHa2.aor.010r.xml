    	<underline method="pen" type="straight" language="IT" text="quante piu parole"/>
    	<underline method="pen" type="straight" language="IT" text="magnificaua il suo ufficio"/>
    	<underline method="pen" type="straight" language="IT" text="qualche bugia"/>
    	<underline method="pen" type="straight" language="IT" text="lodaua il parlare di ciascuno"/>
    	<underline method="pen" type="straight" language="IT" text="Questa risolutione"/>
    	<underline method="pen" type="straight" language="IT" text="prolungata lungo"/>
    	<underline method="pen" type="straight" language="IT" text="per tormentargli piu"/>
    	<underline method="pen" type="straight" language="IT" text="tutti con la speranza"/>
    	<underline method="pen" type="straight" language="IT" text="con la paura"/>
    	<underline method="pen" type="straight" language="IT" text="primo Ca[n]celliere"/>
    	<underline method="pen" type="straight" language="IT" text="il Re"/>
    	<underline method="pen" type="straight" language="IT" text="Costui piu sauio de gli"/>
    	<underline method="pen" type="straight" language="IT" text="non predicaua gli uffici suoi"/>
    	<underline method="pen" type="straight" language="IT" text="spettatore della comedia"/>
    	<underline method="pen" type="straight" language="IT" text="il mio Cancelliere"/>
    	<underline method="pen" type="straight" language="IT" text="non domanda cosa"/>
    	<underline method="pen" type="straight" language="IT" text="ne predica gli uffici suoi"/>
    	<underline method="pen" type="straight" language="IT" text="dalla cortesia"/>
    	<underline method="pen" type="straight" language="IT" text="assai"/>
    	<underline method="pen" type="straight" language="IT" text="piu di quel, che io meritaua"/>
    	<underline method="pen" type="straight" language="IT" text="non come io possa rispondere"/>
    	<underline method="pen" type="straight" language="IT" text="alla liberalit&agrave; sua"/>
    	<underline method="pen" type="straight" language="IT" text="uoglia chiedere cosa alcuna"/>
    	<underline method="pen" type="straight" language="IT" text="tu solo fra tutti non hai"/>
    	<underline method="pen" type="straight" language="IT" text="uostra cortesia, Sire"/>
    	<underline method="pen" type="straight" language="IT" text="che io non n'habbia bisogno"/>
    
        
