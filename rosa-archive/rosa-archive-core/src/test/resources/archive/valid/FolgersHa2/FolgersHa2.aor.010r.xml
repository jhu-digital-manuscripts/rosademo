    <page filename="FolgersHa2.010r.tif" pagination="339" reader="Harvey"/>	<!-- ABO 14right -->
    <annotation>
        
        <!-- Marginalia -->
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                      Vita Eudromi.
                    </marginalia_text>
                	<person name="Eudromus"/>
                </position>
            </language>
        	<translation>
        		The life of Eudromus.
        	</translation>
        </marginalia>
    	<marginalia hand="Italian">
    		<language ident="LA">
    			<position place="head" book_orientation="0">
    				<marginalia_text>
    					( polytechnia, et polypraxia.
    				</marginalia_text>
    			</position>
    		</language>
    		<translation>
    			<!-- CG: These words seem to be neologisms for Harvey. Even if they might have had a Greek meaning,
    			Harvey seems to have an idiosyncratic meaning for them. Perhaps one of the specialists on Harvey could
    			provide a translation? --> <!--JG: I'll email it to the group-->
    		</translation>
    	</marginalia>
    	<marginalia hand="Italian">
    		<language ident="IT">
    			<position place="tail" book_orientation="0">
    				<marginalia_text>
    				( solo Cancelliere, politico. )	
    				</marginalia_text>
    			</position>
    		</language>
    		<translation>
    			( only Chancellor, politician. ) <!--JG: You can start the translation with a capital 'O'-->
    		</translation>
    	</marginalia>
    	<marginalia hand="Italian">
    		<language ident="LA">
    			<position place="tail" book_orientation="0">
    				<marginalia_text>
    					&Ocirc; quot minuta studia? heu egregia quam pauca?
    				</marginalia_text>
    			</position>
    		</language>
    		<translation>
    			O how many insignificant studies? Alas, how few distinguished studies?
    		</translation>
    	</marginalia>
    	<marginalia hand="Italian">
    		<language ident="LA">
    			<position place="left_margin" book_orientation="90">
    				<marginalia_text>
    					Spiritus intus alit: spiritus intus agit. Vnicismus ab Vnico:
    					non &agrave; multis.	
    				</marginalia_text>
    			</position>
    		</language>
    		<translation>
    			The spirit nourishes within: the spirit acts within. Uniqueness from a single thing:
    			not from many things.
    		</translation>
    	</marginalia>
    	<marginalia hand="Italian">
    		<language ident="LA">
    			<position place="right_margin" book_orientation="90">
    				<marginalia_text>
    					Discere Summa; exquirere Summa; retexere Summa;
    					Summa docere; Sequi, extendere, vita mea est. }
    				</marginalia_text>
    			</position>
    		</language>
    		<translation>
    			To learn the highest things; to scrutinize the highest things; to reveal the highest things;
    			to teach the highest things; to follow, to exert myself, this is my life. }
    		</translation>
    	</marginalia>
        
        <!-- Underlines -->
        
    	<underline method="pen" type="straight" language="IT" text="sia un Re molto ricco"/>
    	<underline method="pen" type="straight" language="IT" text="doue spender&ograve; io"/>
