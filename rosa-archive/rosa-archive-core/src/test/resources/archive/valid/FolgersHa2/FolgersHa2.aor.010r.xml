    	<underline method="pen" type="straight" language="IT" text="gli beeua con gli occhi"/>
    	<underline method="pen" type="straight" language="IT" text="non racconti tu quel"/>
    	<underline method="pen" type="straight" language="IT" text="in seruigio mio"/>
