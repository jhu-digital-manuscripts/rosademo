    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor_20141118.xsd">
    
    <page filename="Ha2.001v.tif" pagination="322" reader="Harvey"/>    <!-- ABO 006left -->
    <annotation>
        <!-- Marginalia -->
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        Non Cura, sed remedium. [-] q[ue] iam nunc.
                    </marginalia_text>
                    <!-- Piece of page missing. -->
                </position>
            </language>
            <translation>
                Not the care, but the cure. And [-] just now.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="intext" book_orientation="0">
                    <marginalia_text>Recta linea statim ferit metam: seu</marginalia_text> 
                </position>
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>theorica[m], seu practicam.</marginalia_text>
                </position>
            </language>
            <translation>
                Straight lines immediately hit the target: whether theoretical, or practical.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="IT">
                <position place="intext" book_orientation="0">
                    <marginalia_text>
                        ridicolo.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                ridiculous.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="intext" book_orientation="0">
                    <marginalia_text>
                        ) Quicquid alij dole[nt] aut languent; Eudromus semper alacer, et firmus.
                    </marginalia_text>
                    <person name="Eudromus"/>
                </position>
            </language>
            <translation>
                The more others grieve or languish; Eudromus is always eager, and constant.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="intext" book_orientation="0">
                    <marginalia_text>
                    Interpocula, aut epulas nunq[uam] hebetior Eutrapelus,
                    </marginalia_text>
                    <person name="Eutrapelus"/>
                </position>
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>sed semper viuidior.</marginalia_text>
                </position>
            </language>
            <translation>
                During drinks and meals Eutrapelus is never duller, but always more lively.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="IT">
                <position place="intext" book_orientation="0">
                    <marginalia_text>discretamente.</marginalia_text>
                </position>
            </language>
            <language ident="LA">
                <position place="intext" book_orientation="0">
                    <marginalia_text>Sit sua gloria, non mea.</marginalia_text>
                </position>
            </language>
            <translation>
                discreetly. Let it be his glory, not mine.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="intext" book_orientation="0">
                    <marginalia_text>Vilis conuiua, qui hebescit vino, aut cibo.</marginalia_text>
                </position>
            </language>
            <translation>
                A guest of little value, who is dulled by wine, or food.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        Qui non est homo omnium horarum,
                        asinus est exceptarum horarum.
                    </marginalia_text>
                    <X-ref person="Erasmus" book_title="Adages" language="LA" text="Omnium horarum homo"/> <!-- Adages I, iii, 88: "Omnium horarum homo." Referring to Thomas More. -->
                </position>
            </language>
            <translation>
                Whoever is not a man for all hours is a donkey for occasional hours.    
            </translation>
        </marginalia>
      

            <!--Left margin, orientation 270-->
        <marginalia hand="Italian">
            <language ident="EN">
                <position place="left_margin" book_orientation="270">
                    <marginalia_text>
                        Force your industrie, according to your
                        conceit: choose not so manie instigations
                        of heaven.
                        </marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <!--Right margin, orientation 270-->
        <marginalia hand="Italian">
            <language ident="EN">
                <position place="right_margin" book_orientation="270">
                    <marginalia_text>
                        Euerie his saying, &amp; doing beginnes, proceedes, &amp; endes with a Plaudite.
                        </marginalia_text>
                </position>
            </language>
        </marginalia>

