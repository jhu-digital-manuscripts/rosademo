	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>Qui Venerem nescit, nescit mollitiem, teneritatem,
						lassitudinem, aestus languorem,  
					</marginalia_text>
					<person name="Venus"/>
				</position>
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						frigoris asperitatem, inediam, 
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						sitim, multas lascivorum a[e]gritudines, plurima
					</marginalia_text>
				</position>
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						a&ecirc;ris, vigiliarum, aliorumq[ue] accidentium incommoda, quae
					</marginalia_text> 
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						luxuriosis videntur peracerba, et prop&egrave; intolerabilia.
					</marginalia_text>
				</position>
			</language>
			<translation>
				He who does not know Venus, does not know softness, tenderness, weariness, 
				the fatigue of agitation, the harshness of cold, starvation, thirst, the many afflictions
				of wanton men, the most troublesome things of money, sleeplessness, and other 
				circumstances, which seem very harsh and nearly intolerable to immoderate men. 
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="IT">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						ridicolo.)
					</marginalia_text>
				</position>
			</language>
			<translation>
				Ridiculous.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						Moll[io] dura omnia: |duro durum nihil. Iucundo
						nihil acerbius: acerbo nihil iucundius.
					</marginalia_text>
				</position>
			</language>
			<translation>
				I soften all hard things: I harden nothing hard. I make agreeable nothing harsher:
				I make disagreeable nothing more pleasant.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						He is the tallest man, that hath the tallest minde, &amp; the highest Witt.                         
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="IT">
				<position place="intext" book_orientation="0">
					<marginalia_text>discretamente.</marginalia_text>
				</position>
			</language>
			<translation>
				Discreetly.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">	
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						distinctio ciuilis plurimi in mundo vsus.
					</marginalia_text>
				</position>
			</language>
			<translation>
				The civil distinction of the greatest use in the world. <!-- CG: I'm really unsure about this translation. --> <!--JG: I'll ask Lisa as well-->    
			</translation>
		</marginalia>
		
		<marginalia hand="Italian" anchor_text="Pronto.">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						dextr&egrave;.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Skillfully.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						prose quisq[ue], quarto modo.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Whoever in prose, in the fourth mode. <!-- CG: I really haven't a clue to what this might be. --> <!--JG: Beats me; I might just send an email to the group after having collected several Latin phrases about which we are unsure-->
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						| He is a noble man, that hath a noble minde, &amp; an honorable Witt.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Quotidie assuescere, summum robusti, agilisq[ue] viri bonum.
					</marginalia_text>
				</position>
			</language>
			<translation>
				To accustom one's self daily, the highest good of the robust and agile man.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						s[upr]a )
					</marginalia_text>
				</position>
			</language>
			<translation>
				[See] above.
			</translation>
		</marginalia>
		
