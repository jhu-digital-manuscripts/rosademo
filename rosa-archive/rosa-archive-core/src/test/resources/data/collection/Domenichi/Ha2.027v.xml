	<page filename="Ha2.027v.tif" pagination="374" reader="Harvey"/>	<!-- ABO 032left -->
    	<annotation>
    		
            	<!-- Marginalia -->
    		
        		<marginalia hand="Italian">
        			<language ident="IT">
            			<position place="head" book_orientation="0">
                    				<marginalia_text>
                    					Eccellentissimo del mondo: et singolarissimo 
                      			</marginalia_text>
            				<emphasis method="chalk" type="straight"/>
            			</position>
        				<position place="left_margin" book_orientation="0">
        					<marginalia_text>
        						del suo Tempo.
        					</marginalia_text>
        					<emphasis method="chalk" type="straight"/>
        				</position>
        			</language>
        			<translation>
        				The most excellent in the world, and most singular of his time.
        			</translation>
        		</marginalia>
        		<marginalia hand="Italian">
        			<language ident="LA">
        				<position place="left_margin" book_orientation="0">
        					<marginalia_text> 
        						&Ocirc; digniore: &ograve; viliore. 
        					</marginalia_text>
        				</position>
        			</language>
        			<translation>
        				O, more worthy; o, more vile.
        			</translation>
        		</marginalia>
    		<marginalia hand="Italian">
    			<language ident="LA">
    				<position place="left_margin" book_orientation="0">
    					<marginalia_text>
    						Aulicoru[m] decentiss[im]us. Pragmaticorum subtilissimus:
    						industrioru[m] diligentiss[im]us omniu[m] facil&egrave; 
    						tam pra[e]stantiss[imus] qu&agrave;m candidissimus.
    					</marginalia_text>
    					<emphasis method="chalk" type="straight"/>
    				</position>
    			</language>
    			<translation>
    				The most decent of courtiers. The most subtle of pragmatic men:
    				the most diligent of all industrious men, easily as most excellent as
    				he is most radiant.
    			</translation>
    		</marginalia>
    		<marginalia hand="Italian">
    			<language ident="IT">
    				<position place="intext" book_orientation="0">
    					<marginalia_text>
    						ridicolo.
    					</marginalia_text>
    				</position>
    			</language>
    			<translation>
    				Ridiculous.
    			</translation>
    		</marginalia>
    		<marginalia hand="Italian">
    			<language ident="IT">
    				<position place="intext" book_orientation="0">
    					<marginalia_text>
    						) Tutto per il meglio.
    					</marginalia_text>
    				</position>
    			</language>
    			<translation>
    				) Everything for the best.
    			</translation>
    		</marginalia>
    		<marginalia hand="Italian">
    			<language ident="LA">
    				<position place="tail" book_orientation="0">
    					<marginalia_text>
    						Enteleches
    					</marginalia_text>
    					<person name="Enteleches"/>
    				</position>
    			</language>
    			<language ident="EL">
    				<position place="tail" book_orientation="0">
    					<marginalia_text>
    						ἐν ἀκμῆ, <!-- Greek: en axme -->
    					</marginalia_text>
    				</position>
    			</language>
    			<language ident="LA">
    				<position place="tail" book_orientation="0">
    					<marginalia_text>
    						et Euschem&ocirc;n in flor&eacute;: nunq[uam] sua[e] oblitus dignitatis.
    					</marginalia_text>
    					<person name="Euschemon"/>
    				</position>
    			</language>
    			<translation>
    				Enteleches at his peak, and Euschemon in his prime: never forgetful of his own dignity.
    			</translation>
    		</marginalia>
       
       		<marginalia hand="Italian">
       			<language ident="LA">
       				<position place="right_margin" book_orientation="270">
       					<marginalia_text>
       						Tam pura Theurgia, qu&agrave;m mera Entelechia: aut
       					</marginalia_text>
       				</position>
       			</language>
       			<language ident="ES">
       				<position place="right_margin" book_orientation="270">
       					<marginalia_text>
       						Todos es nada.
       					</marginalia_text>
       				</position>
       			</language>
       			<translation>
       				Both pure theurgy and unmixed actualization: or everything is nothing.
       			</translation>
       		</marginalia>
    		
