					<marginalia_text>
						<!-- Continued from facing page, head, Ha2.060v. -->
						The first Euschemonist
					</marginalia_text>
				</position>
			</language>
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						per Totum.
					</marginalia_text>
				</position>
			</language>
			<translation>
				The first Euschemonist all together.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="IT">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						argutamente. ridicolo. 
					</marginalia_text>
				</position>
			</language>
			<translation>
				Cleverly. Ridiculous.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						) The onlie pragmatician of art, &amp; vertu.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						A posse ad Esse,
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<language ident="EN">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						who but HEE?
					</marginalia_text>
				</position>
			</language>
			<translation>
				From possibilty to being, who but HEE?
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Laudo hominem Semper familiarem; nunq[uam] vulgarem. Quotidi&egrave; lucet Sol: sed adhuc Sol. 
					</marginalia_text>
				</position>
			</language>
			<translation>
				I praise the man who is always familiar, but never vulgar. The sun shines daily, but it is still the sun. 
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="90">
					<marginalia_text>
						Imperfecta[e] Artis, et Virtutis Minuit pra[e]sentia famam: Perfecta[e], auget.						
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<language ident="IT">
				<position place="right_margin" book_orientation="90">
					<marginalia_text>
						Sempre piu oltre; sempre &ograve; Migliore, &ograve; Niente.
					</marginalia_text>
				</position>
			</language>
			<translation>
				The presence of an imperfect art and virtue diminishes fame: the presence of a perfect art increases it.
				Always further and further; always either better or nothing at all.
			</translation>
		</marginalia>
		
		
