	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Historia egregia, et singularis.
					</marginalia_text>
				</position>
			</language>
			<translation>
				A distinguished and singular history.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="IT">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						discretamente.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Discreetly.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						) Non est
						doctus, qui non est sua bibliotheca.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Whoever is not in his library is not learned.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Noster etiam Proteus in quaslibet formas conuertibilis eodem die:
						ter, decies, vicies, centies; quoties, quotiesque opus.
					</marginalia_text>
					<person name="Proteus"/>
				</position>
			</language>
			<translation>
				Likwise our Proteus is changeable into whatever forms on the same day:
				three times, ten times, twenty times, one hundred times; as often as is useful.
			</translation>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="90">
					<marginalia_text>
						Mutata ratione, aut occasione; mutanda opinio, affectio, actio.
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				In the case of a changed reason or occasion, an opinion, purpose, and action must be changed.
			</translation>
		</marginalia>
		
