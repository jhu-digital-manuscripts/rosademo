	<page filename="Ha2.064v.tif" pagination="448" reader="Harvey"/>	<!-- ABO 69left -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Quotidianum gallicinium: Salue festa dies.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Daily at dawn: hail the festive day.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						He is noboddie, that hath not his witt, &amp; toung at will.
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Sera nimis vita est crastina: viue hodi&egrave;.
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				Tomorrow's life is too late: live in the present.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						( Qui poterit sanum fingere, sanus erit. )
					</marginalia_text>
				</position>
			</language>
			<translation>
				Whoever can feign health will be healty.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Panacea.
					</marginalia_text>
					<emphasis method="pen" type="straight" text="Panacea"/>
				</position>
			</language>
			<translation>Panacea.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Viuidus exultet nocte, dieq[ue] vigor.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Let the lively vigor jump about by night and day. 
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Plurima quotidi&egrave; circunfluunt: qua[e] uix (
						ego stans pede in vno.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Many things flow around daily, due to which
				I can barely stay upright on my feet.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="left_margin" book_orientation="270">
					<marginalia_text>
						Virgil refined Ennius: a second Pindar refined
						Virgil. That Pindar may still be refined.
					</marginalia_text>
					<person name="Virgil"/>
					<person name="Ennius"/>
					<person name="Pindar"/>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="90">
					<marginalia_text>
						In cespite paucorum Principiorum latet lepus plurimorum Experimentorum.
					</marginalia_text>
					<X-ref person="Edmund Spenser" book_title="Three proper, and wittie, familiar letters" language="LA" text="In exiguo quandoque cespite latet lepus"/>
				</position>
			</language>
			<translation>
				In the short grass of small beginnings hides the hare of many experiences.
			</translation>
		</marginalia>
		
