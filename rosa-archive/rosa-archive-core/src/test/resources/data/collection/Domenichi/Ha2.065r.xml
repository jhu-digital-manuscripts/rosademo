	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Quid prudentia[e], aut fortitudinis?
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						quantu[m] valoris?
						Veteru[m] excessibus plus cumulatur:
						hodierni defectus nusq[uam] supplentur. 
						Quod opus no[n] est, asse carum est.
					</marginalia_text>
					<X-ref person="Seneca" book_title="Epistles" language="LA" text="Quod opus non est, asse carum est."/>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				What of prudence or fortitude? How much is valor?
				It grows more with the excesses of the ancients,
				today's defects are never supplied. <!-- CG: Unsure of translation, specialist help required. -->
				"What you do not want is dear at a farthing." 
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						sals&egrave; ad mensam
						non philosophicam.
					</marginalia_text>
					<X-ref person="Anonymous" book_title="Mensa philosophica"/>
				</position>
			</language>
			<translation>
				Wittily, according to an unphilosophical measure.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="IT">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						( O migliore; o niente. )
					</marginalia_text>
				</position>
			</language>
			<translation>
				Either better, or nothing at all.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						apt&egrave;.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Appropriately.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Quid h&icirc;c, illic 
					</marginalia_text>
				</position>
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						subtile,
					</marginalia_text>
				</position>
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						aut
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						profundum? quid mirabile, aut potens? quid singulare,
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						aut
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						rarum?
					</marginalia_text>
				</position>
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						Nam
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						vbiq[ue] vulgarium rerum satis, superq[ue]. (
					</marginalia_text>
				</position>
			</language>
			<translation>
				What is subtle here or profound there? What is amazing or powerful? What is singular or exceptional?
				Indeed everywhere there are enough vulgar matteres, actually beyond enough. 
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Quod nimium est, adhuc abundat:
						quod parum est, adhuc deficit.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Whatever is excessive, still overflows;
				whatever is insufficient, is still left wanting. 
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Multi canales scholasticaru[m] regularu[m]: pauca[e] concha[e] pragmaticaru[m] maximarum.
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				Many are the canals of scholastic rules, but few are the holy-water fonts of pragmatic maxims. 
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Deo uero dedicatissima creatura:
						summo Regi addictissimus
						subditus: ca[e]teris, ut decet,
						officiosus:
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
				<position place="tail" book_orientation="270">
					<marginalia_text>
						sibi ipsi, 
						ut refert,
						intentissimus, Eutrapelus.
					</marginalia_text>
					<person name="Eutrapelus"/>
				</position>
			</language>
			<translation>
				Eutrapelus: truly a creature most dedicated to God,
				a subordinate most devoted to the highest kingdom;
				he is dutiful to others, as is proper,
				and as concerns himself, most strict.
			</translation>
		</marginalia>
		
