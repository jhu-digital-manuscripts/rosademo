	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Solus in acerbo dulcis; vt Chrysanthius.
					</marginalia_text>
					<person name="Chrysanthius"/>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				Sweet in bitterness alone; like Chrysanthius.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						diuinum Mnemosynon.
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				A divine reminder.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						( The youthfull age of Proa[e]resius, &amp; Chrysanthius: the pring of the Winter.
						But
					</marginalia_text>
					<person name="Prohaeresius"/>
					<person name="Chrysanthius"/>
					<emphasis method="chalk" type="straight"/>
					<emphasis method="chalk" type="straight"/>
				</position>
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						no such springing Winter, as the great Apollonius Tyaneus.
					</marginalia_text>
					<person name="Apollonius of Tyana"/>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="IT">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Giouio. Critico arrogante.
					</marginalia_text>
					<person name="Paolo Giovio"/>
				</position>
			</language>
			<translation>
				Giovio. An arrogant critic.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						( Mundus quotidi&egrave; repuerascit, infinita generatione.
					</marginalia_text>
				</position>
			</language>
			<translation>
				By infinite generation, the world becomes a child again daily.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="IT">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						ridicolo.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Ridiculous.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Musa[e], filia[e] Jouis, et Mnemosynes: =
					</marginalia_text>
				</position>
				<position place="tail" book_orientation="0">
					<marginalia_text>
						= id est, diuini ingenij, et memoria[e].
					</marginalia_text>
				</position>
			</language>
			<translation>
				The Muses are daughters of Jove and Mnemosyne; that is, of divine genius and memory.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Follow the best: Pardon the rest.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="90">
					<marginalia_text>
						Hic, et ille infinita loqu[u]ntur:
						sed ubi inueniam practicum?
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				This one and that one say infinite things, but where will I find a practical man?
			</translation>
		</marginalia>
		
